    margin:140px auto 0; background-color:#f9f9f9; text-align:center; width:620px;
    -webkit-border-radius:8px; -moz-border-radius:8px; border-radius:8px; -webkit-box-shadow:0px
    0px 15px rgba(0, 0, 0, 0.3); -moz-box-shadow:0px 0px 15px rgba(0, 0, 0,
    0.3); box-shadow:0px 0px 15px rgba(0, 0, 0, 0.3); border:3px solid #ddd\9;
    /* IE8 and below */ } #yt-popup-overlay .popup .popup-header {
    font-weight: bold; font-size: 20px; padding:10px 10px 15px; } #yt-popup-overlay .popup .popup-header
    a.close { font-size:16px; line-height:19px; color:#999; display:block;
    padding-right:24px; float:right; background:transparent url('//cdn.oreillystatic.com/oreilly/images/close-icon-19x19.png')
    right 0 no-repeat; } #yt-popup-overlay .popup .popup-header a.close:hover
    { color:#666; text-decoration:none; background:transparent url('//cdn.oreillystatic.com/oreilly/images/close-icon-19x19.png')
    right -100px no-repeat; }  #yt-popup-overlay .popup
    .popup-header p { margin:0 40px; padding:0; font-size:20px; line-height:28px;
    font-weight:normal; } #yt-popup-overlay .popup-window { margin:10px; text-align:left;} #yt-popup-overlay #yt-popup-frame {display:block; border:none; padding:0; margin:0 auto; width:600px; height:335px;
    text-align:left;}
</style>
<div id="yt-popup-overlay">
<div id="yt-popup" class="popup">
<div class="popup-header"><a class="close" href="#">close</a></div>
<div class="popup-window">&nbsp;</div>
</div>
</div>
<script type="text/javascript">
$(function() {
    var ytPopupOverlay = $('#yt-popup-overlay'),
        ytPopup = $('#yt-popup'),
        ytPopupClose = ytPopup.find('a.close'),
        ytPopupLoad = 0;

    ytPopupOverlay.prependTo('body');

    $('#yt-popup-frame').load(function() {
        if (ytPopupLoad < 2) {
            ytPopupLoad++;
        } else {
            $('#yt-popup-frame').css('opacity', 0);
            //$('#yt-popup-overlay').animate({ opacity: 0 }, 200);
            ytClosePopup();
        }
        //console.log(ytPopupLoad);
    });

    function startYTPopup(conf) {
        var ytPopup = $('#yt-popup');
        ytShowPopup(); 
    } //startYTPopup()

    function ytClosePopup() {
        ytPopupOverlay.animate({
            'opacity': 0
        }, 300, function() {
            ytPopupOverlay.css('display', 'none');
            $('#yt-popup-frame').remove();
        });
        ytPopupLoad = 1;
    } //ytClosePopup()
    ytPopupOverlay.click(function() {
        ytClosePopup();
    });

    ytPopupClose.click(function(e) {
        e.preventDefault();
        ytClosePopup();
    });

    $(document).keydown(function(e) {
        if (e.which == 27) { //escape key
            ytClosePopup();
        }
    });

    ytPopup.click(function(event) {
        event.stopPropagation();
    });

    function ytShowPopup() {
        ytPopup.css('margin-top', ($(window).scrollTop() + 140) + "px");
        ytPopupOverlay.css({
            'opacity': 0,
            'display': 'block',
            'height': jQuery('html').height()
        }).animate({
            'opacity': 1
        }, 200);
        $('#yt-popup-frame').css({"opacity": 1, "background": "white url('http://cdn.oreillystatic.com/en/assets/1/eventprovider/1/loading-hex999_120x40.gif') center center no-repeat"});
    } //ytShowPopup()
    $('a.yt_link').on('click', function(evt) {
        var conf_video = $(this).data('url'),
            selected_div = $('#popup_content');
        evt.preventDefault();
        startYTPopup();
        $('.popup-window').append('<iframe src="'+ conf_video +'" id="yt-popup-frame" frameborder="0" height="335" width="600"></iframe>') 
        
    });
});
</script>
 










 

<script>
$(function() { 
         /* init nav */
         $("#top_level_nav #nav").superfish();

         /*$('#stay_informed iframe').attr('src', 'http://oreilly.com/conferences/bulletin_form.html?conference=strata&locationSource=strata14_ca_widget&colorLink=d3002d&colorText=eee'); */
        $('#stay_informed iframe').attr('src', 'http://cdn.oreillystatic.com/en/assets/1/eventprovider/1/email_signup_widget.htm?conference=strata&locationSource=strata14_ca_widget&colorLink=d3002d&colorText=eee'); 

         /* make links with class=external open up external */
         $("a.external").click(function (e) {
            e.preventDefault();
            window.open($(this).attr("href"));
         }); 

         /* accordian mainly used on speaker resources */
  		 $(".accordion .module h2").accordion({clickTitle: 'h2', slideTime: '30'}); 

        
             /* accordian used on live stream */
             $(".accordion .module h4").accordion({clickTitle: 'h4', slideTime: '30'});
        

        
		
		/* for video embed in widgets row */
		$('#video iframe').attr('src', '//www.youtube.com/embed/videoseries?list=PL055Epbe6d5YX_cD2IDr4dK9Da0Vh6YgI');  
        
});
</script>







<!--lazy load images -->
<script type="text/javascript" charset="utf-8">
	( function( $ ) {
		if ( typeof $.proxy == 'undefined' ) {
			$.proxy = function( callback, context ) {
				var proxy = function() {
					callback.apply( context, arguments );
				}
			
				return proxy;
			};
		}
		
		var vController = function() {
			this.init();
		};
		
		vController.prototype = {
			init: function() {
				var that = this;
				var s = $( '[data-defer="view"]' );
			
				s.each( function() {
					var t = $( this ), p = t.parent(), w = Number( t.attr( "data-img-width" ) ), h = Number( t.attr( "data-img-height" ) );
					p.css( { width: w, height: h } );
				});
			
				this.objs = [];
				var w = $( window ), top = w.scrollTop(), bottom = top + w.height(), i;
				
				s.each( function() {
					var t = $( this ), p = t.parent(), off = p.offset();
					var o = {
						a: t,
						p: p,
						top: off.top,
						bottom: off.top + p.innerHeight()
					};
					
					if ( o.top < bottom && o.bottom > top ) {
						that.load( o );
					}
					else {
						that.objs.push( o );
					}
				});
				
				$( window ).scroll( $.proxy( this.scroll, this ) );
			},
			
			scroll: function() {
				var w = $( window ), top = w.scrollTop(), bottom = top + w.height(), i;
				
				for ( i = this.objs.length - 1; i >= 0; i-- ) {
					if ( this.objs[i].top < bottom ) {
						this.load( this.objs[i] );
					}
				}
			},
			
			load: function( o ) {
				var t = o.a;
				var c = $( "<img />" );
				var f = function() { t.html( c ); };
			
				c.load( f );
				c.attr( "src", t.attr( "data-img-src" ) );
				c.attr( "alt", t.attr( "data-img-alt" ) );
				c.attr( "border", 0 );
			}
		};
		
		$( function() {
			var vC = new vController();
			
		})
	}) ( jQuery );
</script>

