function OrmShowPopup() {

	var ormPopupOverlay = jQuery('#orm-bg-overlay');
	var ormPopup = jQuery('#orm-popup');
	var ormPopupClose = jQuery('#orm-popup').find('a.close');

	ormPopupOverlay.prependTo('body').css({'opacity':0, 'display':'block', 'height':jQuery('html').height()}).animate({'opacity':1}, 200);

	ormPopupOverlay.click(function() {
		ormPopupOverlay.animate({'opacity':0}, 200, function() {
			ormPopupOverlay.css('display','none');
		});
	});
	ormPopupClose.click(function(e) {
		e.preventDefault();
		ormPopupOverlay.animate({'opacity':0}, 200, function() {
			ormPopupOverlay.css('display','none');
		});
	});

	ormPopup.click(function(event) {
		event.stopPropagation();
	});
}

// Display popup on click
jQuery(document).ready(function() {
	$('#newsletter-sample').click(function(e) {
		e.preventDefault();
		OrmShowPopup();
	});
});

