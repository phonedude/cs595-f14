<script type="text/javascript">

(function(){

  var compiledTemplate0 = '';


window.NATIVEADS = window.NATIVEADS || {};
window.NATIVEADS.injectedAt = new Date().getTime();
window.NATIVEADS.onReady = function(ads) {

  ads.setPropertyID("NA-WIRE-11236120");
  ads.setSecondaryPageURL("/sponsored");

  // get the section name from the dart zone for targeting, i.e. design, gadgetlab
  // remove the trailing semicolon
  var dartzone = CN.dart.get('zone').replace(";", "");

  ads.insertPreview({
    label: "Right rail",
    unit: {
      "server":"dfp",
      "id": "/3379/wiredcom.dart/" + dartzone,
      "size":"290x95",
      "targets": {
        "category":"news",
        "placement":"top"
      }
    },
    location: "#promo-item-count3",
    infoText: "",
    infoButtonText: "",
    template: compiledTemplate0,
    onRender: function($element) { console.log('Right rail Polar rendered.')},
    onFill: function(data) { console.log(data) },
    onError: function(error) { console.log('There was a problem with ' + error)}
  });

  ads.injectCSS(" ", "head");


  ads.configureSecondaryPage({
    binding: {
      sponsor: {
        link: "#sponsor-link",
        logo: "#sponsor-logo",
        name: "#sponsor-name"
      },
      title: "#title",
      summary: "#summary",
      content: "#content",
      author: "#author",
      pubDate: "#pub-date",
      image: {
        href: "",
        caption: "#media-caption",
        credits: "#media-credits"
      }
    },
    onFill: function(data) { console.log(data);},
    onRender: function() { console.log('Sponsored article rendered.'); },
    onError: function(error) { console.log('Something went wrong with ' + error); },
    track: function() { }
  });

};

  /*

   This function represents a pre-compiled Handlebars template. Pre-compiled
   templates are not pretty, but they provide a very significant performance
   boost, especially on mobile devices. For more information, see
   http://handlebarsjs.com/precompilation.html.

   Note that this code has been generated from the following markup:

