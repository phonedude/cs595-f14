            <script>
                $( document ).ready(function() {

                    // User Menu tooltips.
                    $("#menu_dashboard_link").tooltip({ offset: [10,0], position: "bottom center", delay: 20});
                    $("#menu_newsletters_link").tooltip({ offset: [10,0], position: "bottom center", delay:20});
                    $("#menu_clockblog_link").tooltip({ offset: [10,0], position: "bottom center", delay: 20});

                    
    

    
        // Load and present the featured video.
$(".lp_trailer_link").click(function() {
    $(".longphoto_block").animate({height:489},1000, 'easeInOutCubic');
    $(".longphoto_block .image_rollup").animate({ height: 'toggle', opacity: 'toggle' }, 1000, "easeInOutCubic");
    $('.longphoto_container').html('<embed src="http://d143w2ttjfsl7l.cloudfront.net/widgets/mediaplayer-5.9-licensed-viral/player.swf" bgcolor="000000" height="489" width="870" allowscriptaccess="always" allowfullscreen="true" flashvars="&controlbar=over&file=http://streaming.longnow.org.s3.amazonaws.com/SALT-Trailer-02007.mp4&plugins=viral-2&autostart=true" />');
    $('.reset_longphoto_link').show();
});

// Unload the featured video and return to default state.
$('.reset_longphoto_link').click(function() {
    $(".longphoto_block .image_rollup").animate({ height: 'toggle', opacity: 'toggle' }, 1000, "easeInOutCubic");
    $(".longphoto_block").animate({height:231},1000, "easeInOutCubic");
    $('.longphoto_container').html('');
    $(this).hide();
});

    

    $('.sponsor_link').click(function(event) {
        event.preventDefault();
        $(this).slideUp();
        $('.sponsor_details').slideDown('slow', function() {});
    });



    var audio_loaded = false;
    var current_wrapper = "full-video";

    function scrollMediaWrapper(obj, mode) {
        $('#seminar_media_bar div div').removeClass('current');
        $(obj).addClass('current');

        if (mode == "full-audio") {
            $('#audio_wrapper').slideDown("slow");
            $('#video_wrapper').slideUp("slow");
            $('#highlight_wrapper').slideUp("slow");
        }

        if (mode == "full-video") {
            $('#highlight_wrapper').slideUp("slow");
            $('#audio_wrapper').slideUp("slow");
            $('#video_wrapper').slideDown("slow");
            $('#audio_wrapper').html('');
        }

        if (mode == "highlight-video") {
            $('#highlight_wrapper').slideDown("slow");
            $('#audio_wrapper').slideUp("slow");
            $('#video_wrapper').slideUp("slow");
            $('#audio_wrapper').html('');
        }
    } // End scrollMediaWrapper


    

    
        $(".full_audio").click(function(event) {
            event.preventDefault();
            scrollMediaWrapper(this, "full-audio");
                $('#audio_wrapper').html('<iframe width="100%" height="166" scrolling="no" frameborder="no" src="http://w.soundcloud.com/player/?url=http%3A%2F%2Fapi.soundcloud.com%2Ftracks%2F141418585&amp;auto_play=false&amp;show_artwork=false&amp;color=2fa3bc"></iframe>');
            $('#audio_wrapper iframe').addClass('presentation_shadow');
        });
    

    
        $(".full_video").click(function(event) {
            event.preventDefault();
            scrollMediaWrapper(this, "full-video");
            $('#video_wrapper iframe').addClass('presentation_shadow');
        });
    


    // Content area (About, Introduction, Summary) is made scrollable.
    $(function() {
        $("#reader_wrapper").scrollable().navigator({
            navi: "ul.nav",
            naviItem: 'li.navi',
            activeClass: 'active',
            history: false
        }).autoheight();
        var api = $("#reader_wrapper").data("scrollable");
        $(window).load(function() {
          
            api.seekTo(2, 0);
          
        });
    });


    // Initiate the lazyloaders so the few screenshots that are in view
    // on initial load get displayed.
    //$(function() {
    //    // Discoverbar
    //    $('img').lazyload({container: $('#discover_wrapper')});
    //});

    // You could add the callback and the input value and display that
    // as the row's main header text. Or beside it.
    //
    // $('#search_input').fastLiveFilter('#search_list', {
    //     timeout: 200,
    //     callback: function(total) { $('#num_results').html(total); }
    // });
    //
    // fastLiveFilter for row 1
    $('#search_input').fastLiveFilter('#search_list', {
        callback: function(total) { $('#num_results1').html(total); }
    });

    // The scroll position is reset to zero.
    var scrollToZero = function() {
        $('#discover_wrapper').scrollTop(0);
    };


    // When we type into the input field in row 1 (fastLiveFilter)
    // we let lazyloader know that the screenshots are coming
    // into view and that we want to load their image data.
    $('#search_input').keyup(function() {

       // Display or hide the clear-input button.
       if ($(this).val() != "") {
           $('.clear-input').animate({opacity:1 }, 50);
       } else {
           $('.clear-input').animate({opacity:0 }, 50);
       }

       // The scroll position is reset to zero.
       scrollToZero();

       // Lets give lazyload a kick...
       $("img").lazyload({container: $("#discover_wrapper")});

    });

    // If the clear-input icon is clicked, we clear the input value
    // and hide the clear-input button.
    $('.clear-input').click(function() {

        // Hide the clear-input button.
        $('#search_input').val("").trigger('change');
        $(this).animate({opacity:0 }, 50);

        // The scroll position is reset to zero.
         scrollToZero();
    });



    // Discover bar scrolling. This should be made DRY.
    var d = 0;
    $("#discover_down").mouseenter(function(){
        d = 1;
        (function slide() {
            if (d == 1) {
              $('#discover_wrapper').animate({scrollTop: '+=10px'}, 100, 'linear', slide);
            };
        }());
    });
    $("#discover_down").mouseleave(function(){d = 0;});

    var u = 0;
    $("#discover_up").mouseenter(function(){
        u = 1;
        (function slide() {
            if (u == 1) {
              $('#discover_wrapper').animate({scrollTop: '-=10px'}, 100, 'linear', slide);
            };
        }());
    });
    $("#discover_up").mouseleave(function(){u = 0;});




    $(".download_popover").tooltip({
        offset: 0,
        delay: 400,
        tipClass: 'salt_downloads_popover'
    });


    





                });
            </script>

        

        

