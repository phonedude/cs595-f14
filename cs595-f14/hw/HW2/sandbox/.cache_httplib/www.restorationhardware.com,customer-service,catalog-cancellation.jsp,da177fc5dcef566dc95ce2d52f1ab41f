		<script>	
			//helper methods to make it all a little clearer ;-)
			function rhcm_conversionStartEventTag(pEventId, pCategoryId, pPoints, pAttributes, pExtra) {
				rhcm_conversionEventTag(pEventId, 1, pCategoryId, pPoints, pAttributes, pExtra);
			}
			
			function rhcm_conversionCompleteEventTag(pEventId, pCategoryId, pPoints, pAttributes, pExtra) {
				rhcm_conversionEventTag(pEventId, 2, pCategoryId, pPoints, pAttributes, pExtra);
			}
			
			//pEventId - maps to the coremetrics event id
			//pAction - maps to the coremetrics action type
			//pCategoryId - maps to the coremetrics Event Category Id
			function rhcm_conversionEventTag(pEventId, pAction, pCategoryId, pPoints, pAttributes, pExtra) {
			
				try {
					cmCreateConversionEventTag(pEventId, pAction, pCategoryId, pPoints, pAttributes, pExtra);
				}
				catch(e) {
					
				}
			}
			
			function rhcm_elementTag(pElementId, pCategory, pAttributes){
				
				try {
					cmCreateElementTag(pElementId, pCategory, pAttributes);
				}
				catch(e){
					
				}
			}

            function rhcm_displayShop5s() {
                cmDisplayShop5s();
            }

			function rhcm_pageviewTag(pPageId, pCategory, pSearchTerm, pSearchResults, pAttributes, pExtra){
				
				try {
					cmCreatePageviewTag(pPageId, pCategory, pSearchTerm, pSearchResults, pAttributes, pExtra);
				}
				catch(e){
					
				}
			}

            // don't use this, except to support legacy calls..should be removed
            function rhcm_createPageviewTag(pageId, searchString, categoryId, numResults){
                cmCreatePageviewTag(pageId, searchString, categoryId, numResults);
            }

			function rhcm_productViewTag(pProductId, pProductName, pCategoryId, pAttributes, pVCategory){
				
				try {
					cmCreateProductviewTag(pProductId, pProductName, pCategoryId, pAttributes, pVCategory);
				}
				catch(e){
				
				}
			}
