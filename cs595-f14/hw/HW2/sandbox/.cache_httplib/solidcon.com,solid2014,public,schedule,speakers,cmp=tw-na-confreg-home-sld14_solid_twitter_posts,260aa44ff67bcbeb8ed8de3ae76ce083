<script>
$(function() {
    var filterBtns = $('.filter_button');
    filterBtns.last().addClass('filtering');
    filterBtns.each( function() {
          $(this).on('click', function(evt) {
         	evt.preventDefault();
         	var speaker_type = $(this).text();
                filterBtns.removeClass('filtering');
                $(this).addClass('filtering');
         	//clear filter first
          	$('div.en_speaker').show();
                if (speaker_type != 'All Speakers') {
          	  $('div.en_session_title').each( function() { 
                   if ($(this).data('sessionType') != speaker_type) { 
                      $(this).parent().hide(); 
                   } 
                 });
               }//if speaker_type!=all speakers
    	});
   
	});
});
</script>

