(function(DM) {
  var elId = 'mini-carousel-wrapper',
      config = {pageSize: 6, pageCount: 4, rotate: false};
  if (typeof DM.Rotator === 'function') { new DM.Rotator(elId, config); }
  else { DM.has(elId, 'Rotator', config) };
}(DM));
</script>

