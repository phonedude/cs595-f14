		$(document).ready(function() {
			if(mql) {
				removeStuff();
				addStuff();
			}
		});

		function addStuff() {
			var newNav = '<div id="newNavContainer"><div id="mobile_topics"><a href="/topics">TOPICS</a></div></div>';

						var topicsDrop = '<div id="mobile_topics_menu"><ul><li><a href="/topics/data-analytics">Data Analytics</a></li><li><a href="/topics/ehrs-meaningful-use">EHRs &amp; Meaningful Use</a></li><li><a href="/topics/hardware-devices">Hardware &amp; Devices</a></li><li><a href="/topics/health-information-management">Health Information Management</a></li><li><a href="/topics/hr-workforce-management">HR &amp; Workforce Management</a></li><li><a href="/topics/legal-regulatory-risk-management">Legal / Regulatory / Risk Management</a></li><li><a href="/topics/patient-care-and-outcomes">Patient Care &amp; Outcomes</a></li><li><a href="/topics/patient-engagement">Patient Engagement</a></li></ul><ul><li><a href="/topics/privacy-hipaa-compliance">Privacy / HIPAA Compliance</a></li><li><a href="/topics/telehealth-e-health">Telehealth &amp; e-Health</a></li><li><a href="http://www.fiercehealthcare.com">Hospitals <img src="/sites/all/themes/fierce/img/arrow-3.gif" /></a></li><li><a href="http://www.hospitalimpact.org">Hospital Leadership <img src="/sites/all/themes/fierce/img/arrow-3.gif" /></a></li><li><a href="http://www.fierceemr.com">EMRs <img src="/sites/all/themes/fierce/img/arrow-3.gif" /></a></li><li><a href="http://www.fiercehealthpayer.com">Healthcare Payers <img src="/sites/all/themes/fierce/img/arrow-3.gif" /></a></li><li><a href="http://www.fiercepracticemanagement.com">Physician Practices <img src="/sites/all/themes/fierce/img/arrow-3.gif" /></a></li></ul></div>';
			//console.log(searchBox);
			//$('.search-form-header').css('display', 'none');
			$('.top-panel').after(newNav);
			$('.search-form-header').appendTo('#newNavContainer');
			$(topicsDrop).appendTo('#newNavContainer');
			$('<br class="clear" />').appendTo('#newNavContainer');
			$('#mobile_topics').click(function() {

				if($('#mobile_topics_menu').css('display') == 'block') {
					$('#mobile_topics_menu').css('display', 'none');
				} else {
					$('#mobile_topics_menu').css('display', 'block');
				}

				return false;

			});

			$('.tags').after($('.pane-popular-stories').addClass('box'));
			//$('.pane-popular-stories').addClass('box').after('.tags');
		}
		function removeStuff() {
			$('.top-nav').after($('.search-form-header'));
			$('#newNavContainer').remove();
			$('.mobile-removeable-br').remove();
			//$('.search-form-header').css('display', 'block');
		}
