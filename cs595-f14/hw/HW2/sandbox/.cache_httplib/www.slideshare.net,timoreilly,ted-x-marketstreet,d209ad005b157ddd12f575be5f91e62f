<script type="text/javascript">

  $(document).ready(function() {
    var ele = $('#svPlayerId');

    player = new SSPlayer(slideshare_object.jsplayer);
    $(player).bind('slidechanged', function(e){
      if(typeof(loadDataForSlide) === 'function'){
      loadDataForSlide(e.ssData.index);
     }
    });

    switch (player.config.player_type) {
      case 'document':
        $('.player').addClass('document_player');
        break;
      case 'infographic':
        $('.player').addClass('infographic_player');
        break;
      case 'video':
        $('.player').addClass('video_player');
       break;
      case 'html':
       $('.player').addClass('html_player');
       break;
    }
  });
</script>
