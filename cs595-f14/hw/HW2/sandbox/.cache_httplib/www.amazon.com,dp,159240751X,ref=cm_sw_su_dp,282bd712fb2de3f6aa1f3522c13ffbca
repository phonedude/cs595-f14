<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "%3Cdiv%3E%20%20%3Cb%3EThe%20riveting%20history%20of%20tuberculosis%2C%20the%20world%26%238217%3Bs%20most%20lethal%20disease%2C%20the%20two%20men%20whose%20lives%20it%20tragically%20intertwined%2C%20and%20the%20birth%20of%20medical%20science.%3C%2Fb%3E%3Cbr%3E%20%20%20%26%23160%3B%3Cbr%3E%20%20%20In%201875%2C%20tuberculosis%20was%20the%20deadliest%20disease%20in%20the%20world%2C%20accountable%20for%20a%20third%20of%20all%20deaths.%20A%20diagnosis%20of%20TB%26%23151%3Boften%20called%20consumption%26%23151%3Bwas%20a%20death%20sentence.%20Then%2C%20in%20a%20triumph%20of%20medical%20science%2C%20a%20German%20doctor%20named%20Robert%20Koch%20deployed%20an%20unprecedented%20scientific%20rigor%20to%20discover%20the%20bacteria%20that%20caused%20TB.%20Koch%20soon%20embarked%20on%20a%20remedy%26%23151%3Ba%20remedy%20that%20would%20be%20his%20undoing.%3Cbr%3E%20%20%20%26%23160%3B%3Cbr%3E%20%20%20When%20Koch%20announced%20his%20cure%20for%20consumption%2C%20Arthur%20Conan%20Doyle%2C%20then%20a%20small-town%20doctor%20in%20England%20and%20sometime%20writer%2C%20went%20to%20Berlin%20to%20cover%20the%20event.%20Touring%20the%20ward%20of%20reportedly%20cured%20patients%2C%20he%20was%20horrified.%20Koch%26%238217%3Bs%20%26%23147%3Bremedy%26%238221%3B%20was%20either%20sloppy%20science%20or%20outright%20fraud.%3Cbr%3E%20%20%20%26%23160%3B%3Cbr%3E%20%20%20But%20to%20a%20world%20desperate%20for%20relief%2C%20Koch%26%238217%3Bs%20remedy%20wasn%26%238217%3Bt%20so%20easily%20dismissed.%20As%20Europe%26%238217%3Bs%20consumptives%20descended%20upon%20Berlin%2C%20Koch%20urgently%20tried%20to%20prove%20his%20case.%20Conan%20Doyle%2C%20meanwhile%2C%20returned%20to%20England%20determined%20to%20abandon%20medicine%20in%20favor%20of%20writing.%20In%20particular%2C%20he%20turned%20to%20a%20character%20inspired%20by%20the%20very%20scientific%20methods%20that%20Koch%20had%20formulated%3A%20Sherlock%20Holmes.%3Cbr%3E%20%20%20%26%23160%3B%3Cbr%3E%20%20%20Capturing%20the%20moment%20when%20mystery%20and%20magic%20began%20to%20yield%20to%20science%2C%20%3Ci%3EThe%20Remedy%3C%2Fi%3E%20chronicles%20the%20stunning%20story%20of%20how%20the%20germ%20theory%20of%20disease%20became%20a%20true%20fact%2C%20how%20two%20men%20of%20ambition%20were%20emboldened%20to%20reach%20for%20something%20more%2C%20and%20how%20scientific%20discoveries%20evolve%20into%20social%20truths.%3C%2Fdiv%3E",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    






