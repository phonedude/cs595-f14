      function survey_select_show_hide(what) {
        var tid = $(what).attr("id") + "_other";
        if ($(what).val() == "Other (please specify)") {
          $("input#"+tid).show();
          $("input#"+tid).focus();
        } else {
          $("input#"+tid).hide();
        }
      };
