<script>
$(document).ready(function () {

  /* init nav */
  /*$("#top_level_nav #nav").superfish(); */

  /* accordian mainly used on speaker resources */
  $(".accordion .module h2").accordion({clickTitle: 'h2', slideTime: '30'});
  
  /* make links with class=external open up external */
  $("a.external").click(function (e) {
    e.preventDefault();
    window.open($(this).attr("href"));
  });

  











var getUrlVars = function(){
  var vars = { };
  var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
    vars[key] = value;   
  });
  return vars;   
};

var tracks = getUrlVars(); 

var filterTopicSessions = function(session_filter, topic, topic_desc){
	  $('#grid_topics a').removeClass('filtering');
        session_filter.addClass('filtering');
		$('.topic_description').hide();
		$('.en_schedule_day').show();

		if (topic == "All Sessions") {
			$('.en_schedule_time').show();
			$('.en_session').show();
		} else {
			$('#en_schedule').before($(topic_desc));
			$(topic_desc).show();

			$('.en_schedule_time').show();
			$('.en_session').show();

            $('.en_plenary').hide();
            $('.vevent').hide();
			$('.en_break').hide();

			$('.en_session_topics').each(function() {
			    var current_topic = $(this).children('a').text();
			    if (current_topic != topic){
			    	$(this).parent().parent().hide();
			    } else {
                                $(this).parent().parent().show();
                             }
				
			});
			$('.en_schedule_time').each(function(){
				var visible_children = $(this).children('div:visible').length;
				if (visible_children == 0) {
					$(this).hide();
				}
			});
		} 
};

$('.topic_filter').each(function() {
		var topic_id = $(this).data('topicId');
		var topic_description = '#topic_' + topic_id;
		var topic_name = $(this).text();
        if (tracks.filter == topic_name) {
          filterTopicSessions($(this), topic_name, topic_description);
		}
		
	$(this).on('click', function(e) {
		e.preventDefault();
		$('#schedule_options li').removeClass('filtered');
        filterTopicSessions($(this), topic_name, topic_description); 
		
	});

});

$('#schedule_options li.day_filter').each(function() {
	$(this).on('click', function(e) {
                e.preventDefault();
                $('#grid_topics a').removeClass('filtering');//remove filter from topics
                $('#schedule_options li').removeClass('filtered');
                $(this).addClass('filtered');
				$('.en_schedule_time').show();
				$('.en_session').show();
                $('.topic_description').hide();

                $('.en_schedule_day').show();
                var filter_day = $(this).children('a').attr('href').slice(1); 
                 $('.en_schedule_day h2 a').each(function() {
                       if ($(this).attr('name') != filter_day) {
                           $(this).parent().parent().hide();
                       }
                  });             
	});
});

var all_day = '<div class="en_schedule_time all_day_events"><h3><a name="all_day">Happening All Day</a></h3></div>';

var author_signing_event = '<div class="en_session vevent"> <div class="en_session_title summary"> <a href="/solid2014/public/schedule/detail/36102" class="url uid" name="session36102">Author Signings</a> </div> Wednesday, 05/21/2014 and Thursday, 05/22/2014 <div class="en_session_roominfo"> <span class="en_session_topics category"> <a href="/solid2014/public/schedule/topic/Events">Events</a><br /></span> <span class="en_session_room">Location: <span class="location">O\'Reilly Booth, Demo Pavilion</span></span> </div> <div class="en_detail_eval" id="en_detail_eval_33139" style="vertical-align:top;"> </div> <div class="en_session_description description"> Author book signings will be held in the O\'Reilly booth on Wednesday and Thursday. Meet O\'Reilly authors and get a free copy of their book. <span class="en_read_more"><a href="/solid2014/public/schedule/detail/36102">Read more</a>. </span> </div> </div>';

var oh_event = '<div class="en_session vevent"> <div class="en_session_title summary"> <a href="/solid2014/public/schedule/topic/1199" class="url uid" name="session">Office Hours</a> </div> Wednesday, 05/21/2014 and Thursday, 05/22/2014 <div class="en_session_roominfo"> <span class="en_session_topics category"> <a href="/solid2014/public/schedule/topic/Events">Events</a><br /></span> <span class="en_session_room">Location: <span class="location">O\'Reilly Booth, Demo Pavilion</span></span> </div> <div class="en_detail_eval" id="en_detail_eval" style="vertical-align:top;"> </div> <div class="en_session_description description"> Office hours will be held in the O\'Reilly booth on Wednesday and Thursday. Office Hours give you a chance to meet face-to-face in a small group setting with some of the best minds in IoT. <span class="en_read_more"><a href="/solid2014/public/schedule/topic/1199">Read more</a>. </span> </div> </div>';

$('.en_schedule_day').each(function() {
   $(this).append(all_day);
});

$('.all_day_events').each(function() {
  $(this).append(author_signing_event);
  $(this).append(oh_event);
});



   $('#venue.row').css('background', "#F6F6F6 url(http://dev.virtualearth.net/REST/v1/Imagery/Map/Road/37.805161,-122.427620/14?mapSize=870,275&pp=37.805161,-122.427620;45&fmt=png&key=Atres0F2a8GMcjA3GSsT3EZydko9vKaVTIroLB6CHi_aC0WCamgYXsFkJ1VdpKuS) center top no-repeat");

});
</script>



