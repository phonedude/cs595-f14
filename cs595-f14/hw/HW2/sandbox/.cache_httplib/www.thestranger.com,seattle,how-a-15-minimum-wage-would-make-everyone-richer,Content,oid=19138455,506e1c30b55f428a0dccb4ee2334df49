                function toggleComments(type,newstatus,stickiness) {
                  if(!stickiness){stickiness="sticky";}
                  if(type=="unregistered"){commentStyle = "anonymous";}else{commentStyle = "registered";}
                  if(newstatus==""){newstatus="On"}
                  if(newstatus=="On"){oldstatus = "Off";}else{oldstatus = "On";}
                  $(type+oldstatus+'Link').hide();
                  $(type+newstatus+'Link').show();
                  if(newstatus=="On") {
                    $$('.comment.'+commentStyle).each(function(a){a.show();});
                    $$('.comment.'+commentStyle+'.collapsed').each(function(a){a.hide();});
                  } else {
                    $$('.comment.'+commentStyle).each(function(a){a.hide();});
                    $$('.comment.'+commentStyle+'.collapsed').each(function(a){a.show();});
                  }
                  if(stickiness!="slippery") {
                    setCookie(type+'Comments',newstatus,'365');
                  }
                  return false;
                }
              </script>
              
