      <script type="text/javascript">
  var openToggle = false;
  function slideToggle(id) {
    if(openToggle == false) {
      new Effect.toggle(id,'slide',{duration:0.5});
      $(id+"-toggle").addClassName("closed");
      $(id+"-toggle").removeClassName("open");
      openToggle = true;
    } else if(openToggle == true) {
      new Effect.toggle(id,'slide',{duration:0.5});
      openToggle = false;
      $(id+"-toggle").addClassName("open");
      $(id+"-toggle").removeClassName("closed");
    }
    
  }
  
  function showAdCruiser(ad_id) {
    var ad = $(ad_id);
    var adtags = ad.select('img', 'a');
    var adframe = ad.select('iframe');
    var adembed = ad.select('embed');
    if (adtags.length == 0 && adframe.length == 0 && adembed.length == 0) {
      // keep hidden
    } else {
      $('FixedBottomCruiserAd').show();
      $('footer').setStyle({
        marginBottom: '80px'
      });
      $('footerCopyright').setStyle({
        paddingBottom: '80px'
      });
    }
  }
</script>

<div id="FixedBottomCruiserAd" class="FixedBottomCruiserAd" style="display:none;">
  <a href="#" onclick="slideToggle('FixedAdImage');return false;" id="FixedAdImage-toggle" class="toggleHide open"> </a>
  <div id="FixedAdImage">
    <div id="adZoneFixedBottomCruiserAd" class="adZone" style="position:relative;">
  
    
    
    <!-- ca-pub-0969663119487598/Stranger_FixedBottomCruiser -->
    <script type="text/javascript">
    GA_googleFillSlot("Stranger_FixedBottomCruiser");
    </script>
  
  
  </div>
  </div>
</div>

<script type="text/javascript">
  showAdCruiser('FixedAdImage');
</script>
