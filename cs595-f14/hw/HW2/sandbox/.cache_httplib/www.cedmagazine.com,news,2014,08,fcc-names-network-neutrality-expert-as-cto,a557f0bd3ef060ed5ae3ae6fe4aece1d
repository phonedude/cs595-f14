      var beaconcodeArray = [];
      var criteriaArray = [];
        
      beaconcodeArray[0] = Drupal.settings.abm_sitescout.abm_sitescout_beaconcode_0;
      criteriaArray[0] = Drupal.settings.abm_sitescout.abm_sitescout_criteria_0;
    
      beaconcodeArray[1] = Drupal.settings.abm_sitescout.abm_sitescout_beaconcode_1;
      criteriaArray[1] = Drupal.settings.abm_sitescout.abm_sitescout_criteria_1;

      beaconcodeArray[2] = Drupal.settings.abm_sitescout.abm_sitescout_beaconcode_2;
      criteriaArray[2] = Drupal.settings.abm_sitescout.abm_sitescout_criteria_2;
        
