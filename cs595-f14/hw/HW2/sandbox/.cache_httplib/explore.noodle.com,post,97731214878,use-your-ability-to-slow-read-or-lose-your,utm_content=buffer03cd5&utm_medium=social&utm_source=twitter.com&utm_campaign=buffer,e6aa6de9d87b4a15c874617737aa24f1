    <script type="text/javascript">
        $(document).ready(function () {
            if (window.location.href === 'http://explore.noodle.com/' || window.location.href.indexOf('http://explore.noodle.com/page/') != -1) {
                $('#pinned_posts').attr('src', 'http://explore-blog.tumblr.com/tagged/pinned');

                $(window).load(function() {
                    $('#pinned_posts').css('min-height',$('#content').height());                    
                });
                $('#pinned_posts').load(function () {               
                    $('#pinned_posts').css('height', Math.max($('#content').height(), $('#pinned_posts').contents().find('#content').height() + 50));
                });
            }
            
            if (window.location.href.index)
            
            if ($('#navigation .next-page').length == 0) {
                $('#navigation .navigation-nums').append('<p class="next-page">Next page &rarr;</p>');
            }
            
            if ($('#navigation .prev-page').length == 0) {
                $('#navigation .navigation-nums').append('<p class="prev-page">&larr; Prev page</p>');
            }
            
            $('a.not_clicked').click(function() {
                if ($(this).hasClass('liked')) {
                    var closest_notes = $(this).closest('.post').find('.notes');
                    var n_likes = parseInt(closest_notes.html());
                    if (n_likes == 1) {
                        closest_notes.css('width', '0px');
                        closest_notes.html('')
                    }
                    else {
                        closest_notes.html((n_likes-1) + ' ' + (n_likes > 2 ? 'NOTES' : 'NOTE'));
                    }
                    $(this).removeClass('liked');
                    return false;
                }
                var post = $(this).closest('.post');
                var id = post.attr('id');
                var oath = post.attr('rel').slice(-8);
                var like = 'http://www.tumblr.com/like/'+oath+'?id='+id;
                $('#likeit').attr('src', like);
                $(this).addClass('liked');
                var closest_notes = $(this).closest('.post').find('.notes');
                closest_notes.css('min-width', '50px');
                if (isNaN(parseInt(closest_notes.html())))
                    closest_notes.html('1 NOTE');
                else
                    closest_notes.html((parseInt(closest_notes.html())+1) + ' NOTES');
                return false;
            });
            
            var allowed_tags = ['art+and+design', 'science+and+technology', 'culture+and+society', 'history+and+literature', 'creativity+and+innovation', 'media+and+communication', 'thought+and+opinion'];
            var url = window.location.href.split('/');

            if (url.length > 4) {
                if (allowed_tags.indexOf(url[4].replace(/-/g, '+')) != -1) {
                    $('nav .' + url[4].replace(/\+/g,'-')).addClass('current_link');
                    $('.logo_link').addClass(url[4].replace(/\+/g,'-'));
                    $('#tag_page_title').addClass(url[4].replace(/\+/g,'-'));
            };}
            var f = function () {
                $('.tags').each(function (k,v){
                    var li_num = 0;
                    var current_order = [];
                    for(i=0; i<v.childNodes.length;i++) {
                        if (v.childNodes[i].nodeName=='A') {
                            var tag = v.childNodes[i].className
                            if (tag !== 'pinned') {
                                if (allowed_tags.indexOf(tag) != -1) {
                                    li_num += 1;
                                    formatted_tag = tag.replace(/\+/g, ' ').replace(/and/,'&').replace( /(^|\s)([a-z])/g , function(m,p1,p2){ return p1+p2.toUpperCase(); } );
                                    formatted_class = tag.replace(/\+/g, '-');
                                    v.childNodes[i].innerHTML = '<span>' + formatted_tag +'</span>';
                                    v.childNodes[i].className = formatted_class;
                                    current_order.push(v.childNodes[i]);
                                    v.removeChild(v.childNodes[i]);
                                }
                                else {
                                    v.childNodes[i].style.display = 'none';
                                    formatted_tag = tag.replace(/\+/g, ' ');  
                                    formatted_url = tag.replace(/\+/g, '-');
                                    $(this).closest('.post').find('.other_tags').append('<a href=http://explore-blog.tumblr.com/tagged/' + formatted_url + '>#' + formatted_tag + '</a>')
                                }
                            }
                            else
                                v.childNodes[i].style.display = 'none';
                        }}
                        var formatted_allowed_tags = ['art-and-design', 'science-and-technology', 'culture-and-society', 'history-and-literature', 'creativity-and-innovation', 'media-and-communication', 'thought-and-opinion'];
                            var correct_order = current_order.sort(function (a,b) {
                                return formatted_allowed_tags.indexOf(a.className) - formatted_allowed_tags.indexOf(b.className);
                            });
                            for (i=0; i<correct_order.length; i++) 
                                v.appendChild(correct_order[i]);
                            for(i=0; i<v.childNodes.length;i++) {
                                if (v.childNodes[i].nodeName=="A") {
                                    v.childNodes[i].style.width = (Math.floor(v.childNodes[i].offsetWidth/li_num)).toString() + 'px';         
                                }
                            }
                        
                    
                })
            }
            f();
             $('#pinned_posts').css('height', Math.max($('#content').height(), $('#pinned_posts').contents().find('#content').height() + 50));
        });
    </script>
