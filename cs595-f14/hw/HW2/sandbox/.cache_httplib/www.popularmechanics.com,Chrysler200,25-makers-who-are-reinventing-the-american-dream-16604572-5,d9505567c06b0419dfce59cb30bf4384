		<script>
		var articlePages = []; // create new array to hold all multi-page article URLs
		var article_totalpages = 5; // set the total number of pages
		var current_page; // prepare var to hold the page number of the current URL in multipage article
		 // iterate through pagination loop to store all article URLs and set the current_page var when is_current is true
			articlePages.push('25-makers-who-are-reinventing-the-american-dream-16604572');
			
		 // iterate through pagination loop to store all article URLs and set the current_page var when is_current is true
			articlePages.push('25-makers-who-are-reinventing-the-american-dream-16604572-2');
			
		 // iterate through pagination loop to store all article URLs and set the current_page var when is_current is true
			articlePages.push('25-makers-who-are-reinventing-the-american-dream-16604572-3');
			
		 // iterate through pagination loop to store all article URLs and set the current_page var when is_current is true
			articlePages.push('25-makers-who-are-reinventing-the-american-dream-16604572-4');
			
		 // iterate through pagination loop to store all article URLs and set the current_page var when is_current is true
			articlePages.push('25-makers-who-are-reinventing-the-american-dream-16604572-5');
			current_page = 5;
		
		
		function showArticlePaging(pagination_cntr) {
			var page_num; // prepare var to hold the page number counter while iterating through pagination loop
			var writeMe; // holds the contents which will eventually be written to the element passed in the function's argument
			var start_page = (current_page < 4) ? 2 : current_page - 2;
			var end_page = (current_page > article_totalpages - 3) ? article_totalpages - 1 : current_page + 2;
			
			writeMe = (current_page == 1) ? '<span class="current">1</span>' : '<a class="pagenumbers" href="' + articlePages[0] + '">1</a>';
			writeMe += (current_page > 4) ? '<span class="ellipsis">...</span>' : '';
			
			for (page_num = start_page; page_num <= end_page; page_num++){
				writeMe +=  (page_num == current_page) ?  ("<span class=\"current\">"+page_num+"</span>") : ("<a class=\"pagenumbers\" href='" + articlePages[page_num - 1] + "'>" + page_num + "</a>");
			}
			
			if (article_totalpages > 1) {
				writeMe += (current_page < article_totalpages - 3) ? '<span class="ellipsis">...</span>' : '';
				writeMe += (current_page == article_totalpages) ? '<span class="current">' + article_totalpages + '</span>' : '<a class="pagenumbers" href="' + articlePages[article_totalpages - 1] + '">' + article_totalpages + '</a>';	
			}
			
			document.getElementById(pagination_cntr).innerHTML += writeMe;
		}
		</script>
