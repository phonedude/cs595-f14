<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "%3Cdiv%3EOver%206.4%20billion%20people%20participate%20in%20a%20%2436.5%20trillion%20global%20economy%2C%20designed%20and%20overseen%20by%20no%20one.%20How%20did%20this%20marvel%20of%20self-organized%20complexity%20evolve%3F%20How%20is%20wealth%20created%20within%20this%20system%3F%20And%20how%20can%20wealth%20be%20increased%20for%20the%20benefit%20of%20individuals%2C%20businesses%2C%20and%20society%3F%20In%20%3CI%3EThe%20Origin%20of%20Wealth%3C%2FI%3E%2C%20Eric%20D.%20Beinhocker%20argues%20that%20modern%20science%20provides%20a%20radical%20perspective%20on%20these%20age-old%20questions%2C%20with%20far-reaching%20implications.%20According%20to%20Beinhocker%2C%20wealth%20creation%20is%20the%20product%20of%20a%20simple%20but%20profoundly%20powerful%20evolutionary%20formula%3A%20differentiate%2C%20select%2C%20and%20amplify.%20In%20this%20view%2C%20the%20economy%20is%20a%20%22complex%20adaptive%20system%22%20in%20which%20physical%20technologies%2C%20social%20technologies%2C%20and%20business%20designs%20continuously%20interact%20to%20create%20novel%20products%2C%20new%20ideas%2C%20and%20increasing%20wealth.%20Taking%20readers%20on%20an%20entertaining%20journey%20through%20economic%20history%2C%20from%20the%20Stone%20Age%20to%20modern%20economy%2C%20Beinhocker%20explores%20how%20%22complexity%20economics%22%20provides%20provocative%20insights%20on%20issues%20ranging%20from%20creating%20adaptive%20organizations%20to%20the%20evolutionary%20workings%20of%20stock%20markets%20to%20new%20perspectives%20on%20government%20policies.%20A%20landmark%20book%20that%20shatters%20conventional%20economic%20theory%2C%20%3CI%3EThe%20Origin%20of%20Wealth%3C%2FI%3E%20will%20rewire%20our%20thinking%20about%20how%20we%20came%20to%20be%20here%26%23151%3Band%20where%20we%20are%20going.%3C%2Fdiv%3E",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    






