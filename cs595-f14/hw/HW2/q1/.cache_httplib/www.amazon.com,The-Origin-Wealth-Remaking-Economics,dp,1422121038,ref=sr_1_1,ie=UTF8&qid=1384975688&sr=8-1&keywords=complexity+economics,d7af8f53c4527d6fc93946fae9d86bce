             + '</span></a>'
             + '    </td>'
             + '</tr>';

            dumpHtml(obj, newAdsHtml,'<div id="AUI_SlDiv_1"><div id="AUI_A9AdsWidgetAdsWrapper"><table style="width:auto; margin-top:10px;" class="AUI_SponsoredLinkColumnAds">  <tbody> ', '</tbody> </table> </div></div>', divWideName);
            
           var heightOfOneAd = $('#' + divMiddleName).height() / adCount;
           if( heightOfOneAd > expectedHeightMiddle ) {
             reformatType = '2';
             var newAdsHtml = '<div class="AUI_SponsoredLinkItem"><div style="padding-top:10px;"><ul><li><a class="a-size-medium" style="font-size:15px; padding-top:2px; display:inline-block;" target="_blank" href="'
             + adPropertiesDelimiter + 'redirectUrl' + adPropertiesDelimiter
             + '" rel="nofollow"><b>'
             + adPropertiesDelimiter + 'title' + adPropertiesDelimiter
             + '</b></a>&nbsp;<a rel="nofollow" href="'
             + adPropertiesDelimiter + 'redirectUrl' + adPropertiesDelimiter
             + '" target="_blank"><img src="http://g-ecx.images-amazon.com/images/G/01/icons/icon-offsite-sl-7069-t4._V171196157_.png" width="23" alt="opens new browser window" align="absbottom" style="padding-bottom:0px; margin-bottom:0px;" height="20" border="0" /></a></li></ul></div>'
             + '<div class="a-size-base" style="padding-left:23px;"><a target="_blank" href="'
             + adPropertiesDelimiter + 'redirectUrl' + adPropertiesDelimiter
             + '" rel="nofollow"><span class="a-color-secondary">'
             + adPropertiesDelimiter + 'visibleUrl' + adPropertiesDelimiter
             + '</span></a>  </div>  <div class="a-size-base" style="padding-left:23px;">'
             + adPropertiesDelimiter + 'description' + adPropertiesDelimiter
             + '</div></div>';

             dumpHtml(obj, newAdsHtml,'<div id="AUI_SlDiv_2"><div id="AUI_A9AdsWidgetAdsWrapper">', '</div></div>', divMiddleName);
           }
 
        }

		if( typeof A9_SL_CSM_JS != 'undefined' ){
		  A9_SL_CSM_JS($, reformatType);
		}

        function dumpHtml( obj, newAdsHtml, topHtml, bottomHtml, divParent ) {
              var i = 0;
              var completeAdsHtml = '';
              for( x in obj ) {
                  var adChunks = newAdsHtml.split( adPropertiesDelimiter );
                  for( var j = 0; j<adChunks.length; j++ ) {
                      if( typeof obj[i][adChunks[j]] != "undefined" ) {
                          adChunks[j] = obj[i][adChunks[j]];
                      }
                  }
                  completeAdsHtml += adChunks.join("");
                  i++;
              }
              newAdsHtml = topHtml + completeAdsHtml + bottomHtml;
              $('#' + divParent).replaceWith(newAdsHtml);
        }
	});

	P.when('jQuery','ready').execute(function($){
	    var jsonAds = '[{"provider":"g","description":"IT Spend was $3.7 Trillion in 2013. How Was Your Share Spent? Find Out!","title":"The <b>Economics</b> of IT","redirectUrl":"http://rd.a9.com/srv/redirect/?info=APWcQ1ZjqTVKW1ILQXL.HzsM8NIcAEmyBbZ.B7hLIQN8ml0EhlpnvGodhqu9HaREv1E9Z1rQ-c2g-NLg-JRD2BE5KpDFFLtF-ev-G0eFyDk35q.f63uE3PV09Kf9tY5Y9A2.K-JZp2f1B4cGsD.URvC2UpvJxL6LdsMJAuxPLj1Exk93vuqsA0SHZ27KV8949Ndyu9QhB4xubriYJykDJUTnewt0IzRDKcRkS4F.C6--2J2fEEKzs6vzMKNtWe5y52duGI3M0Ru0Q0z9ABFOAif1-1h1zzD.IXuAonLEhMCGVvs2t8lCdAAekdVwlki47C2tt5MTXMXtMcyuXXeGgJXlOuw1kko6.qg0izEcg7MMANWHhNLgHqcScwUSP4Nc4zkKWfyuUmX4Vx5lzxxx18bMAjhTrnv.V79yLTSW5dlmNUxk26Y7Oli5RlRBSRTgMEYmXY-Es-hufuEStkwVsLHWt82Qmj1l7zpTjFUrY2oQFcauEI1CBspCZSAwvCnit6ScIOfcONXjfVef8P8yLuoDDjGo-gADJkJ8z.y1Ej0C4hbB3CDWsa8LUCd6uWgZtjERThNN.rw67WmkPjDtnvhDkgKm9zMFP8rYNQFt4UZQjgrdSOwOQokZ2GCQb39skmspfe67oBcPcTdjMiJiHhvPxE4jsPymiSrKKx7xt33Lu8D6HO6tFlTVnsvQI.FLeWzT2cthdOjMk6YxAwpfVFkR74R0OilroZ9hcxOwuAWnJC.jM4glTy9LIBxgL95ez.KqI6srQV-zqJFV8R5wTcb2cx.NbxGzDMh2Q0RWUlBdMZZ-UshD9Xs_&awt=1&s=","visibleUrl":"www.apptio.com/IT-<b>Economics</b>"}]';
	    var feedbackFormAvailable = 0;
	    function showSLF() {
	         if(!feedbackFormAvailable) {
	           // Make a request to the handler with new ads 
	           jQuery.post('/gp/product/aui_feedback_form_handler.html', { 'sid': '184-8323807-2228647','jsonAds' : escape(jsonAds) } ,function(data) {
	               $('#FeedbackFormDiv').html(data);
	               $('#FeedbackFormDiv').show();
	               $('#ShowFeedbackLinkDiv').hide();
	               $('#ResponseFeedbackLinkDiv').hide(); 
	               feedbackFormAvailable = 1;
	           });
	         } else {
	           $('#reports-ads-abuse-form').show();
	           $('#FeedbackFormDiv').show();
	           $('#reports-ads-abuse').show();
	           $('#ShowFeedbackLinkDiv').hide();
	           $('#ResponseFeedbackLinkDiv').hide(); 
