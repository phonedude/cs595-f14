        Atlantic.Ad.sizeListener('topleader', function ($unit, size) {
            if (size) {
                $unit.css('height', 'auto');
                $unit.css('min-height', (size[1] + 5) + "px");
                $unit.css('padding-top', '5px');
                $unit.parent('.ad').css('height', 'auto');
            }
        })
