			'audioSampleSrc' : "",
		    'flashPlayerUrl' : ""
		}
	}
	var data = {
		
	    'litbData' : {
	    	'hasLitb' : true,
	    	'litbReftag' : "sib_dp_pt"
	    },
	    'audibleData' : audibleData,
	    'windowWidthThreshold' : 1150,
	    'configWidths' : [200, 260],
	    'holderRatio' : 0.75,
	    'containerMargin' : 15,
	    'flipLinkMinHeight' : 0,
	    'frontImageWidth' : 217,
	    'frontImageHeight' : 346,
	    'frontImageAspectRatio' : 0.6271676300578035,
	    'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/91xKcSjwoTL.jpg","dimensions":[1595,2560],"thumbUrl":"http://ecx.images-amazon.com/images/I/91xKcSjwoTL._SX75_CR,0,0,75,75_.jpg"}],
	    'centerColMargin' : 40
	};
	return data;
});
