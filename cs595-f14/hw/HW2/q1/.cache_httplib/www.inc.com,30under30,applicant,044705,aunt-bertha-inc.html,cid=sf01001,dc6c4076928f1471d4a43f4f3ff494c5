				<script type="text/javascript" src="http://www.inc.com/js/inc.js?responsiveVideo2"></script>
				<script src="//cdn.optimizely.com/js/572850193.js"></script>
	
	<script language="JavaScript" src="http://j.maxmind.com/app/geoip.js"></script>

			<script language="JavaScript" type="text/javascript">
		
		if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) { mobile='TRUE'; } else { mobile='FALSE'; }
		welus = 'TRUE';welglobal = 'FALSE';weldesktop = 'TRUE';weltablet = 'TRUE';
		
		if((welus=='TRUE'&&geoip_country_code()=='')||(welus=='TRUE'&&geoip_country_code()=='US')||(welglobal=='TRUE'&&geoip_country_code()!='US')) {
			if(window.innerWidth>800 && ((mobile=='TRUE'&&weltablet=='TRUE')||(mobile=='FALSE'&&weldesktop=='TRUE'))) {
				if(getParameterByName('cid').indexOf('ps')!=0) {
					overlay_ad();
				}
			}
		}
		function getParameterByName(name) {
    		name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    		var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        		results = regex.exec(location.search);
    		return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
		}

		
	</script>
		
	<script type="text/javascript">
		
		var inc = Inc.Apps.IncDotCom.spawn({ initialize: false });
		inc.init();

		if (inc.i.article && _.size(inc.i.article) > 0) {
			var bootstrappedArticle = new Inc.Models.Article(inc.i.article);
			if (inc.i.mod == 'create') {
				bootstrappedArticle.cid = 'NewArticle-'+inc.incid;
				bootstrappedArticle.set({
					'inc_pubdate': bootstrappedArticle.defaults().inc_pubdate,
					'inc_promo_date': bootstrappedArticle.defaults().inc_promo_date
				});
			}
			inc.collections.article.add(bootstrappedArticle);
		}
		
	</script>

	    










			
