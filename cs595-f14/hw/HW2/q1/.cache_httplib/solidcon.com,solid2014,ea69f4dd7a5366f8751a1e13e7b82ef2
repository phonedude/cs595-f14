<script type="text/javascript">

function shuffleElements(parentElem) {
  var nodes = parentElem.children;
  for (var i = nodes.length - 2; i > 0; i--) {
    var j = Math.floor(Math.random() * (i + 1)),
      temp1 = nodes[i].cloneNode(true),
      temp2 = nodes[j].cloneNode(true);
    parentElem.replaceChild(temp2, nodes[i]);
    parentElem.replaceChild(temp1, nodes[j]);
  }
}

shuffleElements(document.getElementById('headlining-slides'));
document.getElementById('headlining-slides').children[0].className += " visible";

jQuery(document).ready(function($) {
  var slides = $('#headlining-slides').children('.slide');
  slides.not(':eq(0)').addClass('hidden');

  var slideNav = $('<div id="headlining-slides-nav"></div>');
  for (var i = 0; i < slides.length; i++) {
    if (i == 0) { slideNav.append($('<a href="#" class="item selected"></a>')); }
    else { slideNav.append($('<a href="#" class="item"></a>')); }
  }
  $('#headlining-slides').prepend(slideNav);

  slideNav.children('.item').click(function(e) {
    e.preventDefault();
    var that = $(this);

    if (!that.hasClass('selected')) {
      slideNav.children('.item').removeClass('selected');
      that.addClass('selected');
      clearInterval(slideNavInterval);
      slideNavInterval = setInterval(slideNavAuto,15000);

      $('#headlining-slides').children('.visible').animate({'opacity':0},200, function() {
        $(this).addClass('hidden').removeClass('visible').css('z-index',0);

        slides.eq(that.index()).css('z-index',1).animate({'opacity':1}, 200, function() {
          $(this).addClass('visible').removeClass('hidden');
        });
      });
    }
  });
  function slideNavAuto() {
         if (slideNav.children('.item.selected').index() == slideNav.children('.item:last').index()) {
             slideNav.children('.item:first').click();
         } else {
             slideNav.children('.item.selected').next().click();
         }
  }
  var slideNavInterval;
  slideNavInterval = setInterval(slideNavAuto,15000);


  var videoPlaying = false;
    var isIOS = (/iPhone|iPod|iPad/).test(navigator.userAgent);
    if (isIOS) {
      $('#looped-video').css('-webkit-transform','scale(0.01)');
    }

  $('.default-video-btn').on('click', function(e) {
       e.preventDefault();
       $('#looped-video').append('<source src="http://cdn.oreillystatic.com/conferences/solid/video-2014/solid-video-20131219.webm" type=\'video/webm; codecs="vp8, vorbis"\' /><source src="http://cdn.oreillystatic.com/conferences/solid/video-2014/solid-video-20131219.mp4" type="video/mp4">');
       videoPlaying = true;
       $('#looped-video').clearQueue().animate({'opacity':1},2000).get(0).play();
       $('#headlining-slides').slideUp(2000);
       $('#video-row.row').slideDown(2000);
  }); 

    $('.video-btn').on('click', function(e) {
      e.preventDefault();
      if (!videoPlaying) {
        videoPlaying = true;
        $(this).html('Pause').addClass('pause');
        $('#looped-video').clearQueue().animate({'opacity':1},1000).get(0).play();
      }
      else {
        videoPlaying = false;
        $(this).html('Watch the Video').removeClass('pause');
        $('#looped-video').clearQueue().animate({'opacity':1},1000).get(0).pause();
      }
    });
    
});

function removeVideo(){
    $(function() {
      videoPlaying = false;
      $('#looped-video').html('Watch the Video').removeClass('pause');
      $('#headlining-slides').slideDown(2000);
      $('#video-row.row').slideUp(2000);
    });
      
}


var showNewsletterPopup = true;

function checkNLCookie() {
	//Loop through active cookies to find solidnewsletterpopup
	var ormCookieArray = document.cookie.split(";");
	var x,y;
	for (var i = 0 ; i < ormCookieArray.length; i++) {
		x = ormCookieArray[i].substr(0, ormCookieArray[i].indexOf("="));
		y = ormCookieArray[i].substr(ormCookieArray[i].indexOf("=") + 1);
		x = x.replace(/^\s+|\s+$/g,"");
		if (x == "solidnewsletterpopup") {
			if (y == "viewed") {
				showNewsletterPopup = false;
			}
		}
	}
}

function startNLPopup() {
	var nlPopup = jQuery('#newsletter-popup');

	if (showNewsletterPopup) {
		setTimeout("nlShowPopup()", 3000);
	}

	$('form').find('input[name*="email_addr"]').each(function() {
		var defaultValue = $(this).attr('value');
		$(this).focus(function() {
			if ($(this).attr('value') == defaultValue) {
				$(this).attr('value', '');
			}
		});
		$(this).blur(function() {
			if ($(this).attr('value') == '' || $(this).attr('value') == ' ') {
				$(this).attr('value', defaultValue);
			}
		});
	});

	var nlPopupLoad = 0;
	$('#newsletter-popup-frame').load(function() {
		if (nlPopupLoad < 2) {
			nlPopupLoad ++;
		}
		else {
			$('#newsletter-popup-frame').css('opacity',0);
			jQuery('#newsletter-popup-overlay').animate({ opacity: 0 }, 200);
		}
	});
}

function nlShowPopup() {
	var nlPopupDate = new Date();
	nlPopupDate.setTime(nlPopupDate.getTime()+(90*24*60*60*1000));
	var nlPopupexpires = "; expires="+nlPopupDate.toGMTString();
	document.cookie = "solidnewsletterpopup=viewed; " + nlPopupexpires + "; path=/";

	var nlPopupOverlay = jQuery('#newsletter-popup-overlay');
	var nlPopup = jQuery('#newsletter-popup');
	var nlPopupClose = nlPopup.find('a.close');

	nlPopup.css('margin-top', ($(window).scrollTop() + 140) + "px");
	nlPopupOverlay.prependTo('body').css({'opacity':0, 'display':'block', 'height':jQuery('html').height()}).animate({'opacity':1}, 200);


	function nlClosePopup() {
		nlPopupOverlay.animate({'opacity':0}, 200, function() {
			nlPopupOverlay.css('display','none');
		});
	}

	nlPopupOverlay.click(function() {
		nlClosePopup();
	});
	nlPopupClose.click(function(e) {
		e.preventDefault();
		nlClosePopup();
	});
	jQuery(document).keydown(function(e) {
		if (e.which == 27) { //escape key
			nlClosePopup();
		}
	});

	nlPopup.click(function(event) {
		event.stopPropagation();
	});
}

checkNLCookie();
startNLPopup();
</script>

<script>
$(document).ready(function () {

  /* init nav */
  /*$("#top_level_nav #nav").superfish(); */

  /* accordian mainly used on speaker resources */
  $(".accordion .module h2").accordion({clickTitle: 'h2', slideTime: '30'});
  
  /* make links with class=external open up external */
  $("a.external").click(function (e) {
    e.preventDefault();
    window.open($(this).attr("href"));
  });

  


             $('.cheese-board .control-left').css('background', 'rgba(0, 0, 0, 0) url(//cdn.oreillystatic.com/en/assets/1/event/100/stratany2013_test_arrow_lt.png) right center no-repeat');
             $('.cheese-board .control-right').css('background', 'rgba(0, 0, 0, 0) url(//cdn.oreillystatic.com/en/assets/1/event/100/stratany2013_test_arrow_rt.png) left center no-repeat');

                 if (typeof $.proxy == 'undefined') {
      $.proxy = function(callback, context) {
        var proxy = function() {
          callback.apply(context, arguments);
        }
        return proxy;
      };
    }   
    
    var cheeseDisplay = 4;
    var Cheeseboard = function(speakers, priority_speakers) {
      priority_speakers = typeof priority_speakers !== 'undefined' ? priority_speakers : false;
      this.scrolling = false;
      this.queue = [];
      
      var pi, pr, ps;
      //add priority speakers first and randomize
      while (priority_speakers.length > 0) {
        pi = Math.floor(Math.random() * priority_speakers.length);
        pr = priority_speakers.splice(i, 1)[0];
        
        if (pr == null || pr.id == null) continue;

        this.queue.push(pr);
      }//while

      var i, r, s;
      //randomize speaker list
      while (speakers.length > 0) {
        i = Math.floor(Math.random() * speakers.length);
        r = speakers.splice(i, 1)[0];
        
        if (r == null || r.id == null) continue;
        
        this.queue.push(r);
      }//while
      
      setTimeout($.proxy(this.init, this), 500);
      
      $(".cheese-board .control-left").click( $.proxy( function() {this.scrollRight(cheeseDisplay);}, this));
      $(".cheese-board .control-right").click( $.proxy( function() {this.scrollLeft(cheeseDisplay);}, this));
    };

    
    Cheeseboard.template = "<div class=\"speaker\"><div class=\"head\"><div class=\"frame\"></div></div><div class=\"caption\"></div></div>";
    
    Cheeseboard.prototype = {
      init: function() {
        var that = this;
        var load = function(count, delay) {
          var s = that.queue[cheeseDisplay - count];
          var el = that.createSpeakerElement(s.id, s.name, s.affiliation);
          
          el.css({"display": "none"});
          el.appendTo($(".cheese-board .content .tray"));
          el.fadeIn(delay);
          
          count--;
          if (count > 0) {
            setTimeout(function() {load(count, delay);}, 200);
          }
          else if (that.queue.length > cheeseDisplay) {
            $(".cheese-board .control-left").add(".cheese-board .control-right").removeClass('hidden').animate({'opacity':1}, 500);
          }
        };
        load(cheeseDisplay, 500);
      },

      createSpeakerElement: function(id, name, affiliation) {
        var s = $(Cheeseboard.template);
        s.find(".frame").empty().append('<img src="http://cdn.oreillystatic.com/en/assets/1/eventprovider/1/speaker_' + id + '.jpg" alt="Speaker" style="border: 0 none;" /><a class="border" href="/solid2014/public/schedule/speaker/' + id + '"></a>');
        s.find(".caption").empty().append('<span class="name"><a href="/solid2014/public/schedule/speaker/' + id + '">' + name + '</a></span> <span class="affiliation">' + affiliation + '</span>');
        
        return s;
      },
      
      scrollRight: function(count) {
        if (this.scrolling) return;
        this.scrolling = true;

        var that = this;
        var t = $(".cheese-board .content .tray");

        nextCheese = that.queue.splice(-count,count);

        while (count > 0) {
          var s = nextCheese.pop();
          that.queue.unshift(s);
          var el = this.createSpeakerElement(s.id, s.name, s.affiliation);
          el.prependTo(t);
          count--;
        }
        t.css('margin-left','-816px');

        t.animate({"margin-left": 0}, 750, function() {
          t.css({"margin-left": 0});
          t.find('.speaker:gt(3)').remove();
          that.scrolling = false;
        });
      },
      
      scrollLeft: function( count ) {
        if (this.scrolling) return;
        this.scrolling = true;

        var that = this;
        var t = $(".cheese-board .content .tray");

        var countCheck = (count * 2) - that.queue.length;
        var nextCheese = [];
        if (countCheck >= 0) {
          var nextCheese1 = that.queue.splice(count,count - countCheck);
          var nextCheese2 = that.queue.splice(0,countCheck);
          nextCheese = nextCheese1.concat(nextCheese2);
        }
        else {
          nextCheese = that.queue.splice(count,count);
        }

        while (count > 0) {
          var s = nextCheese[nextCheese.length - count];
          var el = this.createSpeakerElement(s.id, s.name, s.affiliation);
          el.appendTo(t);
          if (that.queue.length >= count) {
            var curCheese = that.queue.shift();
            that.queue.push(curCheese);
          }
          count--;
        }
        while (nextCheese.length > 0) {
          that.queue.unshift(nextCheese.pop());
        }

        t.animate({"margin-left": -816}, 750, function() {
          t.find('.speaker:lt(4)').remove();
          t.css({"margin-left": 0});
          that.scrolling = false;
        });
      }
    }//cheeseboard prototype
  
    //$( function() {
      var speakers = [
		{ id: 176252, name: "Beth Comstock", affiliation: "GE" },
{ id: 122516, name: "Renee DiResta", affiliation: "OATV" },
{ id: 169986, name: "Ayah Bdeir", affiliation: "littleBits" },
{ id: 172391, name: "Rod Brooks", affiliation: "Rethink Robotics" },
{ id: 13790, name: "Rob Coneybeer", affiliation: "Shasta Ventures" },
{ id: 33727, name: "Carl Bass", affiliation: "Autodesk, Inc." },
{ id: 251, name: "Tim O'Reilly", affiliation: "O'Reilly Media" },
{ id: 100561, name: "Paola Antonelli", affiliation: "Museum of Modern Art, NY" },
{ id: 175297, name: "Neil Gershenfeld", affiliation: "MIT Ctr for Bits and Atoms" }
      ];
      
      var cheeseboard = new Cheeseboard(speakers);
    //});












   $('#venue.row').css('background', "#F6F6F6 url(http://dev.virtualearth.net/REST/v1/Imagery/Map/Road/37.805161,-122.427620/14?mapSize=870,275&pp=37.805161,-122.427620;45&fmt=png&key=Atres0F2a8GMcjA3GSsT3EZydko9vKaVTIroLB6CHi_aC0WCamgYXsFkJ1VdpKuS) center top no-repeat");

});
