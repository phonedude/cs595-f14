	function embedAutoHeight() {
		jQuery('.post iframe').each(function(){
			var orig = jQuery(this);
			var ratio = orig.attr("width") / orig.attr("height");
			var actWidth = orig.width();
			if(orig.attr("src").lastIndexOf("//instagram.com",0) == 0) {
				if(actWidth > 612){
					return;
				}
			}
			orig.attr("width", actWidth);
			orig.attr("height", (actWidth / ratio));
		});
	}

