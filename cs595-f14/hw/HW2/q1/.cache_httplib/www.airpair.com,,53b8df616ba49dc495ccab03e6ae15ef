<script type="text/javascript">
function initStaticPage(name, sessionParams) {
  require("/scripts/providers/optimizely")
  require("/scripts/providers/olark")()
  if(typeof addjs === 'undefined') {
    var AddJS = require("scripts/providers/addjs/index");
    window.addjs = new AddJS('0xxx5xrw5q');
  }
  addjs.bindTrackLinks()
  addjs.identify(sessionParams);
  addjs.trackEvent('page', 'view', window.location.pathname, name);
}
</script>

<script type="text/javascript">
  function initPage() {
    initStaticPage('homepage');
    // Scroll to hash. Just add class "scroll"
    $(function() {
      $(".scroll").click(function(e){
        e.preventDefault();
        $("html,body").animate({
          scrollTop:$(this.hash).offset().top
        },500);
      });
    });
  }
</script>
<script src="/javascripts/landing.js" defer onload="initPage()"></script>

