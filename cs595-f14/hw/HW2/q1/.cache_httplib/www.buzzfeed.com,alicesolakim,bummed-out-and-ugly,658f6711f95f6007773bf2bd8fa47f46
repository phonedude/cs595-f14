		var rand = Math.random() * total_probability;
		var i = 0;
		var probability_sum = 0;
		var chosen;
		while ( i <= promos.length - 1 && probability_sum < rand ) {
			chosen = promos[i];
			if( typeof chosen !== 'undefined' && typeof chosen.probability == 'number' ) {
				probability_sum += chosen.probability;
			}
			i++;
		}
		if ( typeof chosen !== 'undefined' && typeof chosen.probability == 'number' ) {
			if(typeof chosen.title !== 'undefined' ){
				title = chosen.title;
			}
