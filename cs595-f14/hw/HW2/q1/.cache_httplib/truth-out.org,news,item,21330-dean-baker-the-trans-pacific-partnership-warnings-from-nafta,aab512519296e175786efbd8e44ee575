jQuery(document).ready(function() {
jQuery(document).scroll(function(){
    if(jQuery(this).scrollTop() > jQuery("#t3-mainbody").offset().top -50){
        jQuery('#t3-mainnav').addClass("navbar-fixed-top"); 
