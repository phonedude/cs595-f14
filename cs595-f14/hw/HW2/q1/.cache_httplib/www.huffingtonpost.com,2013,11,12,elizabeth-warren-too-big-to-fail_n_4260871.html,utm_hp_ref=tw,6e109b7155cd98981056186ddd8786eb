<section id="carousel" class="three-up"  data-beacon='{"p":{"mnid":"threeup_top_wrapper","mlid":"threeup"}}' >
    <div class="group three-up-list">
        <ul class="three-up-holder">
            <li class="three-up-item">
            <article  data-beacon='{"p":{"plid":"5870752","mpid":0}}' >
                <figure>
                    <a href="http://www.huffingtonpost.com/2014/09/23/oklahoma-police-rape_n_5870752.html"  data-beacon='{"p":{"lnid":"img"}}'  onclick="HPTrack.trackPageview('');"><img data-img-path="http://i0.huffpost.com/gen/2093680/thumbs/r-DANIEL-HOLTZCLAW-medium260.jpg" src="http://s1.huffpost.com/images/blank.gif" alt="Image for Cop's Tip For Not Getting Raped By A Cop: 'Don't Get Pulled Over'" class="async-img-load" /></a>
                </figure>
                <strong><a href="http://www.huffingtonpost.com/2014/09/23/oklahoma-police-rape_n_5870752.html"  data-beacon='{"p":{"lnid":"hdln"}}'  onclick="HPTrack.trackPageview('');">Cop's Tip For Not Getting Raped By A Cop: 'Don't Get Pulled Over'</a></strong>
                                <!-- need php for times here -->
                <time class="off" datetime="2014-09-23 18:03:23" pubdate>2014-09-23 18:03:23</time>
                            </article>
        </li>
            <li class="three-up-item">
