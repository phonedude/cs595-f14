	padding-top:10px;
	background:none;
	height:25px;
	list-style:none;
	margin:0;
	padding:0;
	}
	.dropnav li{
		float:right;
		padding:0px;
		}
	.dropnav li a{
		background:none bottom right no-repeat;
		color:#ffffff;
		display:block;
		font-weight:normal;
		line-height:25px;
		margin:0px;
		padding:0px 5px;
		padding-bottom: 6px;
		text-align:center;
		text-decoration:none;
		}
		.dropnav li a:hover, .dropnav ul li:hover a{
			 /* background: #005555 bottom center no-repeat; */
			  color:#FFFFFF;
			  text-decoration:underline; 
			}
