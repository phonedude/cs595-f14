		    <script>
            $(function() {

                $.is_mobile = function() { return false; };
                project_page.campaign_id = 11421;
                project_page.follow_url = "/my/follow/169499";
                project_page.facebook_backed = false;
                project_page.twitter_backed = false;
                project_page.tumblr_backed = false;
                project_page.total_backers = 1143;
                project_page.total_reach = 6142675;
                project_page.time_left = 'Complete!';
                project_page.goal = 250;
                project_page.progress = 457;
                project_page.time_progress = 4981.30773153169;
                
                project_page.facebook_link_copy = "Support on Facebook";
                project_page.twitter_link_copy = "Support on Twitter";

                project_page.init();

                $(".follow_user").bind("click", function(e) {
                    e.preventDefault();

                    if ($("#email").val() != "") {
                        Thunderclap.send({
                            url: project_page.follow_url,
                            success: function(data) {
                                if (data.success) {
                                    if (data.following == true) {
                                        $(".follow_user .update_button .follow").hide();
                                        $(".follow_user .update_button .following").show();
                                        $(".update_button").addClass("following_active");
                                        project_page.following = true;
                                    }
                                    else {
                                        $(".follow_user .update_button .following").hide();
                                        $(".follow_user .update_button .follow").show();
                                        $(".update_button").removeClass("following_active");
                                        project_page.following = false;
                                    }
                                }
                            }
                        });
                    }
                    else {
                        $('#update_email').foundation('reveal', 'open');
                    }
                });

                $("#email_save_form").keypress(function(e) {
                    if (e.keyCode == 13 && !e.shiftKey) {
                        e.preventDefault();
                        $(".email_save").click();
                    }
                });

                $("#email_save_form_outer").keypress(function(e) {
                    if (e.keyCode == 13 && !e.shiftKey) {
                        e.preventDefault();
                        $(".email_save_outer").click();
                    }
                });

                $(".email_save").bind("click", function(e) {
                    e.preventDefault();
                    var payload = $("#email_save_form").serialize();

                    Thunderclap.send({
                        url: $("#email_save_form").attr("action"),
                        type: $("#email_save_form").attr("method"),
                        data: payload,
                        button: $(".email_save"),
                        success: function(data) {
                            if (data.success) {
                                $(project_page.openModal).hide();
                                project_page.openModal = $('#support_confirmed');
                                $(project_page.openModal).fadeIn(project_page.animationOptions);

                                Thunderclap.send({
                                    url: project_page.follow_url, 
                                    success: function(data) {
                                        if (data.success) {
                                            if (data.following == true) {
                                                $(".follow_user .update_button .follow").hide();
                                                $(".follow_user .update_button .following").show();
                                                $(".update_button").addClass("following_active");
                                                project_page.following = true;
                                            }
                                            else {
                                                console.log("not followed");
                                                $(".follow_user .update_button .following").hide();
                                                $(".follow_user .update_button .follow").show();
                                                $(".update_button").removeClass("following_active");
                                                project_page.following = false;
                                            }
                                        }
                                    }
                                });
                            }
                            else {
                                alert("Invalid email address")
                            }
                        },
                        fail: function() {
                            Thunderclap.error("failed to save email")
                        }
                    });
                });

                $(".email_save_outer").bind("click", function(e) {
                    e.preventDefault();
                    var payload = $("#email_save_form_outer").serialize();

                    Thunderclap.send({
                        url: $("#email_save_form_outer").attr("action"),
                        type: $("#email_save_form_outer").attr("method"),
                        data: payload,
                        button: $(".email_save_outer"),
                        success: function(data) {
                            if (data.success) {
                                $('#update_email').foundation('reveal', 'close');
                                $("#email").val($("#email_outer").val())
                                $(".follow_user").click();
                            }
                            else {
                                alert("Invalid email address")
                            }
                        },
                        fail: function() {
                            Thunderclap.error("failed to save email")
                        }
                    });
                });

                // if ($.url_param("l") == "updates") {
                //     $(window).load(function() {
                //         $(".project_updates_button").click();
                //         $("html, body").animate({
                //             scrollTop: $(".project_updates_button:first").offset().top
                //         }, "slow");
                //     });
                // }
                if ($.url_param("network") == "twitter") {
                        $(".support_twitter_button").click();
                }
                if ($.url_param("network") == "facebook") {
                        $(".support_facebook_button").click();
                }
                if ($.url_param("network") == "tumblr") {
                        $(".support_tumblr_button").click();
                }
            });

            var twitter_url = "https://twitter.com/intent/tweet?related=lessig%3AOrganizer%2CThunderclapIt%3APowered+By&url=http%3A%2F%2Fthndr.it%2F1oaFVlj&text=I+just+supported+%28re%29start+the+Mayday+PAC+on+%40ThunderclapIt+%2F%2F+%40lessig";

            var fb_share = {
