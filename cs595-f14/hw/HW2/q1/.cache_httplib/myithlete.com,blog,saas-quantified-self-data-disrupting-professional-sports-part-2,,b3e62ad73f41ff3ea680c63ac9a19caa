	<script src="/blog/wp-includes/js/color.js"></script>

    
  <script>
  var imgDir = '/skin/iThlete/images/new';
  
  jQuery(document).ready(function ($) {
	  
	 $('#s').val('Search our Blog...');
	 
	$('.submit').val('');
	
	$('.hov').each(function(index, element) {
		        
		var oSrc = $(this).attr('src');
		var nSrc;
		
		if(oSrc.match('.png'))
		{
			nSrc = oSrc.replace('.png','-hover.png');
		}
		else if(oSrc.match('.gif'))
		{
			nSrc = oSrc.replace('.gif','-hover.gif');
		}
		else if(oSrc.match('.jpg'))
		{
			nSrc = oSrc.replace('.jpg','-hover.jpg');
		}
		
		$(this).hover(function(e) {
			$(this).attr('src',nSrc);
		}, function(e) {
			$(this).attr('src',oSrc);
		});
		
		$(document).append('<img src="'+nSrc+'" class="preload" />');
		
    });
	
	$('.flyout').hover(function(e) {
		$(this).children('.active').children('.arrow').attr('src',imgDir+'/nav-arrow-active.png');
	},function(e) {
		$(this).children('.active').children('.arrow').attr('src',imgDir+'/nav-arrow.png');
	});
	
	$('.fly').hover(function(e) {
		$(this).children('img').attr('src',imgDir+'/nav-arrow-active.png');
	},function(e) {
		$(this).children('img').attr('src',imgDir+'/nav-arrow.png');
	});

	//Search Box

	$('#s').focus(function(e) {
		if($(this).val() == 'Search our Blog...')
		{
			$(this).val('');
			$(this).css('color','#555555');
		}
	});
	
	$('#s').blur(function(e) {
		if($(this).val() == '')
		{
			$(this).val('Search our Blog...');
			$(this).css('color','#898989');
		}
	});
	
	//Newsletter Signup Box
	
	$('#semail').focus(function(e) {
		if($(this).val() == 'Email address...')
		{
			$(this).val('');
			$(this).css('color','#555555');
		}
	});

	$('#semail').blur(function(e) {
		if($(this).val() == '')
		{
			$(this).val('Email address...');
			$(this).css('color','#898989');
			$(this).animate({
					'backgroundColor':'#ffffff',
					'borderTopColor':'#d7d7d7',
					'borderBottomColor':'#d7d7d7',
					'borderLeftColor':'#d7d7d7',
					'borderRightColor':'#d7d7d7'
				},500);
			
		}
		else
		{
			if(validateEmail($(this).val()))
			{
				$(this).animate({
					'backgroundColor':'#dbffd7',
					'borderTopColor':'#b0ffaa',
					'borderBottomColor':'#b0ffaa',
					'borderLeftColor':'#b0ffaa',
					'borderRightColor':'#b0ffaa'
				},500);
			}
			else
			{
				$(this).animate({
					'backgroundColor':'#ffd7d7',
					'borderTopColor':'#ff8989',
					'borderBottomColor':'#ff8989',
					'borderLeftColor':'#ff8989',
					'borderRightColor':'#ff8989'
				},500);
			}
		}
	});
	
	//Tooltips
	$('.tt').each(function(index, element) {
		
        $(this).hover(function(e) {
			
			if(!$(this).children('.tooltip').height() > 0)
			{
				var html = '<section class="tooltip"><img src="/skin/iThlete/images/new/tooltip-base.png" class="arrow" />'+$(this).attr('data-tooltip')+'</section>';
				$(this).children('img').after(html);
			}		
		},function(e) {
			$('.tooltip').remove();
		});
    });
	
	function s_checkEmail()
{
	if(validateEmail($('#semail').val()))
	{
		return true;
	}
	else
	{
		$('#semail').animate({
			'backgroundColor':'#ffd7d7',
			'borderTopColor':'#ff8989',
			'borderBottomColor':'#ff8989',
			'borderLeftColor':'#ff8989',
			'borderRightColor':'#ff8989'
		},500);
		return false;
	}
}

function validateEmail(email) {
 
   var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
   
   if(reg.test(email) == false) 
   {
      return false;
   }
   else
   {
	   return true;   
   }
}
});

  
  
  
  </script>

	<div id="main" class="clearfix">
    
  
    

