</table><script language="javascript"  type="text/javascript">var url = "http://papers.ssrn.com/AjaxStatic.cfm?file=SuggBoxProcess"; // The server-side scriptvar isWorking = false;var windowwidth = 0;var windowheight = 0;var TZOffset = 0;var w_width = 0;var w_height = 0;if (window.outerWidth) {w_width = window.outerWidth;w_height = window.outerHeight;}else if (document.body.clientWidth) {w_width = document.body.clientWidth;w_height = document.body.clientHeight;}var mydate= new Date();var TZOffset = mydate.getTimezoneOffset()/60;function sendSuggest() {if (document.formSuggestions.txtEmail.value.length > 1000) {alert("Please make sure your suggestion is less then 1000 characters long.");}else {if (!isWorking && http) {var SugPageFrom = "/sol3/papers.cfm";var IpAddr = "128.82.53.39";var FullUrl = "papers.ssrn.com/sol3/papers.cfm";var strEmail = document.getElementById("txtEmail").value;var strFeedback = Trim(document.getElementById("txtFeedback").value);strFeedback = Trim(prepareString(strFeedback));http.open('POST',url,false);http.setRequestHeader('Content-Type','application/x-www-form-urlencoded');http.send("txtFeedback="+strFeedback+"&txtEmail="+strEmail+"&SugPageFrom="+SugPageFrom+"&IPAddr="+IpAddr+"&FullUrl="+FullUrl+"&w_width="+w_width+"&w_height="+w_height+"&TZOffset="+TZOffset);}resetForm();}}function getHTTPObject() {var xmlhttp;if (!xmlhttp && typeof XMLHttpRequest != 'undefined') {try {xmlhttp = new XMLHttpRequest();}catch (e) {xmlhttp = false;}}return xmlhttp;}var http = getHTTPObject(); // We create the HTTP Objectfunction closeBox() {document.getElementById('openbox').style.display='none';document.getElementById('closedbox').style.display='block';}function openBox() {document.getElementById('closedbox').style.display='none';document.getElementById('openbox').style.display='block';}function Trim(stringToTrim) {return stringToTrim.replace(/^\s+|\s+$/g,"");}function checkEmailContent(valueEmail) {if (valueEmail.value && valueEmail.value == '[enter valid email address here]'){if (valueEmail.value != ''){valueEmail.value='';}valueEmail.style.color='black';}checkFields();}function checkFeedback(valueFeedback){if (valueFeedback.value && valueFeedback.value == '[enter your feedback here]'){valueFeedback.value='';valueFeedback.style.color='black';}checkFields();}function checkFields(){var feedbackPost = Trim(document.getElementById('txtFeedback').value);var emailPost = Trim(document.getElementById('txtEmail').value);var bChkEmail = /^([0-9a-zA-Z]+[-._+&])*[0-9a-zA-Z]+@([-0-9a-zA-Z]+[.])+[a-zA-Z]{2,6}$/;if (feedbackPost == '[enter your feedback here]' || feedbackPost.length == 0 || !bChkEmail.test(emailPost)) {document.formSuggestions.btnSuggest.disabled=true;}else {document.formSuggestions.btnSuggest.disabled=false;}}function resetForm(){// Reset form fieldsdocument.forms['formSuggestions'].reset();// Close Feedback ModulecloseBox();// Disable submit buttondocument.formSuggestions.btnSuggest.disabled=true;// Set fields colorif (Trim(document.getElementById('txtFeedback').value) == '[enter your feedback here]'){document.getElementById('txtFeedback').style.color='#999999';}if (Trim(document.getElementById('txtEmail').value) == '[enter valid email address here]'){document.getElementById('txtEmail').style.color='#999999';}}/*This function prepare string for special characteres.javascript use:	str = str.replace(/stringToSearch/g,'stringToReplace');cfscript use:		str = replaceAll(str, 'stringToSearch','stringToReplace');*/function prepareString ( str ){str = str.replace(/%/g,'|-.2.-|');str = encodeURI(str);str = str.replace(/&/g,'|-.1.-|');str = str.replace(/‘/g,'|-.7.-|');str = str.replace(/’/g,'|-.8.-|');// need to make these replacement b/c a bug that appeared after the CF upgradestr = str.replace(/%u2019/g,"'");str = str.replace(/%u2018/g,"'");// need to make these replacement b/c a bug in Safaristr = str.replace(/%u201C/g,'%22');str = str.replace(/%u201D/g,'%22');str = str.replace(/%u2013/g,'-');str = str.replace(/%u2014/g,'-');str = str.replace(/%uF0FC/g,'*');str = str.replace(/%u2022/g,'*');str = str.replace(/%uF0A7/g,'*');return str;}function imposeMaxLength(Object, MaxLen){var tecla, int_value, out_value;if (Object.value.length > MaxLen){in_value = Object.value;out_value = in_value.substring(0,MaxLen);Object.value = out_value;return false;}return true;}
