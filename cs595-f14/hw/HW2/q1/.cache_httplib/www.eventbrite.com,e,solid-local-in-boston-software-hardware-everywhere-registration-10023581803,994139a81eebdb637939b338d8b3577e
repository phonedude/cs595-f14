<script>

function getViewportClass() {
    try {

        var viewportWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
    } catch(err) {
        return "NA";
    }

    if (!viewportWidth) {
        return "NA";
    }
    
    if (viewportWidth >= 800) {
        return "Large";
    }

    if (viewportWidth >= 480) {
        return "Medium";
    }

    return "Small";
}


// Register super properties from the template context
mixpanel.register({
    'TLD': '.com',
    'Authenticated?': false,
    'User Type': null,
    'Native?': false,
    'Locale': 'en_US',
    'Language': 'en\u002Dus'
});

// Register super properties via Javascript
mixpanel.register({
    'Viewport': getViewportClass()
});

window.MIXPANEL_DEFAULT_PROPERTIES = {
    'URL Name': 'ebapps.event_page.dispatch.event_landing_dispatch'
};
</script>

        
