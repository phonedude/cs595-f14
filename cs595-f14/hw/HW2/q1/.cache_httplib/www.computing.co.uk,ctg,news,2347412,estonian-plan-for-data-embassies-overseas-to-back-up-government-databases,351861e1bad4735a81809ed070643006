    //$.noConflict();
    jQuery(document).ready(function($){
        $("#mainmenu .top_menu li").hover(function(){
            $(this).addClass("active");
        }, function(){
            if ($(this).hasClass("active").toString()) 
                $(this).removeClass("active");
        });
        $(".submenu .top_sub_menu li").hover(function(){
            $(this).addClass("active");
        }, function(){
            if ($(this).hasClass("active").toString()) 
                $(this).removeClass("active");
        });
        $(".tabb_block li").hover(function(){
            $(this).addClass("active");
        }, function(){
            if ($(this).hasClass("active").toString()) 
                $(this).removeClass("active");
        });
    });
</script>
