</noscript>

<script type="text/javascript">
     $(function(){
         $('[name="poll_options_id"]').click(function(){
           if ($('[name="poll_options_id"]:checked')) {
               $('.vote_button').removeAttr('disabled');
             }
             else {
                $('.vote_button').attr('disabled', 'disabled');
              }
           });
                
         });
