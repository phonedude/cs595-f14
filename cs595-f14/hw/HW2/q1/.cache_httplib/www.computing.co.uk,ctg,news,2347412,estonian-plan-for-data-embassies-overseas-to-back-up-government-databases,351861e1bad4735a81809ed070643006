<script type="text/javascript">
jQuery(function($){
            
            //check window width onload
			if ($(window).width()>1054){
 				$(".largeHorizontal_share_buttons").hide();
				$("#large_share_wrapper").show();
 			} else {
  				$("#large_share_wrapper").hide();
				$(".largeHorizontal_share_buttons").show();
	 		}
	 
	 		//resize function 
			$(window).resize(function () {
		
				if ($(window).width()>1054){
			
				$("#large_share_wrapper").slideDown("fast");
				$(".largeHorizontal_share_buttons").slideUp("fast"); 
	
				} else {
	
				$(".largeHorizontal_share_buttons").slideDown("fast");
				$("#large_share_wrapper").slideUp("fast");
				}
			});
			//resize function ends here
            
                var shareWrap = $("#large_share_wrapper");
                var originalShareTop = shareWrap.offset().top;
                var view = $(window);
                view.bind("scroll resize", function(){
                    var viewTop = view.scrollTop();
                    if ((viewTop > originalShareTop) &&
                    !shareWrap.is(".share_fixed")) {
                    
                        shareWrap.removeClass("share_absolute").addClass("share_fixed");
                        
                    }
                    else 
                        if ((viewTop <= originalShareTop) &&
                        shareWrap.is(".share_fixed")) {
                        
                            shareWrap.removeClass("share_fixed").addClass("share_absolute");
                        }
                });
                
            });

        </script>
		
