<script type="text/javascript">
    function flameAuthor_submission(){
        $.ajax({
            type: "POST",
            url: '/home/flame_author',
            data: $("#flameid").serialize(),
            dataType: "text",
            beforeSend: function(){
            },
            error: function(){
            },
            success: function(update_data){
                if (update_data.match(/Errors\-(.*)/)) {
                    var a = update_data.split('Errors-')
                    
                    $("#flame_author_message").html(a[1]);
                }
                else {
                    location.reload();
                }
            }
        });
    }
    
    
    $(function(){
        $("#flame_author_id>a").click(function(event){
            event.preventDefault();
            jQuery("#flame_author").dialog("destroy");
            $("#flame_author").dialog({
                modal: true,
                buttons: {
                    Cancel: function(){
                        $(this).dialog('close');
                    },
                    'Submit': function(){
                        flameAuthor_submission();
                    }
                }
            })
            $(".ui-widget-overlay").css({
                'opacity': '0.3'
            });
        });
        
    });
    
$(function(){
    if (getCookie("thanks_comment")) {
        $("div#thanks_for_comment").html($("<h3></h3>").text(getCookie("thanks_comment").replace(/\+/g, " ")))
        deleteCookie("thanks_comment", "/;domain=.computing.co.uk")
        $(".dynamic_disp").css("display", "none");
        window.location.hash = "#comments";
    }
});
</script>
