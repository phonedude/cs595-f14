    (function()
    {
        MobileTest = function()
        {
            var winWidth = window.screen.width;
            var winHeight = window.screen.height;
            var maxWidth = 1023;
            var maxHeight = 768;
            
            return (maxWidth > winWidth && maxHeight > winHeight)
                || (maxWidth > winHeight && maxHeight > winWidth);
        };

        MobileCheckForRedirect = function()
        {
            if (MobileTest())
            {
                var portalName = Xsl._4;
                
                // Do not allow redirect for the following portals.
                switch (portalName)
                {
                    case "safariselect":
                    case "proquestselect":
                    case "proquestpubliclibrary":
                    case "academic":
                        return false;
                }

                var queryString = '';
                queryString += 'portal=' + encodeURIComponent(portalName);

                if (Xsl._13)
                {
                    queryString += '&key=' + encodeURIComponent(Xsl._5);
                    queryString += '&sessionid=' + encodeURIComponent(Xsl._6);
                }
                
                try
                {
                    // get promocode from cookie
                    var re1 = new RegExp('State=([^;]+)');
                    var cookie = re1.exec(document.cookie)[0];
                    var re2 = new RegExp('[&=]promocode=([^&]+)', 'gi');
                    var val = re2.exec(cookie)[1];
                       
                    if (val !== null && val.length > 0)
                        queryString += '&promocode=' + val;
                }
                catch(ex)
                {
                }
                
                var path = '';
                if (Xsl._11 == '/book')
                {
                    var fpid = Xsl._3;
                    var pos = fpid.indexOf('/');
                    if (pos > -1)
                        fpid = fpid.subStr(0, pos);
                    
                    queryString += '&fpid=' + encodeURIComponent(fpid);
                    path = 'public/content';
                }
                else
                {
                    path = 'login';
                }
                
                var subDomain = '';
                if (Xsl._18 != '1')
                    subDomain += 'rel2';

                switch (portalName)
                {
                    case "proquest":
                    case "proquestcombo":
                    case "proquestbus":
                    case "connect":
                    case "proquest_tech_de":
                    case "proquest_techbus_de":
                    case "proquest_business_de":
                        subDomain += ".proquest";
                        break;
                }
                
                if (portalName == "cedge")
                    window.location.replace('http://m' + subDomain + '.creativeedge.com/hd/' + path + '?' + queryString);
                else
                    window.location.replace('http://m' + subDomain + '.safaribooksonline.com/hd/' + path + '?' + queryString);

                return true;
            }
            
            return false;
        };
        
