var network, siteId, serverUrl;

if (prod) {
  network = 'storify.fyre.co';
  siteId = 355931;
  serverUrl = 'https://storify.admin.fyre.co';
} else {
  network = 'storify-int-0.fyre.co';
  siteId = 304796;
  serverUrl = 'https://storify-int-0.admin.fyre.co';
}

var authDelegate = new Livefyre.authDelegates.Remote(s.story.sid, siteId, serverUrl);
authDelegate.login = function() {
  s.auth.loginPopup(function() {
    Livefyre.user.login(s.auth.user.lftoken);
  });
};

var collectionMeta = {
    network: network,
    siteId: siteId, 
    articleId: s.story.sid, 
    title: s.story.title,
    url: s.story.permalink 
};

if (!prod) {
  collectionMeta.environment = 't402.livefyre.com';
}

var config = {
  selectors: '.s-element:not(.s-element-text) > div, .s-text > p, .s-text:not(:has(p, h2))',
  authDelegate: authDelegate,
  collectionMeta: collectionMeta
};

if (!prod) {
  config.environment = 't402.livefyre.com';
}

config.threadBtnDelegate = function (data) {
  if (data.blockType !== 4) {
    return null;
  }
  if (data.count) {
    return '<a class="fycon-action-view" id="{{ id }}"><label>{{ count }}</label></a>'.replace('{{ id }}', data.id).replace('{{ count }}', data.count);
  }
  return '<a class="fycon-action-view" id="{{ id }}"></a>'.replace('{{ id }}', data.id);
};

if (s.story.author && s.story.author.options && s.story.author.options.comments) {
  var sidenotes = new Livefyre.Sidenotes(config);

  sidenotes.on('sidenotes.initialized', function() {
    if (s.auth && s.auth.user && s.auth.user.lftoken)
      Livefyre.user.login(s.auth.user.lftoken);

  });
}


