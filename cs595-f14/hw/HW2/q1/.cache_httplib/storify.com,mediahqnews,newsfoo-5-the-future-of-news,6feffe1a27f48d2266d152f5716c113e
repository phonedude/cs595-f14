  s.page = new s.pages.Story();
  window.addEventListener('message', function(e) {
    if (e.origin === s.protocol + ':' + s.base_url) {
      try {
        var data = JSON.parse(e.data);
        if (data.sourceName.match(/_grid$/) && data.method === 'resize') {
          $('#story-frame')[0].height = (data.value + 10) + 'px';
        }
      } catch(err) {}
    }
  });
});

