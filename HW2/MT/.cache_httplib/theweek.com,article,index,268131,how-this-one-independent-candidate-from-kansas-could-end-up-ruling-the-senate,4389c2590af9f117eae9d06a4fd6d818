				<script>
var mclicksscroll_offset = 0;
var old_html;
var html;
var mclicks_limit = 6;
var article_id = 268131;
var loadingAnimation = new Image();
loadingAnimation.src = "https://7e8c.https.cdn.softlayer.net/807E8C/origin.theweek.com/images/loadingAnimation.gif?209";

jQuery(function(){
    
    // inital population of cells
    fillmclicksCells(6, 1);

});

function fillmclicksCells(limit, offset) {
	var mclicks_displayed = 0;
	old_html = jQuery("#mclicksscroll").html();
 	jQuery("#mclicksscroll").html('<img id="mclicks_loader" border="0" src="https://7e8c.https.cdn.softlayer.net/807E8C/origin.theweek.com/images/loadingAnimation.gif?209" />');
	
    console.log("Filling scroll cells, limit: "+(limit+1)+" offset: "+offset);
    jQuery.getJSON('/section/mclicks/'+(limit+1)+'/'+offset+'/'+article_id, function(data) { 
        html = [];
        jQuery.each(data, function(idx, article) {
        //for(i=data.length; i>=0; i--) {
            	mclicks_displayed++;
        		html.push('<div class="mclicksscrollcell"><a href ="'+article.url+'" onclick="_gaq.push([\'_trackEvent\', \'Miscellaneous\', \'Mclick_bottom_article\', \'Mclick_bottom_article\']);"><img border=0 style="max-width: 223px;padding-bottom: 12px;height:149px;" src="'+article.thumb+'"/>'+
            	        '<span style="padding-left: 0.5px;padding-right: 0.5px;">'+article.short_hed_captured+'</span></a></div>');
    	        if(mclicks_displayed == limit) return false; // break 
        //} 
        });
        
        var htmlstr = '';
        jQuery.each(html, function(idx, str) {
        	htmlstr += str;
        });
        
        jQuery("#mclicksscroll").html(htmlstr);
            if(data.length < limit) { 
            	jQuery("#mclicksscroll").append(jQuery(old_html).slice(0, limit-data.length)); // make sure to fill all cells
            }
            console.log(old_html);
    });
}
</script>

