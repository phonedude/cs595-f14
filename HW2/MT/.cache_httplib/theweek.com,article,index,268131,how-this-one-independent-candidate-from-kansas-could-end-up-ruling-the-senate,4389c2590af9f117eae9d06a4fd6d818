<script>
function isMsie() {
	var ua = window.navigator.userAgent;
	var msie = ua.indexOf("MSIE ");

        if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))
            return true
        else               
   			return false;
}

jQuery(function() { 
	// initial state = displayed for IE, hidden for everything else
	if (isMsie()) {
		var noscroll = true;
		showComments(noscroll);
	} 
	else { 
		hideComments();
	}

	jQuery( ".clickme" ).click(function(event) {
		event.preventDefault();
		//console.log("CLICKME (COMMENTS SHOW) WAS CLICKED");
		showComments();
  		//jQuery("#disqus_thread").show();
		//jQuery("#comments_hide_article").show();
		//window.location.hash="#disqus_thread";
		
	});
	if(window.location.hash=='#disqus_thread') {
		//jQuery(".clickme").trigger('click');
		showComments();
	}
});

(function($) {
    $.fn.goTo = function() {
        $(this)[0].scrollIntoView(true);
        return this; // for chaining...
    }
})(jQuery);

function showComments(noscroll) {
	jQuery("#disqus_thread").show();
	jQuery("#comments_hide_article").show();
	if(noscroll !== true) jQuery("#disqus_thread").goTo();
}

function hideComments() {
	jQuery("#disqus_thread").hide();
	jQuery("#comments_hide_article").hide();
}
</script>

<span id="comments_hide_article" style="display:none;" onClick="hideComments()">Hide Comments</span>

