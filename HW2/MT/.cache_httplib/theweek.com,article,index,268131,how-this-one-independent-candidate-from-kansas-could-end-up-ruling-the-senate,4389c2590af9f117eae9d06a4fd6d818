						function testAB()
						{
							var rand = Math.floor(Math.random()*2);
							var linkPath = linksArray[rand];
							var impressions = jQuery(linkPath).find("a#ab").attr("href");
							impressions = impressions.substr(impressions.length - 4);
							_gaq.push(['_trackEvent', 'Miscellaneous', 'A/B test impressions', impressions, null, true]);
							document.getElementById("test_A_B").innerHTML = linkPath;
	
						}
