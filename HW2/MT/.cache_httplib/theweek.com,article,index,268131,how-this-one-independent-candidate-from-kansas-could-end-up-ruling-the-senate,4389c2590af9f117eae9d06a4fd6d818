<script>
var newsletterTicked1
var newsletterTicked2
jQuery(document).ready(function() {
	jQuery("#subsrcibe").validate({
		rules: {
			email: {
				required: true
				,email: true
			}
		},
		errorPlacement: function(error, element) {
			error.appendTo( element.parent("div").next("div") );
		},
		submitHandler: function(form) {
			jQuery("#signup_btn").attr('disabled', 'disabled').css('opacity','.4');
			jQuery.ajax({
				url: '/api/newsletter/subscribe',
				data: {email: jQuery("#email-test").val(), dailybriefing: 1, coffeebreak: 1},
				dataType: 'json',
				jsonp: false,
				success: function(data) {
					if(data.success==true) {
						parent.newsletterTicked1 = 1;
						parent.newsletterTicked2 = 1;
						parent.formSubmitted = true;
						goUrl = '/home/newsletter/' + jQuery('#email-test').val().toLowerCase();
						window.location.href = goUrl;

					}
					else {
						jQuery("#signup_btn").attr('disabled', false).css('opacity', '1');
						alert(data.message);
					}
				}
			});

		}
	});

});


</script>

