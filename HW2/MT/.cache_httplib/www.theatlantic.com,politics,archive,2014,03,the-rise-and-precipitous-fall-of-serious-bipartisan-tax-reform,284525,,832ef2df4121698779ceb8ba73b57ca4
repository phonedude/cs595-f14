    <!-- Newsletter Promo -->
    <div class="rotating-article-promo" id="article-promo-newsletter">
        <strong>Get Today's Top Stories in Your Inbox</strong> 
        <a href="http://www.theatlantic.com/newsletters/top-stories/"
            data-omni-click="r'nwsltrpreview',r'dynamic'"
            onclick="window.open(this.href,'Sample Weekly Atlantic Website Newsletter','width=785,height=700,scrollbars=yes'); return false;"
            target="_blank">(preview)</a>
        <div class="message"></div>
        <form action="http://theatlantic.us2.list-manage.com/subscribe/post?u=ed2d318978b7dc9b30b1f464f&amp;id=f2eeb0a9f3"
              method="post"
              id="mc-embedded-subscribe-form"
              name="mc-embedded-subscribe-form"
              target="_blank">
            <label for="newsletter-email-address">Email Address</label>
            <input name="EMAIL" class="email-address" placeholder="Email Address" />
            <input class="checkbox" style="display:none;" type="checkbox" 
                name="group[7413][128]"
                value="128" 
                checked="checked" />
            <input class="button" type="submit" value="Sign Up" />
        </form>
    </div>

    <script type="text/javascript">
    (function(){
        var newsletterEnrollment = Atlantic.Utils.readCookie('newsletter-enrollment');
        var topStoriesNewsletterId = '7413:128';
        var promoDisplayNumber;

        // Set up interaction on the newsletter unit. Only gets called if the
        // newsletter unit is shown.
        var setUpNewsletter = function () {
            var $newsletterSignup = $('#article-promo-newsletter');
            var $form = $newsletterSignup.find('form');

            var successMsg = function (msg) {
                $form.hide();
                $newsletterSignup.find('.message').removeClass('error').addClass('success').html(msg);
            };

            var errorMsg = function (msg) {
                $newsletterSignup.find('.message').removeClass('success').addClass('error').html('<strong>Error:</strong> ' + msg);
            };

            $('#mc-embedded-subscribe-form').submit(function(event) {
                event.preventDefault();
                $(this).ajaxSubmit({
                    url: '/newsletters/ajax-sign-up/',
                    dataType: 'json',
                    success: function(data, statusText, xhr, $form) {
                        if (data && data.success) {
                            if (data.existing_user === true) {
                               successMsg("Your newsletter subscription preferences have been updated.");
                            } else {
                               successMsg("<h3>You're almost finished.</h3>" +
                                " We just need to confirm your email address." +
                                "<br/><br />" +
                                "To complete the subscription process, please click " +
                                "the link in the email we just sent you.");
                            }
                            window.Atlantic.Omniture.trackLink('nwsltrsubmit`dynamic');
                            Atlantic.Utils.createCookie('newsletter-enrollment', topStoriesNewsletterId, 30);
                        } else if (data && data.error) {
                            errorMsg(data.error);
                        } else {
                            errorMsg("An unknown error occurred.");
                        }
                    },
                    error: function() {
                        errorMsg("An unknown error occurred.");
                    }
                });
                return false;
            });
        };

        if ((newsletterEnrollment === null ||
            newsletterEnrollment.split(',').indexOf(topStoriesNewsletterId) === -1) &&
            document.location.pathname.search(/^\/magazine/) === -1) {

            // Generate a random integer between 1 and 5
            promoDisplayNumber = Math.ceil(Math.random() * 4);
        } else {
            promoDisplayNumber = Math.ceil(Math.random() * 3);
        }

        switch (promoDisplayNumber) {
            case 1:
                $('#article-promo-zen-ebook').css('display', 'block');
                break;
            case 2:
                $('#article-promo-wwi-magazine').css('display', 'block');
                break;
            case 3:
                $('#article-promo-magazine').css('display', 'block');
                break;
            case 4:
                $('#article-promo-newsletter').css('display', 'block');
                setUpNewsletter();
                break;
        }

    })();
    </script>
</div>

