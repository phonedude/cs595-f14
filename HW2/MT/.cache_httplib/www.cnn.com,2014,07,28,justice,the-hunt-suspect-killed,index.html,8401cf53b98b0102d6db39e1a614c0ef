ns.ContentMetadata = ns.ContentMetadata || {};
ns.ContentMetadata.pageTopVideo = {
autostart: true
};
}(CNN));
window.jQuery(document).ready(function() {
function isMobileClient () {
return (typeof window.orientation !== 'undefined');
}
window.CNNVIDEOS = window.CNNVIDEOS || {};
var CVPOptions = {
video: 'crime/2014/07/29/ctn-feyerick-suspect-killed-standoff.cnn',
thumb: 'http://i2.cdn.turner.com/cnn/dam/assets/140710110951-charles-mozdir-the-hunt-story-top.jpg',
preset: 'storypage',
diagnostics: {
section: 'articlepage',
placement: 'page-top'
},
