<script type="text/javascript">
  
jQuery(function($) {

  var redirect = function() {
    
  };

  BIT.Events = new BIT.Collections.EventList([{"rsvp_url":"http://www.bandsintown.com/event/6717373/facebook_rsvp?artist=The+Pimps+of+Joytime","artist_event_link_label":null,"ticket_url":null,"formatted_location":"Crested Butte, CO","venue_id":312067,"title":"Aug 28 - The Pimps of Joytime @ Crested Butte Mountain Resort - Crested Butte, CO","venue_name":"Crested Butte Mountain Resort","artist_id":11075790,"venue_location":{"region":"Colorado","latitude":38.89834,"longitude":-106.966515,"country":"United States","city":"Crested Butte"},"facebook_event_id":null,"formatted_datetime":"Wednesday, August 28, 2013 at 8:00pm","artist_event_id":10007087,"url":"http://www.bandsintown.com/event/6717373?artist=The+Pimps+of+Joytime","page_id":"38468488482","datetime":"2013-08-28T20:00:00","bit_ticket_url":null,"artist_name":"The Pimps of Joytime","published":true,"id":6717373,"artist_event_link_url":null,"ticket_type":"Tickets"}])

  $(".i-was-there").click(function(event) {
    var target = $(event.target),
    loading_image = $(".loading-small");
    loading_image.show();

    BIT.FB.Permissions.verify("email,user_likes,user_location,user_actions.music,rsvp_event", function(granted) {
      if (granted) {
        target.addClass("active");

        $.fb_ajax({
          url: "/facebook/rsvp",
          type: "POST",
          data: {
            came_from: "143",
            bandsintown_event_id: 6717373,
            artist_id: 11075790,
            rsvp_status: 'attending',
            track_artist: true
          },
          success: function(data) {
            BIT.FB.Timeline.go_to_event({
              event: BIT.Events.get(6717373),
              success: redirect,
              error: redirect
            });
          },
          error: redirect
        });
      }

      loading_image.hide();
    });
  });

});

</script>
