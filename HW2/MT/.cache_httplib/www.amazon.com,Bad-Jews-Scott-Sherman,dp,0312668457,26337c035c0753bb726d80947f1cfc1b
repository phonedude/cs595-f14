<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "%3CDIV%3E%3CDIV%3E%3CDIV%3E%3CP%3EScott%20Sherman%20has%20taken%20it%20upon%20himself%20to%20compile%20a%20list%20of%2050%20Oy%20vey-inducing%20members%20of%20the%20tribe%26%238212%3Bfrom%20politics%2C%20entertainment%20and%20white%20collar%20crime%26%238212%3Bwho%20make%20it%20tougher%20than%20it%20already%20%3CI%3Eis%20%3C%2FI%3Eto%20be%20a%20Jew%20these%20days.%20Notables%20include%3A%20%3C%2FP%3E%3CP%3E%3C%2FP%3E%3CP%3EBernie%20Madoff%3A%20Where%20to%20begin%3F%20Life%20is%20hard%20enough%20without%20having%20to%20be%20concerned%20that%20your%20esteemed%20brethren%20are%20really%20sleeper-schmucks%20lying%20in%20wait%2C%20biding%20their%20time%20for%20the%20perfect%20moment%20to%20completely%20ruin%20your%20life.%20%3C%2FP%3E%3CP%3ELaura%20Schlessinger%3A%20Referred%20to%20herself%20in%20a%20%3CI%3ELos%20Angeles%20Times%20Magazine%20%3C%2FI%3Eprofile%20as%20%26%238220%3Ba%20prophet.%26%238221%3B%20Let%26%238217%3Bs%20be%20nice%20and%20call%20that%20a%20stretch.%3C%2FP%3E%3CP%3EEliot%20Spitzer%3A%20Oh%20if%20only%20we%20could%20return%20to%20the%20days%20when%20Jewish%20mothers%20could%20call%20their%20little%20aspiring%20politicians%20%27The%20Next%20Eliot%20Spitzer%21%27%20without%20it%20sounding%20absolutely%20disgusting.%3C%2FP%3E%3CP%3EBarbra%20Streisand%3A%20Those%20imitating%20her%20shtick%20can%27t%20be%20tuned%20out%20because%2C%20much%20to%20our%20chagrin%2C%20they%20live%20with%20us.%20Think%20of%20all%20the%20poor%20young%20Jews%20who%20had%20to%20go%20to%20school%20unwashed%20because%20their%20sister%27s%20shower-based%20rendition%20of%20%22Papa%2C%20Can%20You%20Hear%20Me%3F%22%20took%20forever%20to%20complete.%20%3C%2FP%3E%3CP%3EJudge%20Judy%3A%20Who%20thought%20it%20was%20a%20good%20idea%20to%20give%20a%20Jewish%20grandmother%20a%20television%20show%20in%20which%20her%20opinions%20are%20legally%20binding%3F%20Releasing%20the%20wrath%20of%20bubbie%20onto%20the%20world%20is%20a%20cruel%2C%20cruel%20thing%20to%20do.%20%3C%2FP%3E%3CP%3ELeonard%20Nimoy%3A%20A%20Jew%20playing%20a%20pointy-eared%20half-alien%3F%20Everybody%20thinks%20we%26%238217%3Bre%20strange%20and%20foreign%20enough%20as%20it%20is%21%20%3C%2FP%3E%3C%2FDIV%3E%3C%2FDIV%3E%3C%2FDIV%3E",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    






