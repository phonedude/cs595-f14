             imgArray[8] = '<a class="open_gallery" href="http://binaryapi.ap.org/61c50d8bcf83452da66eb460d47ef94d/460x.jpg" title="10ThingstoSeeSports - A runner starts in the women&#39s 4x400-meter relay heat at the European Athletics Championships in Zurich, Switzerland, Saturday, Aug. 16, 2014. (AP Photo/Petr David Josek, File)"><img src="http://binaryapi.ap.org/61c50d8bcf83452da66eb460d47ef94d/460x.jpg" alt="10ThingstoSee-Sports" title="10ThingstoSee-Sports" /></a>'; 
             
    </script> 
    
        
    <div class="field-name-caption"><block><p>10ThingstoSeeSports - Atlanta Falcons center Peter Konz walks onto the field for an NFL football training camp practice Thursday, Aug. 14, 2014, in Houston. The Falcons are practicing with the Houston Texans before their preseason game Saturday. (AP Photo/David J. Phillip, File)</p></div>    

    <div id="information-small" class="small-12 show-for-small-only">
	<div class="small-10 columns byline-dateline">
            	</div>
        <a id="infoGallery"><span class="caption overlay-caption" data-thumb-index="0">i</span></a>
    </div>
</section>
 
    <script type="text/javascript">
    $(document).ready(function() {

        if (total_imgs == 1) {
            var aImg = $('.popup-gallery li a');
            
            var text = aImg.attr('title');
            
            $('.field-name-caption').html('<block><p>' + text + '</p></block>');
        }     
        
        var chrMax = 100;
        
        var origText = $('.field-name-caption block p').text();

        var charLength = $('.field-name-caption block p').text().length;
        
        var screen_width = $( window ).width();
        var width_mobile = 650;
        
        var pAbsoluteShow = 0;
        
        if (charLength > chrMax) {
            var shortText = jQuery.trim(origText).substring(0, chrMax).split(" ").slice(0, -1).join(" ") + "... ";
            $('.field-name-caption block p').text(shortText);            
            $('.field-name-caption block p').append('<a id="readMore">Read more</a>');
        }
        

        var screen_width = $( window ).width();

        if (screen_width <= width_mobile) {  
        
        $$('.mfp-figure figure').tap(function() { 
            var screen_width = $( window ).width();

            if (screen_width <= width_mobile) {  
                switchPDescription(); 
            }
        }); 

        $$('#p-absolute').unbind('tap').tap(function() { 
            var screen_width = $( window ).width();

            if (screen_width <= width_mobile) {                      
                switchPDescription(); 
            }
        }); 

        $$('.mfp-figure figure').unbind('swipeLeft').swipeLeft(function() { 
            var screen_width = $( window ).width();

            if (screen_width <= width_mobile) {                      
                if (mfp.index == (total_imgs - 1)) {
                    mfp.index = 0;
                }
                else {
                    mfp.index = mfp.index + 1;
                }      
                mfp.updateItemHTML();
            }
        });

        $$('.mfp-figure figure').unbind('swipeRight').swipeRight(function() {                
            var screen_width = $( window ).width();

            if (screen_width <= width_mobile) {                   
                if (mfp.index == 0) {
                    mfp.index = total_imgs - 1;
                }
                else {
                    mfp.index = mfp.index -1;
                }
                mfp.updateItemHTML();
            }
        });    
        
        }

       function configMobile() {
            var screen_width = $( window ).width();
            
            if (screen_width <= width_mobile) {        
                showDescription(pAbsoluteShow);        
            }
        }


        $('.popup-gallery').magnificPopup({
            delegate: 'a',
            type: 'image',
            tLoading: 'Loading image #%curr%...',
            mainClass: 'mfp-img-mobile',
            
            gallery: {
              enabled: true,
              navigateByImgClick: true,
              preload: [0,1] // Will preload 0 - before current, and 1 after the current image
            },
            
            image: {
                tError: '<a href="%url%">The image #%curr%</a> could not be loaded.',
                titleSrc: function(item) {
                    return item.el.attr('title');
                }
            },

            callbacks: {
                
                open: function() {

                    $('.mfp-bottom-carrousel').html("");

                    var img_total = 0;

                    $('.popup-gallery li a').each(function(idx,el){

                        img_total = img_total + 1;
                        var _img = document.createElement('img');

                        $(_img).attr('id', 'thumb-gallery-'+idx );
                        $(_img).attr('src', $(el).attr('href'));
                        $(_img).attr('alt', $(el).attr('title'));


                        if (idx == 0) $(_img).addClass('select');

                        $(_img).click(function(){
                            mfp.direction = (idx >= mfp.index);
                            mfp.index = idx;
                            mfp.updateItemHTML();

                            updateCarousel(idx);
                        });

                        if (only_one_photo == 0) {
                            $('.mfp-bottom-carrousel').append(_img);
                        }

                    })

                    var width_img_total = (img_total * 120) + 3;

                    $('.mfp-bottom-carrousel').width(width_img_total + 'px');
                    
                    configMobile();
                },
                close: function() {
                    $('#p-absolute').hide();
                    pAbsoluteShow = 0;
                }
                // e.t.c.
            }
        });
        
        $('#readMore').click(function(){
            $('.open_gallery').click();
            mfp.index = 0;
            mfp.updateItemHTML();
            updateCarousel(0);
        });  
        
        $('#infoGallery').click(function(){
            pAbsoluteShow = 1;
            $('.open_gallery').click();
            mfp.index = 0;
            mfp.updateItemHTML();
            updateCarousel(0);
        });   
        
        $('#button_comments').click(function(){
            if( $('#articleComments').is(":visible") ) {
                $('#articleComments').css('cssText', 'display: none !important');
                $('#button_comments').removeClass('comments_visible');
                $('#button_comments').html('SEE COMMENTS');
            }
            else {
                $('#articleComments').css('cssText', 'display: block !important');
                $('#button_comments').addClass('comments_visible');
                $('#button_comments').html('HIDE COMMENTS');                
            }
        });
        
        
    });
    </script>

 

<div  class="hnews-hidden"><div  class="hnews-author-org">By: <span  class="author vcard"><a href="/content/associated-press" class="fn">The Associated Press</a></span> (<span  class="source-org vcard"><span  class="org fn">AP</span></span>)</div><a  rel="item-license" id="license-27eb0547218b519551163a2a0a65e7b8" href="#license-27eb0547218b519551163a2a0a65e7b8">Copyright 2014 The Associated Press. All rights reserved. This material may not be published, broadcast, rewritten or redistributed.</a></div><div class="field field-name-body field-type-text-with-summary field-label-hidden entry-content"><div class="field-items"><div class="field-item even"><p><block><p>Here's a look at some of the defining moments, exciting scenes, triumphs and defeats from the world of sports.</p>
<p>This week's images include players from Australia and the Czech Republic at the Little League World Series in Williamsport, Pa.; a bullfighter painstakingly aiming his sword in Madrid, Spain; and synchronized swimmers performing at a championship meet in Berlin.</p>
<p>This gallery contains photos published Aug. 13-17.</p>
