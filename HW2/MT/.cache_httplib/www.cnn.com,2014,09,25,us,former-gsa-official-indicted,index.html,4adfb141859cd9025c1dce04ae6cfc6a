<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand32">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/120719031006-ac-griffin-gsa-investigation-00052508-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>Investigating wasteful spending at GSA</span></cite>
</div>
<script>
var currExpandable="expand42";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
