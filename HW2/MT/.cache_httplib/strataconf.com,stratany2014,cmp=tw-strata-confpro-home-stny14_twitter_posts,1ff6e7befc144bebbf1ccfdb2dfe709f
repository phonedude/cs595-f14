<script type="text/javascript">
  function initHomepageVideo() {
    var videoPlaying = false;

    var isIOS = (/iPhone|iPod|iPad/).test(navigator.userAgent);
    if (isIOS) {
      $('#looped-video').css('-webkit-transform','scale(0.01)');
    }

    $('#video-play').on('click', function(e) {
      e.preventDefault();
     var w = Math.max( $(window).width(), window.innerWidth);
      if (!videoPlaying) {
        videoPlaying = true;
        $(this).animate({'top':'50px'},1000).addClass('pause');
        $('#looped-video').clearQueue().animate({'opacity':1},1000).get(0).play();
        $('#video-quote').clearQueue().animate({'opacity':0},500);
        if (w > 634) {
           $('#video-section > .overlay').animate({'height':'74px','opacity':0},1000);
        }
        $('#video-btn').animate({'bottom':'49px','opacity':0},1000, function() {
          $(this).css('display','none');
        });
      }
      else {
        videoPlaying = false;
        $(this).animate({'top':'155px'},1000).removeClass('pause');
        $('#looped-video').clearQueue().animate({'opacity':1},1000).get(0).pause();
        if (w > 634) {
            $('#video-quote').clearQueue().animate({'opacity':1},500);
            $('#video-section > .overlay').animate({'height':'160px','opacity':1},1000);
        }
        if (w > 775) {
            $('#video-btn').css('display','block').animate({'bottom':'135px','opacity':1},1000);
        }
      }
    });
  }

             /* init homepage video */
             initHomepageVideo();

</script>

