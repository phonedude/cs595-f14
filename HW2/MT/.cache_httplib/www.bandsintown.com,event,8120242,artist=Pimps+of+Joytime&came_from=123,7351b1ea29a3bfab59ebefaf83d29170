<script type="text/javascript">
  
jQuery(function($) {

  var redirect = function() {
    
  };

  BIT.Events = new BIT.Collections.EventList([{"artist_event_link_label":null,"venue_id":911208,"venue_name":"Gardner's Farm","ticket_url":"http://www.bandsintown.com/event/8120242/buy_tickets?affil_code=fb_150146901795697\u0026artist=Chris+%22Hollywood%22+English","artist_id":6451587,"title":"Aug 21 - Chris \"Hollywood\" English @ Gardner's Farm - Stephentown, NY","bit_ticket_url":"http://www.bandsintown.com/event/8120242/buy_tickets?affil_code=fb_150146901795697\u0026artist=Chris+%22Hollywood%22+English","facebook_event_id":"653977491364081","page_id":"150146901795697","formatted_datetime":"Thursday, August 21, 2014 at 11:00am","artist_name":"Chris \"Hollywood\" English","artist_event_id":9908724,"url":"http://www.bandsintown.com/event/8120242?artist=Chris+%22Hollywood%22+English","formatted_location":"Stephentown, NY","datetime":"2014-08-21T11:00:00","ticket_type":"Tickets","artist_event_link_url":null,"published":true,"id":8120242,"venue_location":{"country":"United States","city":"Stephentown","region":"New York","latitude":42.54529,"longitude":-73.364975},"rsvp_url":"http://www.bandsintown.com/event/8120242/facebook_rsvp?artist=Chris+%22Hollywood%22+English"}])

  $(".i-was-there").click(function(event) {
    var target = $(event.target),
    loading_image = $(".loading-small");
    loading_image.show();

    BIT.FB.Permissions.verify("email,user_likes,user_location,user_actions.music,rsvp_event", function(granted) {
      if (granted) {
        target.addClass("active");

        $.fb_ajax({
          url: "/facebook/rsvp",
          type: "POST",
          data: {
            came_from: "143",
            bandsintown_event_id: 8120242,
            artist_id: 6451587,
            rsvp_status: 'attending',
            track_artist: true
          },
          success: function(data) {
            BIT.FB.Timeline.go_to_event({
              event: BIT.Events.get(8120242),
              success: redirect,
              error: redirect
            });
          },
          error: redirect
        });
      }

      loading_image.hide();
    });
  });

});

</script>
