<script type="text/javascript">

	// Disqus rotation
	// If before 8:30AM ET, set date to yesterday (noon)
	// If weekend set time to Friday
	// return YYYYMMDD string
	var disqusRotate = {
		validDate : new Date(),
		resetTimeUTC : { hour: 13, min : 30}, // 13:30 -  mkt open 14:30 (9:30 AM EST)
		urlqueryid : '?fngid=',	//id to use for disqus url (id)
		
		isresettime : function() {
			retval	 = false;
			currHour = this.validDate.getUTCHours();
			currMin  = this.validDate.getUTCMinutes();
			if((currHour > this.resetTimeUTC.hour) || (currHour = this.resetTimeUTC.hour && currMin >= this.resetTimeUTC.min)) {
				retval = true;
			}
			return retval;
		},

		isweekday : function() {
			currDay = this.validDate.getUTCDay()	// 0-6  (Sun-Sat)
			if(currDay > 0 && currDay < 6) {
				return true;
			}
			else {
				return false;
			}
		},

		travelbackaday : function (){
				hrpassed		= this.validDate.getUTCHours() + 1; //  hours since yesterday rounded up
				msecpassed		= hrpassed * 60 * 60 * 1000;	
				ydayinmsec		= this.validDate.getTime() - msecpassed;
				this.validDate	= new Date(ydayinmsec);
		},
		
		setdisqusvar : function () {
			if(this.isweekday()) {
				if(this.isresettime()) {
					month	= this.validDate.getUTCMonth()+1;
					day 	= this.validDate.getUTCDate();
					if(month < 10) { month = '0' + month; }
					if(day < 10) {	 day = '0' + day; }

					this.urlqueryid += this.validDate.getUTCFullYear() + month + day;
					//set disqus variable

					if(typeof (cnnmsoc_overrides) === 'undefined') 
					{
						cnnmsoc_overrides = {
					         disqus : {
					             url : 'http://money.cnn.com/data/fear-and-greed/' + this.urlqueryid,
					             id  : 'http://money.cnn.com/data/fear-and-greed/' + this.urlqueryid
					         }
					    }						
					}
					else {
						cnnmsoc_overrides.disqus = {
					             url : 'http://money.cnn.com/data/fear-and-greed/' + this.urlqueryid,
					             id  : 'http://money.cnn.com/data/fear-and-greed/' + this.urlqueryid
					         }
					}


				}
				else {
					// set time to yesterday
					this.travelbackaday();
					this.setdisqusvar();					
				}
			}
			else {
				// set time to yesterday
				this.travelbackaday();
				this.setdisqusvar();
			}
			
		}
		
	}
	
jQuery(document).ready(function(){
	try {
		Navigation.init();
		intlEdition_Dropdown();
		portfolioIframeChanger.init();
		fgAccordion();
		
		disqusRotate.setdisqusvar(); // must be set before CNNM.Social.init() is called 
		try{ CNNM.Social.init(); }catch(e){}
		
		setMagLinks();
				
		updateLinks('section_markets');	

		// Social - twitter follow and facebook like buttons
		try{
			cnnmsocial.facebook.loadscript();
			cnnmsocial.twitter.loadscript();
		}catch(e){};
		
		//twitter intent (nav + other areas)
		twitter_popup();
	
		// find all the input elements with title attributes
		jQuery('input[title!=""]').hint();
		
	} catch(e) {}
});


</script>

	
	<div id="csiIframe" style="visibility:hidden;height:0px;width:0px;"></div>
</div>
</body></html>
