<script language="Javascript" type="text/javascript">
  var jtd_array=new Array;
  jQuery(document).ready(function(){
    doSearch();
    what_gets_clicked();
  });

  function what_gets_clicked() {
    areas = [
      'Trail-List',
      'Recommendations-Product-Page',
      'Other-Editions',
      'More-Copies',
      'Other-Titles',
      'goodreads',
      'related_aisles',
      'dontmiss',
      'margin_random_author',
      'prefooter_list',
      'add_to_cart',
      'bookinfo',
      'sharethis_horizontal',
      'blurbs',
      'calendar',
      'awards',
      'customer_comments',
      'nav',
      'boxAboutThisBook',
      'more-copies-table-lowest-price'
    ];
    jQuery.each(areas, function(index, value) {
      jQuery("#" + value + " a").bind('click', function(){
        area = value;
        link_type = (jQuery(this).attr('rel')) ? jQuery(this).attr('rel') : null ;
        TrackEvent("Product-Page-What-Gets-Clicked",area,link_type,null,true);
      });
    });
  }
</script>

