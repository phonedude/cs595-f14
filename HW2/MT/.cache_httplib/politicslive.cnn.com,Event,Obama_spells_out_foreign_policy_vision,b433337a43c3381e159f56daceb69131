        var LeadCapFront;
        
        

        

        jQuerySL( document ).ready(function() {	

			

            if ( LiveBlog.IsjQueryAvailable == true && typeof SlideShow !== "undefined" )
            {
                SlideShow.Configure( false );
                
                if (SCRIBBLE.ui.SlideShow.Metrics ){
                    SCRIBBLE.ui.SlideShow.Metrics.init( {
                        endpoint: "http://cdnlove.scribblelive.com/slideshow/",
                        threadId: 619804,
