                        postContainer: "#Discussion #Posts",
                        sl_class: ".SL_SlideShow",
                        la_class: ".LiveArticle",
                        post_class: "LiveBlog_Post"
                    } );
                }
            }

            

           var opts = {
                PostShareText: "",
