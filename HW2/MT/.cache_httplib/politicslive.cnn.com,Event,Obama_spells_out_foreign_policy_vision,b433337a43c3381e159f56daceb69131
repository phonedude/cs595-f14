            jQuerySL('.scrbbl-fb-action-like').on('click', function () {
                jQuerySL(this).toggleClass('scrbbl-fb-action-liking');
                var FBURL = jQuerySL(this).parents().eq(1).find('.scrbbl-fb-date a').attr('href');
                var FBURLRegex = /.+?facebook\.com\/(\d+)\/posts\/(\d+)/i;
                var Matches = FBURLRegex.exec(FBURL);
                var that = this;

                if(Matches != null && Matches.length > 2) {
                    GigyaSL.CurrentFBPost = Matches[1] + '_' + Matches[2];
                    GigyaSL.CurrentFBCallback = function() {                        
                        jQuerySL(that).toggleClass('scrbbl-fb-action-liked');
