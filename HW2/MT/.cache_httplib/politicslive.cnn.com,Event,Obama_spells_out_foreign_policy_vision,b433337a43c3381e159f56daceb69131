<!-- ScribbleLive Internal Metrics -->

<div id="ScribbleMetriscControl_ScribbleLiveCounterBlock">
	

<script type="text/javascript">
    

if(typeof IsSLBackend == 'undefined' || IsSLBackend == null || IsSLBackend===false){
    (function ()
    {
        var CounterImage = null;
        var CookiesEnabled = true;
        var StatsUserId = null;
        var UniquesList = null;
        var SourceType = GetSourceType();

        function HitCounter(pIsPageView)
        {
            try
            {
                if (StatsUserId == null)
                {
                    StatsUserId = getCookie("SLStatUid");

                    if (StatsUserId == null)
                    {
                        // Generate a unique ID for the user
                        StatsUserId = (document.domain + "").hashCode() + "_" + (new Date()).getTime() + "_" + Math.floor((Math.random() * 10000000) + 1);
                        setCookie("SLStatUid", StatsUserId);

                        CookiesEnabled = (getCookie("SLStatUid") != null);
                    }
                }

                var ItemKey = null;

                if ( typeof SCRIBBLE !== "undefined" && typeof SCRIBBLE.globals !== 'undefined' )
                {
                    ItemKey = SCRIBBLE.globals.thread.id;
                }
                else if (typeof ThreadId != "undefined")
                {
                    ItemKey = ThreadId;
                }
                else if (typeof ScribbleLive_TopPostsWidget != "undefined")
                {
                    ItemKey = ScribbleLive_TopPostsWidget.ThreadId;
                }
                else if ( typeof LiveArticleEmbed != "undefined" && LiveArticleEmbed.GetInstance() != "undefined" && LiveArticleEmbed.GetInstance() != null )
                {
                    ItemKey = LiveArticleEmbed.GetInstance().ThreadId;
                }

                if ( typeof ItemKey !== "undefined" && ItemKey !== null )
                {
                    ItemKey = ( ItemKey + "" ).replace(/\s/ig, "");
                    var IsFirstView = pIsPageView;
                    var Client = -1;

                    if ( typeof SCRIBBLE !== "undefined" && typeof SCRIBBLE.globals !== 'undefined' )
                    {
                        Client = SCRIBBLE.globals.website.client.id;
                    }else if(typeof ClientId !== "undefined")
                    {
                        Client = ClientId;
                    }
                    if (pIsPageView)
                    {
                        if (UniquesList == null)
                        {
                            UniquesList = getCookie("SLStatHist");
                        }

                        if (UniquesList == null) { UniquesList = ""; }

                        // If we've already seen that item
                        if (UniquesList.match(new RegExp("(^|\|)" + ItemKey)))
                        {
                            IsFirstView = false;
                        }
                        else
                        {
                            var MAXCOOKIELENGTH = 3500;
                            var MAXVARLENGTH = 20000;

                            IsFirstView = true;
                            UniquesList += "|" + ItemKey;

                            if (UniquesList.length > MAXVARLENGTH)
                            {
                                UniquesList = UniquesList.substring(UniquesList.length - MAXVARLENGTH);
                                UniquesList = UniquesList.substring(UniquesList.indexOf("|"));
                            }

                            var CookieUniquesList = UniquesList;

                            if (CookieUniquesList.length > MAXCOOKIELENGTH)
                            {
                                CookieUniquesList = CookieUniquesList.substring(CookieUniquesList.length - MAXCOOKIELENGTH);
                                CookieUniquesList = CookieUniquesList.substring(CookieUniquesList.indexOf("|"));
                            }

                            setCookie("SLStatHist", CookieUniquesList);
                        }

                        //document.title = UniquesList;
                    }

                    CounterImage = new Image();
                    CounterImage.src = "//counter.scribblelive.com/?page=" + ItemKey
                            + (pIsPageView ? "&pageview=1" : "")
                            + (IsFirstView ? "&first=1" : "")
                    //+ (CookiesEnabled ? "" : "&nocookies=1")
                            + (StatsUserId ? "&uid=" + StatsUserId : "")
                            + "&rand=" + Math.round(100000000 * Math.random())
                            + "&Source=" + encodeURIComponent( GetSourceUrl( SourceType ) )
                            + "&SourceType=" + SourceType
                            + (Client != -1 ? "&Client=" + Client : "");

                    setTimeout(function ()
                    {
                        HitCounter();
                    }, getRandomInt(45000, 57000));
                }
            }
            catch (err) { }
        }

        function GetSourceUrl( pSourceType )
        {
            var Source = "";

            // Embed
            if ( pSourceType == 3 )
            {
                Source = document.referrer !== "" ? document.referrer : document.location.href;
            }
            else
            {
                Source = document.location.href;
            }

            return Source;
        }

        function GetSourceType()
        {
            // default source type is API unless we determine otherwise
            var $SourceType = 1;

            // Whitelabel
            if ( typeof IsWhitelabel !== "undefined" && IsWhitelabel === true )
            {
                $SourceType = 2;
            }
            // Embed
            else if ( typeof EmbedWidth !== "undefined" || (typeof SCRIBBLE !== "undefined" && typeof SCRIBBLE.globals !== "undefined" && SCRIBBLE.globals.embed === true) )
            {
                $SourceType = 3;
            }
            //  Article
            else if ( typeof IsArticlePage !== "undefined" && IsArticlePage === true )
            {
                $SourceType = 4;
            }
            // ScribbleLive.mobi Mobile Template
            else if ( (typeof IsMobileSite !== "undefined" && IsMobileSite === true) || (typeof SCRIBBLE !== "undefined" && typeof SCRIBBLE.globals !== "undefined" && SCRIBBLE.globals.mobile === true) )
            {
                $SourceType = 5;
            }
            // LiveArticle Embed
            else if ( typeof LiveArticleEmbed != "undefined" && LiveArticleEmbed.GetInstance() != "undefined" && LiveArticleEmbed.GetInstance() != null )
            {
                $SourceType = 6;
            }
            // Recent Posts Embed
            else if ( typeof RecentPosts !== "undefined" && typeof ScribbleLiveInstance !== "undefined" )
            {
                $SourceType = 7;
            }
            // Single Post Page
            else if ( typeof IsSinglePost !== "undefined" && IsSinglePost === true )
            {
                $SourceType = 8;
            }
            // Top Posts Widget
            else if (typeof ScribbleLive_TopPostsWidget != "undefined")
            {
                $SourceType = 9;
            }

            return $SourceType;
        }

        function getRandomInt(min, max)
        {
            return Math.floor(Math.random() * (max - min + 1)) + min;
        }

        function getCookie(name)
        {
            var start = document.cookie.indexOf(name + "=");
            var len = start + name.length + 1;
            if ((!start) && (name != document.cookie.substring(0, name.length)))
            {
                return null;
            }
            if (start == -1) return null;
            var end = document.cookie.indexOf(';', len);
            if (end == -1) end = document.cookie.length;
            return unescape(document.cookie.substring(len, end));
        }

        function setCookie(name, value, expires, path, domain, secure)
        {
            var today = new Date();
            today.setTime(today.getTime());
            if (expires)
            {
                expires = expires * 1000 * 60 * 60 * 24;
            }
            var expires_date = new Date(today.getTime() + (expires));
            document.cookie = name + '=' + escape(value) +
		            ((expires) ? ';expires=' + expires_date.toGMTString() : '') + //expires.toGMTString()
		            ((path) ? ';path=' + path : '') +
		            ((domain) ? ';domain=' + domain : '') +
		            ((secure) ? ';secure' : '');
        }

        String.prototype.hashCode = function ()
        {
            var hash = 0;
            var char2;
            if (this.length == 0) return hash;
            for (i = 0; i < this.length; i++)
            {
                char2 = this.charCodeAt(i);
                hash = ((hash << 5) - hash) + char2;
                hash = hash & hash; // Convert to 32bit integer
            }
            return hash;
        }

        HitCounter(true);
    })();

}


</script>


</div>
</body>

</html>
