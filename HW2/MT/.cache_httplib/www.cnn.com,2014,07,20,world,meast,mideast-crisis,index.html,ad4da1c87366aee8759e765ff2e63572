<a name="em15"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand148">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/140715184258-tsr-bts-blair-hamas-israel-middle-east-conflict-00030106-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>Tony Blair: 'Time is running out'</span></cite>
</div>
<script>
var currExpandable="expand248";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
mObj.type='video';
