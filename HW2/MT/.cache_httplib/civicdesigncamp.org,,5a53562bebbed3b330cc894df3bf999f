<script type="text/javascript" charset="utf-8">
	// check for duplicate gallery-containers on the page.. keep the last ones
	function checkForDupeGall(){
		if ($('.id-gallery-container').length > 1) {
			$('.id-gallery-container:first').remove();
			checkForDupeGall();
		}
	}
	checkForDupeGall();
</script>

