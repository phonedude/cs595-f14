<script type="text/javascript">
	(function( $ ) {
		var items=[];
		$.getJSON("http://www.wired.com/wiredcms/most_recent_posts.json", function( data ) {
			items.push('<li class="promo title"><h4 class="promo-title">MOST RECENT WIRED POSTS</li></h4></li>');
			// console.log( data );
			$.each( data, function( idx, post ) {
				// find a thumbnail
				var post_thumbnail = 'http://www.wired.com/wp-content/themes/wired/assets/images/200x100-fallback.jpg';
				if( post.custom_fields.imagePath200x100 != undefined && post.custom_fields.imagePath200x100 != '' ) {
					var post_thumbnail = post.custom_fields.imagePath200x100;
				} else if( post.custom_fields.imagePath300x160 != undefined && post.custom_fields.imagePath300x160 != '' )  {
					var post_thumbnail = post.custom_fields.imagePath300x160;
				}
	    		items.push( '<li class="promo-item" id="promo-item-count'+(idx+1)+'"><div class="promo-thumbnail"><a href="'+post.url+'"><img nopin="nopin" src="'+post_thumbnail+'" class="post-thumb" alt="'+post.title_plain+'" /></a></div>');
	    		items.push( '<div class="promo-wrapper"><div class="promo-headline"><a class="module-rsswidget" href="'+post.url+'">'+post.title+'</a></div></div></li>');
    		});
    		$("<ul/>", {
    			"class":"rss-widget item rss-promo",
    			html: items.join(""),
    		}).appendTo($("#sidebar-posts-aggregate"));
    	});
    	// console.log('MOST RECENT WIRED POSTS partial-sidebar-promo complete');
	})(jQuery);
</script></div>
