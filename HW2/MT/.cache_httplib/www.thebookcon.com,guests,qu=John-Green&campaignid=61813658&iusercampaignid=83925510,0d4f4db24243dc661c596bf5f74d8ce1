<script type="text/javascript">
//Remove any pseudos or styles from image anchors in the content area.
jQuery('img').parent('#content a').addClass('stripLink');
//Add placeholder text to search input.
jQuery("#ctl00_rightContentPlaceHolder_searchControlRHS_ctlAutoComplete_txtSearch").attr("placeholder", "Search for...");
// Header/nav styles on scroll
var $navAnchorStyles = jQuery("#topNavigation .level1 > li > a");
jQuery(window).scroll(function() {
	if (jQuery(window).scrollTop() > 0) {
		$navAnchorStyles.css({'line-height': '60px'});
	} else {
		$navAnchorStyles.css({'line-height': '90px'});
	}
});

jQuery(document).ready(function() {
	/*Query String Reader*/
	(function(jQuery){
		jQuery.getQuery = function( query ) {
			query = query.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
			var expr = "[\\?&]"+query+"=([^&#]*)";
			var regex = new RegExp( expr );
			var results = regex.exec( window.location.href );
			if( results !== null ) {
				return results[1];
				return decodeURIComponent(results[1].replace(/\+/g, " "));
			} else {
				return false;
			}
		};
	})(jQuery);
	//Pull query string below
	var qstring=jQuery.getQuery('qu');
	//Add hash before string
	var qelm = "#" + qstring;
	//If string, then call modal of choice here
	if(qstring){
		jQuery(qelm).foundation('reveal', 'open');
		//For callbacks, bind event since Foundation 4 somehow forgot this?
		/*jQuery(qelm).bind('opened', function() {
		  jQuery('.reveal-modal').append('<scr'+'ipt type="text/javascript" src="http://platform.twitter.com/widgets.js"></scr'+'ipt>');
		});*/
	}//end loader
	// Toolbar toggle functonality
	var $hide = jQuery('.hideToolbar-btn'),
		$show = jQuery('.showToolbar-btn'),
		$toolbar = jQuery('.fixedToolbar'),
		fixedToolbar = jQuery.cookie('fixedToolbar'),
		$this = jQuery(this);
	
	$toolbar.removeClass('none');
	// When you click the hide button...
	$hide.on("click", function(a){
		a.preventDefault();
		// ...do this...
		$toolbar.addClass('is-collapsed');
		$show.addClass('is-expanded');
		// Create/update cookie valid across entire site.
		jQuery.cookie('fixedToolbar', 'collapsed', { expires: 2, path: '/' }); // End addClass
	});// End click function
	
	// When you click the show button...
	$show.on("click", function(b){
		b.preventDefault();
		$show.removeClass('is-expanded')
		$toolbar.removeClass('is-collapsed');
		// Create/update cookie
		jQuery.cookie('fixedToolbar', 'expanded', { expires: 2, path: '/' });
	});// End click function
	
	// Make the yummy cookies work!
	// Set the user's selection for the left column
	if (fixedToolbar == 'collapsed') {
		$toolbar.addClass('is-collapsed');
		$show.addClass('is-expanded');
		jQuery.cookie('fixedToolbar', 'collapsed', { expires: 2, path: '/' });
	};// End collapsed cookie function & toolbar functions
	//Start sidr menu. Hack to prevent onClick from selecting a sidr anchor: https://github.com/artberri/sidr/issues/103
	jQuery('#hidden-toggle-link').sidr({ 
		name: 'sidr',
		speed: 400, 
		source: '#topNavigation'
		});
		jQuery('.menuToggle').on('click', function(e){
			e.preventDefault();
			jQuery('#sidr a').addClass('ui-disabled').attr('disabled','disabled');
			jQuery.sidr('toggle', 'sidr', function(){
				jQuery('#sidr a').removeClass('ui-disabled').removeAttr('disabled');
		});
	});
	/* Start original sider menu that breaks on some mobile. https://github.com/artberri/sidr/issues/103
	jQuery('.menuToggle').sidr({
		name: 'sidr',
		speed: 400,
		// side: 'left', // left or right 
		source: '#topNavigation' // A jQuery selector, an url or a callback function. 
		// renaming: true, // When filling the sidr with existing content, choose to rename or not the classes and ids.
		// body: 'body' // Version 1.1.0 an above. For doing the page movement the 'body' element is animated by default, you can select another element to animate with this option.
	});	*/
	//End sidr menu

	//Start Touchscreen
	if(Modernizr.touch){
	//Start touchSwipe
	jQuery(window).swipe({
		swipeRight: function() {
			// Open
			jQuery.sidr('open', 'sidr');
		},
		swipeLeft: function() {
			// Close
			jQuery.sidr('close', 'sidr');
		},
		preventDefaultEvents: false
	});
	//End touchSwipe
	//Remove href attribute from nav anchors that have children, but aren't leafs. Add after duplicating nav, i.e. sidr.
	jQuery('#topNavigation .level1 .hasChildren a').not("#topNavigation .level1 .leaf a").removeAttr('href');
	}//End Touchscreen
	jQuery.smartbanner({
		title: 'BookCon', // What the title of the app should be in the banner (defaults to <title>)
		author: 'DoubleDutch', // What the author of the app should be in the banner (defaults to <meta name="author"> or hostname)
		inAppStore: 'On the App Store', // Text of price for iOS
		inGooglePlay: 'In Google Play', // Text of price for Android
		icon: 'https://d359hczpzy9933.cloudfront.net/config/assets/b832762e-4b29-470a-9b7e-281f21bcf06c/deead52a-672a-4213-aa5c-f55332fad6bc/icon.png', // The URL of the icon (defaults to <link>)
		iconGloss: false, // Force gloss effect for iOS even for precomposed (true or false)
		button: 'VIEW', // Text on the install button
	})
});//end all scripts
</script> 
