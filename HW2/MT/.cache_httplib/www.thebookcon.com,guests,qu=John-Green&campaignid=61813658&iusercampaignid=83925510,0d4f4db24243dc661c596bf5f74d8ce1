    <script type="text/javascript">
        var _nls = new function () {
            this.q = _nls || [];
            this.requested = [];
            this.push = function (script) {
                this.q.push(script);
            };

            this.lazyLoad = function () {
                var that = this;
                for (var index = 0; index < this.q.length; index++) {
                    this.q[index].scriptPath = NovaJs.page.addVToUrl(this.q[index].scriptPath);
                    if(NovaJs.array.indexOf(this.q[index].scriptPath, this.requested) == -1) {
                        this.requested.push(that.q[index].scriptPath);

                        jQuery.ajax({
                            crossDomain: true,
                            dataType: "script",
                            cache: true,
                            url: this.q[index].scriptPath,
                            success: this.q[index].afterLoad,
                            complete: function () {
                                ////console.log('completed');
                            }
                        });

                    } else {
                        ////console.log('already requested');
                    }
                }
            };
        };

        jQuery(window).bind('load',
                                    function () {
                                        _nls.lazyLoad();
                                    });
    </script>
