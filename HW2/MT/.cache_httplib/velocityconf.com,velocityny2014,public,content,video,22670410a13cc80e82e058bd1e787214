         $(".accordion .module h2").accordion({clickTitle: 'h2', slideTime: '30'});

         /* make links with class=external open up external */
         $("a.external").click(function (e) {
             e.preventDefault();
             window.open($(this).attr("href"));
         });     

         
             /* accordian used on live stream */
            $(".accordion .module h4").accordion({clickTitle: 'h4', slideTime: '30'});
         

         

       
            if ($('div.banners').length) {
              $( "div.banner_ad" ).each( function ( i, e ) {
                      var banners = new Array();
                      $( e ).find( 'a[data-defer="random"]' ).each( function( i0, e0 ) {
                              banners.push( $( e0 ) );
                          });
                      
                      var b = banners[ Math.floor( Math.random() * banners.length ) ];
                      var i = $( "<img />" );
                      i.load( function() { b.html( i ); b.fadeIn( 1000 ); } );
                      i.attr( "src", b.attr( "data-img-src" ) );
                      i.attr( "alt", b.attr( "data-img-alt" ) );
                  });
             }//endif
         

         

         
             
        });
      }
  });
  

  

  </script>


