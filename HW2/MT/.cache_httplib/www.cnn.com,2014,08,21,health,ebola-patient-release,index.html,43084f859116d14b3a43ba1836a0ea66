<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand216">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/140815200612-erin-dnt-machado-american-ebola-doctor-00004422-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>U.S. doctor risks life to fight Ebola</span></cite>
</div>
<script>
var currExpandable="expand316";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
