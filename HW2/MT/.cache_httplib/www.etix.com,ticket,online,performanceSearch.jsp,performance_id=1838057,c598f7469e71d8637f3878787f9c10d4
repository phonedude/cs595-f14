<script type="text/javascript">

$(function () {
    var iframe = $('<iframe frameborder="0" marginwidth="0" marginheight="0" scrolling = "no" align = "middle" allowfullscreen></iframe>');
    var dialog = $("<div></div>").append(iframe).appendTo("body").dialog({
        autoOpen: false,
        modal: true,
        resizable: false,
        width: "1350",
        height: "750",
        close: function () {
            iframe.attr("src", "");
        }
    });
    $(".thumb a").on("click", function (e) {
        e.preventDefault();
        var src = $(this).attr("href");
        var title = $(this).attr("data-title");
        var width = $(this).attr("data-width");
        var height = $(this).attr("data-height");
        iframe.attr({
            width: +width,
            height: +height,
            src: "http://www.seats3d.com/theater/carpenter_theatre/#/level_2/"
        });
        
        dialog.dialog("option", "title", title).dialog("open");
    });
});

</script>

