<script language="JavaScript" type="text/javascript">
	$('.event_details').hide();
	$('.event_toggle').click(function(evt) {
		evt.preventDefault();
		var tb =  $(this).parent().find('.event_img_toggle');
		
		if(tb.attr('src').indexOf('expand') == -1)
		{
			tb.attr('src',"/ticket/online/images/more_info_expand.png" ) ;
		}
		else
		{
			tb.attr('src',"/ticket/online/images/more_info_collapse.png" ) ;
		}
		
		$(this).next('.event_details').slideToggle();
		
	});
	$('.event_img_toggle').click(function(evt) {
		evt.preventDefault();
		if($(this).attr('src').indexOf('expand') == -1)
		{
			$(this).attr('src',"/ticket/online/images/more_info_expand.png" ) ;
		}
		else
		{
			$(this).attr('src',"/ticket/online/images/more_info_collapse.png" ) ;
		}
		$(this).parent().find('.event_details').slideToggle();
	});
	
</script>
