mObj.origImageY="120";
mObj.contentType='video';
CNN.expElements.expand119Store=mObj;
</script>
<a name="em5"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand119">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/130812122259-exp-hm-student-vaccinations-00000901-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>What vaccines do children need?</span></cite>
</div>
<script>
var currExpandable="expand219";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
