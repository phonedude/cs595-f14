mObj.videoSource='CNN';
mObj.videoSourceUrl='http://ac360.blogs.cnn.com/?hpt=hp_tvvideo';
mObj.lgImage="http://i2.cdn.turner.com/cnn/dam/assets/140528134917-01-phoenix-va-hosptial-0517-story-body.jpg";
