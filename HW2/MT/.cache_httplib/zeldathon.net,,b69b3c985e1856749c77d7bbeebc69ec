		<!-- ngg_resource_manager_marker -->				<script type="text/javascript">
			
			var remaining_time = -30831698000;
			var marathon_start_time = 1380916800000;
			var marathon_end_time = 1381089600000;
			var marathon_duration = 172800000;
			var checked = false;
			
			jQuery(document).ready (
				function() {
								
					checkDonations();
					setInterval('checkDonations()', 300000);
					setInterval('timeCounter()', 1000);
				}
			);
			function checkDonations() {
				
				if(checked == false) {
					return;
				}
				//var page = "/DonationAdmin/json.php";
				var page="/DonationAdmin/organics.php";
				jQuery.getJSON(page,
					function(json) {
					}
				)
				.success(
					function(json) {
						
						var newTotal = json.total;
						jQuery("#total-donations").html(newTotal);
					}
				);
			}
			function timeCounter() {
				
				remaining_time = remaining_time - 1000;		// Subtract one second to timer
				
				if(remaining_time <= 0) {
					remaining_time = 0;
				}
				
				display_time = remaining_time;
				
				var playedHours = pad(Math.floor(display_time / 3600000));
				display_time = display_time % 3600000;
				
				var playedMinutes = pad(Math.floor(display_time / 60000));
				display_time = display_time % 60000;
				
				var playedSeconds = pad(Math.floor(display_time / 1000));
				
				
				if(remaining_time <= marathon_duration) {
					jQuery("#time-remaining").html(playedHours + ":" + playedMinutes + ":" + playedSeconds);
					
					if(checked == false) {
						checked = true;
						checkDonations();
					}
				}
			}

			function pad(number) 
			{
				
				if(number.toString().length < 2)
					return "0" + number.toString();
				
				return number.toString();
			}
		</script>
