     data-seo-title="fault-in-our-stars-fan-event-in-nashville" data-ssts="entertainment/people"
     data-cst="entertainment/people" data-base-page-type="gallery"
     
     data-topic="lifestyle"
      data-gal-pageurl="http://www.tennessean.com/picture-gallery/entertainment/people/2014/05/08/fault-in-our-stars-fan-event-in-nashville/8880499/"><div class="viewport gallery-viewport"><div class="slide gallery-viewport-slide horizontal active"  itemprop="primaryImageOfPage"  itemscope itemtype="http://schema.org/ImageObject"><div class="gallery-photo-border"><img itemprop="image" class="gallery-photo horizontal"
                                
                                    
