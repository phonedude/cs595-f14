    <script type="text/javascript">
    // trigger floating share tool to show after scroll past 2nd paragraph
    var p = jQuery( "#story-page-embedded-after3-ad" ); var pposition = p.position();
    var e = jQuery( "#taboola-container" ); var eposition = e.position();
    var win_x = jQuery(window).width(); var win_y = jQuery(window).height();
    jQuery(document).scroll(function(){
        if( win_x < 1023 ){
            var scroll = jQuery(window).scrollTop();
            console.log( "marker top: " + pposition.top + " | marker end : " + eposition.top + " | cur: " + scroll );
            if( scroll + win_y > pposition.top && scroll < eposition.top ){ 
                console.log('show');
                jQuery('.share-container-bottom').css('display','block');
            } else { 
                console.log('hide');
                jQuery('.share-container-bottom').css('display','none');
            }
        }
        //console.log( 'window: ' + win_x + 'x' + win_y );
    });
    jQuery(document).ready(function(){
        // pull aggregate stat
        get_share_stats( window.location, '.total_share_count', '<img src="http://www.rawstory.com/rs/wp-content/themes/worldwide-v1-02/images/shares.jpg">' );
    });
    </script>
    
