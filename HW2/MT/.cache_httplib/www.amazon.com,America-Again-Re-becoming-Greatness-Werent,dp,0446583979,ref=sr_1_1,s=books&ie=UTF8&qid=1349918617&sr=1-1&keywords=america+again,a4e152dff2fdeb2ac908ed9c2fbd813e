<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "Book%20store%20nation%2C%20in%20the%20history%20of%20mankind%20there%20has%20never%20been%20a%20greater%20country%20than%20%3CST1%3ACOUNTRY-REGION%20w%3Ast%3D%22on%22%3E%3CST1%3APLACE%20w%3Ast%3D%22on%22%3EAmerica%3C%2FST1%3APLACE%3E%3C%2FST1%3ACOUNTRY-REGION%3E%3Ci%3E.%20%3C%2Fi%3EYou%20could%20say%20we%27re%20the%20%231%20nation%20at%20being%20the%20best%20at%20greatness.%3Cbr%3E%3Cbr%3EBut%20as%20perfect%20as%20%3CST1%3ACOUNTRY-REGION%20w%3Ast%3D%22on%22%3EAmerica%3C%2FST1%3ACOUNTRY-REGION%3E%20is%20in%20every%20single%20way%2C%20%3CST1%3ACOUNTRY-REGION%20w%3Ast%3D%22on%22%3E%3CST1%3APLACE%20w%3Ast%3D%22on%22%3EAmerica%3C%2FST1%3APLACE%3E%3C%2FST1%3ACOUNTRY-REGION%3E%20is%20broken%21%20And%20we%20can%27t%20exchange%20it%20because%20we%27re%20236%20years%20past%20the%2030-day%20return%20window.%20Look%20around--we%20don%27t%20make%20anything%20anymore%2C%20we%27ve%20mortgaged%20our%20future%20to%20%3CST1%3ACOUNTRY-REGION%20w%3Ast%3D%22on%22%3E%3CST1%3APLACE%20w%3Ast%3D%22on%22%3EChina%3C%2FST1%3APLACE%3E%3C%2FST1%3ACOUNTRY-REGION%3E%2C%20and%20the%20Apologist-in-Chief%20goes%20on%20world%20tours%20just%20to%20bow%20before%20foreign%20leaders.%20Worse%2C%20the%20%3CST1%3APLACE%20w%3Ast%3D%22on%22%3E%3CST1%3ACITY%20w%3Ast%3D%22on%22%3EL.A.%3C%2FST1%3ACITY%3E%3C%2FST1%3APLACE%3E%20Four%20Seasons%20Hotel%20doesn%27t%20even%20have%20a%20dedicated%20phone%20button%20for%20the%20Spa.%20You%20have%20to%20dial%20an%20extension%21%20Where%20did%20we%20lose%20our%20way%3F%21%3Cbr%3E%3Cbr%3EIt%27s%20high%20time%20we%20restored%20%3CST1%3APLACE%20w%3Ast%3D%22on%22%3E%3CST1%3ACOUNTRY-REGION%20w%3Ast%3D%22on%22%3EAmerica%3C%2FST1%3ACOUNTRY-REGION%3E%3C%2FST1%3APLACE%3E%20to%20the%20greatness%20it%20never%20lost%21%3Cbr%3E%3Cbr%3ELuckily%2C%20%3Ci%3EAMERICA%20AGAIN%3C%2Fi%3E%20will%20singlebookedly%20pull%20this%20country%20back%20from%20the%20brink.%20It%20features%20everything%20from%20chapters%2C%20to%20page%20numbers%2C%20to%20fonts.%20Covering%20subject%27s%20ranging%20from%20healthcare%20%28%22I%20shudder%20to%20think%20where%20we%27d%20be%20without%20the%20wide%20variety%20of%20prescription%20drugs%20to%20treat%20our%20maladies%2C%20such%20as%20think-shuddering%22%29%20to%20the%20economy%20%28%22Life%20is%20giving%20us%20lemons%2C%20and%20we%27re%20shipping%20them%20to%20the%20Chinese%20to%20make%20our%20lemon-flavored%20leadonade%22%29%20to%20food%20%28%22Feel%20free%20to%20deep%20fry%20this%20book-it%27s%20a%20rich%20source%20of%20fiber%22%29%2C%20Stephen%20gives%20America%20the%20dose%20of%20truth%20it%20needs%20to%20get%20back%20on%20track.",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    






