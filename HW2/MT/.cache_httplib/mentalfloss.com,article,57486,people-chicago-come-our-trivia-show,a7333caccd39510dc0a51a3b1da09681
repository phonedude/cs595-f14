    <div class="meta">
      <div class="meta-inner clearfix">
        <div class="field-group-format group_meta field-group-div group-meta  speed-fast effect-none"><div class="field field-name-field-enhanced-authors field-type-computed field-label-hidden"><div class="field-items"><div class="field-item even"><a href="/authors/jason-english">Jason English</a></div></div></div></div>        <span class="share-top">
        <!-- AddThis Button BEGIN --><span class="addthis_toolbox addthis_default_style "><a class="addthis_button_print"></a><a class="addthis_button_email"></a><a class="addthis_button_stumbleupon"></a><a class="addthis_button_twitter"></a><a class="addthis_button_facebook"></a></span><script type="text/javascript" src="http://s7.addthis.com/js/250/addthis_widget.js#pubid=ra-508584d6026095b3"></script><!-- AddThis Button END -->        </span>
