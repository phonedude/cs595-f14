<script type="text/javascript">
    $(document).ready(function () {
        if ($(this).val() == "") {
            $(this).val("Your Email");
        }
        $('#ContentPlaceHolder1_leftupcomingeventsubscribers_txtEmail').focus(function () {
            if ($(this).val() == "Your Email") {
                $(this).val("");
            }
        });
        $('#ContentPlaceHolder1_leftupcomingeventsubscribers_txtEmail').focusout(function () {
            if ($(this).val() == "") {
                $(this).val("Your Email");
            }
            validateFO(false, 'ContentPlaceHolder1_leftupcomingeventsubscribers_txtEmail', "Email Address");
        });
    });
    function validate() {
        return validateFO(true, 'ContentPlaceHolder1_leftupcomingeventsubscribers_txtEmail', "Email Address");
    }
</script>
