    <script type="text/javascript" charset="utf-8">
        jqCal(function () {
            Date.firstDayOfWeek = 0;
            Date.format = 'mm/dd/yyyy';
            if ($('#ContentPlaceHolder1_leftcalendar_hfDate').val() != "") {
                jqCal('.turn-me-into-datepicker')
					.datePickerHome({ inline: true, startDate: '01/01/2011' })
                    .dpSetSelected($('#ContentPlaceHolder1_leftcalendar_hfDate').val())
					.bind(
						'dateSelected',
						function (e, selectedDate, $td) {
						    $('#ContentPlaceHolder1_leftcalendar_hfDate').val(selectedDate.asString());
						    __doPostBack('ctl00$ContentPlaceHolder1$leftcalendar$hfDate', '');
						}
					);
            }
            else {

                jqCal('.turn-me-into-datepicker')
					.datePickerHome({ inline: true, startDate: '01/01/2011' })
					.bind(
						'dateSelected',
						function (e, selectedDate, $td) {
						    $('#ContentPlaceHolder1_leftcalendar_hfDate').val(selectedDate.asString());
						    __doPostBack('ctl00$ContentPlaceHolder1$leftcalendar$hfDate', '');
						}
					);
            }
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            if ($(this).val() == "") {
                $(this).val("Enter Artist/Event/Venue");
            }
            $('#txtsearch').focus(function () {
                if ($(this).val() == "Enter Artist/Event/Venue") {
                    $(this).val("");
                }
            });

            $('#txtsearch').focusout(function () {
                if ($(this).val() == "") {
                    $(this).val("Enter Artist/Event/Venue");
                }
                $('#txtsearch').validateSpecCharSearch(iChars, "search");
            });
            $('#txtsearch').keypress(function (event) {
                if (event.keyCode == '13') {
                    $('#txtsearch').validateSpecCharSearch(iChars, "search");
                    if (($('#txtsearch').val() != "") && ($('#txtsearch').val() != "Enter Artist/Event/Venue")) {
                        __doPostBack('ctl00$imgbtnsearch', '');
                    }
                    return false;
                }
            });
        });
        function validateSearch() {
            $('#txtsearch').validateSpecCharSearch(iChars, "search");
            if (($('#txtsearch').val() == "") || ($('#txtsearch').val() == "Enter Artist/Event/Venue"))
                return false;
            return true;
        }    
    </script>
    <script type="text/javascript" language="javascript">
        function is_loaded() {
            $('#preloader').fadeOut('slow', function () { $(this).remove(); });
        }
    </script>
    
