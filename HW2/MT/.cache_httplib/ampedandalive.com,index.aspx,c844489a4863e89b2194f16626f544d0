    <script type="text/javascript">
        function alertEffect() {
            $('#preloader').fadeOut('slow', function () { $(this).remove(); });
            $(".btnAddToCart").click(function (e) {
                var msg;
                var valid = false;
                var top;
                var left;
                var width = 269;
                var height = 47;
                var inputFactor = 30;
                var selectFactor = 20;
                if ($(this).parent().parent().find("select").size() > 0) {
                    var selectVal = $(this).parent().parent().find("select").val();
                    top = $(this).parent().parent().find("select").offset().top - (height / 2) + 10;
                    left = ($(this).parent().parent().find("select").offset().left - width) - selectFactor;
                    if (selectVal == "0") {
                        msg = "Please select a Price.";
                    }
                    else {
                        valid = true;
                    }
                }
                else {
                    valid = true;
                }
                if (valid) {
                    var textVal = $(this).parent().parent().find("input:text").val();
                    if (textVal == "") {
                        msg = "Please fill out Quantity.";
                        top = $(this).parent().parent().find("input:text").offset().top - (height / 2) + 10;
                        left = ($(this).parent().parent().find("input:text").offset().left - width) - inputFactor;
                        valid = false;
                    }
                    else if (isNaN(textVal)) {
                        msg = "Please fill out a valid Quantity.";
                        top = $(this).parent().parent().find("input:text").offset().top - (height / 2) + 10;
                        left = ($(this).parent().parent().find("input:text").offset().left - width) - inputFactor;
                        valid = false;
                    }
                    else if (textVal == 0) {
                        msg = "Please fill out a valid Quantity.";
                        top = $(this).parent().parent().find("input:text").offset().top - (height / 2) + 10;
                        left = ($(this).parent().parent().find("input:text").offset().left - width) - inputFactor;
                        valid = false;
                    }
                    else if (parseInt(textVal) > 29) {
                        msg = "Sorry. Quantity must be less than 30.";
                        top = $(this).parent().parent().find("input:text").offset().top - (height / 2) + 10;
                        left = ($(this).parent().parent().find("input:text").offset().left - width) - inputFactor;
                        valid = false;
                    }
                }
                if (!valid) {
                    $('body').append("<div class='divFlyMessage'></div><div class='divMessage' style='top:" + top + "px;left:" + left + "px;width:" + width + "px;height:" + height + "px;'><div>" + msg + "</div></div>");
                    $('.divFlyMessage').fadeTo("slow", .30);
                    $('.divMessage').fadeTo("slow", 1.00);
                    $('.divFlyMessage').click(function () {
                        if ($('.divFlyMessage').size() > 0) {
                            $('.divFlyMessage').fadeOut(400, function () {
                                $('.divFlyMessage').remove();
                            });
                            $('.divMessage').fadeOut(400, function () {
                                $('.divMessage').remove();
                            });
                        }
                    });
                    e.preventDefault();
                }
                else {
                    $('#preloader').fadeTo("slow", 1.00);
                }
            });
        }
        function showLimitExceed(controlId, msg) {
            var top;
            var left;
            var width = 269;
            var height = 47;
            var inputFactor = 30;
            top = $("#" + controlId).offset().top - (height / 2) + 10;
            left = ($("#" + controlId).offset().left - width) - inputFactor;
            $('body').append("<div class='divFlyMessage'></div><div class='divMessage' style='top:" + top + "px;left:" + left + "px;width:" + width + "px;height:" + height + "px;'><div>" + msg + "</div></div>");
            $('.divFlyMessage').fadeTo("slow", .30);
            $('.divMessage').fadeTo("slow", 1.00);
            $('.divFlyMessage').click(function () {
                if ($('.divFlyMessage').size() > 0) {
                    $('.divFlyMessage').fadeOut(400, function () {
                        $('.divFlyMessage').remove();
                    });
                    $('.divMessage').fadeOut(400, function () {
                        $('.divMessage').remove();
                    });
                }
            });
            alertEffect();
        }
    </script>
    <script type="text/javascript">
        function IAMIntheUserControl() {
            alertEffect();
        }
    </script>
