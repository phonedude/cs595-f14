<script src="//df17938sh9pb.cloudfront.net/shared/js/social-buttons.js"></script>
<script src="//df17938sh9pb.cloudfront.net/shared/bower_components/jquery/dist/jquery.min.js"></script>
<script src="//df17938sh9pb.cloudfront.net/shared/bower_components/modernizr/modernizr.js"></script>
<script src="//df17938sh9pb.cloudfront.net/shared/bower_components/webshim/js-webshim/minified/polyfiller.js"></script>
<script src="//df17938sh9pb.cloudfront.net/shared/bower_components/moment/min/moment.min.js"></script>


  <script type="text/javascript">
    'use strict';

    webshims.setOptions('forms', {
      lazyCustomMessages: true,
      addValidators: true,
      iVal: {
        fx: 'no'
      }
    });
    webshims.setOptions('forms-ext', {
    });
    webshims.polyfill('forms forms-ext');

    $(function(){

      if(Modernizr.touch) {
        return;
      }

      var $fadeIn = $('.fade-in-on-scroll');
      $fadeIn.each(function(i, elem){
        $(elem).addClass('fade-out');
      });

      $(document).scroll(function(){
        var height = $(window).height();
        var scrollTop = $(document).scrollTop();
        var bottom = scrollTop + height * 0.85;
        var $headerContent = $('.header-content');
        var headerContentTop = $headerContent.size() ? $headerContent.position().top : 0;
        var headerBottom = headerContentTop + $headerContent.height() * 0.75;

        $headerContent.toggleClass('fade-out', headerBottom < scrollTop);

        $fadeIn.each(function(i, elem){
          if(bottom > $(elem).position().top) {
            $(elem).removeClass('fade-out');
          }
        });
      });

    });


    $(function(){
      $('.ticket-order-form .count-field').on('valuevalidation', function(e, extra){
        var sum = 0;
        $('.ticket-order-form .count-field').each(function(i, field){
          var val = parseInt($(field).val())
          if(val >= 0)
            sum += val;
        });
        if(sum > 8)
          return 'You may not buy more than 8 tickets. Sorry!';
        if(sum == 0)
          return 'You missed to specify a ticket';
      });
    });

    $(function(){
      $(".tickets-order-view .tickets-release-date").each(function(i, elem){
        var $elem = $(elem);
        var date = moment($elem.attr('data-release-date'));
        var tz;

        if(window.Intl !== undefined)
          tz = Intl.DateTimeFormat().resolved.timeZone

        if(!tz || tz.length <= 0)
          tz = new Date().toString().match(/\(([^)]*)\)$/)[1]

        if(tz && tz.length > 0)
          $elem.text(date.format('MMM Do HH:mm') + ' ' + tz);
        else
          $elem.text(date.format('MMM Do HH:mm Z'));

      });
    });
</script>



