var custChatWin=null;
dojo.forEach( dojo.query("a.custChatLink"), function(tag) {
  tag.className = tag.className.replace( /(?:^|\s)custChatLink(?!\S)/g , '' ); // hack: remove class custChatLink so that we don't try to set another popup handler later (base wsj template does, for instance). this class is not targeted by any CSS or any other JS right now
  dojo.connect(tag,"onclick", function(event) {
    dojo.stopEvent(event);
