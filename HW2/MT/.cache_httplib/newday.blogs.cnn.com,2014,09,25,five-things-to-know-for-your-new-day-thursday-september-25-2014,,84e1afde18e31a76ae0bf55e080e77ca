			<script>
			(function ($j, win, doc, undef) {
				'use strict';
				/**> Set video_embed_count **/
				if (win.cnn_metadata === undef) {
					win.cnn_metadata = {};
				}
				$j.extend(true, win.cnn_metadata, {"business":{"cnn":{"page":{"video_embed_count":4}}}});
			}(jQuery, window, document, undefined));
		</script>
	<!-- cnn footer -->
<div class="cnn_blog_footer_area">
	<script>
	(function( $j, win, undef ) {
					var warn = 'Template cnn_metadata.business.cnn.page.publish_date already set to:';
			win.cnn_metadata = $j.extend( {}, { 'business': { 'cnn': { 'page':{} } } } , ( win.cnn_metadata || {} ) );

			if ( undef === cnn_metadata.business.cnn.page.publish_date ) {
				cnn_metadata.business.cnn.page.publish_date = "2014/09/25";
				warn = '';
			}
			if ('' !== warn && 'function' === typeof console.log) {
				warn = warn +  cnn_metadata.business.cnn.page.publish_date;
				console.log(warn);
			}
				//adding a new branding_ad value to cnn_metadata object
		cnn_metadata.business.cnn.page.branding_ad = "";

		//creates the cnn_metadata.section array and populats sub section if metadataSection2 has content
			}( jQuery, window , undefined ) );
</script>
