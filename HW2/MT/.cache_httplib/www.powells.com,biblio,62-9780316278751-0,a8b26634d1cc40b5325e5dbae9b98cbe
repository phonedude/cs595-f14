<script type="text/javascript">
<!-- We Only Call general_startup From Here... -->
   jQuery(document).ready(function(){
      general_startup();
      select_main_search();
      //margin_tracking();
      //browser_size_event_tracking();
      //setTimeout(function() {random_banner()}, 1000);
   });

   function select_main_search() {
      jQuery('#search-box').focus();
   }

   function random_banner() {
      var ab_array = ['a','b'];
      if (!jQuery.cookie('a_b')) {
         var options = { path: '/', expires: 7 };
         var set_it = jQuery.cookie('a_b', ab_array[zero_one], options);
         var track_the_randomness = TrackEvent('Misc','Set Cookie (' + jQuery.cookie('a_b') + ')');
      }
      var banner = new Array();
      // Green color is '#939601'
      banner['a'] = ['/images/header/masthead_freeshipping_a.jpg','ffffff'];
      banner['b'] = ['/images/header/masthead_freeshipping_b.gif','969600'];
      jQuery('#masthead_banner').css({
         'background-color' : '#' + banner[jQuery.cookie('a_b')][1],
         'background-image' : 'url(' + banner[jQuery.cookie('a_b')][0] + ')',
         'display':'none'
      });
      var track_the_banner = TrackEvent('Misc',"Show Banner (" + jQuery.cookie('a_b') + ")");
      jQuery('#masthead_banner a').attr('rel','Header:Free Shipping Banner(' + jQuery.cookie('a_b') + ')');
      return false;
   }

   function browser_size_event_tracking() {
      //if (typeof pageTracker != 'undefined') {
         Math.round(Number(jQuery(window).width())/10)*10
         setTimeout(function () {
            var browser_on_load = TrackEvent('Browser Dimensions','Load',Math.round(Number(jQuery(window).width())/50)*50 + 'x' + Math.round(Number(jQuery(window).height())/50)*50);
         },500);
         jQuery(window).resize(function () {
            if (jQuery(this).data('gatimer')) {
               clearTimeout(jQuery(this).data('gatimer'));
               jQuery(this).data('gatimer', null);
            }
            jQuery(this).data('gatimer', setTimeout(function () {
            var browser_on_resize = TrackEvent('Browser Dimensions','Resize',Math.round(Number(jQuery(window).width())/50)*50 + 'x' + Math.round(Number(jQuery(window).height())/50)*50);
            },500));
         });
      //}
   }
   
   
   
   
   
   
   
   
   
   <!-- margin_open.html -->

   
   
   function margin_tracking() {
   areas = [
      'dontmiss'
   ];
   jQuery.each(areas, function(index, value) {
      jQuery("#" + value + " a").bind('click', function(){
         area = value;
         link_type = (jQuery(this).attr('rel')) ? jQuery(this).attr('rel') : null ;
         TrackEvent("Sitewide-Margin-What-Gets-Clicked",area,link_type,0,true);
      });
   });
}
   
   
</script>

