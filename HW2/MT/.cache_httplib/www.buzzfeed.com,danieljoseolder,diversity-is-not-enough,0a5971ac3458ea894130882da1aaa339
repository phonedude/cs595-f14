	(function adobeTrack() {
		if (typeof s != 'object') {
			setTimeout(function(){
				adobeTrack();
			}, 100);
		} else {
			var s_code = s.t();
			if (s_code) document.write(s_code);
		}
	})()
</script>





 
  

