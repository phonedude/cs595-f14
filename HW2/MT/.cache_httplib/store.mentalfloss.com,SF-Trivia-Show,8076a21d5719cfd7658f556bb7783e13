	<script type="text/javascript">
		function appendNav(lvl)	{
			var currentLevel = $("#leftnav").find("li:has(.level-" + lvl + ")"), prevLevel = currentLevel.prev("li:has(.level-" + (lvl - 1) + ")").append("<ul class='lvl-" + (lvl - 1) + "'></ul>");
			currentLevel.find("a").removeClass("level-" + lvl);
			currentLevel.each(function(){
				$(this).appendTo($(this).prevAll(":has(.lvl-" + (lvl - 1) + "):first").find("ul:first"));
			});
			lvl--;
			if(lvl > 0) appendNav(lvl);
		}
		$("#leftcolumnsource").hide().after("<ul id='leftnav'/>");
		var leftNav = $("#leftnav"), navLvl = 0, currentLvl = 0;
		$("#leftcolumnsource a:not(:has(img)):not(:empty):not(:contains(Shopping Cart)):not(:contains(View Cart)):not(:contains(Search))").addClass(function(){
			currentLvl = ($(this).parent().parent().find("td").length + $(this).parent().parent().find("td:first").attr("colspan") - 2);
			navLvl = (navLvl < currentLvl) ? currentLvl : navLvl;
			return "level-" + currentLvl;
		}).clone().appendTo(leftNav).wrap("<li/>");
		leftNav.find(".textboldnolink").removeClass("textboldnolink").parent().addClass("active");
		appendNav(navLvl);
		leftNav.find("li:has(.active)").addClass("active");
	</script>
