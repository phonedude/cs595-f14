								function es_searchStore(){
									var s_location = '';
									var s_searchText = encodeURIComponent(jQuery('#search-text').val());
										
									var s_siteLocation = window.location.pathname;
									if (s_siteLocation.indexOf("The-Week")>-1) {
										s_location = 'http://store.mentalfloss.com/The-Week-Search-Results';
									} else if(s_siteLocation.indexOf("Slate")>-1) {
										s_location = 'http://store.mentalfloss.com/Slate-Search-Results';
									} else {
										s_location = 'http://store.mentalfloss.com/Mental-Floss-Search-Results';
									}
									
									var s_html = "";
									var s_params = '?searchText='+s_searchText;
									//alert(s_location+s_params);
									window.location.href = s_location+s_params;
									return false;
								}
