<script type="text/javascript">
(function($) {	
	/* ARROW LEFT RIGHT HOMESLIDER */
	$(".arrow-navigation").click(function(e){
		e.preventDefault();
		var indeximg = $('#homeslideroutterwrapper img').length-1;
		var activenav = $('#homeslidernav').find('.active');
		var indexactive = activenav.attr('data-index');
		if ($(this).hasClass('arrow-right')) {
			if(indexactive < indeximg){
			 	activenav.parent().next().find('a').click();
			}
			else{
			 	$('#homeslidernav').find('a[data-index=0]').click();
			}
		} else {
			if(indexactive != 0){
			 	activenav.parent().prev().find('a').click();
			}
			else{
			 	$('#homeslidernav').find('a[data-index='+ indeximg + ']').click();
			}
		}
	});
})(jQuery);
</script>
