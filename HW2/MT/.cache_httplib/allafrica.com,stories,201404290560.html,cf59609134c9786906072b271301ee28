var FBIsLoaded = false;

window.fbAsyncInit = function() {
  FB.init({
    appId      : 100369188719,
    xfbml      : true,
    version    : 'v2.0'
  });
  try {
    if ( FB ) {
      FBIsLoaded = true;
      if ( FB.Event && FB.Event.subscribe) {

        // Likes
        FB.Event.subscribe('edge.create', function(targetUrl) {
          __aaEventTracker.trackSocial( 'Facebook', 'Like', targetUrl );
        });
        FB.Event.subscribe('edge.remove', function(targetUrl) {
          __aaEventTracker.trackSocial( 'Facebook', 'Unlike', targetUrl );
        });

        // Facebook Send
        FB.Event.subscribe('message.send', function(targetUrl) {
          __aaEventTracker.trackSocial( 'Facebook', 'Send', targetUrl );
        });

        // Facebook Comments
        FB.Event.subscribe('comment.create',function(targetUrl) {
          __aaEventTracker.trackSocial( 'Facebook', 'Comment', targetUrl );
        });
        FB.Event.subscribe('comment.remove',function(targetUrl) {
          __aaEventTracker.trackSocial( 'Facebook', 'Uncomment', targetUrl );
        });
      }
    }
  } catch (e) {}
};

