<script type="text/javascript">
  var dataLayer = {
    push: function () {
      var obj = arguments[0];
      //console.log( JSON.stringify( obj, '', '  ' ) );
      if ( ! obj.event ) return;
        switch (obj.event) {
          case "experiment":
            __aaEventTracker.trackEvent(obj.nameSpace, obj.testName, obj.cohort + ' | Total', undefined, true);
            break;
          case "event":
            __aaEventTracker.trackEvent(obj.eventCategory, obj.eventAction, obj.eventLabel + ' | ' + obj.eventName, undefined, false);
              break;
          case "ScrollDistance":
            //console.log( JSON.stringify( obj, '', '  ' ) );
            if ( obj.eventAction == 'Elements' ) {
              // register interaction when user scrolls through entire story
              if ( obj.eventLabel == '#aans-body-end' ) { obj.eventNonInteraction = false }
            }
            __aaEventTracker.trackEvent(obj.eventCategory, obj.eventAction, obj.eventLabel, obj.eventValue, obj.eventNonInteraction);
              break;
          default:
            console.log( 'uncaught event in dataLayer.push(): ' + JSON.stringify( obj, '', '  ' ) );
        }
    }
  };

  _ready.push(function($) {
    if ( $('body.story').length > 0 && $('body.story.restricted .inline-story-login').length == 0 ) {
      $.scrollDepth({
        minHeight: 2000,
        elements: ['#aans-body-mid', '#aans-body-end'],
        percentage: true,
        userTiming: false,
        pixelDepth: true,
        nonInteraction: true
      });
    }

    var cohorts_masthead_layout = new Cohorts.Test({
      name: 'Masthead Layout',
      testScope: 'user',
      sampleRate: 1,
      storageAdapter: CohortsGoogleAnalyticsAdapter,
      cohorts: {
        "Ad Below Nav": {
          onChosen: function() {
            $('#div-gpt-async-ad-leaderboard-a').remove();
            $("body").addClass("masthead-test-2");
            $('body > .masthead .corner-container').show();
            $('body > .masthead div.search').show();
            if ( $('body.commerce-user').length == 0 ) {
              $('body > .subnav').show();
              $('body > .subnav + div.clear').show();
            }
            //$('#div-gpt-async-ad-leaderboard-a').first().swapWith( $('#leaderboard-a-placeholder').first() );
            if ( window['googletag'] && $('#div-gpt-async-ad-superleaderboard-a').length > 0 ) {
              googletag.cmd.push(function() { googletag.display('div-gpt-async-ad-superleaderboard-a'); });
            }
          }
        },
        "Ad Above Logo": {
          onChosen: function() {
            $('#div-gpt-async-ad-superleaderboard-a').remove();
            $("body").addClass("masthead-test-1");
            $('body > .header').show();
            $('body > .masthead .nav.secondary .account').show();
            $('body > .masthead .nav.language.tabs').show();
            $('body > .masthead .utility').show();
            if ( window['googletag'] && $('#div-gpt-async-ad-leaderboard-a').length > 0 ) {
              googletag.cmd.push(function() { googletag.display('div-gpt-async-ad-leaderboard-a'); });
            }
          }
        }
      }
    });
    $('body > .masthead div.search .my-account, body > .masthead .nav.secondary .my-account').click(function() {
      cohorts_masthead_layout.event('Clicked My-Account Link');
    });
    $('body > .masthead .nav.language.bar a.active, body > .masthead .nav.language.tabs a.active, ').click(function() {
      cohorts_masthead_layout.event('Clicked Active Language Tab');
    });
    $('body > .masthead .nav.language.bar a:not(.active), body > .masthead .nav.language.tabs a:not(.active)').click(function() {
      cohorts_masthead_layout.event('Clicked Inactive Language Tab');
    });

  });
</script>

