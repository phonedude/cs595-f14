<script type="text/javascript">cnnad_createAd("bce477164dd7d4f2","http://ads.cnn.com/html.ng/site=cnn_money&cnn_money_position=728x90_top&cnn_money_rollup=media&params.styles=fs&page.allowcompete=no","90","728");</script>
