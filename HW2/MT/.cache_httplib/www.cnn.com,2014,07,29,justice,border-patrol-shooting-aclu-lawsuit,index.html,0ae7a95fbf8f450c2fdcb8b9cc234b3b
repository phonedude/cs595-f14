<a name="em2"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand29">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/140612165054-lead-dnt-savidge-us-mexico-border-undocumented-immigrant-kids-00011415-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>33,000 kids flood US border</span></cite>
</div>
<script>
var currExpandable="expand39";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
mObj.type='video';
