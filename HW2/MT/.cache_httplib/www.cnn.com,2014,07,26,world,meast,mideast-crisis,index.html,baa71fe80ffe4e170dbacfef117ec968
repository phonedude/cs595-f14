mObj.videoSourceUrl='http://situationroom.blogs.cnn.com/';
mObj.lgImage="http://i2.cdn.turner.com/cnn/dam/assets/140725172354-tsr-dnt-wedeman-gaza-deadly-demonstrations-00010522-story-body.jpg";
