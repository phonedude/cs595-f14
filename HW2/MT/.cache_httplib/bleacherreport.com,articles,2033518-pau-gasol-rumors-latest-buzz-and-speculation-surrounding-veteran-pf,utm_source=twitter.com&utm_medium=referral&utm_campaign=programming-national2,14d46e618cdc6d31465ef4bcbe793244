        FB.init({
            appId      : BR.serverData.fbAppId, // App ID from the app dashboard
            cookie: true,
            channelUrl : 'http://bleacherreport.com/facebook_channel.html', // Channel file for x-domain comms
            status     : true, // Check Facebook Login status
            xfbml      : true, // Look for social plugins on the page
            version    : 'v2.0'
        });

        // Additional initialization code such as adding Event Listeners goes here
        (function($){
