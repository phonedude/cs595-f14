                                                                                                                <span class="trackable-component component-wrapper two-col" data-component="Resource:promo-related-article US Gay Rights by State:microapp guardiannews-interactives-static:Gay rights by state inline links">
                                                    <script src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.3.3/underscore-min.js" type="text/javascript" charset="utf-8"></script>
<span id="gia-StatePopup"></span>
<div id="gia-stateSidePromo"></div>
<script id="gia-StatePopupTemplate" type="template/underscore">
  <span class="gia-stateContent">
    <img src='//gia.guim.co.uk/2012/05/gay-rights/images/<%= abbr %>.png'>
    <span class="state">Gay rights in<br><%= (abbr == "DC") ? "the " : "" %><%= state %></span>
    <span class="caption">Click for interactive</span>
  </span>
  <span class="gia-stateTag"></span>
</script>

<script id="gia-StatePromoTemplate" type="template/underscore">
  <a href="<%= url %>">
    <img src='//gia.guim.co.uk/2012/05/gay-rights/images/<%= abbr %>.png'><br>
    <h3>Gay rights in<br><%= (abbr == "DC") ? "the " : "" %><%= state %></h3>
    <span class="caption">Click for interactive</span>
  </a>
</script>

<style>
.gia-State {
  border-bottom: 1px dotted;
  background: #eee;
  position:relative;
  text-decoration: none;
}

.gia-State:hover {
  border-bottom: 1px solid;
  text-decoration: none;
}

#gia-StatePopup {
  display: none;
  position:absolute;
  width:140px;
  height:280px;
  bottom:1em;
  left:0;
  cursor:pointer;
  overflow:hidden;
  z-index:100;
}

#gia-StatePopup .gia-stateContent {
  display:block;
  position:absolute;
  left:0;
  right:0;
  bottom:16px;
  padding:4px;
  margin:6px 6px 0 6px;
  text-align:center;
  background:#fff;
  border:1px solid #ddd;
  border-radius: 4px;
  box-shadow: 2px -2px 2px rgba(33, 33, 33, .2);
}

#gia-StatePopup .gia-stateTag {
  position:absolute;
  left:0;
  bottom: 3px;
  right:0;
  display: block;
  height: 14px;
  background:url(//gia.guim.co.uk/2012/05/gay-rights/images/tag.png) center center no-repeat;
}

#gia-StatePopup img {
  width:100px;
  height:100px;
}

#gia-StatePopup .state {
  font-family: georgia, serif;
  color: #666;
  margin: 0 0 2px 0;
  line-height: 1;
}

#gia-StatePopup .caption {
  color: #005689;
  font-size: 10px;
  margin: 2px 0 2px 0;
  line-height: 1;
}

#gia-stateSidePromo {
  display: none;
  text-align: center;
  border-top:1px dotted #999;
  padding: 6px;
  height: 185px;
}

#gia-stateSidePromo a {
  display: block;
}

#gia-stateSidePromo img {
  height: 100px;
  width: 100px;
}

#gia-stateSidePromo a {
  text-decoration: none;
}

#gia-stateSidePromo h3 {
  color: #666;
}

#gia-stateSidePromo span {
  color: #005689;
}

</style>
<script>
/*
 * jQuery replaceText - v1.1 - 11/21/2009
 * http://benalman.com/projects/jquery-replacetext-plugin/
 *
 * Copyright (c) 2009 "Cowboy" Ben Alman
 * Dual licensed under the MIT and GPL licenses.
 * http://benalman.com/about/license/
 */
(function($){$.fn.replaceText=function(b,a,c){return this.each(function(){var f=this.firstChild,g,e,d=[];if(f){do{if(f.nodeType===3){g=f.nodeValue;e=g.replace(b,a);if(e!==g){if(!c&&/</.test(e)){$(f).before(e);d.push(f)}else{f.nodeValue=e}}}}while(f=f.nextSibling)}d.length&&$(d).remove()})}})(jQuery);

(function($) {
  'use strict';
  // console.log("jquery version", $.fn.jquery);

  var states = {
    'Alabama': 'AL',
    'Alaska': 'AK',
    'Arizona': 'AZ',
    'Arkansas': 'AR',
    'California': 'CA',
    'Colorado': 'CO',
    'Connecticut': 'CT',
    'Delaware': 'DE',
    'District of Columbia': 'DC',
    'Florida': 'FL',
    'Georgia': 'GA',
    'Hawaii': 'HI',
    'Idaho': 'ID',
    'Illinois': 'IL',
    'Indiana': 'IN',
    'Iowa': 'IA',
    'Kansas': 'KS',
    'Kentucky': 'KY',
    'Louisiana': 'LA',
    'Maine': 'ME',
    'Maryland': 'MD',
    'Massachusetts': 'MA',
    'Michigan': 'MI',
    'Minnesota': 'MN',
    'Mississippi': 'MS',
    'Missouri': 'MO',
    'Montana': 'MT',
    'Nebraska': 'NE',
    'Nevada': 'NV',
    'New Hampshire': 'NH',
    'New Jersey': 'NJ',
    'New Mexico': 'NM',
    'New York': 'NY',
    'North Carolina': 'NC',
    'North Dakota': 'ND',
    'Ohio': 'OH',
    'Oklahoma': 'OK',
    'Oregon': 'OR',
    'Pennsylvania': 'PA',
    'Rhode Island': 'RI',
    'South Carolina': 'SC',
    'South Dakota': 'SD',
    'Tennessee': 'TN',
    'Texas': 'TX',
    'Utah': 'UT',
    'Vermont': 'VT',
    'Virginia': 'VA',
    'Washington': 'WA',
    'West Virginia': 'WV',
    'Wisconsin': 'WI',
    'Wyoming': 'WY'
  };

  var clickHref = 'http://www.guardian.co.uk/world/interactive/2012/may/08/gay-rights-united-states';

  function highlightStates() {
    var replaceWith = '<a data-state="$&" class="gia-State">$&</a>';
    _.each(states, function(abbr, state) {
      $('#article-body-blocks > p').replaceText(new RegExp("\\b" + state + "\\b"), replaceWith);
    });

    // Remove links if they break over a link as the centering goes off.
    var $singleHeight = $('<span>&nbsp;</span>');
    $('#article-body-blocks').append($singleHeight);
    var singleHeight = $singleHeight.height();
    $('.gia-State').each(function(i, d) {
      var $d = $(d);
      if ($d.height() > singleHeight) {
        $d.contents().unwrap();
      }
    });
  }

  function clickHighlightedState() {
    var template = _.template($('#gia-StatePopupTemplate').html());

    $('#article-body-blocks').delegate('.gia-State', {
      mouseenter: function() {
        var $popUp = $('#gia-StatePopup');
        var state = $(this).text();
        $popUp.html(template({state: state, abbr: states[state]}));
        var left = -(($popUp.width() - $(this).width()) / 2);
        $popUp.css({display: 'block', left: left});
        $(this).prepend($popUp);
      },
      mouseleave: function() {
        var $popUp = $('#gia-StatePopup');
        $popUp.html('');
        $popUp.hide();
      },
      click: function(e) {
        e.preventDefault();
        var state = $(this).attr('data-state');
        window.location.href = clickHref + '#state/' + state.toLowerCase().replace(/ /g, '-');
      }
    });
  }

  var stateSidePromoTemplate = _.template($('#gia-StatePromoTemplate').html());

  function stateSidePromo() {
    var randomStates = _.shuffle(_.keys(states)), index = 0;
    var fadeSpeed = 250;
    var $promo = $('#gia-stateSidePromo');

    var randomState = randomStates[0];
    $promo.html(stateSidePromoTemplate({ state: randomState, abbr: states[randomState], url: clickHref + '#state/' + randomState.toLowerCase().replace(/ /g, '-') }));
    $promo.fadeIn(fadeSpeed);

    setInterval(function() {
      index++;
      if (index == randomStates.length) { index = 0 }
      randomState = randomStates[index];
      $promo.fadeOut(fadeSpeed, function() {
        $promo.html(stateSidePromoTemplate({ state: randomState, abbr: states[randomState], url: clickHref + '#state/' + randomState.toLowerCase().replace(/ /g, '-') }));
        $promo.fadeIn(fadeSpeed);
      });
    }, 7000);
  }

  $(function() {
    var tags = _.pluck($('[rel="tag"]'), 'text');
    if (_.include(tags, 'United States')) {
      highlightStates();
      clickHighlightedState();
      stateSidePromo();
    }
  });

})(jQuery);
</script>
                                                    </span>
                                            				
			        


	    			                
    
	
		
											
	
	    



    
