						return;

					// Verify message is in a format we expect
					if ( 'object' !== typeof e.data || undefined === e.data.msg_type )
						return;

					switch ( e.data.msg_type ) {
						case 'poll_size:response':
							var iframe = document.getElementById( e.data._request.frame_id );

							if ( iframe && '' === iframe.width )
								iframe.width = '100%';
							if ( iframe && '' === iframe.height )
								iframe.height = parseInt( e.data.height );

							return;
						default:
							return;
					}
				}

				if ( 'function' === typeof window.addEventListener ) {
					window.addEventListener( 'message', funcSizeResponse, false );
				} else if ( 'function' === typeof window.attachEvent ) {
					window.attachEvent( 'onmessage', funcSizeResponse );
				}
			}
			if (document.readyState === 'complete') { func.apply(); /* compat for infinite scroll */ }
			else if ( document.addEventListener ) { document.addEventListener( 'DOMContentLoaded', func, false ); }
			else if ( document.attachEvent ) { document.attachEvent( 'onreadystatechange', func ); }
		} )();
		</script>
		</div>
		</div><div id="nbcs_adsense_ad-2" class="module module_type_widget clearfix widget_ad"><iframe width='300' height='250' class="ad" id="ad-widget-adsense" src="http://prosoccertalk.nbcsports.com/?get_ad=widget-adsense&amp;position=2&amp;ord=58921724325" border="0" frameborder="0" scrolling="no"></iframe>
</div><div id="text-3" class="module module_type_widget clearfix widget_text">			<div class="textwidget"><a href="https://bs.serving-sys.com/BurstingPipe/adServer.bs?cn=tf&amp;c=20&amp;mc=click&amp;pli=10050381&amp;PluID=0&amp;ord=[cachebuster]">
<img src="http://nbcprosoccertalk.files.wordpress.com/2014/06/us_wspickem_300x60.gif" style="display:block;" />
</a></div>
		</div><div id="text-9" class="module module_type_widget clearfix widget_text">			<div class="textwidget"><a href="http://www.nbcsports.com/emails/?cid=email_pst/">
<img src="http://nbcprosoccertalk.files.wordpress.com/2014/03/nbcs-signup-banner-pl.gif" style="display:block;" />
</a></div>
		</div><div id="text-6" class="module module_type_widget clearfix widget_text">			<div class="textwidget"><!--Begin NBC SOCCER BOX -->

