</div>
<script>
var currExpandable="expand244";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
mObj.type='video';
mObj.contentId='';
mObj.network='cnn';
mObj.source='us/2014/08/10/dnt-misouri-teen-shooting-protests.kmov';
