<a name="em5"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand225">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/140912213219-ac-nfl-adrian-peterson-ray-rice-00010629-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>How the NFL's bad week got worse</span></cite>
</div>
<script>
var currExpandable="expand325";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
mObj.type='video';
mObj.contentId='';
