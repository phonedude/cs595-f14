<script type="text/javascript">
	MF.fn.push(function(){
		if($("#movie-utility.center .movie-utility-body").length>0){
			mfJsObj.movieList = {'totalSlides':10,'currentSlide':1}
			mfJsObj.movieList.init = function() {
				mfJsObj.movieList.totalSlides = $("#movie-utility.center .movie-utility-body").length;	
				$("#arrows .next").unbind("click").click(function() {
					mfJsObj.movieList.currentSlide++;
					if(mfJsObj.movieList.currentSlide>mfJsObj.movieList.totalSlides){ mfJsObj.movieList.currentSlide=1;}
					$("#movie-utility.center .movie-utility-body").hide();
					$("#movie-utility #movie"+mfJsObj.movieList.currentSlide).show();
				});
				$("#arrows .prev").unbind("click").click(function() {
					mfJsObj.movieList.currentSlide--;
					if(mfJsObj.movieList.currentSlide<1){ mfJsObj.movieList.currentSlide=mfJsObj.movieList.totalSlides;}
					$("#movie-utility.center .movie-utility-body").hide();
					$("#movie-utility #movie"+mfJsObj.movieList.currentSlide).show();
				});
			}
			mfJsObj.movieList.init();
		}
	});
</script><!-- Sailthru -->
