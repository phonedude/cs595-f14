	if ($("#frm-search").length) {
		$("#frm-search").submit(function(e) {
			return mfJsObj.smartSearch.submitForm();
		});
		mfJsObj.smartSearch.ajaxUrl = "http://www.moviefone.com/smart-search";
		mfJsObj.smartSearch.searchUrl = "http://www.moviefone.com/search/";
		mfJsObj.smartSearch.timerInterval = 500;
		mfJsObj.smartSearch.initSearch();
	}
	
	if ($("#search-text").length) {
		$("#search-text").focus(function() {
			$(this.value = '')
		});
	}

	//INIT search page paginations
	if ($("#paginate-bucket").length) {
		mfJsObj.paginateSearch.init();
	}

	//faq page toggle
	if($("#faq-main").length) {
		$("[name='question']").each(function(index) {
			$(this).click(function(e) {
				e.preventDefault();
				$(this).toggleClass('down');
				$("#answer"+(index+1)).toggle();
			});
		});
	}

	
	if(($("#mapShowtimesTheater").length) || ($("#mapQuestMap").length)) {
