				}

				if ($slideshow.data().knot && /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) { // If you're on a mobile device and the knot object is available
					if ($slideshow.find(".aol-twist-mobile-fix").length == 0) { // If there's no caption div on the DOM
						$slideshow.find(".aol-twist-slide-wrapper").after("<div class='aol-twist-mobile-fix'></div>"); // Add the caption div
					};
