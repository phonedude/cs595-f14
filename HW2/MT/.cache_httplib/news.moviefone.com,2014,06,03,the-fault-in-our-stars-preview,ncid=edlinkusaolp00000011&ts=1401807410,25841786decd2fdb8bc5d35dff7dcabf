	(function ($) {
		'use strict';
		$.twist.prototype.plugins.push({
			name: 'flyoutOpen',
			_init: function (self) {
				self.topic('flyoutOpen').subscribe(function (self) {
					$('body').addClass('twist-flyout-open');
				});
				self.topic('flyoutClosed').subscribe(function (self) {
					$('body').removeClass('twist-flyout-open');
				});
				return $.Deferred( function (deferred) {
				deferred.resolve(self);
				}).promise();
			}
		});
	}(jQuery));
