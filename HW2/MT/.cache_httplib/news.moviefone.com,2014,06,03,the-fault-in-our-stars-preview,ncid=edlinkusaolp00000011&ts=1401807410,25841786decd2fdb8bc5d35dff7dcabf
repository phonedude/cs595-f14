	$.each(twist, function(i, element) {
		var $slideshow = $(element);
		var id = $slideshow.attr('data-slideshow-id');
		var mn300x250, winWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth
		mn300x250d = "93369451", 
		mn300x250t = "93424896";
		if (winWidth > 1024) {
			mn300x250 = mn300x250d;
		} else if ((winWidth > 767) && (winWidth <= 1024)) {
			mn300x250 = mn300x250t;
		} 
		$.twist.prototype._render.ad = function ($slide, callback) {
			var render = function (deferred) {
				var slideAdId = Math.round(Math.random() * 1E8);
				var tempData = $slide.data();
				tempData.adId = 'twistAdSlide-' + slideAdId;
				$slide.data(tempData);
				var $html = $('<div id="twistAdSlide-' + slideAdId + '"></div>');
				$slide.append($html);	        
				var slideData = tempData;
				if (window.htmlAdWH && slideData.magicNumber && slideData.adId) {
					window.htmlAdWH(slideData.magicNumber, 'RR', 'RR', "ajax", slideData.adId);
				}
				$html.css({
					marginTop: ($slide.height() - $html.height()) / 2
				}).bind('click', function (e) {
					e.stopImmediatePropagation();
				});
				deferred.resolve();
			}
			return $.Deferred(render).promise();
		};		
		$slideshow.twist({
			data: '/_/slideshow/'+id+'/with-slides',
			restrictSize: true,
			speed: 200,
			adMN: mn300x250,
			arrows: true,
			flyout: /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ? false : true,
			fullscreenAdMN: mn300x250,
			fullscreenAdHeight: "250",
			fullscreenAdWidth: "300",
			refreshCount: 4,
			galleryId:id,
			preloadVideo:true,
			onSlideChangeComplete: function () {
				knotShareFunction();
			},
			fullscreenRefreshDivId: ['knotFullscreenAd-' + id],
			contentMap: {
				rightRailHtml: function (context) {
					return [
						'<div class="aol-knot-fullscreen-exit"><i class="icon-remove"></i></div>',
						'<div class="aol-knot-fullscreen-right-title"></div>',
						'<span class="aol-knot-fullscreen-right-share movie-share"><a class="aol-share" name="aol-media-share" class="knot-right-share"></a></span>',
