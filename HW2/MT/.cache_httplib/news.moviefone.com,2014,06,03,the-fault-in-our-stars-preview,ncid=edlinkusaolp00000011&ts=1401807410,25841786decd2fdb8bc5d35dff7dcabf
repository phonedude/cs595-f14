	var siteDevId = 'ao19852k0fksIu';/**/
var checkFav=0;
function MF2HandleAuth(json,$e) {
    var $elem = $e, /*$(this.authLink),*/
    response = json.response,
    userAttr,
	$reviewWrite = $('.review-write');
	$ratingStars = $('.rating-wrap');

    if (response.statusCode === 200) {
        userAttr = response.data.userData.attributes;
        mfJsObj.showtimes.cookie.set('mftk',response.data.token.a,365);
        $elem.html(userAttr.displayName+', sign out');
        $("[name='favTheaters']").each( function() {
            $(".theater-fav").fadeIn();
        });
		
	$reviewWrite.trigger('reviewWrite:signedIn', { response:response });
	$ratingStars.trigger('ratingStars:signedIn', { response:response });
 	$('body').trigger('ratingsManager:signedIn', { response:response });

        $("[name='mfSignIn']").each( function() {
            var e = this;
            var theaterId = $(e).attr("rel");
            if($(e).hasClass("add-fav-auth")==true) {
                $("#theaterItem"+ theaterId).html('<span class="fav-add-link'+theaterId+'" rel="'+theaterId+'" name="favTheaters"><a id="fav'+theaterId+'" href="javascript:addRemoveFavoriteTheater(\'http://www.moviefone.com/addFavoriteTheaters?theaterId='+theaterId+'\', \'http://www.moviefone.com/deleteFavoriteTheaters?theaterId='+theaterId+'\', document.getElementById(\'fav'+theaterId+'\'), document.getElementById(\'favflyout'+theaterId+'\'), \'\',\''+theaterId+'\');" class="favorite-add">Add to Favorites</a></span>');
            }
        });
        if(checkFav==0) {
            checkFav = 1;
            if($('#mfSignInTheater').length>0) {
                $.ajax({
                    url: '/ajax/showtimes/favorite-theaters?stamp=1411920100&selectedShowDate=20140928',
                    success: function(response, status, xhr) {
                        $("#main-column").html(response);
                        $("#main-column").fadeIn();
                        $(".favoritetheaters .date-loc").show();
                        $(".favoritetheaters .showtime-tools").show();
                        mfJsObj.showtimeSelect.init();
                        mfJsObj.modulePopup.init();
                        mfJsObj.watchIt.pageurl="http://www.moviefone.com/theaters/favorites";
                        mfJsObj.watchIt.type = "favtheaters";
                        mfJsObj.watchIt.init();
                    },
                    error: function(response, status, xhr) {
                        $("#mfUserNameTheater").html("There was an error. Please try again.");
                    }
                });
            }
        }

    } else {
        $elem.html('Sign In');

                $(".add-fav-auth").text('Add to Favorites');

        $("[name='favTheaters']").each( function() {
            var e = this;
            var theaterId = $(e).attr("rel");
            $(".theater-fav").fadeOut();
        });
	$reviewWrite.trigger('reviewWrite:signedOut');
	$ratingStars.trigger('ratingStars:signedOut');
 	$('body').trigger('ratingsManager:signedOut');

       
        mfJsObj.showtimes.cookie.set('mftk','',365);
    }

	if ($.fn.reviewWrite) {
		$reviewWrite.reviewWrite();
	}
