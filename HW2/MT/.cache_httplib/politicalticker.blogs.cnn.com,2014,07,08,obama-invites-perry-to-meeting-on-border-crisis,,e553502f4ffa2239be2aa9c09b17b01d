	<script>
	window.framePath = (function (cookie) {
		var edition = 'us',
			matches = /SelectedEdition=(www|edition);/.exec(cookie);

		if (matches !== null && matches.length && matches[1].length) {
			edition=matches[1];
		}

		return 'http://'+edition+'.cnn.com';
	}(document.cookie));


	( function ( $j, win, doc, undef ) {
              $j( 'div.cnn_video' ).each(function () {
					var $iframe = $j('<iframe ></iframe>', {
						'src':window.framePath+"/video/embed/frame.html#"+encodeURIComponent(document.location.href),
						'scrolling':'no',
						'style':'border:0px  none;',
