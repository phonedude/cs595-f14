return (typeof window.orientation !== 'undefined');
}
window.CNNVIDEOS = window.CNNVIDEOS || {};
var CVPOptions = {
video: 'us/2014/09/17/tsr-bash-training-syrian-rebels-house-vote.cnn',
thumb: 'http://i2.cdn.turner.com/cnn/dam/assets/140917173540-tsr-bash-training-syrian-rebels-house-vote-00001603-story-top.jpg',
preset: 'storypage',
diagnostics: {
section: 'articlepage',
placement: 'page-top'
},
network: 'cnn',
adsection: cnnCVPAdSectionT1 || '',
headline: 'House approves helping Syrian rebels',
videoSource: 'CNN',
videoSourceUrl: 'http://situationroom.blogs.cnn.com/',
url: ''
};
/**
* Autostart only for desktop and domestic articles.
* Mobile web will not be auto-started
*/
if (false && !isMobileClient()) {
CVPOptions.autostart = true;
}
CNNVIDEOS[ 'T1' ] = CNNVIDEO('cnnCVP1', CVPOptions);
/**
* Pause other videos playing on page
*/
window.jQuery('#cnnCVP1').on('onContentBegin', function(e, obj) {
for (var instance in window.CNNVIDEOS) {
if (instance !== 'T1' && window.CNNVIDEOS.hasOwnProperty(instance)) {
try {
window.CNNVIDEOS[instance].pause();
} catch(e) {
}
}
}
});
});
