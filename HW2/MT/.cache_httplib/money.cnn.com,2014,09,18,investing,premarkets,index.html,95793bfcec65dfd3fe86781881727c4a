cnnad_createAd("785283ab96f91755","http://ads.cnn.com/html.ng/site=cnn_money&cnn_money_position=728x90_top&cnn_money_rollup=investing&cnn_money_section=opening_bell&params.styles=fs&page.allowcompete=no","90","728");
