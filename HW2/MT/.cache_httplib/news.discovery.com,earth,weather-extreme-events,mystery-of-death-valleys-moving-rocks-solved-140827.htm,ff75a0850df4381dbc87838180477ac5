	<script>
	(function(){
		if((typeof pageMetricsData !== 'undefined') && (typeof DDM.adManager !== 'undefined'))
		{
			var videoRefId = (typeof videoData !== 'undefined' && videoData[0].reference_id !== 'undefined') ? videoData[0].reference_id : false;
			DDM.adManager.initialize({
			    attributes  : {
			        siteSection :  "news.discovery.com/earth/weather-extreme-events/article/mystery-of-death-valleys-moving-rocks-solved-140827/mystery-of-death-valleys-moving-rocks-solved-140827.htm" ,
			        videoRefId  : videoRefId,
			        networkId   : "91559",
			        server      : "165a7",
			        refresh     :  "ui" 			    }
			});
		}
		else
		{
			setTimeout(arguments.callee,50);
		}
	})();

	/*
		This is a hack b/c some of the interactives directly call
		DDM.oasc.refresh() in the raw code.  This will allow that method
		to do a FW refresh when OASC is no longer in place
	*/
	if(typeof DDM.oasc === 'undefined')
	{
		DDM.oasc = {};
		DDM.oasc.refresh = function ()
		{
			DDM.adManager.refresh();
		}
	}
	</script>
		
