	try{
		var bkMetaArray = document.getElementsByTagName("meta");
	
		for(var i=0;i<bkMetaArray.length;i++){
			if(bkMetaArray[i].getAttribute("property")){			
				if(bkMetaArray[i].getAttribute("property").indexOf("og:type")>-1){
					bk_addPageCtx("metatype", bkMetaArray[i].getAttribute("content"));
				}
			}
			if(bkMetaArray[i].getAttribute("property")){			
				if(bkMetaArray[i].getAttribute("property").indexOf("og:site_name")>-1){
					bk_addPageCtx("metasite", bkMetaArray[i].getAttribute("content"));
				}
			}
			if(bkMetaArray[i].getAttribute("property")){			
				if(bkMetaArray[i].getAttribute("property").indexOf("og:title")>-1){
					bk_addPageCtx("metatitle", bkMetaArray[i].getAttribute("content"));
				}
			}
		}	
	}
	catch(e){}
