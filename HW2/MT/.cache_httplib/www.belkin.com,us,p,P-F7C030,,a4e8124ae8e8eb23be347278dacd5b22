	 cablecount++;
       });
       
      // alert("cablecount->"+cablecount);
	if(cablecount == 0){
		$('#product-color-options').append('<p class=product-dropdown style="display: none"><select id="cableLength"><option value="invalid"></option></select></p>');
	}
	
      var cordcount = 0;
      $("#product-dropdown-cord").find('option').each(function(i) {
	 cordcount++;
       });

      // alert("cordcount->"+cordcount);
	if(cordcount == 0){
		$('#product-color-options').append('<p class=product-dropdown style="display: none"><select id="cordLength"><option value="invalid"></option></select></p>');
	}
 
    var clothingcount = 0;
    $("#product-dropdown-clothing").find('option').each(function(i) {
 	   	clothingcount++;
     });

	 if(clothingcount == 0){
		 $('#product-color-options').append('<p class=product-dropdown style="display: none"><select id="clothingSize"><option value="invalid"></option></select></p>');
	 }	
	 
