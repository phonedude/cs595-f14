/*<![CDATA[*/

function initWriteReviewAction() {
	$('#write_review_action_main').click(function(e){
		e.preventDefault();
		$.scrollTo('#prod_tabs', 300, {axis: 'y'});
		$('#reviews').hide();
		$('#write_reviews').show();
		$( "#prod_tabs" ).tabs( "option", "selected", $('#tab_strip').children().size() - 1 );
	});
}

function initBasedOnReviewsAction() {
	$('#based_on_reviews').click( function(e) {
		e.preventDefault();
		$.scrollTo('#prod_tabs', 300, {axis: 'y'});
		$( "#prod_tabs" ).tabs( "option", "selected", $('#tab_strip').children().size() - 1 );
		$('#write_reviews').hide();
		$('#reviews').show();
		$('#read_reviews_action').click();
		
	});
}

function initAddToCart() {
	//TODO: what should be here?
}

function initPageEvents() {
	if($(".modal")) {
		$(".modal").colorbox();
	}
	$("#carousel_alternate img").click(function() {
		$("#primary_image .modal img").attr("src", $(this).attr("primaryImageSrc"));
		$("#zoomLink").attr("href", "/us/p/P-F7C030/zoomImages?galleryPosition=" + $(this).attr("galleryposition"));
		$("#imageLink").attr("href", "/us/p/P-F7C030/zoomImages?galleryPosition=" + $(this).attr("galleryposition"));
	});

	initWriteReviewAction();
	initBasedOnReviewsAction();
	initAddToCart();


	$("#Size").change(function () {
		var url = "";
		var selectedIndex = 0;
		$("#Size option:selected").each(function () {
			url = $(this).attr('value');
			selectedIndex = $(this).attr("index");
		});
		if (selectedIndex != 0) {
			window.location.href="/us" + url;
		}
	});

	$("#variant").change(function () {
		var url = "";
		var selectedIndex = 0;
		
		$("#variant option:selected").each(function () {
			url = $(this).attr('value');
			selectedIndex = $(this).attr("index");
		});
		if (selectedIndex != 0) {
			window.location.href="/us" + url;
		}
	});
}

$(document).ready(function() {
	
	initPageEvents();
	
