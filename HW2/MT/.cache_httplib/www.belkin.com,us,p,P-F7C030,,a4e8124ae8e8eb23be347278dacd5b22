    $('#findARetailer').click(function (e) {
        if (selectVariant(this)) {
            if ($('.productCode').val().length > 0) {
                $('#findARetailerForm').submit();
            } else {
                return false;
            }
        } else {
            alert('Select a variant');
            return false;
        }
    });

    $('#addTowishList').click(function (e) {
