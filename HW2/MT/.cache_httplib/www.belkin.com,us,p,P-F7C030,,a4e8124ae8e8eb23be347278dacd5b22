        $('.productCode').val(''); 
		
        selectVariant($("li#color").filter("[info='" + $("variant").filter("[code='F7C030fc']").attr("color") +"']"));
		if(doubleVariant && cablecount > 0){
			updateApplicableColorsCable(this);
		} else if(doubleVariant && cordcount > 0){
			updateApplicableColorsCord(this);
		} else if(doubleVariant && clothingcount > 0){
