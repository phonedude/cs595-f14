			child.addClass('active');
		     }
	       });
       }else{
	//hide color
	$('#color-options').hide();
	$('#color-variant-label').hide();
       }
	
        
        $('#product-buy-now').show();
        $('#product-coming-soon').hide();
        $('#product-sold-out').hide();
        $('#product-not-available').hide();
