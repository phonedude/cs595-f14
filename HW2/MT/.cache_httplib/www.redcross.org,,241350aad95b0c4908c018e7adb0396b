// Find DOM elements with GA attributes and push values to GA for events.
(function($){
  var G='data-ga-',C=G+'category',A=G+'action',D=G+'change',M=G+'module';
  //$('['+M+']').each(gaModule);
  $('['+D+']').bind('change.ga',gaChangeValue);
  $('#container').delegate('['+A+'], input.button', 'click.ga', gaClick);
  $('#ftr').delegate('['+A+'], input.button', 'click.ga', gaClick);
  $('input[value="Add this donation"]').bind('click.ga', gaCourseDonateClick);
