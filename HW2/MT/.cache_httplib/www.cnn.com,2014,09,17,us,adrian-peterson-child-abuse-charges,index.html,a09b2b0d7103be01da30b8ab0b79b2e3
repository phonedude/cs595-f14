<a name="em2"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand25">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/140915222406-ac-adrian-peters-triggers-new-debate-on-abuse-00014914-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>Lemon: I don't condone what Peterson did</span></cite>
</div>
<script>
var currExpandable="expand35";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
mObj.type='video';
mObj.contentId='';
mObj.network='cnn';
mObj.source='bestoftv/2014/09/16/ac-adrian-peters-triggers-new-debate-on-abuse.cnn';
mObj.videoSource='CNN';
mObj.videoSourceUrl='ac360.blogs.cnn.com';
mObj.lgImage="http://i2.cdn.turner.com/cnn/dam/assets/140915222406-ac-adrian-peters-triggers-new-debate-on-abuse-00014914-story-body.jpg";
