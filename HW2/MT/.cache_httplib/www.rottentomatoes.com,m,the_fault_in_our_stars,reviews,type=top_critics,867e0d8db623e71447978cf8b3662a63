	    	  type: 'GET',
	          url: 'http://pixel.quantserve.com/api/segments.json?a=p-0fuRlG_jy3lfA',
	          dataType: 'jsonp',
	    }).done(function( result ){
	        var quantSegs="";
	        for (var i = 0; i < result.segments.length; i++) {
	            quantSegs += result.segments[i].id;
