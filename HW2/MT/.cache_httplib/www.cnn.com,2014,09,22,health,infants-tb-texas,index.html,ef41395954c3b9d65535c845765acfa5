<a name="em1"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand17">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/131221053242-kmir-ca-tb-test-00002510-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>1,800 tested for tuberculosis at school</span></cite>
</div>
<script>
var currExpandable="expand27";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
mObj.type='video';
