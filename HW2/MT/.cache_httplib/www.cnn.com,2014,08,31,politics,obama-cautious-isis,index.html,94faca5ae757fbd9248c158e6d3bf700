</script>
<a name="em4"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand117">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/140831134852-exp-gps-0831-take-00002013-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>Fareed's Take: How to beat ISIS</span></cite>
</div>
<script>
var currExpandable="expand217";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
mObj.type='video';
mObj.contentId='';
mObj.network='cnn';
mObj.source='bestoftv/2014/08/31/rs-red-news-blue-news-isis-threat-to-the-us-.cnn';
mObj.videoSource='CNN';
mObj.videoSourceUrl='';
mObj.lgImage="http://i2.cdn.turner.com/cnn/dam/assets/140831122622-rs-red-news-blue-news-isis-threat-to-the-us-00012622-story-body.jpg";
