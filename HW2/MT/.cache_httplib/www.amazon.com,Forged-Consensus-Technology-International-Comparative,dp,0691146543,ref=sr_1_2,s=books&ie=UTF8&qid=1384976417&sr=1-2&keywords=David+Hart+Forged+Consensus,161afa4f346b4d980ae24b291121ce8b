<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "In%20this%20thought-provoking%20book%2C%20David%20Hart%20challenges%20the%20creation%20myth%20of%20post-World%20War%20II%20federal%20science%20and%20technology%20policy.%20According%20to%20this%20myth%2C%20the%20postwar%20policy%20sprang%20full-blown%20from%20the%20mind%20of%20Vannevar%20Bush%20in%20the%20form%20of%20Science%2C%20the%20Endless%20Frontier%20%281945%29.%20Hart%20puts%20Bush%27s%20efforts%20in%20a%20larger%20historical%20and%20political%20context%2C%20demonstrating%20in%20the%20process%20that%20Bush%20was%20but%20one%20of%20many%20contributors%20to%20this%20complex%20policy%20and%20not%20necessarily%20the%20most%20successful%20one.%20Herbert%20Hoover%2C%20Karl%20Compton%2C%20Thurman%20Arnold%2C%20Henry%20Wallace%2C%20Robert%20Taft%2C%20and%20Curtis%20LeMay-along%20with%20more%20familiar%20figures%20like%20Bush-are%20among%20those%20whose%20endeavors%20he%20traces.%20Hart%20places%20these%20policy%20entrepreneurs%20in%20the%20broad%20scheme%20of%20American%20political%20development%2C%20connecting%20each%20one%27s%20vision%20of%20the%20state%20in%20this%20apparently%20esoteric%20policy%20area%20to%20the%20central%20issues%2C%20events%2C%20and%20figures%20of%20mid-century%20America%20and%20to%20key%20theoretical%20debates.%20Hart%27s%20work%20reveals%20the%20wide%20range%20of%20ideas%2C%20often%20in%20conflict%20with%20one%20another%2C%20that%20underlay%20what%20later%20observers%20interpreted%20as%20a%20%22postwar%20consensus%22%20In%20Hart%27s%20view%2C%20these%20visions-and%20the%20interests%20and%20institutions%20that%20shape%20their%20translation%20into%20public%20policy-form%20the%20enduring%20basis%20of%20American%20politics%20in%20this%20important%20area.%20Policymakers%20today%20are%20still%20grappling%20with%20the%20legacies%20of%20the%20forged%20consensus.",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    






