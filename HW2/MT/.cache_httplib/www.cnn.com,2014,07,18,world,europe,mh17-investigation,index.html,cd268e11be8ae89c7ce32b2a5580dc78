mObj.origImageX="214";
mObj.origImageY="120";
mObj.contentType='video';
CNN.expElements.expand46Store=mObj;
</script>
<a name="em4"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand46">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/140717225613-ac-passenger-jets-shot-down-00015021-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>When passenger jets become targets</span></cite>
</div>
<script>
var currExpandable="expand56";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
