<script type="text/javascript">
P.when('jQuery', 'cf').execute(function($, cf){
  P.load.js('http://z-ecx.images-amazon.com/images/G/01/browser-scripts/imageBlock-udp-airy/imageBlock-udp-airy-2644634036._V1_.js');
