amznJQ.addLogical("VideoAdsLightbox", ["http://z-ecx.images-amazon.com/images/G/01/browser-scripts/VideoAdsLightbox/VideoAdsLightbox-2666008280._V1_.js"]);

window['VideoAds'] = window['VideoAds'] || {};


amznJQ.onReady('VideoAdsLightbox', function() {});

P.when('jQuery').execute(function($) {
  var layoutId = 'va-lightbox';
  var isContainerReady = false;
  var isContainerRequested = false;
  var clickCount = 0;
  var lastClickedId;
  var lastClickTime;

  $("#va-rel-videos-carousel .va-video-item").each(function(i, element) {
    var thumbnail = $(element);

    thumbnail.find('a').click(function() {
      try {
        window['VideoAds'].activatePopover(thumbnail.attr('id'));
        
        return false;
      } catch (e) {
        
        return true;
      }
    });
  });

  var activatePopover = function() {
    requestContainer();
    if (!isContainerReady) {
      return;
    }
    window['VideoAds'].LightboxBinder.activatePopover({
      layoutId: layoutId,
      triggerId: lastClickedId,
      clickTime: lastClickTime,
      clickCount: clickCount,
      asin: $('#' + lastClickedId).attr('data-asin')
    });
    lastClickTime = undefined;
    lastClickedId = undefined;
    clickCount = 0;
  };

    
  window['VideoAds'].activatePopover = function(id) {
    clickCount++;
    if (id !== lastClickedId) {
        lastClickTime = + new Date;
        lastClickedId = id;
    }
    
    activatePopover();
  };

  var requestContainer = function() {
    if (isContainerRequested) {
        return;
    }
    isContainerRequested = true;
    P.when('va-lightbox-binder').execute(function(lightbox) {
      
      P.when('cf').execute(function() {
        
        lightbox.createContainer({
          productAsin: 'B00LTA0JBU',
          layoutId: layoutId,
          containerEndpoint: "/vap/lightboxContainer", 
          contentEndpoint: "/vap/lightbox",
          itemDetailsEndpoint: "/vap/itemDetails",
          server: "*",
          isMobileBrowser: false,

          isInternal: true,

          metricsMetadata: {
            baseUrl: "http://fls-na.amazon.com/1/action-impressions/1/OP/vap-metrics/action",
            client: 'VideoAds',
            method: 'Dvd.Lightbox',
            marketplaceId: 'ATVPDKIKX0DER',
            requestId: '0DBA8J4GRJYAX4DFZ5ZR',
            sessionId: '176-9265090-8147314'
          },
          completionCallback: function() {
              isContainerReady = true;
              setTimeout(function() {
                  if (lastClickedId) {
                      activatePopover();
                  }
              }, 0);
          }
        });
        
          lightbox.addProduct('va-lightbox', {
            asin: "B00IYS87GE",
            title: 'The Fault in Our Stars - Trailer',
