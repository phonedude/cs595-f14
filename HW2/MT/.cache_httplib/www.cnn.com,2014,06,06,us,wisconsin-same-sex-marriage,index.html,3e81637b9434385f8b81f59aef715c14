<a name="em3"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand33">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/120830042704-huckabee-speech-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>Inside Politics: Marriage Defense</span></cite>
</div>
<script>
var currExpandable="expand43";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
