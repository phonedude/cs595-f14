/* AD LOADER. Only accepts one optional argument, DOM id (actual DOM id must have 'ad-' prefix, but argument omits this. So, for #ad-container, pass in 'container') */
function cnnintLoadAd(domId) {
	
	/* Defines default DOM id */
	if (!domId) { domId = "container" }
	
	var winWidth = jQuery(window).width();
	
	if (winWidth > 760) {
		
		cnnad_createAd(domId, cnnintAds[cnnSectionName][0],"90","728", null, "");
		cnnad_registerSpace(cnnintAds[cnnSectionName][1],"728","90");
	
	} else {
		
		jQuery('#ad-'+domId).html('<iframe style="margin: 0 auto;" height="50" width="320" seamless src="http://bea4.v.fwmrm.net/ad/g/1?nw=48804&csid=cnn.com_mobile_mobileweb_'+cnnSectionName+'&resp=ad&pvrn='+Math.floor((Math.random()*1000000)+1)+';;ptgt=s&envp=g_iframe&w=320&h=50&adUnit=320x50_spon&slau=320x50_spon"></iframe>');
		
	}
	
}

