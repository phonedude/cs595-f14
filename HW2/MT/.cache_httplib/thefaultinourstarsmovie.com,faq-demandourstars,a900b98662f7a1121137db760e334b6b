<script type="text/javascript">

	if(window.location.href.indexOf("/tagged/demandourstars") > -1) {
		//$('#home-link').show();
		$('#demandourstars').show();
		$('#trailer-wrap').hide();
		$('.type_description').hide();
		$('#tweet-post').hide();
		$('#demand-legal').show();
		$('.posts-holder').attr('id', 'demand-posts');
	}
	if(window.location.href.indexOf("/tagged/soundtrack") > -1) {
		$('#soundtrack').show();
		$('#trailer-wrap').hide();
		$('.type_description').hide();
		$('#tweet-post').hide();
		$('body').addClass('soundtrack');
		$('.posts-holder').attr('id', 'soundtrack-posts');
	}
	if(window.location.href.indexOf("/#tickets") > -1) {
		$('#tickets').removeClass('hidden');
	}
	/*
	if ( window.location.hash.indexOf('#signup') === -1 && window.location.pathname.indexOf('/post') === -1 )
	{
		$('#tickets').removeClass('hidden');
	}*/
	if(window.location.href.indexOf("/#signup") > -1) {
		$('#sign-up').removeClass('hidden');
	}

	/*
	 if(window.location.href.indexOf("#fan") > -1 )) {
	 $('#fan-sign-up').removeClass('hidden');
	 }*/

	var images = [
		'http://static.tumblr.com/60e5264563ba22b745e14ba441c54a45/4pxlsbd/Ihanbzx3g/tumblr_static_dmbdygovt48o4oc0co00gg8wc.jpg'
	
	
	
	
	];

	var chosenImage = images[Math.floor(Math.random() * images.length)];

	$('.header-image').css({
		'background-image': 'url(' + chosenImage + ')'
	});

	$('.header-image > img').attr("src", chosenImage);

	$( '.header-image' ).imagesLoaded(function(){
		$('header').addClass('on');
	});

</script>

<script>
	// GO BIG Frontend JS
	$(document).ready(function () {
		function n() {
			var e = 10;
			$(".single-post").length === 0 ? e = 5 : e = 10;
			$(".photoset-grid, this").photosetGrid({
				highresLinks: !0,
				rel: "",
				gutter: e + "px",
				onComplete: function () {
					$(".photoset-grid").attr("style", "");
					$(".photoset-grid a").colorbox({
						photo: !0,
						scalePhotos: !0,
						maxHeight: "90%",
						maxWidth: "90%",
						transition: "none",
						title: function () {
							return $("img", this).attr("alt")
						}
					});
					$(".photoset-grid").each(function () {
						var e = $(this).attr("data-id");
						$(this).find(".photoset-cell").attr("rel", e)
					})
				}
			})
		}
		if ($(".header-image.parallax").length > 0 && $(window).width() > 1024) {
			var e = $(".header-image.parallax"),
					t = $("header .center .logo a");
			$(window).scroll(function () {
				e.css("transform", "translate3d(0px," + window.scrollY / 3 + "px, 0px)");
				t.css("transform", "translate3d(0px," + window.scrollY / 4 + "px, 0px)")
			})
		}
		$(".the-posts article").each(function () {
			n()
		});
		$(".the-posts .posts-grid").imagesLoaded(function () {
			$(".the-posts .posts-grid").masonry({
				itemSelector: "article",
				columnWidth: ".the-posts .grid-sizer",
				hiddenStyle: {
					opacity: 0,
					transform: "translateY(100%)"
				},
				visibleStyle: {
					opacity: 1,
					transform: "translateY(0)"
				}
			});
			$("nav").hasClass("sticky") && $("nav").waypoint(function (e) {
				e === "up" ? $(this).removeClass("fixed") : e === "down" && $(this).addClass("fixed")
			}, {
				offset: 0
			})
		});
		var counter = 1;
		$(".the-posts .posts-grid").infinitescroll({
			navSelector: ".pagination",
			nextSelector: ".pagination a.next",
			itemSelector: ".the-posts .posts-grid article.dynamic-post",
			bufferPx: 100,
			loading: {
				img: "http://static.tumblr.com/e6lc7yi/PnCmidqaj/1px.png",
				speed: 200,
				msgText: "!",
				finishedMsg: "x"
			},
			errorCallback: function () {
				$(".load-more").addClass("off")
			}
		}, function (e, t) {
			var r = $(e).css({
				opacity: 0
			});

			// wrap count each page loaded
			r.wrapAll('<div id="page-' + counter + '"></div>');
			counter++
			n();
			r.imagesLoaded(function () {
				$(".the-posts .posts-grid").masonry("appended", r, !0)
			});
			var i = t.state.currPage;
			Tumblr.LikeButton.get_status_by_page(i);
			$(".dsq-comment-count").length > 0 && function () {
				var e = document.createElement("script");
				e.async = !0;
				e.type = "text/javascript";
				e.src = "//" + disqus_shortname + ".disqus.com/count.js";
				(document.getElementsByTagName("HEAD")[0] || document.getElementsByTagName("BODY")[0]).appendChild(e)
			}()
		});
		if ($(".load-more").length > 0) {
			$(window).unbind(".infscr");
			$(".load-more").click(function () {
				$(".the-posts .posts-grid").infinitescroll("retrieve")
			})
		}
		$(".scroll-top").click(function () {
			$("body,html").animate({
				scrollTop: 0
			}, 800, "easeInOutQuint");
			return !1
		});
		$(".header-prompt").length > 0 && $(".header-prompt").click(function () {
			$("html, body").animate({
				scrollTop: $("nav").offset().top
			}, 800, "easeInOutQuint");
			return !1
		});
		$(".the-posts").on("click", ".social-export", function (e) {
			$(this).parent().toggleClass("on")
		})
		//Place the Scrapbook link as the last nav link
		var myScrapbookLink = $('a[title="Scrapbook"]').parent();
		$('nav ul').not('.social-menu').append(myScrapbookLink.detach());
		
		var mySweepstakesLink = $('a#sweepstakes-link').parent();
		$('nav ul').not('.social-menu').append(mySweepstakesLink.detach());
		
		//Place the mylittleinfinity link as the last nav link
		var myLittleLink = $('a#mylittleinfinity-link').parent();
		$('nav ul').not('.social-menu').append(myLittleLink.detach());

		//Place the pre-order link as the last nav link
		var myPreorderLink = $('a#buy-link').parent();
		$('nav ul').not('.social-menu').append(myPreorderLink.detach());
		
	});
</script>
