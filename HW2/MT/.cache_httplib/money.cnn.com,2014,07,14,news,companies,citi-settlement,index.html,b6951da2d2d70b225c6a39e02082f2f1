<script type="text/javascript">cnnad_createAd("0d1e7ce212f36873","http://ads.cnn.com/html.ng/site=cnn_money&cnn_money_pagetype=story_sync&cnn_money_position=970x418_top&cnn_money_rollup=business_news&cnn_money_section=company_news&params.styles=fs&page.allowcompete=no","418","970");</script>
