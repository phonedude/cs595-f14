mObj.videoSource='CNN';
mObj.videoSourceUrl='http://www.cnn.com';
mObj.lgImage="http://i2.cdn.turner.com/cnn/dam/assets/140602154229-orig-npr-us-army-sgt-bowe-bergdahl-released-by-taliban-00011728-story-body.jpg";
mObj.lgImageX=300;
