	<script>
	// Script to represent count as the units of k, if its > 10000
		$('.count').each(function() {
			var rep = $(this).html();
			if (rep >= 10000) {
				rep = (rep/1000).toFixed(rep % 1000 != 0)+'k';
				$(this).html(rep);
			}
		});
	</script>

	
