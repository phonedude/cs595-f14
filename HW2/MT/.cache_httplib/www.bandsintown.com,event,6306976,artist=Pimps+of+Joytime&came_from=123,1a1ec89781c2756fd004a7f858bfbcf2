<script type="text/javascript">
  
jQuery(function($) {

  var redirect = function() {
    
  };

  BIT.Events = new BIT.Collections.EventList([{"artist_event_link_label":null,"venue_id":596875,"venue_name":"Camp Euforia","ticket_url":"http://www.bandsintown.com/event/6306976/buy_tickets?affil_code=fb_182001385145932\u0026artist=D.+Bess","artist_id":947521,"title":"Jul 12 - D. Bess @ Camp Euforia - Lone Tree, IA","bit_ticket_url":"http://www.bandsintown.com/event/6306976/buy_tickets?affil_code=fb_182001385145932\u0026artist=D.+Bess","facebook_event_id":"392235570887730","page_id":"182001385145932","formatted_datetime":"Friday, July 12, 2013 at 7:00pm","artist_name":"D. Bess","artist_event_id":6962565,"url":"http://www.bandsintown.com/event/6306976?artist=D.+Bess","formatted_location":"Lone Tree, IA","datetime":"2013-07-12T19:00:00","ticket_type":"Tickets","artist_event_link_url":null,"published":true,"id":6306976,"venue_location":{"country":"United States","city":"Lone Tree","region":"Iowa","latitude":41.4880556,"longitude":-91.4258333},"rsvp_url":"http://www.bandsintown.com/event/6306976/facebook_rsvp?artist=D.+Bess"}])

  $(".i-was-there").click(function(event) {
    var target = $(event.target),
    loading_image = $(".loading-small");
    loading_image.show();

    BIT.FB.Permissions.verify("email,user_likes,user_location,user_actions.music,rsvp_event", function(granted) {
      if (granted) {
        target.addClass("active");

        $.fb_ajax({
          url: "/facebook/rsvp",
          type: "POST",
          data: {
            came_from: "143",
            bandsintown_event_id: 6306976,
            artist_id: 947521,
            rsvp_status: 'attending',
            track_artist: true
          },
          success: function(data) {
            BIT.FB.Timeline.go_to_event({
              event: BIT.Events.get(6306976),
              success: redirect,
              error: redirect
            });
          },
          error: redirect
        });
      }

      loading_image.hide();
    });
  });

});

</script>
