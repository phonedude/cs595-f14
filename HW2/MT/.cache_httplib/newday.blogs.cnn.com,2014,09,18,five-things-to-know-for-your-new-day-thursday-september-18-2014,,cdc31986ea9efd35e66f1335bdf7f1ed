	(function( $j, win, undef ) {
					var warn = 'Template cnn_metadata.business.cnn.page.publish_date already set to:';
			win.cnn_metadata = $j.extend( {}, { 'business': { 'cnn': { 'page':{} } } } , ( win.cnn_metadata || {} ) );

			if ( undef === cnn_metadata.business.cnn.page.publish_date ) {
				cnn_metadata.business.cnn.page.publish_date = "2014/09/18";
