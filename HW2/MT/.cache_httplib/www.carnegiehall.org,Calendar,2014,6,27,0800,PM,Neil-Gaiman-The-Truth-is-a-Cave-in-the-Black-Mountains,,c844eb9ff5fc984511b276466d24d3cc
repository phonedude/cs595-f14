            var presaleContentId = "";
            
            var loopCount = 0
                       
            $.ajax({
                type: "GET",
                contentType: "application/json; charset=utf-8",
                cache: false,
                data: { "action": "ReturnEventPageBuyInfo", "evnt": 22567},
                url: "/ch/handlers/buybuttonhandler.ashx",

                success: function (data) {
                    if (data.Debug.indexOf("Unexpected") > -1 && loopCount == 0) {
                        // call it again.
                        //alert("asdf")
                        loopCount++;
                        $.ajax({
                            type: "GET",
                            contentType: "application/json; charset=utf-8",
                            cache: false,
                            data: { "action": "ReturnEventPageBuyInfo", "evnt": 22567},
                            url: "/ch/handlers/buybuttonhandler.ashx",

                            success: function (data) {
                               
                                if (data.LargeDisplay != "" && data.CssClass == "available") //ADDITIONAL CASES ADDED BY BJM BECAUSE WE WERENT SHOWING THE NOTIFICATION EVER IF THE LARGEDSPLAY WAS NOT NULL.
                                { 
                                    $("#buttonbuttonholder").html(data.LargeDisplay); 
                                    //alert("largedisp;laoy?");
                               }else {
                                    $("#buttonbuttonholder").html(data.Notification);
                                    //alert("notification");
                               }

                              $(document).ready(function(){
                                  $("a[rel^='prettyPhoto']").prettyPhoto({show_title: false});
                              });

                            }
                        });
                    }


                    if (data.LargeDisplay != "" && data.CssClass == "available") //ADDITIONAL CASES ADDED BY BJM BECAUSE WE WERENT SHOWING THE NOTIFICATION EVER IF THE LARGEDSPLAY WAS NOT NULL.
                    { 
                        $("#buttonbuttonholder").html(data.LargeDisplay); 
                        //alert("largedisp;laoy?");
                   }else {
                        $("#buttonbuttonholder").html(data.Notification);
                        //alert("notification");
                   }

                  $(document).ready(function(){
                      $("a[rel^='prettyPhoto']").prettyPhoto({show_title: false});
                  });

                }
            });

			// $.ajax({
            //           cache: false,
            //    		url: "/ch/handlers/rsvp.ashx?action=init&eventid=4295003121",
            //    		success: processRSVPLinks
            // 		});

			 /*$.ajax({
			     cache: false,
                        url: "/ch/handlers/rehearsalpass.ashx?action=init&eventid=4295003121&url=http://www.carnegiehall.org/Event.aspx?id=4295003121",
              			success: processRehearsalPassLinks
          		}); */
            //BJM presale public
             $.ajax({
			     cache: false,
                        url: "/ch/handlers/presales.ashx?action=getBuyButton&evtEktId=4295003121",
              			success: function (data) {
                            //alert($("#presalebuttonholder"));
                            if(!data.isPrivate && data.presaleType && data.presaleType.toLowerCase() != "token"){
                                var promoInput = "";
                                var presaleContentId = data.presaleContentId;
                                var presaleType = data.presaleType;
                                var presaleMOS = data.presaleMos;
                                var eventId = 22567
                                if (data.presaleType.toLowerCase() == "promo"){
                                    promoInput = "<br/>Promo Code:<br/> <input type=text name='presaleInput' id='presaleinput' size=10><br/>";
                                }else if (data.presaleType.toLowerCase() == "bin"){
                                    promoInput = "<br/>Please enter the first six digits of your credit card:<br/> <input type=text name='presaleInput' id='presaleinput' size=10><br/>";
                                }else if (data.presaleType.toLowerCase() == "mastercardbinpromo"){
                                    promoInput = "<br/>Please enter the first six digits of your MasterCard:<br/> <input type=text name='presaleInput' id='presaleinput' size=10 maxlength='6'><br/>";
                                }else {
                                   // promoInput = "<br/>Token:<br/> <input type=text name='presaleInput' id='presaleinput' size=10><br/>";
                                }
                                var buttonInfo = ""; 
                                if (!data.userIsAlreadyInPresaleMOS) {
                                    buttonInfo = promoInput + "<a class='md_btn_red' id='presalebuybutton' href=javascript:validatePresale(" + presaleContentId + ",'" + presaleType + "','" + presaleMOS + "','" + eventId + "') title='BUY PRESALE'>BUY PRESALE</a>";
                                }
                                if (data.displayPresaleBuyButton){
                                    $("#presalebuttonholder").html(data.introCopy + "<br/>" + buttonInfo);
                                
                                }
                            }
                        }
          		});

                


        });
       function validatePresale(presaleEktronId, presaleType, presaleMos, eventId){
            $("#presalevalidationmessages").html("");
            $("#presalevalidationmessages").hide();

            if ($("#presaleinput").val() == "") {
                $("#presalevalidationmessages").html("Please enter a value."); 
                $("#presalevalidationmessages").show();
            }else{
                var ajaxUrl = "";
                if (presaleType.toLowerCase() == "promo"){
                    ajaxUrl = "/ch/handlers/presales.ashx?action=validateandassignpromo&promo=" + $('#presaleinput').val() + "&contentid=" + presaleEktronId;
                    //alert(ajaxUrl);
                }else if(presaleType.toLowerCase() == "bin"){
                    ajaxUrl = "/ch/handlers/presales.ashx?action=validateandassignbin&bin=" + $('#presaleinput').val() + "&contentid=" + presaleEktronId;
                }else if(presaleType.toLowerCase() == "mastercardbinpromo"){
                    ajaxUrl = "/ch/handlers/presales.ashx?action=validateandassignmsbin&bin=" + $('#presaleinput').val() + "&contentid=" + presaleEktronId;
                }else{
                    //ajaxUrl = "/ch/handlers/presales.ashx?action=validatetoken&token=" + $('#presaleinput').val() + "&contentid=" + presaleEktronId;
                }
                $.ajax({
			        cache: false,
                    url: ajaxUrl,
              		success: function (data) {
                        //alert($("#presalebuttonholder"));
                        if (data.isValid){
                            //alert("success");
                            setValidatedPresale(presaleEktronId, eventId);
                            //ALTER MOS HERE AND SEND THEM ON THEIR WAY.
                            //window.location.href = $(".buy-button-wrapper").find('a').attr('href');

                             
                        }else{
                            if (presaleType != "mastercardbinpromo") {
                                $("#presalevalidationmessages").html("We could not activate your presale. Please try again."); 
                            }else {
                                $("#presalevalidationmessages").html("We could not activate your presale. Please try again with a valid MasterCard."); 
                            }
                            $("#presalevalidationmessages").show();
                        }    
                    }
          		});
            }
        }
        function setValidatedPresale(presaleEktronId, eventId){
             $.ajax({
			    cache: false,
                url: "/ch/handlers/presales.ashx?action=setvalidatedpresale&validationText="+ $('#presaleinput').val() + "&evtEktId=" + 4295003121,
                success: function (data2) {
                         
                    if (data2.isValid){
                        window.location.href = "/SiteCode/Purchase/SYOS/SeatSelection.aspx?startWorkflow=true&quickBuy=false&quantity=0&eventId=" + eventId;
                    }else {
                        $("#presalevalidationmessages").html("We could not activate your presale. If you have items in your cart, please complete that purchase before trying again."); 
                        $("#presalevalidationmessages").show();
                    }
                }
            });
        }
    </script>

    
