var mObj={};
mObj.type='video';
mObj.contentId='';
mObj.network='cnn';
mObj.source='us/2014/08/17/nr-field-ferguson-pathologist-brown-case.cnn';
mObj.videoSource='CNN';
mObj.videoSourceUrl='www.cnn.com ';
mObj.lgImage="http://i2.cdn.turner.com/cnn/dam/assets/140817142657-nr-field-ferguson-pathologist-brown-case-00000619-story-body.jpg";
mObj.lgImageX=300;
