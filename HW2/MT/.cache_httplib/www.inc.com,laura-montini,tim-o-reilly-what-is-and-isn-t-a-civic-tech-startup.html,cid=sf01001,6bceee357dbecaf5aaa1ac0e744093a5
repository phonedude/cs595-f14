					$(document).ready(function(){ });
					var stampChecker = setInterval("checkStamp()", 1000);
					function checkStamp() {
						if($(".leaderboard_abovenav > div > iframe") && $(".leaderboard_abovenav > div > iframe").width()>0) {
							if($(".leaderboard_abovenav > div > iframe").width()<750) {
