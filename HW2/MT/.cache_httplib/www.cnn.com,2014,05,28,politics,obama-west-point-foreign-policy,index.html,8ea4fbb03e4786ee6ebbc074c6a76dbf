mObj.videoSource='CNN';
mObj.videoSourceUrl='http://ac360.blogs.cnn.com/';
mObj.lgImage="http://i2.cdn.turner.com/cnn/dam/assets/140527210738-ac-peter-bergen-00025509-story-body.jpg";
mObj.lgImageX=300;
mObj.lgImageY=169;
mObj.origImageX="214";
