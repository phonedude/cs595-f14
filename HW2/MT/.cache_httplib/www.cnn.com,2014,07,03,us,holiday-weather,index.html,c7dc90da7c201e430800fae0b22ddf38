<a name="em5"></a>
<div class="cnn_strylftcntnt cnn_strylftcexpbx" id="expand240">
<div class="cnn_strylceclbtn"><img src="http://i.cdn.turner.com/cnn/.e/img/3.0/mosaic/bttn_close.gif" alt="" border="0" height="23" width="58"/></div>
<img src="http://i2.cdn.turner.com/cnn/dam/assets/140702180119-exp-tsr-johns-carolina-preps-for-arthur-00002001-story-body.jpg" alt="" border="0" class="box-image" height="120" width="214"/><cite class="expCaption"><span>Carolina preps for Arthur</span></cite>
</div>
<script>
var currExpandable="expand340";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
mObj.type='video';
mObj.contentId='';
