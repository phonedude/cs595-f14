  #col3 .linklist {
      margin-top: -6px;
      font-family: "Gill Sans", Arial, Helvetica, Calibri, sans-serif;
    }
      #col3 .linklist li {
        line-height: 16px;
        margin-top: 6px;
      }
      #col3 .linklist li img {
        margin: -2px 5px 0 0;
        vertical-align: middle;
      }
  
    #col3 .avatar-wrapper img {
      -webkit-border-radius: 3px;
      -moz-border-radius: 3px;
      border-radius: 3px;
      border: 3px solid #f4f4f4;
    }
    
    #description .inner {
      padding: 6px 11px 8px;
      line-height: 18px;
      font-style: italic;
      color: #666;
    }
      #description a {
        text-decoration: underline;
      }
      #description p {
        margin-top: 9px;
      }
      #description p:first-child {
        margin-top: 0;
      }
    
    #search .inner {
      padding: 10px 11px;
      border-top: 0;
    }
    #search-input-wrapper {
      border: 1px solid #999;
      background: #f4f4f4;
      padding: 2px;
      -webkit-border-radius: 3px;
      -moz-border-radius: 3px;
      border-radius: 3px;
    }
      #search-input-wrapper input {
        background: #fff url(http://static.tumblr.com/cc4ocei/7Dgkzk7t3/search.png) no-repeat 2px 1px;
        color: #222;
        border: 0;
        margin: 0;
        padding: 3px 4px 3px 20px;
        display: block;
        width: 122px;
        font-family: "Gill Sans", Arial, Helvetica, Calibri, sans-serif;
      }
        #search-input-wrapper input:focus {
          background-color: #fafafa;
          outline: 0;
        }
      
    #authors ul {
      margin: 0 -1px;
    }
      #authors li {
        clear: both;
        font-family: "Century Schoolbook", Century, Georgia, serif;
      }
      #authors a {
        display: block;
        margin: 8px 0 4px;
      }
      #authors .avatar-wrapper {
        float: left;
        margin: 2px 8px 0 0;
      }
      #authors strong {
        font-size: 14px;
        font-weight: bold;
        display: block;
        margin-bottom: 4px;
      }
      #authors small {
        font-size: 11px;
        font-style: italic;
        display: block;
      }
     
    #networks ul {
      width: 162px;
      margin-right: -10px;
    }
      #networks li {
        width: 71px;
        float: left;
        margin-right: 10px;
      }
  
    #following #following-avatars-wrapper {
      margin: 0 -10px -5px -1px;
    }
      #following a {
        float: left;
        margin: 0 7px 7px 0;
      }
  
    #footer {
      border-top: 1px solid #b2b2b2;
      height: 11px;
      color: #666;
    }
      #footer .inner {
        padding: 10px 4px 40px;
      }
      #footer-left {
        float: left;
      }
      #footer-right {
        float: right;
      }
    #footer li {
      display: inline;
      font-family: "Gill Sans", Arial, Helvetica, Calibri, sans-serif;
      font-size: 11px;
    }
      #footer li a {
        text-transform: uppercase;
        letter-spacing: 1px;
      }
      #footer-left li {
        margin-right: 8px;
      }
      #footer-right li {
        margin-left: 8px;
      }  
      
    .hidden { display: none !important; }
  .visible { display: block !important; }
 
  .tag, .search_query { font-weight: bold; }
  .tag:before, .search_query:before { content:'“'; }
.tag:after, .search_query:after { content:'”'; }
