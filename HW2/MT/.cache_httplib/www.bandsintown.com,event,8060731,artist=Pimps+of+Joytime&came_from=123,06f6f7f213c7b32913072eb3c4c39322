jQuery(function($) {

  var redirect = function() {
    
  };

  BIT.Events = new BIT.Collections.EventList([{"artist_event_link_label":null,"venue_id":2258826,"venue_name":"Canal Club","ticket_url":"http://www.thecanalclub.com/","artist_id":659721,"title":"May 22 - Proverbial @ Canal Club - Richmond, VA","bit_ticket_url":"http://www.bandsintown.com/event/8060731/buy_tickets?affil_code=fb_225911220458\u0026artist=Proverbial","facebook_event_id":"837651976249687","page_id":"225911220458","formatted_datetime":"Thursday, May 22, 2014 at 9:00pm","artist_name":"Proverbial","artist_event_id":9304590,"url":"http://www.bandsintown.com/event/8060731?artist=Proverbial","formatted_location":"Richmond, VA","datetime":"2014-05-22T21:00:00","ticket_type":"Tickets","artist_event_link_url":null,"published":true,"id":8060731,"venue_location":{"country":"United States","city":"Richmond","region":"Virginia","latitude":37.9358333,"longitude":-122.3466667},"rsvp_url":"http://www.bandsintown.com/event/8060731/facebook_rsvp?artist=Proverbial"}])

  $(".i-was-there").click(function(event) {
    var target = $(event.target),
    loading_image = $(".loading-small");
    loading_image.show();

    BIT.FB.Permissions.verify("email,user_likes,user_location,user_actions.music,rsvp_event", function(granted) {
      if (granted) {
        target.addClass("active");

        $.fb_ajax({
          url: "/facebook/rsvp",
          type: "POST",
          data: {
            came_from: "143",
            bandsintown_event_id: 8060731,
            artist_id: 659721,
            rsvp_status: 'attending',
            track_artist: true
          },
          success: function(data) {
            BIT.FB.Timeline.go_to_event({
              event: BIT.Events.get(8060731),
              success: redirect,
              error: redirect
            });
          },
          error: redirect
        });
      }

      loading_image.hide();
    });
  });

});

</script>
