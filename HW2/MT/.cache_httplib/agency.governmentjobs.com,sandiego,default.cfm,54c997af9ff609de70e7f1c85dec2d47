<script type="text/javascript">
	//<![CDATA[
		function check_all(elementName) {
			set_checked_all_to(elementName, 1);
		}

		function uncheck_all(elementName) {
			set_checked_all_to(elementName, 0);
		}

		function set_checked_all_to(elementName, val) {
			var dml= document.frmSearchJobs;
			var leng = dml.elements.length;
			for (var i=0; i< leng; i++)
				if (dml.elements[i].name == elementName)
					dml.elements[i].checked = val;
		}

		function revealRow(ElementID) {
			document.getElementById(ElementID).style.display = 'block';
		}

		function showHideExplanations() {
			if (document.getElementById("keywordSearchHelp")) {
				document.getElementById("keywordSearchHelp").style.display = "none";
			}
			if (document.getElementById("salarySearchHelp")) {
				document.getElementById("salarySearchHelp").style.display = "none";
			}
		}
	//]]>
</script>



