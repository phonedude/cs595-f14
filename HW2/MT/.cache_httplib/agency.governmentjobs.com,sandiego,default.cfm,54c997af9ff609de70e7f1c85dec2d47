					<input type="hidden" name="pageCount" id="pageCount_start_print" value="2" />
					<input type="hidden" name="sortBy" id="sortBy_start_print" value="CLASSIFICATION" />
					<input type="hidden" name="sortOrder" id="sortOrder_start_print" value="ASC" />
					<input type="hidden" name="pageNumber" id="pageNumber_start_print" value="1" />
					<input type="hidden" name="headerFooter" id="headerFooter_start_print" value="0" />

					

					<table width="98%" border="0" cellpadding="0" cellspacing="0" style="margin:0 auto;">
						<tr>
							<td>
								&nbsp;&nbsp;
								<img src="/images/printpage.gif" width="15" height="11" alt="Print this page" longdesc="/images/longDesc/printPage.html" style="border:none;"/>
								&nbsp;&nbsp;
								<a href="javascript:void(0)" onclick="printResults();">Print this page</a>
