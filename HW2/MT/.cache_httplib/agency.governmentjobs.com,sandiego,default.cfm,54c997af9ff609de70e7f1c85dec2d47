		<script type="text/javascript">
			//<![CDATA[
				function sortResults(sortBy, sortOrder) {
					document.getElementById('sortBy_start').value = sortBy;
					document.getElementById('sortOrder_start').value = sortOrder;
					document.getElementById('pageNumber_start').value = 1;
					document.getElementById('jobPagination_start').submit();
				}

				function printResults() {
					var action = document.getElementById('jobPagination_start_print').action;
					var printSearch = /&printPage=1/;
					document.getElementById('jobPagination_start_print').target = "_blank";
					if(document.getElementById('jobPagination_start_print').action.search(printSearch) < 0)
						document.getElementById('jobPagination_start_print').action = action + '&printPage=1';
					document.getElementById('jobPagination_start_print').submit();
				}

				function stripNonNumeric(e){
					var allowed = /\d/;
					var key;
					var keychar;

					if(window.event) {
						key = e.keyCode;
					}
					else if(e.which) {
						key = e.which;
					}

					if (isNaN(key)) return true;

					keychar = String.fromCharCode(key);

					// check for backspace or delete, or if Ctrl was pressed
					if (key == 8)
					{
						return true;
					}

					return allowed.test(keychar);
				}
			//]]>
		</script>
	
