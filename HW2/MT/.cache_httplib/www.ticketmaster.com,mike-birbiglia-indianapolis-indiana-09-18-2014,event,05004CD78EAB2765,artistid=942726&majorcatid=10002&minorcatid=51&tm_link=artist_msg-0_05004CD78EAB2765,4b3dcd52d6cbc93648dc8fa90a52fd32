			RR.getCharacterLimits = function (module, page) {
				if (!(module in rr_character_limits)) {
					return undefined;
				}
				var module_limits;
				module_limits = rr_character_limits[module];
				if (!(page in module_limits)) {
					return undefined;
				}
				return module_limits[page];
			};

