  <script>
  var split_column_target = Math.ceil($('#topic_dropdown .topic_filter').length / 2);
  
  $('#topic_dropdown .topic_filter:lt(' + split_column_target + ')').wrapAll('<div class="topic_column left"></div>');
  $('#topic_dropdown .topic_filter:gt(' + (split_column_target-1) + ')').wrapAll('<div class="topic_column right"></div>');
var schedule_types = {
  "Keynotes" : {
    "description" : "Keynotes scheduled for Strata + Hadoop World 2014.",
    "offpage_target" : false
  },
  "Tutorials" : {
    "description" : "All tutorials take place Wednesday, October 15.",
    "offpage_target" : false
  },
  "All-Day Tutorials" : {
    "description" : "These tutorials take place all day, Wednesday, October 15.",
    "offpage_target" : false,
    "day_tutorials" : [37031, 37032, 37033, 37034, 37035, 37037]
  },  
  "Sponsored" : {
    "description" : "Sponsored sessions scheduled for Strata + Hadoop World 2014.",
    "offpage_target" : false
  },  
  "Events" : {
    "description" : "Events scheduled for Strata + Hadoop World 2014.",
    "offpage_target" : false
  },  
  "Office Hours" : {
    "description" : "",
    "offpage_target" : true,
    "offpage_link" : "/stratany2014/public/schedule/topic/1214"
  } 
};

/* populate "Filter By Type" select */
for (var type in schedule_types) {
  var add_class = (schedule_types[type].offpage_target) ? " offpage_target" : "";
  var add_href = schedule_types[type].offpage_link || "#"; 
  $('#schedule_topics').append("<a href=\"" + add_href + "\" class=\"topic_filter" + add_class + "\" data-topic-id=\"" + type +"\" data-description=\"" + schedule_types[type].description + "\">" + type +"</a>");
  $('#type_select').append("<option value=\"" + type + "\">" + type + "</option>");
}

/* populate "Filter By Topic" select */
$('#topic_dropdown .topic_filter').each(function() {
  $('#topic_select').append("<option value=\"" + $(this).data('topicId') + "\" data-description=\"" + $(this).data('description') + "\">" + $(this).text() + "</option>");
});

var topic_dropdown = $('#topic_dropdown');
$('#topics_link').on('click', function(evt) {
  evt.preventDefault();
  topic_dropdown.toggleClass('hide');
});

/* filter by URL parameter */
var tracks = getUrlVars();
var track_filter = decodeURIComponent(tracks.filter);

if (track_filter !== "undefined" && track_filter !== undefined) {
  var filter_id = $('.topic_filter').filter(function() {
    return ($(this).text() === track_filter) ? $(this).data('topicId') : '';
  });
  if (filter_id !== '') filterSessions(filter_id, track_filter);
}

/* filter by select (mobile view) */
$('#type_select, #topic_select').on('change', function() {
  var selected = ($(this).is('#type_select')) ? $('#type_select option:selected') : $('#topic_select option:selected');
  if ($.inArray(selected.val(), getOffPageTargets(schedule_types)) > -1) {
    window.location(schedule_types[name].offpage_link);
  } else {
    filterSessions(selected.val(), selected.text());
  }
});

/* filter using click event */
$('.topic_column, #schedule_topics').on('click', '.topic_filter', function(evt) {
  var filter = $(this);
  if ($.inArray(filter.data('topicId'), getOffPageTargets(schedule_types)) > -1 || filter.data('topicId') === "Topics") {
    return;
  } else {
    evt.preventDefault();
  }
    
  topic_dropdown.addClass('hide');
  filterSessions(filter.data('topicId'), filter.text());
});

$('#schedule_days li.day_filter a').each(function() {
  $(this).on('click', function(e) {
    e.preventDefault();
    $('#schedule_days li').removeClass('filtered');   
    $(this).parent().addClass('filtered');
    $('#schedule_topics a').removeClass('filtering');//remove filter from topics   
    $('.en_session, .en_schedule_day').removeClass('hide');
    $('.current_filter').remove();

    var filter_day = $(this).attr('href').slice(1); 
    $('.en_schedule_day h2 a').each(function() {
      if ($(this).attr('name') !== filter_day) {
         $(this).parent().parent().addClass('hide');
      }
    }); 
    //getLazy();            
  });
});

function filterSessions(topic_id, topic_name) {
  $('#schedule_topics a.topic_filter').removeClass('filtering');
  $('.current_filter').remove();
  $('.en_schedule_day, .en_session').removeClass('hide');
  
  if (topic_id !== "All") {
    $('.en_plenary, .en_break').addClass('hide');
    $('.topic_column .topic_filter, #schedule_topics .topic_filter, #topic_select option:selected').filter(function() {
      var that = $(this);
      if (that.parent().hasClass('topic_column')) {
        $('#topic_link').addClass('filtering');
      } 
      if (that.data('topicId') === topic_id || that.val() === topic_id) {
        var topic_info = "<div class=\"current_filter\"><h2>" + that.text() + "</h2><p>" + htmlDecode(that.data('description')) + "</p></div>";
        $('#en_schedule').before(topic_info);  
        that.addClass('filtering');   
      }
    });
    if (topic_id === "Tutorials") {
      var filter_day = $('#d2014-10-15').attr('id'); 
      $('.en_schedule_day h2 a').each(function() {
        if ($(this).attr('id') !== filter_day) {
           $(this).parent().parent().addClass('hide');
        }
      });            
    } else if (topic_id === "All-Day Tutorials") {
      $('.en_session').addClass('hide');
      schedule_types[topic_id].day_tutorials.map(function(session_id) {
        $('#psched' + session_id).parent().removeClass('hide');
      });     
    } else {
      $('.en_session_topics').each(function() {
        var current_topic = $(this).text().trim();
        if (current_topic.indexOf(topic_name) === -1){
          $(this).parent().parent().addClass('hide');
        } else {
          $(this).parent().parent().removeClass('hide');
        }
      }); 
    }    
  } else {
    $('#all').addClass('filtering');
  } 
  //getLazy();
}//filterSessions()

function htmlDecode(value) {
  return (typeof value === undefined) ? "" : $('<div/>').html(value).text();
}

function getUrlVars() {
  var vars = { };
  var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
    vars[key] = value;   
  });
  return vars;   
}

function getOffPageTargets(data) {
  var targets = [ ];
  for (var prop in data) {
    if (data[prop].offpage_target) {
      targets.push(prop);
    } 
  }
  return targets;
}
</script>
</div>
</div>


