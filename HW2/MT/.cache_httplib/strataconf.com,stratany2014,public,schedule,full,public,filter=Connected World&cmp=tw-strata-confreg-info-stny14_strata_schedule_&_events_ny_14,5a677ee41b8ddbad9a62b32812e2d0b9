  <script type="text/javascript">
  $(function() {

  

  


  

  

  


var getUrlVars = function(){
  var vars = { };
  var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
    vars[key] = value;   
  });
  return vars;   
};

var tracks = getUrlVars(); 

var filterTopicSessions = function(session_filter, topic, topic_desc){
	  $('#grid_topics a').removeClass('filtering');
        session_filter.addClass('filtering');
		$('.topic_description').hide();
		$('.en_schedule_day').show();

		if (topic == "All Sessions") {
			$('.en_schedule_time').show();
			$('.en_session').show();
		} else if (topic == "Tutorials" ) {                        
                        var tutorial_day = $('#d2014-10-15');
                        tutorial_day.addClass('filtered');
                        $('.en_schedule_time').show();
                        $('.en_session').show();
                        $('.topic_description').hide();

                        $('.en_schedule_day').show();
                        var tutorial_desc = $('#tutorials');
                        $('#en_schedule').before(tutorial_desc);

                        tutorial_desc.show();
                        var filter_day = 'd2014-10-15'; 
                         $('.en_schedule_day h2 a').each(function() {
                               if ($(this).attr('name') != filter_day) {
                                   $(this).parent().parent().hide();
                               }
                          });
                } else {
			$('#en_schedule').before($(topic_desc));
			$(topic_desc).show();

			$('.en_schedule_time').show();
			$('.en_session').show();

            $('.en_plenary').hide();
            $('.vevent').hide();
			$('.en_break').hide();

			$('.en_session_topics').each(function() {
			    var current_topic = $(this).text().trim();
			    if (current_topic.indexOf(topic) === -1){
                                console.log(current_topic);
			    	$(this).parent().parent().hide();
			    } else {
                                $(this).parent().parent().show();
                             }
				
			});
			$('.en_schedule_time').each(function(){
				var visible_children = $(this).children('div:visible').length;
				if (visible_children == 0) {
					$(this).hide();
				}
			});
		} 
          getLazy();
};

$('#grid_topics .topic_filter').each(function() {
		var topic_id = $(this).data('topicId');
		var topic_description = '#topic_' + topic_id;
		var topic_name = $(this).text();
                var track_filter = decodeURIComponent(tracks.filter);
        if (track_filter == topic_name) {
          filterTopicSessions($(this), topic_name, topic_description);
		}
		
	$(this).on('click', function(e) {
              if (topic_id != 1208 && topic_id != 1209  && topic_id != 1237  && topic_id != 1214) {
		e.preventDefault();
              
		$('#schedule_options li').removeClass('filtered');
        filterTopicSessions($(this), topic_name, topic_description); 
	      }//if not a full day session	
	});

});

$('#topic_list').on('change', function(e) {
   var selected = $('#topic_list option:selected');
   var topic_id = selected.val(),
         topic_name = selected.text(),
         topic_description = '#topic_' + topic_id;

   if(topic_id == "tutorials") {
       topic_description = '#tutorials';
   }

   if (topic_id.indexOf('stratany2014') == -1) {
     e.preventDefault();
     filterTopicSessions($(this), topic_name, topic_description);
   }  else {//forward to page
        window.location.href = topic_id;
   }
});

$('#schedule_options li.day_filter').each(function() {
	$(this).on('click', function(e) {
                e.preventDefault();
                $('#grid_topics a').removeClass('filtering');//remove filter from topics
                $('#schedule_options li').removeClass('filtered');
                $(this).addClass('filtered');
				$('.en_schedule_time').show();
				$('.en_session').show();
                $('.topic_description').hide();

                $('.en_schedule_day').show();
                var filter_day = $(this).children('a').attr('href').slice(1); 
                 $('.en_schedule_day h2 a').each(function() {
                       if ($(this).attr('name') != filter_day) {
                           $(this).parent().parent().hide();
                       }
                  });             
	});
});




  });
  </script>

  
  
 

  


  <script>
  $(document).ready(function () {

  

    /* make links with class=external open up external */
    $("a.external").click(function (e) {
      e.preventDefault();
      window.open($(this).attr("href"));
    });

    
       $('.en_session').addClass('pad');
    

   

  });
  </script>

  
   

