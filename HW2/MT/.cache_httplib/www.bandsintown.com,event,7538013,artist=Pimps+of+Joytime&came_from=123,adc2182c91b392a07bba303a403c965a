  BIT.Events = new BIT.Collections.EventList([{"rsvp_url":"http://www.bandsintown.com/event/7538013/facebook_rsvp?artist=Medicine+for+the+People","artist_event_link_label":null,"ticket_url":"http://www.wakarusa.com/tickets/","formatted_location":"Ozark, AR","venue_id":1659150,"title":"Jun 5 - Medicine for the People @ Wakarusa - Ozark, AR","venue_name":"Wakarusa","artist_id":1197847,"venue_location":{"region":"Arkansas","latitude":35.4869444,"longitude":-93.8275,"country":"United States","city":"Ozark"},"facebook_event_id":"651307134926525","formatted_datetime":"Thursday, June 5, 2014 at 7:00pm","artist_event_id":8355337,"url":"http://www.bandsintown.com/event/7538013?artist=Medicine+for+the+People","page_id":"163063701540","datetime":"2014-06-05T19:00:00","bit_ticket_url":"http://www.bandsintown.com/event/7538013/buy_tickets?affil_code=fb_163063701540\u0026artist=Medicine+for+the+People","artist_name":"Medicine for the People","published":true,"id":7538013,"artist_event_link_url":null,"ticket_type":"Tickets"}])

  $(".i-was-there").click(function(event) {
    var target = $(event.target),
    loading_image = $(".loading-small");
    loading_image.show();

    BIT.FB.Permissions.verify("email,user_likes,user_location,user_actions.music,rsvp_event", function(granted) {
      if (granted) {
        target.addClass("active");

        $.fb_ajax({
          url: "/facebook/rsvp",
          type: "POST",
          data: {
            came_from: "143",
            bandsintown_event_id: 7538013,
            artist_id: 1197847,
            rsvp_status: 'attending',
            track_artist: true
          },
          success: function(data) {
            BIT.FB.Timeline.go_to_event({
              event: BIT.Events.get(7538013),
              success: redirect,
              error: redirect
            });
          },
          error: redirect
        });
      }

      loading_image.hide();
    });
  });

});

</script>
