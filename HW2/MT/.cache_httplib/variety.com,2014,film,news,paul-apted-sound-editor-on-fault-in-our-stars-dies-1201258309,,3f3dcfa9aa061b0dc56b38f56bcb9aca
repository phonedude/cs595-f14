				<!-- Article Headline -->
				<h1>Paul Apted, Sound Editor on &#8216;Fault in Our Stars,&#8217; Dies at 47</h1>

				<div class="indent">
					<!-- Share Tools -->
					<section class="module-share-tools">

	<div class="share-ui-container">

		<ul class="social-buttons social">
							<li class="share-email">
