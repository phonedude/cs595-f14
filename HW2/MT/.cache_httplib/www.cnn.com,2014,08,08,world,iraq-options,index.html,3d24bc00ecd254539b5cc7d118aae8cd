<script>
var currExpandable="expand331";
if(typeof CNN.expandableMap==='object'){CNN.expandableMap.push(currExpandable);}
var mObj={};
mObj.type='video';
mObj.contentId='';
mObj.network='cnn';
mObj.source='world/2014/08/08/nr-intv-lt-col-rick-francona-isis-controls-mosul-dam.cnn';
