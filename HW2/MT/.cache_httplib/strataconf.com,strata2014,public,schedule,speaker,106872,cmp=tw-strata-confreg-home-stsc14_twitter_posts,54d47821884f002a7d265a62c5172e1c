<script>
$(document).ready(function () {

  /* init nav */
  $("#top_level_nav #nav").superfish();

  /* accordian mainly used on speaker resources */
  $(".accordion .module h2").accordion({clickTitle: 'h2', slideTime: '30'});
  
  /* make links with class=external open up external */
  $("a.external").click(function (e) {
    e.preventDefault();
    window.open($(this).attr("href"));
  });

  $('#stay_informed iframe').attr('src', 'http://oreilly.com/conferences/bulletin_form.html?conference=strata&locationSource=strata14_ca_widget&colorLink=d3002d&colorText=eee'); 
 
         $('div.stay_connected a.chicklet').css({'background-color': '#000', 'background-image': 'url(//cdn.oreillystatic.com/conferences/velocity/assets2012/social-buttons/social.png)', 'background-repeat': 'no-repeat'});
         $('#sponsor_title').css('background', '#cacaca url(//cdn.oreillystatic.com/en/assets/1/eventprovider/1/sponsors_vertical.gif) 97% center no-repeat');


    

    
});    
