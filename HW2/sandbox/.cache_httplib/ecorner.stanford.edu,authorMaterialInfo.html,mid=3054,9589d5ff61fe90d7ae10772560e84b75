		    var wcPluginState = 0; //hidden
		    function switchWCPlugin(player,tab) {
		    	if (wcPluginState != tab) {  // 1 = Subtitles tab opened 
		    			if (tab == 1) {
		    				player.dispatchEvent('WEBCAPTIONS_OPEN_TAB1',null);
		    			} else {
		    				player.dispatchEvent('WEBCAPTIONS_OPEN_TAB2',null);
		    			}
		    		wcPluginState = tab;
		    	} else {
		    		player.dispatchEvent('WEBCAPTIONS_CLOSE');
		    		wcPluginState = 0;
		    	}
		    }
	</script>
		
