		<div id="AdapTvTitleCard"></div>
		<iframe id="AdapTvTitleCard_Mask"></iframe>




		
			
<script type="text/javascript">
	(function(){
		var __multiLevelReferrer = 'multilevel_embed';
		var JSON = window.JSON || {};

		JSON.parse = JSON.parse || $.evalJSON;
		JSON.stringify = JSON.stringify || $.toJSON;

		if (window.sessionStorage) {

			var ursid = window.sessionStorage.getItem('ursid'),
				contentId = '9659430',
				contentType = 'channel',
				rsid = false,
				tsDelta;

			if (ursid) {
				ursid = (JSON.parse(ursid)).value;
				rsid = ursid.rsid;
				tsDelta = +new Date() - ursid.ts;

				if (ursid.media !== contentType + '' + contentId) {
					// mas media van, rsid kuka
					window.sessionStorage.removeItem('ursid');
					ursid = rsid = false;
				}

				if (tsDelta > 8000) {
					// tovabb tartott ez mint egy oldalujratoltes, rsid kuka
					window.sessionStorage.removeItem('ursid');
					ursid = rsid = false;
				}
			}

			if (typeof $(window).unload !== 'undefined') {
				$(window).unload(function(){
					//var ursid = window.sessionStorage.getItem('ursid');
					var strg = window.sessionStorage.getItem('ursid'),
						ursid = (strg) ? (JSON.parse(strg)).value : false;

					ursid.ts = +new Date();

					window.sessionStorage.setItem('ursid', JSON.stringify({value: ursid}));
				});
			}
		}


		/*
		 	SITE-13496 Embed another flash viewer for testing QoS version
		 */
		var qos_test = function () {

			//SystemProperty_Dao::getCustomProperty('qos_player.test.enabled');
			//SystemProperty_Dao::getCustomProperty('qos_player.test.percent');

			// 100% minden user
			// 0.03% is lehet
			// 0% senki

			function draw () {
				var drawed = (Math.floor(Math.random() * 100000 + 1) / 100000) * 100;
				$.console.log('QOS PLAYER - ' + drawed + ' vs ' + percent);
				return drawed < percent;
			}

			function validTestClient () {

				if (location.protocol.indexOf('https') > -1) {
					return false;
				}

				// validate client, if test is skipped:
				// return false;

				return true;
			}

			var enabled = (ustream.vars.qos_player && !!+ustream.vars.qos_player.enabled) || false,
				percent = (ustream.vars.qos_player && +ustream.vars.qos_player.percent) || 0,
				forced = window.location.search.indexOf('qosplayer') > -1;

			if ('channel' == 'channel' && '9659430' == ustream.vars.qos_player.cid) {
				enabled = !!ustream.vars.qos_player.cidpercent;
				percent = +ustream.vars.qos_player.cidpercent;
			}

			enabled = enabled && validTestClient();

			// testing
			if (!enabled) {
				enabled = forced;
				percent = enabled ? 100 : 0;
			} else if (forced) {
				percent = 100;
			}

			if (!enabled) {
				$.console.log('QOS PLAYER - not enabled');
				return false;
			}

			if (!draw()) {
				$.console.log('QOS PLAYER - skip');
				return false;
			}

			$.console.log('QOS PLAYER - running');
			return true;
		}();

		var isIE = ustream.browser.ie,
			flashMode = isIE ? 'opaque' : 'direct',
			uViewer = {
				id: "UstreamViewer",
				version: "10.3",
				movie: (qos_test) ? "http://static-cdn1.ustream.tv/swf/live/viewerqos:2.swf" :  "http://static-cdn1.ustream.tv/swf/live/viewer:297.swf?vrsl=c:680&amp;ulbr=100",
				width: "100%",
				height: "100%",
				attributes: {
					id: "UstreamViewer",
					name: "UstreamViewer"
				},
				params: {
					wmode: flashMode,
					allowfullscreen: true,
					bgcolor: "#000000",
					allowscriptaccess: "always"
				},
				flashvars: {"cid":9659430,"locale":"en_US","sessionid":false,"autoplay":"true","enablejsapi":"1","sv":6}			};

		var __referrer = (function () {
			if (window.parent !== window.top) {
				try {
					if (window.parent.location.host == location.host) {
						if (window.parent.multiLevelEmbed) {
							return __multiLevelReferrer;
						}
						return window.parent.document.referrer || 'unknown';
					}
					return __multiLevelReferrer;
				} catch (e) {
					return __multiLevelReferrer;
				}
			}
			return document.referrer || 'unknown';
		}());

		if (ustream.vars.customEmbed) {
			uViewer.flashvars.referrer = encodeURIComponent(__referrer);
		}

				if ($.cookie("siteVisited") == 1) {
			delete uViewer.flashvars.volume;
		}

				if (ustream.vars.isNewHomePage) {
			uViewer.flashvars.hometype = "new";
		}

		$.cookie("siteVisited", 1);

		if (rsid) {
			uViewer.flashvars.rsid = rsid;
		}

		// SITE-10751 Add ts flashvar to player
		uViewer.flashvars.ts = (new Date()).getTime();

		if (!rsid) {
			window.sessionStorage.setItem('ursid', JSON.stringify({value: {
				media: contentType + '' + contentId,
				ts: +new Date()
			}}));
		}

		window.swfobject.embedSWF(
			uViewer.movie,
			"UstreamViewer",
			uViewer.width,
			uViewer.height,
			uViewer.version,
			false,
			uViewer.flashvars,
			uViewer.params,
			uViewer.attributes,
			function (i) {

				if (!i.success) {
					$('#NoFlashContent').show();
				}
				
				if (!ustream.flash) {
					ustream.flash = {};
				}
				if (!ustream.flash.viewer) {
					ustream.flash.viewer = {};
				}
				ustream.flash.viewer.UstreamViewer = {};
				ustream.flash.viewer.UstreamViewer.instance = i.ref;
			});
	})();
	/*
		endof
		SITE-8971 Page Refresh Proto
	*/
</script>

<div id="UstreamViewer">

	<div id="NoFlashContent" style="display: none; padding-top: 80px; color: #fff;">
		<h5 style="text-align: center;">
			Your Flash plugin is outdated		</h5>
		<p style="text-align: center;">
			Get the latest Flash player to view this content,
		</p>
		<p style="text-align: center;">

			<a href="http://www.adobe.com/go/getflashplayer">
				<img
					src="http://www.adobe.com/images/shared/download_buttons/get_flash_player.gif"
					alt="Get Adobe Flash player" />
			</a>
		</p>
	</div>
</div>

<noscript>
	<object
	classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000"
	width="100%"
	height="100%"
	name="UstreamViewer"
	id="UstreamViewer">

		<param name="movie" value="http://static-cdn1.ustream.tv/swf/live/viewer:297.swf?vrsl=c:680&amp;ulbr=100" />
