<script type="text/javascript">
  amznJQ.onReady('jQuery', function() {
    jQuery(window).bind('kindle_price_block_show_promotions', function(event, data) {
      if (data && (data.featuredPromoText || (data.morePromosText && data.morePromosLabel))) {
        jQuery('#priceBlock .savingsRow').hide();
        var $promoDisplay      = jQuery('#priceBlockClaimedApplicablePromotions');
        var $featuredPromo     = $promoDisplay.find('.featuredPromotion');
        var $morePromosLabel   = $promoDisplay.find('.morePromotionsLabel');
        var $morePromosPopover = $promoDisplay.find('.morePromotionsPopover');
        var $morePromosLink    = $promoDisplay.find('.morePromotionsLink');

        if (data.featuredPromoText) {
          $featuredPromo.html(data.featuredPromoText).show();
        } else {
          $featuredPromo.html('').hide();
        }

        if (data.morePromosLabel && data.morePromosText) {
          $morePromosLabel.html(data.morePromosLabel);
          $morePromosPopover.html(data.morePromosText);
          $morePromosLink.show();
        } else {
          $morePromosLabel.html('');
          $morePromosPopover.html('');
          $morePromosLink.hide();
        }

        $promoDisplay.show();
      }
    });
  });

  amznJQ.onReady('popover', function() {
    jQuery('#priceBlockClaimedApplicablePromotions .morePromotionsLink').amazonPopoverTrigger({
      localContent: jQuery('#priceBlockClaimedApplicablePromotions .morePromotionsPopover'),
      locationElement: jQuery('#priceBlockClaimedApplicablePromotions .morePromotionsLabel'),
      openEventInclude: "CLICK_TRIGGER",
      showCloseButton: false,
      showOnHover: true
    });
  });
</script>
