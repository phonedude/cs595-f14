  <script type="text/javascript">

    $(document).ready(function(){
      var $kontext = $('#kontext');
      $kontext.hide();

      $('a.finderlink').mouseover(function(){
        var nCompany = $(this).attr('rel');
        var sHref = $(this).attr('href');

        var thispos = $(this).position();

        $.getJSON(sHref,{preview:1},function(oData){
          if(oData.html!='') {
            $kontext.html(oData.html);
            $kontext.css({top:thispos.top+'px', left:thispos.left+'px'});
            $kontext.fadeIn('100');
          }
        }) ;

      });

      //remove all kontext windows on body click
      $(document).click(function() {
        closeKontext()

      });

      //disable teh body click event for teh kontext window istself
      $kontext.click(function(e) {
        e.stopPropagation();
      });



    });

    function closeKontext()
    {
      $('#kontext:visible').fadeOut('fast');
      $('#kontext').html('');
    }

  </script>

