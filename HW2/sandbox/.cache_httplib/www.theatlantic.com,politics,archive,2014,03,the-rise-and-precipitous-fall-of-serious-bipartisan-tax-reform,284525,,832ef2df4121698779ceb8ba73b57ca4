    <script type="text/javascript">
        if(Math.round(Math.random())) {
            $('#top-circ-promo').hide();

            /**
             * Randomly pick a top promo at random.
             */
            var $promos = $('.shop-atlantic');
            if ($promos.length > 1) {
                var i = Math.round(Math.random() * $promos.length - 1);
                $promos.eq(i).show();
                $promos.not(":eq(" + i + ")").hide();
            };

        }
    </script>
