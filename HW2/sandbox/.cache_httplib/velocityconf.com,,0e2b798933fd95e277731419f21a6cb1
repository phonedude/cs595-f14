<script src="//cdn.oreillystatic.com/en/assets/1/eventprovider/1/yepnope.1.5.4-min.js"></script>
<script>
yepnope({
    load: ['//cdn.oreillystatic.com/ajax/libs/jquery/1.7.2/jquery.min.js', '//cdn.oreillystatic.com/en/assets/1/eventprovider/1/lazyload_min.js'],
    complete: function () {
        $(function(){
            $('#join_bulletin iframe').attr('src', 'http://oreilly.com/conferences/bulletin_form.html?conference=velocity&locationSource=velocity_franchise_hp&colorLink=fff&colorText=fff');
             //popups
             var nlPopupOverlay = $('#newsletter-popup-overlay'),
        nlPopup = $('#newsletter-popup'),
        nlPopupClose = nlPopup.find('a.close'),
        nlPopupLoad = 0;

    nlPopupOverlay.prependTo('body');

    $('#newsletter-popup-frame').load(function() {
        if (nlPopupLoad < 2) {
            nlPopupLoad++;
        } else {
            $('#newsletter-popup-frame').css('opacity', 0);
            //$('#newsletter-popup-overlay').animate({ opacity: 0 }, 200);
            nlClosePopup()
        }
        console.log(nlPopupLoad);
    });

    function startNLPopup(conf) {
        var nlPopup = $('#newsletter-popup');


        nlShowPopup();
        $('form').find('input[name*="email_addr"]').each(function() {
            var defaultValue = $(this).attr('value');
            $(this).focus(function() {
                if ($(this).attr('value') == defaultValue) {
                    $(this).attr('value', '');
                }
            });
            $(this).blur(function() {
                if ($(this).attr('value') == '' || $(this).attr('value') == ' ') {
                    $(this).attr('value', defaultValue);
                }
            });
        });


    } //startNLPopup()

    function nlClosePopup() {
        nlPopupOverlay.animate({
            'opacity': 0
        }, 300, function() {
            nlPopupOverlay.css('display', 'none');
        });
        nlPopupLoad = 1;
    } //nlClosePopup()
    nlPopupOverlay.click(function() {
        nlClosePopup();
    });

    nlPopupClose.click(function(e) {
        e.preventDefault();
        nlClosePopup();
    });

    $(document).keydown(function(e) {
        if (e.which == 27) { //escape key
            nlClosePopup();
        }
    });

    nlPopup.click(function(event) {
        event.stopPropagation();
    });

    function nlShowPopup() {
        nlPopup.css('margin-top', ($(window).scrollTop() + 140) + "px");
        nlPopupOverlay.css({
            'opacity': 0,
            'display': 'block',
            'height': jQuery('html').height()
        }).animate({
            'opacity': 1
        }, 200);
        $('#newsletter-popup-frame').css('opacity', 1);
    } //nlShowPopup()
    $('a#ca, a#eu, a#ny, a#ch').on('click', function(evt) {
        var conf = $(this).attr('id'),
            selected_div = $('#popup_content'),
            iframe_src = $('#newsletter-popup-frame').attr('src');
       /* iframe_src = iframe_src.split('?')[0];*/
        evt.preventDefault();
        $('#newsletter-popup-frame').attr('src', iframe_src + 'conference=velocity&locationSource=velocity_franchise_' + conf + '_popup');
        if (conf === 'ny') {
            selected_div.html('<h3>Velocity New York 2014<br />Save the date.</h3><p>Sign up to be notified when the Velocity New York Call for Proposals launches.</p>');
        } else if (conf === 'eu') {
            selected_div.html('<h3>Velocity Europe 2014<br />Save the date.</h3><p>Sign up to be notified when the Velocity Europe Call for Proposals launches.</p>');
        } else if (conf === 'ca') {
            selected_div.html('<h3>Velocity Santa Clara 2014<br />Save the date.</h3><p>Sign up to be be notified when the Velocity Santa Clara Call for Proposals launches.</p>');
        } else if (conf === 'ch') {
            selected_div.html('<h3>Velocity China 2015<br />Dates coming soon.</h3><p>Sign up to receive weekly web operations and performance news, and insights from industry insiders.</p>');
            $('#newsletter-popup-frame').attr('src', iframe_src + 'conference=velocitynews&locationSource=velocity_franchise_' + conf + '_popup');
        }
        startNLPopup();
    });//end popups

    //banner ads
    if ($( "div.banner_ad" ).length) {
      $( "div.banner_ad" ).each( function ( i, e ) {
          var banners = new Array();
          $( e ).find( 'a[data-defer="random"]' ).each( function( i0, e0 ) {
              banners.push( $( e0 ) );
            });
          
          var b = banners[ Math.floor( Math.random() * banners.length ) ];
          var i = $( "<img />" );
          i.load( function() { b.html( i ); b.fadeIn( 1000 ); } );
          i.attr( "src", b.attr( "data-img-src" ) );
          i.attr( "alt", b.attr( "data-img-alt" ) );
        });
                     }//end banner ads

                        
        });//end $(function()
      }//end complete
});
</script>


