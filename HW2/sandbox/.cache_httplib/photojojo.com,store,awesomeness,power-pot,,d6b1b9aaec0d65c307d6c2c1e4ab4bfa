<script type="text/javascript">

	function optSetCookie(c_name, value, exdays) {
	    var exdate = new Date();
	    exdate.setDate(exdate.getDate() + exdays);
	    var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
	    document.cookie = c_name + "=" + c_value +"; path=/";
	}
	if(window['optimizely'] !== undefined && window['optimizely'].variationIdsMap[253149139] !== undefined) {
		optSetCookie('optimizely_domestic_ship_bucket', window['optimizely'].variationIdsMap[253149139], 200);
	}

	function setupDomesticShippingTest() {
		// Select our first option in the list
		$("input:radio[name=shipping_option]:not(:disabled):first").trigger('click');

		// Copied from cart.jsL249 - the trigger(event) would not actually trigger this code!
		// METHOD:RATE
		var ship_option = $("input:radio[name=shipping_option]:not(:disabled):first").val();

		// split on colon: method:rate - Make sure we have something to split.
		if (typeof ship_option != 'undefined') {
			var split = ship_option.split(':');
		} else {
			return; // We don't have anything, so stop trying to do anything
		}

		// get shipping rate
		var selected_shipmethod = $.trim(split[0]);
		var shipping_value = $.trim(split[1]);

		// store in hidden inputs
		$('#selected_shipmethod').val(selected_shipmethod);
		$('#shipping_value').val(shipping_value);

		$.cookie('selected_shipmethod', selected_shipmethod, {expires: 30, path: '/'});

		// trigger calc of totals
		cart_totals();
	};

	setupDomesticShippingTest();

</script>

