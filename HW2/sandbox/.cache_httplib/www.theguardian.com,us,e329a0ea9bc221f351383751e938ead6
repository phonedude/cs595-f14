                                                                        <script>
    // This fixes the right hand column being pushed down when 
    // the important story component is used
    function impStoryHotFix() {
        var impStoryNode = jQ('#inner-wrapper .important-story');
        if (impStoryNode.length) {
            var edgeColNode = jQ('#inner-wrapper > .two-col.edge'),
                offset = edgeColNode.position().top - impStoryNode.position().top

            edgeColNode.css('margin-top', '-'+offset+'px')
        }
    };

    jQ(document).ready(impStoryHotFix);
    jQ(window).load(impStoryHotFix);
</script>

                                            				
			        

			            						
					
					
                                                                                                                                                                                                                            

