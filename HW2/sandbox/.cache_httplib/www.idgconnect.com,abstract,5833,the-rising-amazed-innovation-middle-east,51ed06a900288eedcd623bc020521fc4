		});
</script>

<script type="text/javascript">
$(function(){
	$('.social-links').toggle(function(){
		  $(this).next('ul').show();
		},
		function(){
		  $(this).next('ul').hide();
	});


	$('.mainmenu li.active a.global_400').toggle(function(){
	  			$('.mainmenu>li').not('.mainmenu>li:first,.mainmenu>li:last').show();
		},function(){
		 		$('.mainmenu>li').not('.mainmenu>li:first,.mainmenu>li:last').hide();
	});

	$('.menu_400').toggle(function(){
		  $(this).next('#menu').show();
		},
		function(){
		  $(this).next('#menu').hide();
	});
});
</script>


