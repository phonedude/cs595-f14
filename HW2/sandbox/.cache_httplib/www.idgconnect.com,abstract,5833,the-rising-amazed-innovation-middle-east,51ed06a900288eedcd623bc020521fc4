<script>
	$(function() {
    $('#subscriber_email_id').blur(function() {
      var error_field = $('span[for="subscriber_email_id"]').is(':visible')
      if($(this).attr('value')!="" && error_field==false){
        $.ajax({
          url : '/wpl/check_subscriber_exist',
          data : 'email=' + $(this).attr('value') ,
          type : 'POST',
          dataType : 'json',
          success : function(data) {
            if (data.status == 'false') {
              if (confirm('This email id already exists,please go to login page')) {
                location.href = "/login";
              }
            }
          }
        });
      }
    });
});  
</script>
<script>
	$(function() {
		$('#asset_reg_submit').click(function() {
			var email = $("#subscriber_email_reg").val();
			if(email==null||email=="")
			{
				alert("Please enter the email");
				return false;
			}
			else
			{		
			$.ajax({
				url : '/wpl/check_subscriber_exist',
				data : 'email=' + email ,
				type : 'POST',
				dataType : 'json',
				beforeSend: function(xhr){
                                             xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))},
				success : function(data) {
					if (data.status == 'false') {
						if (confirm('This email id already exists,please go to login page')) {
							location.href = "/login";
						}
					}
				else
					{
						$("#custom_reg_form").submit();
					}
				}
			});
			}
		});
	}); 
</script>
<script type="text/javascript">
	$(function() {
		$('#register_form').validate({
			rules : {
				"subscriber[email_id]" : {
					required : true,
					email : true
				},
				"email[confirmation]" : {
					required : true,
					email : true,
					equalTo : "#subscriber_email_id"
				},
				"subscriber[password]" : {
					required : true,
					minlength : 6,
					//alphanumeric: true
				},
				"password[confirmation]" : {
					required : true,
					minlength : 6,
					//alphanumeric: true,
					equalTo : "#subscriber_password"
				},
				"subscriber[first_name]" : {
					required : true,
				},
				"subscriber[last_name]" : {
					required : true,
				},
				
				"subscriber_property[end_user_company_name]" : {
					required : true,
				},
				"subscriber_property[phone]" : {
					required : true,
					number : true
				},
				"subscriber_property[mobile]" : {
					number : true,
				},
				"subscriber_property[Job_title_id]" : {
					required : true,
				},
				"subscriber_property[terms_acceptance]" : {
					required : true,
				},
		
			},
			messages : {
				"subscriber[email_id]" : {
					required : "The Email field should not be empty",
					email : "email is invalid"
				},
				"email[confirmation]" : {
					required : "Please Enter the confirm Email address",
					email : "email is invalid",
					equalTo : "Please enter correct confirm Email address"
				},
				"subscriber[user_name]" : {
					required : "Username field should not be empty",
					minlength : "Please enter at least 6 characters."
				},
				"subscriber[password]" : {
					required : "Password field should not be empty",
					minlength : "Please enter at least 6 characters."
				},
				"password[confirmation]" : {
					required : "Re-Password field should not be empty",
					minlength : "Please enter at least 6 characters.",
					equalTo : "Please enter correct Re-Password"
				},
				"subscriber[first_name]" : {
					required : "The FirstName field should not be empty",
				},
				"subscriber[last_name]" : {
					required : "The LastName field should not be empty",
				},
				
				"subscriber_property[end_user_company_name]" : {
					required : "The company field should not be empty",
				},
				
				"subscriber_property[phone]" : {
					number : "Please enter number only",
				},
				"subscriber_property[Job_title_id]" : {
					required : "The Job Title field should not be empty",
				},
               "subscriber_property[terms_acceptance]" : {
					required : "The field should not be empty",
				},
			}
		});

	});

</script>
