<script type="text/javascript">
		   $(function(){
			$('#search_result_menu ul li a').toggle(function(){
				$('.search_result .search_result_content').hide();
			   		$('#search_result_menu ul li').removeClass('active');
			   		$(this).parent().addClass('active');
			   		var data_active = $(this).data('id');
			   		$(data_active).show();
			   },function(){
			   		$(this).parent().removeClass('active');
			   		var data_active = $(this).data('id');
			   		$(data_active).hide();
			   });
		    });
</script>
