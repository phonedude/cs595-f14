          "&contentid=" + encodeURI("524966") +
                      "&cc=" + encodeURI("Mobile") +
                                "&ap=" + encodeURI("News") +
                              "&end";

        $("a.rightslink").prop('href', rightslinkURI);

      });
    </script>

  </p>
  </section>

