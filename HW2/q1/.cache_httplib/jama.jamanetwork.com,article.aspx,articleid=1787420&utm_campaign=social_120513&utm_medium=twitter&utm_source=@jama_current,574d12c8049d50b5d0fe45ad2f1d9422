	<script type="text/javascript" charset="utf-8">
		//PollDaddy callback on case/discussion pages
		var pd_callback = function (json) {

			var obj = jQuery.parseJSON(json);

			if (obj.result == 'view-results' || obj.result == 'already-registered') {
				$('#scm6MainContent_DiscussionTab').click();

				//scroll to the discussion tab
				var container = $('.contentBodyContainer').first(),
					scrollTo = $('.poll').first();

				container.scrollTop(
					scrollTo.offset().top - container.offset().top + container.scrollTop()
				);
			}

		};
</script>

