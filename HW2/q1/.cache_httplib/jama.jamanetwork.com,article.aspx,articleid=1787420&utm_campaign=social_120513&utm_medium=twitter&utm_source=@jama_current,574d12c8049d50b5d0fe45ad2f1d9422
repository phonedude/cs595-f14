<script language="javascript" type="text/javascript">
    function cvUsernameEmail_ClientValidate(sender, args) {
        // Clear the label which may contain error message from
        // the server side validation associated with the submit button
        // having been clicked.
        $('#lblPasswordResetRequestResult').empty();
        
        // Make sure the UserName and Password are required.
        var userName = $('#ctl02_txtReqPwdUsername').val();
        var emailAddress = $('#ctl02_txtReqPwdEmailAddress').val();

        if ((userName.length == 0) || (emailAddress.length == 0)) {
            // Either the UserName or Email Address or both are blank.
            args.IsValid = false;
        }
        else {
            args.IsValid = true;
        }
    }
</script>

