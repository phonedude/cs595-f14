<script type="text/javascript">
    $(window).load(function () {
        //For each unique instance of this control setup simple validation for input fields
        $('.globalSignIn').each(function () {
            var globalSignIn = $(this);
            $('.signInButton', $(globalSignIn)).click(function (e) {
                if ($(globalSignIn).find('.field input').val() == '') {
                    $(globalSignIn).find('.errorMessageSection .validationControl').html("The username and/or password is blank. Please try again.");
                    //Prevent propagation of post-back binding generated via .net
                    return false;
                }
            });
        });
    });
</script>

