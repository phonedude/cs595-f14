<script type="text/javascript">
    $(document).ready(function () {

        //Submit Letter Link
        $('.letterLink').click(function () {
            var accessMode = $("#hfSubmitLetter").val();

            // If user cannot access this features because of permissions.
            // accessMode = "CanAccess" if user has this feature/permissions.
            if (accessMode != "CanAccess") {
                var articleID;
                articleID = $("#hfArticleId").val();

                openSignInPopupWithMode(accessMode, articleID);
                return;
            }

            $("div.contentContainer").hide();
            $(".tabNav div a").removeClass("selected");
            $(".tabNav .atab:nth-child(6) a").addClass("selected").show();
            $("#tab6").show();
            $(this).parents('.letterBody').find('#letterSubmitForm').toggle(); //.css('display', 'block');
            $('#letterSubmitForm').show();
            $('.submitLetterButton').hide();
        });

        $('.cancelButton').click(function () {
            $('#letterSubmitForm').hide();
            $('.submitLetterButton').show();
        });


    });
</script>
