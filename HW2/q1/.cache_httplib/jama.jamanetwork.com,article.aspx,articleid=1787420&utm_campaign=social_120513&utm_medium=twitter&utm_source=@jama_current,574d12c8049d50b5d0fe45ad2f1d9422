	<script src="https://display-mathml.googlecode.com/hg/display-mathml.js?r=stable" type="text/javascript"></script>
	<script type="text/javascript">
        var whichPanel = "wrapper";
        var mobileFireEvent = "article";
        var count = 0;
        var JumpId = '';
        var emailUserAlert = "";
        var emailOnPopupChanged = false;

        //******************USER ALERT RELATED FUNCTIONS******************
        function displayAlertMessage(msg) {
            $(".alertPopupMessage").html(msg);
            $('.UserAlertEmailConfirmation').hide();    // Hide Textbox and Confirm Button
            $('.userAlertPopupProgressBar').hide(); // hide the progress bar after the message is displayed.
        }
        function trim(stringToTrim) {
            return stringToTrim.replace(/^\s+|\s+$/g, "");
        }
        // Capture the new/Changed Email Address when and if the user changes it on User Alerts popup dialog.
        function getNewEmailAddress(o) {
            emailOnPopupChanged = true;
            emailUserAlert = o.value;
        }
        // function that is called for User Alerts Callback 
        function callBackUserAlert() {
            var email;
            email = emailOnPopupChanged ? emailUserAlert : $('#hfOriginalEmailAddress').val();
            if (emailOnPopupChanged) {
                emailOnPopupChanged = false;
            }
            var emailPattern = /\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/;
            var emailTrimmed = trim(email);
            if (emailTrimmed.length == 0) {
                $(".alertPopupMessage").html('Email Address is blank. Please try again.');
                return false;
            }
            var regEx = new RegExp(emailPattern);
            var bMatch = regEx.test(emailTrimmed);
            if (bMatch == false) {
                $(".alertPopupMessage").html('Email address must be valid. Ex: name@domain.com');
                return false;
            }
            $('.userAlertPopupProgressBar').show();
            callBackSaveAlert(emailTrimmed);
        }
        function detailsHandler(href) {
            var hiddenField = document.getElementById('hfArticleId').value;
            href = href + "&articleid=" + hiddenField;
            window.location = href;
        }
        function displaypopup() {
            $('#displaypopup').attr("style", "displaylock;font-family:Arial;font-weightold;font-size:10pt;");
            $("#displaypopup").dialog({
                position: 'center',
                modal: true,
                height: 150,
                width: 525,
                dialogClass: 'boxshadow',
                closeOnEscape: false,
                overlay: { opacity: 0.7, background: 'gray' },
                close: function (evnt, args) {
                    window.location.href = "account/myalerts.aspx";
                }
            });
        }
        function eventhandler(evt) {
            var value = evt.data.service;
            var key = 'Share';
            this.UserSessionItem = new UserSessionItem("d4xwpjm3qn20xy0ms00gpv2a", 0, 0);
            this.ActionType = new ActionType(1, key);
            doTracking(this.ActionType, value.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItem, (window.location).toString());
        }
        $(document).ready(function () {
            $('#txtUserAlertEmailAddress').val($('#hfOriginalEmailAddress').val());
            var browserversion = $.browser;
            if (browserversion.mozilla && parseInt(browserversion.version) < 2) {
                $('.saveMyStuff').click(function () {
                    if (previewMode) {
                        alert('Saving is disabled in preview mode.');
                    } else {
                        var _id = this.id;
                        AddItemstoMyStuff(_id);
                    }
                });
            }
            if ($('.customvalidator').css('visibility') == 'hidden') {
                $('.errorMessageSection').hide();
                $('.customvalidator').css('visibility', 'hidden');
            } else {
                $('.errorMessageSection').show();
                $('.customvalidator').css('visibility', 'visible');
            }
        });
    $(window).load(function () {
        $('.crsRef').click(function () {
            var tab = $(this).context.innerText;
            if (!tab) {
                tab = $(this).context.textContent;
            }
            var key = 'CrossReferenceLink';
            this.UserSessionItem = new UserSessionItem("d4xwpjm3qn20xy0ms00gpv2a", 0, 0);
            this.ActionType = new ActionType(1, key);
            doTracking(this.ActionType, tab.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItem, (window.location).toString());
        });
        $('.pubmedLink').click(function () {
            var tab = $(this).attr('href');
            var key = 'CrossReferenceLink';
            this.UserSessionItem = new UserSessionItem("d4xwpjm3qn20xy0ms00gpv2a", 0, 0);
            this.ActionType = new ActionType(1, key);
            doTracking(this.ActionType, tab.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItem, (window.location).toString());
        });
        $('.rightpopup li a').click(function () {
            var id = $(this).attr('class');
            var value = id.toString() + ' is Clicked';
            var key = 'ToolBox';
            this.UserSessionItem = new UserSessionItem("d4xwpjm3qn20xy0ms00gpv2a", 0, 0);
            this.ActionType = new ActionType(1, key);
            if (id == 'linkPDF') {
                doTracking(this.ActionType, value.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItem, ActionURL, "sync");
            } else {
                doTracking(this.ActionType, value.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItem, ActionURL);
            }
        });
        $(".tabtracking").click(function () {
            var key = "Tabs";
            var tab = $(this).context.innerText;
            if (!tab) {
                tab = $(this).context.textContent;
            }
            var value = tab + " was clicked ";
            this.ActionType = new ActionType(1, key);
            this.UserSessionItem = new UserSessionItem("d4xwpjm3qn20xy0ms00gpv2a", 0, 0);
            doTracking(this.ActionType, value.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItem, ActionURL);
        });
        $(".jumpLinks a").click(function () {
            var thisLink = $(this).index();
            var tab = $(this).context.innerText;
            if (!tab) {
                tab = $(this).context.textContent;
            }
            var imageClicked = tab + " was clicked ";
            this.ActionType = new ActionType(1, tab);
            this.UserSessionItem = new UserSessionItem("d4xwpjm3qn20xy0ms00gpv2a", 0, 0)
            doTracking(this.ActionType, imageClicked.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItem, ActionURL);
        });
        $(".popUpTable").click(function () {
            var thisLink = $(this).index();
            var tab = $(this).context.innerText;
            if (!tab) {
                tab = $(this).context.textContent;
            }
            var imageClicked = tab + " was clicked ";
            this.ActionType = new ActionType(1, tab);
            this.UserSessionItemDetail = new UserSessionItemDetail("d4xwpjm3qn20xy0ms00gpv2a", 0, 0, 88040, 0)
            doTracking(this.ActionType, imageClicked.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItemDetail, ActionURL);
        });
        $(".popUpFigure").click(function () {
            var thisLink = $(this).index();
            var tab = $(this).context.innerText;
            if (!tab) {
                tab = $(this).context.textContent;
            }
            var imageClicked = tab + " was clicked ";
            this.ActionType = new ActionType(1, tab);
            this.UserSessionItemDetail = new UserSessionItemDetail("d4xwpjm3qn20xy0ms00gpv2a", 0, 0, 88040, 0)
            doTracking(this.ActionType, imageClicked.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItemDetail, ActionURL);
        });
        $(".saveMyStuff").click(function () {
            if (!previewMode) {
                var thisLink = $(this).index();
                var tab = $(this).context.innerText;
                if (!tab) {
                    tab = $(this).context.textContent;
                }
                var id = $(this).attr('id');
                var imageClicked = tab + " was clicked with id = " + id;
                this.ActionType = new ActionType(1, tab);
                this.UserSessionItem = new UserSessionItem("d4xwpjm3qn20xy0ms00gpv2a", 0, 0)
                doTracking(this.ActionFilterType, imageClicked.toString(), "9/24/2014 6:00:32 PM", this.UserSessionItem, ActionURL);
            }
        });
        $('.signInButton').each(function () {
            $(this).click(function () {
                if ($('.customvalidator').css('visibility') == 'hidden') {
                    $('.errorMessageSection').hide();
                    $('.customvalidator').css('visibility', 'hidden');
                } else {
                    $('.errorMessageSection').show();
                    $('.customvalidator').css('visibility', 'visible');
                }
            });
        });
    });
