		(function(){
				var visitNum;
				var vn = 1;
				var parts = document.cookie.split("s_vnum=");
				if (parts.length === 2) {
					vn = parts.pop().split(";").shift().split("vn%3D").pop();
				}
				vn = parseInt(vn);
				if (vn === 1) {
					visitNum = "1";
				} 
				else if (vn > 1 && vn < 5) {
					visitNum = "2-4";
				}
				else if (vn > 4) {
					visitNum = "5 or more";
				}
				else {
					visitNum = "unknown";
				}
				googletag.pubads().setTargeting('visitNum', visitNum );
		})();
		


		
		
		(function(){
				var visitDepth;
				var dov = 1;
				var parts = document.cookie.split("s_depth=");
				if (parts.length === 2) {
					dov = parts.pop().split(";").shift();
				}
				dov = parseInt(dov);
				if (dov < 5) {
					visitDepth = dov.toString();
				} 
				else if (dov < 11) {
					visitDepth = "5-10";
				}
				else if (dov < 21) {
					visitDepth = "11-20";
				}
				else {
					visitDepth = "21 or more";
				}
				googletag.pubads().setTargeting('visitDepth', visitDepth);
		})();
		

		
		
