jQuery( function($)
{
    $( '#newsletter-signup-node_id' ).val( $( 'body' ).attr( 'data-context-node-id' ) );

    // custom newsletter signup box text
    var section = $( 'meta[ property="article:section" ]' ).attr( 'content' );
    switch( section ) {
        case "Energy Voices":
            $( '#action-block-wrapper #action-2-msg-1' ).html( 'Subscribe to Recharge' );
            $( '#action-block-wrapper #action-2-msg-2' ).html( 'Get Monitor global energy news and analysis delivered weekly to your inbox.' );
            break;
    }

    // customize take action box
    if( $( '#ezve-right-col-2-r14.ezn-867085' ).length) {
        $( '#action-block-wrapper #action-1-btn a' ).html( 'Take Action:&nbsp; Environment' );
        $( '#action-block-wrapper #action-1-btn a' ).attr( 'href', '/Take-Action/Environment' );
    } else if( $( '#ezve-right-col-2-r14.ezn-867087' ).length ) {
        $( '#action-block-wrapper #action-1-btn a' ).html( 'Take Action:&nbsp; Education' );
        $( '#action-block-wrapper #action-1-btn a' ).attr( 'href', '/Take-Action/Education' );
    } else if( $( '#ezve-right-col-2-r14.ezn-867090' ).length ) {
        $( '#action-block-wrapper #action-1-btn a' ).html( 'Take Action:&nbsp; Human Trafficking' );
