        $( '#action-block-wrapper #action-1-btn a' ).html( 'Take Action:&nbsp; Conflict Resolution' );
        $( '#action-block-wrapper #action-1-btn a' ).attr( 'href', '/Take-Action/Conflict-Resolution' );
