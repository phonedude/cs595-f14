            <div id="content">

                
                    	    





		
										

        



                                        





<ul class="article-attributes trackable-component b4"         	        data-component="Article:byline">
	        		<li class="byline">
							        	<div class="contributor-full">
        
		<span itemscope itemprop="author" itemtype="http://schema.org/Person"><span itemprop="name"><a class="contributor" rel="author" itemprop="url" href="http://www.theguardian.com/profile/dominic-rushe">Dominic Rushe</a></span></span> in New York	</div>
