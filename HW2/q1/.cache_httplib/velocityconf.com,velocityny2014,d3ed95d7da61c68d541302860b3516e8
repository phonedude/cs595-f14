  <script src="//cdn.oreillystatic.com/en/assets/1/eventprovider/1/yepnope.1.5.4-min.js"></script>
  <script>
  
     yepnope('//cdn.oreillystatic.com/en/assets/1/event/117/velocityny2014_btf_home_css.css');  
  
  yepnope({
    load: ['//cdn.oreillystatic.com/ajax/libs/jquery/1.7.2/jquery.min.js', '//cdn.oreillystatic.com/en/assets/1/eventprovider/1/superfish_v1.3.1.js', '//cdn.oreillystatic.com/en/assets/1/eventprovider/1/lazyload_min.js', '//cdn.oreillystatic.com/en/assets/1/eventprovider/1/accordion.js'],
    complete: function () {
        $(function(){
          //load large images and BTF images
           /*$('#hero_image').css('background-image', 'url(//cdn.oreillystatic.com/en/assets/1/event/117/velocityny2014_hero2.jpg)');
          /$('#photostrip.row').css('background-image', 'url(//cdn.oreillystatic.com/en/assets/1/event/117/velocityny2014_photo_strip.jpg)'); */
           $('#headlining.row').css('background', '#008a96 url(//cdn.oreillystatic.com/en/assets/1/event/117/velocityny2014_bolt.png) center top no-repeat');
           $('#stay_informed iframe').attr('src', '//cdn.oreillystatic.com/en/assets/1/eventprovider/1/email_signup_widget.htm?conference=velocity&locationSource=velocity14_ny_widget&colorLink=e0922f&colorText=eee'); 
           $('div.stay_connected a.chicklet').css({'background-color': 'transparent', 'background-image': 'url(//cdn.oreillystatic.com/conferences/velocity/assets2012/social-buttons/social.png)', 'background-repeat': 'no-repeat'});
 //          $('#sponsor_title').css('background', '#cacaca url(//cdn.oreillystatic.com/en/assets/1/eventprovider/1/sponsors_vertical.gif) 97% center no-repeat');   

         /* init nav */
         $("#top_level_nav #nav").superfish();



         /* accordian mainly used on speaker resources */
         $(".accordion .module h2").accordion({clickTitle: 'h2', slideTime: '30'});

