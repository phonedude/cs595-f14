  s.src = ('https:' == document.location.protocol ? 'https://' : 'http://')  + disqus_shortname + '.disqus.com/count.js';
  (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
</script>
