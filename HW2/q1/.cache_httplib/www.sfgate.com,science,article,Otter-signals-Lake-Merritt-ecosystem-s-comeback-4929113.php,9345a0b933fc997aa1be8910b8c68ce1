    <h1 class="headline entry-title">Otter signals Lake Merritt ecosystem's comeback</h1>
    <!-- e src/business/templates/hearst/article/headline.tpl -->
            <!-- src/business/templates/hearst/article/types/byline.tpl -->

<h5 class="byline"><span class="name">Will Kane</span></h5><!-- e src/business/templates/hearst/article/types/byline.tpl -->
                                                                <h5 class="timestamp updated" title="2013-10-27T07:31:08Z">
                Updated 7:31&nbsp;am, Sunday, October 27, 2013
            </h5>
        </div>

<!-- e src/business/templates/hearst/article/types/standard_title.tpl -->

