#widgets_row_uw.row h2 { margin: 0 0 20px; color: #fff; font-size: 1.6em; font-weight: normal; line-height: 1.2em; }
#widgets_row_uw.row p { color: #eee; }
#widgets_row_uw.row a {color: #d3002d; text-decoration: none;}
#widgets_row_uw.row > div {  width: 870px; margin: 0 auto; }
#widgets_row_uw.row > div > div { padding-top: 0; display: inline-block; vertical-align: top; }
#widgets_row_uw.row > div > div:last-child { border-right: none;}

