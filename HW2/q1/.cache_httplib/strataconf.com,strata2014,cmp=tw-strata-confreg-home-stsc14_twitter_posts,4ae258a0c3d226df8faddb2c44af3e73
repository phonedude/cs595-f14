	<div class="content">
		<div class="tray">
		</div>
	</div>
	<div class="control control-right"></div>
</div>
<script type="text/javascript" charset="utf-8">
	( function( $ ) {
		if ( typeof $.proxy == 'undefined' ) {
			$.proxy = function( callback, context ) {
				var proxy = function() {
					callback.apply( context, arguments );
				}
				return proxy;
			};
		}
		
		var CheeseBoard = function( speakers, priority_speakers ) {
			priority_speakers = typeof priority_speakers !== 'undefined' ? priority_speakers : false;
			this.scrolling = false;
			this.queue = [];
			
			var pi, pr, ps;
			//add priority speakers first
			while ( priority_speakers.length > 0 ) {
				pi = Math.floor( Math.random() * priority_speakers.length );
				pr = priority_speakers.splice( i, 1 )[0];
				
				if ( pr == null || pr.id == null ) continue;
				
				//s = this.createSpeakerElement( r.id, r.name, r.affiliation );
				
				this.queue.push( pr );
			}

			var i, r, s;
			while ( speakers.length > 0 ) {
				i = Math.floor( Math.random() * speakers.length );
				r = speakers.splice( i, 1 )[0];
				
				if ( r == null || r.id == null ) continue;
				
				//s = this.createSpeakerElement( r.id, r.name, r.affiliation );
				
				this.queue.push( r );
			}
			
			setTimeout( $.proxy( this.init, this ), 500 );
			
			$( ".cheese-board .control-left" ).click( $.proxy( function() { this.scrollRight( 4 ); }, this ) );
			$( ".cheese-board .control-right" ).click( $.proxy( function() { this.scrollLeft( 4 ); }, this ) );
		};
		
		CheeseBoard.template = "<div class=\"speaker\"><div class=\"head\"><div class=\"frame\"></div></div><div class=\"caption\"></div></div>"
		
		CheeseBoard.prototype = {
			init: function() {
				var that = this;
				var load = function( count, delay ) {
					var s = that.queue.shift();
					var el = that.createSpeakerElement( s.id, s.name, s.affiliation );
					
					el.data( 'record', s );
					s.element = el;
					
					el.css( { "display": "none" } );
					el.appendTo( $( ".cheese-board .content .tray" ) );
					el.fadeIn( delay );
					
					count--;
					if ( count > 0 ) setTimeout( function() { load( count, delay ); }, 200 );
				};
				
				load( 4, 500 );
			},
			
			scrollRight: function( count ) {
				if ( this.scrolling ) return;
				this.scrolling = true;
				
				var that = this;
				var s = that.queue.pop(), p = $( ".cheese-board .content .tray .speaker" ).filter( ":first" ), t = $( ".cheese-board .content .tray .speaker" ).filter( ":last" );
				var el = s.element || this.createSpeakerElement( s.id, s.name, s.affiliation );
				
				el.data( 'record', s );
				s.element = el;
				
				el.css( { "margin-left": "-95px" } );
				el.insertBefore( p );
				el.animate( { "margin-left": 0 }, 80, 'linear', function() { el.css( { "margin-left": 0 } ); that.queue.unshift( t.data( 'record' ) ); t.remove(); that.scrolling = false; if ( count > 1 ) that.scrollRight( count - 1 ); } );
			},
			
			scrollLeft: function( count ) {
				if ( this.scrolling ) return;
				this.scrolling = true;
				
				var that = this;
				var s = that.queue.shift(), t = $( ".cheese-board .content .tray .speaker" ).filter( ":first" );
				var el = s.element || this.createSpeakerElement( s.id, s.name, s.affiliation );
				
				el.data( 'record', s );
				s.element = el;
				
				el.css( { "margin-left": 0 } );
				el.appendTo( $( ".cheese-board .content .tray" ) );
				t.animate( { "margin-left": "-95px" }, 80, 'linear', function() { t.css( { "margin-left": "-95px" } ); that.queue.push( t.data( 'record' ) ); t.remove(); that.scrolling = false; if ( count > 1 ) that.scrollLeft( count - 1 ); } );
			},

			
			createSpeakerElement: function( id, name, affiliation ) {
				var s = $( CheeseBoard.template );
				s.find( ".frame" ).empty().append( '<img src="//cdn.oreillystatic.com/en/assets/1/eventprovider/1/speaker_' + id + '.jpg" alt="" style="border: 0 none;" /><a class="border" href="/strata2014/public/schedule/speaker/' + id + '"></a>' );
				s.find( ".caption" ).empty().append( '<span class="name"><a href="/strata2014/public/schedule/speaker/' + id + '">' + name + '</a></span> <span class="affiliation">' + affiliation + '</span>' );
				
				return s;
			}
		}
	
		$( function() {
			var speakers = [
				{ id: 163541, name: "Kimberly Steadman", affiliation: "Analytics Manager/Analyst" },
				{ id: 168641, name: "Chris Re", affiliation: "Assistant Professor, Stanford" },
				{ id: 147852, name: "Matei Zaharia", affiliation: "CTO, Databricks" },
				{ id: 102993, name: "Joe Hellerstein", affiliation: "Trifacta & UC Berkeley" },
				{ id: 157232, name: "Carlos Guestrin", affiliation: "CEO of GraphLab" },
				{ id: 169215, name: "David McRaney", affiliation: "Raycom Media" },
                { id: 169560, name: "Kira Radinsky", affiliation: "CTO, SalesPredict" },
                { id: 169365, name: "Magda Balazinska", affiliation: "University of Washington" },
                { id: 170997, name: "David Epstein", affiliation: "Sports Illustrated" }
			];

			
			var priority_speakers = [
				{ id: 164910, name: "James Burke", affiliation: "Historian, Futurist, Author" }
			];
			
			
var cheeseboard = new CheeseBoard( speakers, priority_speakers );
					
		});



	}) ( jQuery );
</script>
