<script>  
$(function() {
    $('#looped-video').children('source').each( function() { var video_source = $(this).data('videosrc'); $(this).attr('src', video_source); });
    var videoPlaying = false;
    var isIOS = (/iPhone|iPod|iPad/).test(navigator.userAgent);
    if (isIOS) {
      $('#looped-video').css('-webkit-transform','scale(0.01)');
    }

  $('#default-video-btn').on('click', function(e) {
       e.preventDefault();
       $('#looped-video').append('<source src="http://cdn.oreillystatic.com/conferences/strata/2014/video-sc/strata-video.webm" data-videosrc="http://cdn.oreillystatic.com/conferences/strata/2014/video-sc/strata-video.webm" type=\'video/webm; codecs="vp8, vorbis"\' /><source src="http://cdn.oreillystatic.com/conferences/strata/2014/video-sc/strata-video.mp4" data-videosrc="http://cdn.oreillystatic.com/conferences/strata/2014/video-sc/strata-video.mp4" type="video/mp4">');
       videoPlaying = true;
       $('#looped-video').clearQueue().animate({'opacity':1},2000).get(0).play();
       $('#nav_headlining.row').slideUp(2000);
       $('#video-row.row').slideDown(2000);
  }); 

    $('.video-btn').on('click', function(e) {
      e.preventDefault();
      if (!videoPlaying) {
        videoPlaying = true;
        $(this).html('Pause').addClass('pause');
        $('#looped-video').clearQueue().animate({'opacity':1},1000).get(0).play();
      }
      else {
        videoPlaying = false;
        $(this).html('Watch the Video').removeClass('pause');
        $('#looped-video').clearQueue().animate({'opacity':1},1000).get(0).pause();
      }
    });
    
});
function removeVideo(){
    $(function() {
      videoPlaying = false;
      $('#looped-video').html('Watch the Video').removeClass('pause');
      $('#nav_headlining.row').slideDown(2000);
      $('#video-row.row').slideUp(2000);
    });
      
}
</script>




<!--lazy load images -->
<script type="text/javascript" charset="utf-8">
	( function( $ ) {
		if ( typeof $.proxy == 'undefined' ) {
			$.proxy = function( callback, context ) {
				var proxy = function() {
					callback.apply( context, arguments );
				}
			
				return proxy;
			};
		}
		
		var vController = function() {
			this.init();
		};
		
		vController.prototype = {
			init: function() {
				var that = this;
				var s = $( '[data-defer="view"]' );
			
				s.each( function() {
					var t = $( this ), p = t.parent(), w = Number( t.attr( "data-img-width" ) ), h = Number( t.attr( "data-img-height" ) );
					p.css( { width: w, height: h } );
				});
			
				this.objs = [];
				var w = $( window ), top = w.scrollTop(), bottom = top + w.height(), i;
				
				s.each( function() {
					var t = $( this ), p = t.parent(), off = p.offset();
					var o = {
						a: t,
						p: p,
						top: off.top,
						bottom: off.top + p.innerHeight()
					};
					
					if ( o.top < bottom && o.bottom > top ) {
						that.load( o );
					}
					else {
						that.objs.push( o );
					}
				});
				
				$( window ).scroll( $.proxy( this.scroll, this ) );
			},
			
			scroll: function() {
				var w = $( window ), top = w.scrollTop(), bottom = top + w.height(), i;
				
				for ( i = this.objs.length - 1; i >= 0; i-- ) {
					if ( this.objs[i].top < bottom ) {
						this.load( this.objs[i] );
					}
				}
			},
			
			load: function( o ) {
				var t = o.a;
				var c = $( "<img />" );
				var f = function() { t.html( c ); };
			
				c.load( f );
				c.attr( "src", t.attr( "data-img-src" ) );
				c.attr( "alt", t.attr( "data-img-alt" ) );
				c.attr( "border", 0 );
			}
		};
		
		$( function() {
			var vC = new vController();
			
		})
	}) ( jQuery );
</script>

