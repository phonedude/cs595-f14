var initiated = false;
function element_swap(ida,idb) {
	x = document.getElementById(ida);
	y = document.getElementById(idb);
	if ((x != null) && (y != null)) {
		x.innerHTML = y.innerHTML;
	}
};
function init() {
	if (initiated) return;
	initiated = true;
	
	x = document.getElementById('thePage');
	if (x != null) x.style.height = 'auto';
	
	//Swap out hidden Google ads to hack the FOUC
