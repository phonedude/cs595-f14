<script type="text/javascript">
var impReq;

function putAdImpressions () {
	URL = "/assets/scripts/putimpression.x?pub=" + escape(pub); 
	var ads = getElementsByClassName('placedAd'); 
	for(var i=0,j=ads.length; i<j; i++) URL = URL + "&ads[]=" + escape(ads[i].value);
	
	//alert(URL);
	
	if (window.XMLHttpRequest) {
		impReq = new XMLHttpRequest();
		impReq.open("GET", URL, true);
		impReq.send(null);
	} else if (window.ActiveXObject) {
		impReq = new ActiveXObject("Microsoft.XMLHTTP");
		if (impReq) {
			impReq.open("GET", URL, true);
			impReq.send();
		}
	}
}

function putAdImpressionsImproved () {
	var ads = [];
	$( ".placedAd" ).each(function() {
		v = $( this ).val();
		if (0 > $.inArray(v,ads)) ads.push( v );
	});
	$.get( "/assets/scripts/putimpression.x", { pub: pub, "ads[]": ads });
}
//putAdImpressions();
putAdImpressionsImproved();
</script>

