<script type="text/javascript">
var clickReq;

function logClick (campaign, ad) {
	URL = "/assets/scripts/putclick.x?ad=" + escape(ad) + "&c=" + escape(campaign) + "&pub=" + escape(pub); 
	if (window.XMLHttpRequest) {
		clickReq = new XMLHttpRequest();
		//clickReq.onreadystatechange = clickProcReq;
		clickReq.open("GET", URL, true);
		clickReq.send(null);
	} else if (window.ActiveXObject) {
		clickReq = new ActiveXObject("Microsoft.XMLHTTP");
		if (clickReq) {
			//clickReq.onreadystatechange = clickProcReq;
			clickReq.open("GET", URL, true);
			clickReq.send();
		}
	}
}

function flipToggle(id,tri) {
	x=document.getElementById(id);
	y=document.getElementById(tri);
	if ((x == null) || (y == null)) return;
	
	if (x.style.display=='block') {
		y.src='/assets/images/closed8.png';
		y.alt='Open:';
		x.style.display='none';
	} else {
		x.style.display='block';
		y.src='/assets/images/open8.png';
		y.alt='Close:';
	}
}

var jsReq;

function tryJs() {
	var URL = "/assets/scripts/tryjs.x"; 
	if (window.XMLHttpRequest) {
		jsReq = new XMLHttpRequest();
		jsReq.open("GET", URL, true);
		jsReq.send(null);
	} else if (window.ActiveXObject) {
		jsReq = new ActiveXObject("Microsoft.XMLHTTP");
		if (jsReq) {
			jsReq.open("GET", URL, true);
			jsReq.send();
		}
	}
}
tryJs();

</script>



