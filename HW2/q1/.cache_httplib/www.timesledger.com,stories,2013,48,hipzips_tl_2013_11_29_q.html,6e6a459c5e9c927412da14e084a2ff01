function tryMegaMap () {

	if (typeof Mapigator != 'undefined') {
		var response = true;
		var n = 0; var mapthing = [];
		while (response == true) {
			mapthing[n] = new Mapigator(n);
			
			if (typeof mapthing[n].megaMapInit != 'undefined') response = mapthing[n].megaMapInit();
			else response = false;
			n++;
		} 
	}
	
	if (typeof GUnload != 'undefined') {
		node = document.getElementsByTagName("body")[0];
		node.onunload = GUnload;
	}
}

function getElementsByClassName(classname)  {
	node = document.getElementsByTagName("body")[0];
	var a = [];
	var re = new RegExp('\\b' + classname + '\\b');
	var els = node.getElementsByTagName("*");
	for(var i=0,j=els.length; i<j; i++)
		if(re.test(els[i].className))a.push(els[i]);
	return a;
}

function loadJWVideo (theFile, theId, splash, height, width, dispht, stretch) {
	var flashvars = {};
	flashvars.allowfullscreen = "false";
	flashvars.usefullscreen = "false";
	flashvars.file = theFile;
	flashvars.autostart = "false";
	flashvars.repeat = "false";
	flashvars.shuffle = "false";
	flashvars.height = height;
	flashvars.width = width;
	flashvars.displayheight = dispht;
	flashvars.thumbsinplaylist = "false";
	flashvars.overstretch = stretch;
	flashvars.showdownload = "true";
	flashvars.callback = "analytics";
	flashvars.image = splash;
	
	//alert(theFile);
	
	swfobject.embedSWF("/assets/scripts/mediaplayer.swf", theId, width, height, "9.0.0", false, flashvars);
	return true;
}
