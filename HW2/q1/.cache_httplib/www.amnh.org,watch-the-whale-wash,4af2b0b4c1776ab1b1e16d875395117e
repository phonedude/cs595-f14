	<script>
	
    jQuery(document).ready(function() {     	    
	    
	    $( "#subscribeFormPopUp" ).submit(function( event ) {
              event.preventDefault();

              var msg = '';
	          var emailReg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	          emailAddr = document.getElementById('email_address_in_page').value;
	          
	          if (emailReg.test(emailAddr) == false) {
	            msg+= 'Please enter a valid email address.\n';
	          }

	          if (msg.length == 0) {
	            $.cookie('newsletter_popup_count', ( window.newsletter_max_popups + 1), { expires: 3650 });
	            
	            $.post( "/amnh/subscribe", $( "#subscribeFormPopUp" ).serialize() ).done(function( data ) {console.log( data );});
	            
	            $( "#newsletter_popup" ).html('<h1>Thanks for subscribing.</h1><p>Thank you for signing up for the Museum&#39;s monthly Newsletter and Calendar Highlights.</p><button class="button buybutton" onclick="$.fancybox.close();">Continue</button>');
	            return false;
	          }
	          else {
		        alert(msg);
	          	return false;
	          }
        });
    });
		
	</script>
  



    
                                                                                                                                                                                                                                                                                    
                        
