<script>
    
                                                                                                                                                                    
    

                            

    

              
    window.newsletter_max_popups = 0;

    
    function are_cookies_enabled()
    {
        var cookieEnabled = (navigator.cookieEnabled) ? true : false;

        if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled)
        {
            document.cookie="testcookie";
            cookieEnabled = (document.cookie.indexOf("testcookie") != -1) ? true : false;
        }
        return (cookieEnabled);
    }

    if( window.newsletter_max_popups && are_cookies_enabled() )
    {
        jQuery(document).ready(function() {
            screenMode = $(window).width() < 768 ? 'mobile' : 'standard';
            // check if the newsletter pop up didn't appear in this session
            if( $.cookie('newsletter_popup_session') === undefined &&
                (
                    ( window.screenMode != 'mobile' && $("#mobile-interstitial").length >= 1) ||
                    $("#mobile-interstitial").length == 0 ||
                    ($.cookie('show-interstitial') !== undefined && $.cookie('show-interstitial')=='true' )
                )
            )
            {

                // create the session variable, so we are going to check if we need to display in this session
                $.cookie('newsletter_popup_session', 1);

                // check if it appeared up to window.newsletter_max_popups times
                if( ( $.cookie('newsletter_popup_count') !== undefined && parseInt( $.cookie('newsletter_popup_count') ) < window.newsletter_max_popups ) || $.cookie('newsletter_popup_count') === undefined )
                {
                    if($.cookie('newsletter_popup_count')===undefined)
                    {
                        $.cookie('newsletter_popup_count', 1, { expires: 3650 });
                    }
                    else
                    {
                        $.cookie('newsletter_popup_count', ( parseInt( $.cookie('newsletter_popup_count') )+1 ), { expires: 3650 });
                    }

                    if($.cookie('newsletter_popup_count')!==undefined)
                    {
                        $(".newsletter_popup_link").fancybox().trigger('click');
                    }
                }

            }
        });
    }
    

  
  // check screen width to determine mobile or standard mode
  window.screenMode = '';
  function checkScreenMode() {
    screenMode = $(window).width() < 768 ? 'mobile' : 'standard';
  }
  checkScreenMode();
  window.onresize = checkScreenMode;

	jQuery(document).ready(function() {
    var size = window.getComputedStyle(document.body,':after').getPropertyValue('content');
		if (size == 'mobile') {
			//turn off slab nav on mobile
			//$('.subbox').remove();
            // initialize fancybox desktop
            $("a.donate.button.fancybox").fancybox({
                'type':'inline',
                'closeClick':false,
                'scrolling':'no',
                'wrapCSS':'mobileDonationPopup',
                'autoSize':false,
                'width':230,
                'height':'auto',
                'fitToView':false,
                helpers: {
                  overlay: {
                    locked: false
                  }
                }
            });

            $('.mobile-expand').expander({
                slicePoint: 250,
                expandText: '<span class="icon-down-caret"></span> Read More',
                userCollapseText: '<p><span class="icon-up-caret"></span> Read Less</p>'
            });

            $('.mobile-expand-300').expander({
                slicePoint: 300,
                expandText: '<span class="icon-down-caret"></span> Read More',
                userCollapseText: '<p><span class="icon-up-caret"></span> Read Less</p>'
            });

            $('.mobile-expand-200').expander({
                slicePoint: 200,
                expandText: '<span class="icon-down-caret"></span> Read More',
                userCollapseText: '<p><span class="icon-up-caret"></span> Read Less</p>'
            });

		}else {
            // initialize fancybox desktop
            $("a.donate.button.fancybox").fancybox({
                'type':'inline',
                'closeClick':false,
                'width':400,
                'height':350,
                'autoSize':false
            });
        }
	});
	
