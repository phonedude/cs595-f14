<script>
  $(document).ready(function() {
    // Slided body
    $('.secondary-nav').on('click', function() {
      if ($('.slided-body').hasClass('active')) {
        $('.slided-body').animate({
          left: 0
        }, 300, function() {
          // Animation complete.
          $('.slided-body').removeClass('active');
          $('#sidenav').hide();
        });
      }
      else {
        $('#sidenav').show();

        $('.slided-body').addClass('active').animate({
          left: $(window).width() - 35
        }, 300, function() {
          // Animation complete.
        });
      }
    });

    // On resizing, keeps to show only 35px of the slided body
    $(window).on('resize', function() {
      if ($('.slided-body').hasClass('active')) {
        $('.slided-body').animate({
          left: $('.slided-body-wrapper').width() - 35
        }, 0, function() {
          // Animation complete.
        });
      }
    });
  });
</script>


