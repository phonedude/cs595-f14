<script>

function validateForm() {
  var msg = '';
  var emailReg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
  emailAddr = $('#email_address').val();

  if (emailReg.test(emailAddr) == false) {
    msg+= 'Please enter a valid email address.\n';
  }

  $('#thx').val($('#thx').val() + '&email=' + $('#email_address').val());

  if (msg.length == 0) {
    return true;
  }
  else {
	  alert(msg);
  	return false;
  }
}

</script>
