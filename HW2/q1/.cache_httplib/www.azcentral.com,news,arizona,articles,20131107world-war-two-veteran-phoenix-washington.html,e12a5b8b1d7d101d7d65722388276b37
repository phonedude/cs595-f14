// All the postings
var allPostings = new Array("placeholder"
, Array('Drivers', 'Jack B. Kelley, LLC', 'Drivers Needed for Local work!! Jack B. ...', 'http://www.careerbuilder.com/jobseeker/jobs/RedirectAOL.aspx?Job_DID=J3F1FK6NTXD625NK6RP&amp;show=yes&amp;lr=cbga_tap&amp;siteid=cbga_tap060&show=yes')
