<script type="text/javascript">
$(document).ready(function(){
    var user = getCookie("azc_username");
    if (user) {
        $('#rightform-logged-in').show();
        $('#ul-rightform-logged-out').hide();
        $('#rightform-logged-in #username2').html(user);
    }
    else {
        $('#rightform-logged-in').hide();
        $('#ul-rightform-logged-out').show();
    }
});

function getCookie(cname){
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i].trim();
        if (c.indexOf(name)==0) return c.substring(name.length,c.length);
    }
    return "";
};


function _abh(o) {
    var s = s_gi('gpaper158');
    s.linkTrackVars='None';
    s.linkTrackEvents='None';
    s.tl(o,'e','Subscribe: 3 for 1 header');
    window.location.href='https://fullaccess.azcentral.com/campaigns/3FOR1HOMEPAGE?onSuccessRedirectURL=http%3A%2F%2Fux.azcentral.com%2F';
}
function _abTracking(obj,name) {
    var s = s_gi('gpaper158');
    s.linkTrackVars='None';
    s.linkTrackEvents='None';
    s.tl(obj,'e',name);
}
</script>
