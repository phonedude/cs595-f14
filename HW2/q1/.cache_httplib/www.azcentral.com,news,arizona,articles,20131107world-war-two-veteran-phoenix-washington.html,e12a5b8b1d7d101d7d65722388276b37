	var $carouselDiv = $('#az-scores-schedules .carousel:eq(0)');
	if($carouselDiv.length > 0) {
	var carousel = new AZC.Carousel($carouselDiv);
	var selectedDiv = '#'+$('#scores_container .selected').attr('id').split("-")[0];
	var selectID = (selectedDiv).split("-")[0];
	switch(selectID)
	{
		case '#nfl': caption = "SCHEDULE | Arizona Cardinals | <a href=\'http://www.azcentral.com/sports/cardinals/articles/20130419arizona-cardinals-2013-football-schedule.html\'>Full Schedule</a>";
		break;
		case '#mlb': caption = "SCHEDULE | Arizona Diamondbacks | <a href=\'http://www.sportsnetwork.com/merge/tsnform.aspx?c=azcentral&page=mlb/teams/055/schedulesn.aspx?team=055,season=\'>Full Schedule</a>";
		break;
		case '#nba': caption = "SCHEDULE | Phoenix Suns | <a href=\'http://www.sportsnetwork.com/merge/tsnform.aspx?c=azcentral&page=nba/teams/107/schedulesn.aspx?team=107,season=\'>Full Schedule</a>";
		break;
		case '#nhl': caption = "SCHEDULE | Phoenix Coyotes | <a href=\'http://www.sportsnetwork.com/merge/tsnform.aspx?c=azcentral&page=nhl/teams/141/schedulesn.aspx?team=141,season=\'>Full Schedule</a>";
		break;
		case '#wnba': caption = "SCHEDULE | Phoenix Mercury";
		break;
		case '#ncaa_fb': caption = "SCHEDULE | ASU Football";
		break;
		case '#ncaa_bb': caption = "SCHEDULE";
		break;
		default: caption = "NO SCHEDULE";
	}
		$('#az-scores-schedules .heading').html(caption);
		}
})();
</script>



<script type="text/javascript">
$('#most-popular ul.tabs li a').click(function(){
	var that = this;
	$('#most-popular ul.tabs li a').each(function(){
		var href = $(this).attr('href');
		if( this === that )
		{
			$(this).parent().addClass('selected');
			$(href).show();
		}
		else
		{
			$(this).parent().removeClass('selected');
			$(href).hide();
		}				
	});
	return false;
});
	
$(".tool-print").click(function(){
	AZC.Util.print();
	return false;
});
</script>



