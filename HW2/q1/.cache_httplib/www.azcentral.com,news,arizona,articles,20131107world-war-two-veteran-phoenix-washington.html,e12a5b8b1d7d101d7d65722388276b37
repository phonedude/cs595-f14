            <script type="text/javascript">
              $(document).ready(function(){

                     // focus/blur handlers for hinted text fields
                     $("input.hinted").focus(function(){                          
                       var $field = $(this);
                       if( typeof $field.attr("readonly") != "undefined" )
                        return false;
                      if( $field.hasClass("hinted") )
                      {
                        $field.removeClass("hinted");
                        $field.val("");
                      }
                    }).blur(function(){
                     var $field = $(this);
                     if( typeof $field.attr("readonly") != "undefined" )
                      return false;
                    if( $field.val() === "" )
                    {
                      $field.addClass("hinted");
                      $field.val($field.attr("title"));
                    }
                  });
                    
                  });
            </script>

