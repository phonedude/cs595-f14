<script type="text/javascript">
//$(function() {
$(window).load(function() {
  if($('#fbCommentCount').length > 0) {
    $('#goToComments').show();
    updateCommentCount();
  }
});

function updateCommentCount() {
  var cc = $('.fb_comments_count:first').text();
  if(cc == '') {
    setTimeout('updateCommentCount()', 500);
  } else {
    if(cc == '1') {
      $('#commentCounts').text(' 1 Comment');
    } else {
      $('#commentCounts').text(' '+cc+' Comments');
    }
  }
}
    
function scrollToComments() {
  var d = $('.blogEntry').offset();
  $('html, body').animate({scrollTop: d.top}, 600);
}
</script>

