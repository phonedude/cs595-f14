<script type="text/javascript">
  $('#main ul.tabs li a').click(function(){
    $('#main ul.tabs li').removeClass('selected');
    $(this).parent().addClass('selected');
    if($(this).text() == 'Article') {
     $('#media').show();
     $('#content-newsitem').show();
     $('#mediaOverline').show();
     $('.addComment').show();
     $('.blogEntry').hide();
     $('#social:last').show();
   } else {
     $('#media').hide();
     $('#content-newsitem').hide();
     $('#mediaOverline').hide();
     $('.addComment').hide();
     $('.blogEntry').show();
     $('#social:last').hide();
   }
   return false;
 });
  $('.addComment .heading a').click(function(){
   $("#main ul.tabs li.selected").removeClass('selected');
   $("#main ul.tabs li").last().addClass('selected');
   $('#media').hide();
   $('#content-newsitem').hide();
   $('#mediaOverline').hide();
   $('.addComment').hide();
   $('.blogEntry').show();
   $('#social:last').hide();
   $(window).scrollTop(0);
   return false;
 });
</script>
