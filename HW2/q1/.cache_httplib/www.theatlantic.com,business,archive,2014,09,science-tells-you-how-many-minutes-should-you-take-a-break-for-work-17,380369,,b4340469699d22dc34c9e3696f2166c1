<script type="text/javascript">
(function($) {
    var $carousel = $('#carousel');
    var $carouselNav = $carousel.find('nav'),
    carouselInstance, pauseAfterSlide, hasBoundSlideEvent;

    $carousel.carousel({
        interval: 0,
        pause: "hover",
        onBeforeSlide: function(carousel, $active, $next) {
            $carouselNav.find('.active').removeClass('active');
        },
        onSlide: function(carousel, $prev, $active) {
            var slideNum = $active.attr('data-slide-num');
            $carouselNav.find('a[data-slide-num=' + slideNum + ']').addClass('active');
        },
        onLoop: function(carousel) {
            carousel.stopCycling();
        }
    });

    carouselInstance = $carousel.data('carousel');
    pauseAfterSlide = function(evt) {
        carouselInstance.pause();
        carouselInstance.hovered = true;
    };
    hasBoundSlideEvent = false;

    $carouselNav.find('a').on('click', function() { return false; })
    $carouselNav.find('a').bind('mouseenter.carousel', function() {
        if (hasBoundSlideEvent) {
            $carousel.off('slid.carousel', pauseAfterSlide);
            hasBoundSlideEvent = false;
        }
        hasBoundSlideEvent = true;
        $carousel.one('slid.carousel', pauseAfterSlide);
        carouselInstance.hovered = false;
        carouselInstance.to(parseInt(this.getAttribute('data-slide-num'), 10));
    }).bind('mouseleave.carousel', function() {
        if (hasBoundSlideEvent) {
            $carousel.off('slid.carousel', pauseAfterSlide);
            hasBoundSlideEvent = false;
        }
    });

    $carousel.bind('mouseleave.carousel', function() {
        if (hasBoundSlideEvent) {
            $carousel.off('slid.carousel', pauseAfterSlide);
            hasBoundSlideEvent = false;
        }
        carouselInstance.cycle();
    });
})(jQuery);
</script>

