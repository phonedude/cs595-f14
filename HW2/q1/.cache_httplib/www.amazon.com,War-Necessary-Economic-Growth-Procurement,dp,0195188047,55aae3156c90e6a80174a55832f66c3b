<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "Military%20and%20defense-related%20procurement%20has%20been%20an%20important%20source%20of%20technology%20development%20across%20a%20broad%20spectrum%20of%20industries%20that%20account%20for%20an%20important%20share%20of%20United%20States%20industrial%20production.%20In%20this%20book%2C%20the%20author%20focuses%20on%20six%20general-purpose%20technologies%3A%20interchangeable%20parts%20and%20mass%20production%3B%20military%20and%20commercial%20aircraft%3B%20nuclear%20energy%20and%20electric%20power%3B%20computers%20and%20semiconductors%3B%20the%20INTERNET%3B%20and%20the%20space%20industries.%20In%20each%20of%20these%20industries%2C%20technology%20development%20would%20have%20occurred%20more%20slowly%2C%20and%20in%20some%20case%20much%20more%20slowly%20or%20not%20at%20all%2C%20in%20the%20absence%20of%20military%20and%20defense-related%20procurement.%3Cbr%3E%3Cbr%3EThe%20book%20addresses%20three%20questions%20that%20have%20significant%20implications%20for%20the%20future%20growth%20of%20the%20United%20States%20economy.%20One%20is%20whether%20changes%20in%20the%20structure%20of%20the%20United%20States%20economy%20and%20of%20the%20defense-industrial%20base%20preclude%20military%20and%20defense-related%20procurement%20from%20playing%20the%20role%20in%20the%20development%20of%20advanced%20technology%20in%20the%20future%2C%20comparable%20to%20the%20role%20it%20has%20played%20in%20the%20past.%20A%20second%20question%20is%20whether%20public%20support%20for%20commercially%20oriented%20research%20and%20development%20will%20become%20an%20important%20source%20of%20new%20general-purpose%20technologies.%20A%20third%20and%20more%20disturbing%20question%20is%20whether%20a%20major%20war%2C%20or%20the%20threat%20of%20major%20war%2C%20will%20be%20necessary%20to%20mobilize%20the%20scientific%2C%20technical%2C%20and%20financial%20resources%20necessary%20to%20induce%20the%20development%20of%20new%20general-purpose%20technologies.%3Cbr%3E%3Cbr%3EWhen%20the%20history%20of%20United%20States%20technology%20development%20in%20the%20next%20half%20century%20is%20written%2C%20it%20will%20focus%20on%20incremental%20rather%20than%20revolutionary%20changes%20in%20both%20military%20and%20commercial%20technology.%20It%20will%20also%20be%20written%20within%20the%20context%20of%20slower%20productivity%20growth%20than%20of%20the%20relatively%20high%20rates%20that%20prevailed%20in%20the%20United%20States%20in%20the%201950s%20and%201960s%20or%20during%20the%20information%20technology%20bubble%20that%20began%20in%20the%20early%201990s.%20These%20will%20impose%20severe%20constraints%20on%20the%20capacity%20of%20the%20United%20States%20to%20sustain%20a%20global-class%20military%20posture%20and%20a%20position%20of%20leadership%20in%20the%20global%20economy.",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    






