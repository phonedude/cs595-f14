    <h1 class="headline entry-title">Brigades helps communities build better websites</h1>
    <!-- e src/business/templates/hearst/article/headline.tpl -->
            <!-- src/business/templates/hearst/article/types/byline.tpl -->

<h5 class="byline"><span class="name">Nellie Bowles</span></h5><!-- e src/business/templates/hearst/article/types/byline.tpl -->
                                                                <h5 class="timestamp updated" title="2013-10-18T17:03:31Z">
                Published 5:03&nbsp;pm, Friday, October 18, 2013
            </h5>
        </div>

