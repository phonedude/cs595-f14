<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "%3Cp%3EThis%20new%20bestseller%20from%20leading%20economist%20Mariana%20Mazzucato%20%26%23x2013%3B%20named%20by%20the%20%26%23x2018%3BNew%20Republic%26%23x2019%3B%20as%20one%20of%20the%20%26%23x2018%3Bmost%20important%20innovation%20thinkers%26%23x2019%3B%20today%20%26%23x2013%3B%20is%20stirring%20up%20much-needed%20debates%20worldwide%20about%20the%20role%20of%20the%20State%20in%20innovation.%20Debunking%20the%20myth%20of%20a%20laggard%20State%20at%20odds%20with%20a%20dynamic%20private%20sector%2C%20Mazzucato%20reveals%20in%20case%20study%20after%20case%20study%20that%20in%20fact%20the%20opposite%20situation%20is%20true%2C%20with%20the%20private%20sector%20only%20finding%20the%20courage%20to%20invest%20after%20the%20entrepreneurial%20State%20has%20made%20the%20high-risk%20investments.%20Case%20studies%20include%20examples%20of%20the%20State%26%23x2019%3Bs%20role%20in%20the%20%26%23x2018%3Bgreen%20revolution%26%23x2019%3B%2C%20in%20biotech%20and%20pharmaceuticals%2C%20as%20well%20as%20several%20detailed%20examples%20from%20Silicon%20Valley.%20In%20an%20intensely%20researched%20chapter%2C%20she%20reveals%20that%20every%20technology%20that%20makes%20the%20iPhone%20so%20%26%23x2018%3Bsmart%26%23x2019%3B%20was%20government%20funded%3A%20the%20Internet%2C%20GPS%2C%20its%20touch-screen%20display%20and%20the%20voice-activated%20Siri.%20Mazzucato%20also%20controversially%20argues%20that%20in%20the%20history%20of%20modern%20capitalism%20the%20State%20has%20not%20only%20fixed%20market%20failures%2C%20but%20has%20also%20shaped%20and%20created%20markets%2C%20paving%20the%20way%20for%20new%20technologies%20and%20sectors%20that%20the%20private%20sector%20only%20ventures%20into%20once%20the%20initial%20risk%20has%20been%20assumed.%20And%20yet%20by%20not%20admitting%20the%20State%26%23x2019%3Bs%20role%20we%20are%20socializing%20only%20the%20risks%2C%20while%20privatizing%20the%20rewards%20in%20fewer%20hands.%20This%2C%20she%20argues%2C%20hurts%20both%20future%20innovation%20and%20equity%20in%20modern-day%20capitalism.%20Named%20one%20of%20the%20%26%23x2018%3B2013%20Books%20of%20the%20Year%26%23x2019%3B%20by%20the%20%26%23x2018%3BFinancial%20Times%26%23x2019%3B%20and%20recommended%20by%20%26%23x2018%3BForbes%26%23x2019%3B%20in%20its%202013%20%26%23x2018%3Bcreative%20leaders%26%23x2019%3B%20list%2C%20this%20book%20is%20a%20must-read%20for%20those%20interested%20in%20a%20refreshing%20and%20long-awaited%20take%20on%20the%20public%20vs.%20private%20sector%20debate.%3C%2Fp%3E",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    






