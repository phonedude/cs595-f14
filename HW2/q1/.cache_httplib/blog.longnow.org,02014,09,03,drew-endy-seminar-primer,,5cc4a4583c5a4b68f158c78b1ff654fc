<script>
    $(document).ready(function() {

        // Find the archive list and prefix years with a zero.
        if ($('.archive_list')) {
            var n = $(".archive_list li").length;
            $('.archive_list li a').each(
                function(n, element){
                    year = $(element).text();
                    long_year = ("0" + year);
                    $(element).text(long_year);
                }
            );
        }

    }); // closes $(document).ready()
</script>

