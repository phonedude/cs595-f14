      function openGplus(){var e="https://plus.google.com/share?url=http://1984day.com/";window.open(e,"Share on Google Plus","width=500,height=436");return false}*/
      function openTwitter(){
        var t="https://twitter.com/home?status=Thousands of people gathered on the steps of the capitol to fight back against NSA surveillance -> https://rally.stopwatching.us #StopWatchingUs&related=stopwatchingus,sinak,neutralthoughts";window.open(t,"Twitter","width=550,height=420");
          return false}

    function openFacebook() {
        var e = "https://www.facebook.com/sharer/sharer.php?u=https://rally.stopwatching.us";
        window.open(e, "Share on Facebook", "width=500,height=500");
        return false
    }

      $(document).ready( function () {
        if($(window).width() >= 840){
            $('.tintup').attr('data-columns', 2);
        } 
        /*$("#facebook").attr("onclick","openFacebook(); return false;");
        $("#gplus").attr("onclick","openGplus(); return false;");*/
        $("#tweet-box").attr("onclick","openTwitter(); return false;")
        $("#tweet-box2").attr("onclick","openTwitter2(); return false;")
        $("#tweet-box3").attr("onclick","openTwitter(); return false;")
        $("#facebook").attr("onclick", "openFacebook(); return false;");

         $.ajax('https://d28jjwuneuxo3n.cloudfront.net/?networks=facebook,twitter,googleplus&url=https://rally.stopwatching.us/', {
            success: function (res, err) {
              $.each(res, function(network, value){
                var count = value;
                if(count / 10000 > 1) {
                  count = Math.ceil(count/1000) + 'k'
                }
                $('[data-network="'+network+'"]').attr('count', count);
              })
            },
            dataType: 'jsonp'
          })


          // I only have one form on the page but you can be more specific if need be.
          $('.hero-email-input').on('focus', function (){
            $('.jumbotron .form-group').slideDown(550);
          })

          $('.sidebar-email-input').on('focus', function (){
            $('.sidebar .form-group').slideDown(550);
          })



          $('.volunteer-form').on('submit', function(ev){
            var form = $(ev.currentTarget);
            var data = form.serializeObject();
            console.log(data);
            //$('.status1', form).text('Checking email...')
            //$('button', form).text('SIGNING UP...')
            //$('.status1', form).empty();
            $.ajax({
              url: 'https://skipchimp.herokuapp.com/subscribe',
              data: data,
              type: 'POST',
              success: function () {
                $(form).empty();
                $(form).html('Thanks for your support.');
                setTimeout(function () {
                  //window.location = "/share.html";
                }, 500);
              }
            })
            return false;
          });

