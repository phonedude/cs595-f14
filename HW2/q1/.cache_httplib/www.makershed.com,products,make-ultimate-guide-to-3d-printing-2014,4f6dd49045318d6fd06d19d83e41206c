	var productDeepestCollection = null;
	$(function(){
		
		var breadcrumbLinks = $($('.breadcrumbsList:visible').children()[1]).children()[0];
		if($(breadcrumbLinks).length){
			var topCollection = $($(breadcrumbLinks).attr('href').split('/')).last()[0];
			var lastPageHandle = '#collection-list-'+topCollection;
			if ($(lastPageHandle).length){
				$(lastPageHandle).removeClass('hidden');
			}
			else {
				lastPageHandle = '#'+$('.product-collection-list').first().attr('id');
				$(lastPageHandle).removeClass('hidden');
			}
		}
		else {
			var lastPageHandle = '#'+$('.product-collection-list').first().attr('id');
			$(lastPageHandle).removeClass('hidden');
		}
		var children = $('.breadcrumbsList:visible').children();
		var url = $(children[children.length-2]).find('a').attr('href');
		$('.product-collection-list a[href="'+url+'"]').parent().addClass('active');
		productDeepestCollection = $($(children[children.length-2]).find('a').attr('href').split('/')).last()[0];
		
		$('.product-collection-list .active').each(function(index, el) {
			var parent = $(this).parent();
			while (!$(parent).hasClass('product-collection-list')){
				if ($(parent).hasClass('sub-menu')){
					$(parent).show();
				}
				if ($(parent).hasClass('dropdown')){
					$(parent).find('>a.dropdown-toggle').text('-');
				}
				parent = $(parent).parent();
			}
		});
		$('.product-collection-list .dropdown-toggle').click(function(event) {
			event.preventDefault();
			var submenu = $(this).parents('.dropdown').children('.sub-menu');
			if ($(submenu).is(':visible')){
				$(this).text('+');
				$(submenu).hide();
			}
			else {
				$(this).text('-');
				$(submenu).show();
			}
			
		});
	});
</script>
