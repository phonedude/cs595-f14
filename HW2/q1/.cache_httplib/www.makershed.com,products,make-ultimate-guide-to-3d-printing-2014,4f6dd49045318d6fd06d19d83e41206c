						<script>
							var addedHandles = [ "makerbot-digitizer-desktop-3d-scanner","ultimaker-2-3d-printer","felix-2-0-3d-printer-kits","makerbot-replicator-2x-3d-printer-dual-extruder"]
							function updateRecommended(product){
								var curRow = $('#recommended-products-grid').children().last();
								if (!curRow || !curRow.length || $(curRow).children().length == 2) {
									curRow = $('<div class="row"></div>');
								}
								$(curRow).append($('<div class="product-grid-item col-xs-30"> \
									<a href="/products/'+product.handle+'"> \
										<table class="product-image-container"> \
											<tr><td><img src="'+product.featured_image+'" alt="'+product.title+'"></td></tr> \
										</table> \
										<div class="product-title">'+product.title+'</div> \
										<span class="shopify-product-reviews-badge" data-id="'+product.id+'"></span> \
										<div class="product-price">'+BCCShopify.formatMoney(product.price,"${{amount}}")+'</div> \
									</a> \
									<input type="checkbox" name="addons" value="'+product.variantID+'"> \
									<label for="addons">Add to Cart</label> \
								</div>'));
								if (!$(curRow).parent().length){
									$('#recommended-products-grid').append(curRow);
								}
							}
							
								
									
									
									BCCShopify.getProduct("makerbot-digitizer-desktop-3d-scanner", function(data){
										var sku = 0;
										var productData = {
											featured_image: data.featured_image,
											handle: data.handle,
											title: data.title,
											id: data.id,

										};
										if (!productData.featured_image) productData.featured_image = '//cdn.shopify.com/s/images/admin/no-image-1024x1024.gif';
										if (!sku) {
											productData.price = data.variants[0].price;
											productData.variantID = data.variants[0].id;
										}
										else {
											variant = variantForSku(data,sku);
											productData.price = variant.price;
											productData.title += ' - ' + variant.title;
											productData.variantID = variant.id;
										}
										updateRecommended(productData);
									});
								
									
									
