						$('body').on('click','#product-images .scrolling-image-container-inner a',function(e){
							e.preventDefault();

							if ($('#mobile-product-carousel').is(':visible')){
								var index = $(this).data('index');
								$('.carousel-indicators li[data-slide-to='+index+']').click();
							}
							else {
								$('#product-images .scrolling-image-container-inner .product-image.active').removeClass('active');
								$(this).parent().addClass('active');

								var src = $(this).attr('data-url');
								$('#photo img').attr('src',src);
							}
							
						});
						$('body').on('click','#video-images .scrolling-image-container-inner a',function(e){
							e.preventDefault();
							$('#video-images .scrolling-image-container-inner .product-image.active').removeClass('active');
							$(this).parent().addClass('active');

							var type = $(this).data('video-type');
							var src = $(this).data('video-id');
							if (type == 'yt'){
								$('#ytplayer').show();
								$('#vimeo').hide();
								$('#vimeo').html('');
								if (!player) initYT(src);
								else {
									player.loadVideoById(src, 0,"large");
								}
							}
							else if (type == 'vimeo'){
								if (player) player.stopVideo();
								$('#ytplayer').hide();
								$('#vimeo').show();
								$.getJSON('http://www.vimeo.com/api/oembed.json?url=' + encodeURIComponent('http://vimeo.com/'+src) + '&width=400&callback=?', function(data){
										$('#vimeo').html(data.html);
								});
							}
						});
						var margin = 22;
						var lastWidth = $('.scrolling-image-container-outer').width();
						function scrollProductImages(direction, container){
							var outerEl = $(container).find('.scrolling-image-container-outer');
							var innerEl = $(container).find('.scrolling-image-container-inner');
							var width = $(outerEl).width() + margin;
							var maxScroll = -$(innerEl).width() + width - margin;
							var currentScroll = Number($(innerEl).data('cur-position'));
							var newScroll = 0;
							if (direction == 'prev'){
								newScroll = currentScroll + width;
							}
							else if (direction == 'next') {
								newScroll = currentScroll - width;
							}
							if (newScroll > 0) newScroll = 0;
							if (newScroll < maxScroll) newScroll = maxScroll;
							$(innerEl).data('cur-position',newScroll);
							$(innerEl).css('-webkit-transform','translate('+newScroll+'px,0px)',
											'-moz-transform','translate('+newScroll+'px,0px)',
											'-ms-transform','translate('+newScroll+'px,0px)',
											'-o-transform','translate('+newScroll+'px,0px)',
											'transform','translate('+newScroll+'px,0px)');
						}
						function updateProductImages(){
							console.log('calling updateProductImages()');
							$('.scrolling-image-container').each(function(index, el) {
								var outerEl = $(this).find('.scrolling-image-container-outer');
								var innerEl = $(this).find('.scrolling-image-container-inner');
								var images = $(this).find('.product-image');
								var borderWidth = 2;
								var width = $(outerEl).width() - margin*2;
								var remainder = width % 3;
								var indWidth = Math.floor(width/3)-borderWidth;
								var count = $(images).length;
								var totalWidth = 0;
								var height = 0;
								$(images).each(function(index, el) {
									var mod = 0;
									if (index % 3 < remainder) mod++;
									totalWidth+=indWidth+mod+borderWidth;
									var curWidth = indWidth+mod;
									$(this).width(curWidth);
									$(this).height(indWidth);
									$(this).find('img').css('max-width',(curWidth)+'px');
									$(this).find('img').css('max-height',(indWidth)+'px');
								});
								$(innerEl).width(totalWidth+(count-1)*margin);
								var newWidth = $(outerEl).width();
								if (lastWidth != newWidth){
									$(innerEl).data('cur-position',0);
									$(innerEl).css('-webkit-transform','translate(0px,0px)',
													'-moz-transform','translate(0px,0px)',
													'-ms-transform','translate(0px,0px)',
													'-o-transform','translate(0px,0px)',
													'transform','translate(0px,0px)');
								}
								lastWidth = newWidth;
							});
						}
						$('.scrolling-image-container .product-nav').click(function(event) {
							event.preventDefault();
							scrollProductImages($(this).data('direction'), $(this).parent());
						});
						$(function(){
							$('.scrolling-image-container').imagesLoaded(function(){
								updateProductImages();
							})
							$(window).resize(updateProductImages);
						});
					</script>
