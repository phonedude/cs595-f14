	<ol id="default" class="breadcrumbsList maker-page hidden">
<li>Make: Ultimate Guide to 3D Printing 2014</li>
<li><a href="/">Home</a></li>
</ol>
<script>
		$(function() {
			var url = $(document.referrer.split('#'))[0].split('?')[0];
			var lastPageHandle = '#'+$(url.split('/')).last()[0].toLowerCase().replace('&','').replace('--','-');
			if ($(lastPageHandle).length){
				$(lastPageHandle).removeClass('hidden');
			}
			else {
				$('.breadcrumbsList').each(function(index, el) {
					var isShopByCategory = $(this).find('a[href="/collections/shop-by-category"]').length > 0;
					if (isShopByCategory) lastPageHandle = '#'+$(this).attr('id');
				});
				if (!$(lastPageHandle).length){
					lastPageHandle = '#'+$('.breadcrumbsList').first().attr('id');
				}
				$(lastPageHandle).removeClass('hidden');
			}
			$(lastPageHandle).children().each(function(i){
				$(lastPageHandle).prepend(this)
			});
		});
	</script>
</div>
<script>
		$(function(){
			$('.nav > li.dropdown').hover(function() {
				$(this).addClass('open');
			}, function() {
				$(this).removeClass('open');
			});
			<!--======================== SEARCH REDIRECT IF EMPTY ========================--> 
              $( "#search_form input[type='submit']" ).click(function(e) {
				var str = $("#search_form input[type='text']").val();
				if (!str || str == '') {
					e.preventDefault();
					location.href = '/collections/all';
				}
			  });
              $( "#mobile_search_form input[type='submit']" ).click(function(e) {
				var str = $("#mobile_search_form input[type='text']").val();
				if (!str || str == '') {
					e.preventDefault();
					location.href = '/collections/all';
				}
			  });
		});		
	</script>
</header>
