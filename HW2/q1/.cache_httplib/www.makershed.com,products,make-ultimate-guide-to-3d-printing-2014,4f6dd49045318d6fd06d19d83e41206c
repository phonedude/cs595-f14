						<script>
							function variantForOptions(option1,option2,option3){
								var variant = -1;
								$(product.variants).each(function(index, el) {
									if ((!option1 || this.option1 == option1) &&
										(!option2 || this.option2 == option2) &&
										(!option3 || this.option3 == option3)) {
										variant = this;
										return;
									}
								});
								return variant;
							} // End of variantForOptions()
							function updateSelectors(variant, currentGroup, override){
								var optionSelectors = $('#product-options-table .product-options-array .btn-group');
								var options = [];
								options[$(currentGroup).data('option')] = variant['option'+$(currentGroup).data('option')];

								$(optionSelectors).each(function(index, btnGroup) {
									if (this == currentGroup && !override) return;
									
									var title = $(this).data('title');
									var curO = $(this).data('option');
									if (optionSelectors.length > 1){
										$(optionSelectors).each(function(j, el) {
											if (this == currentGroup || el == btnGroup) return;
											var selO = $(this).find('button').html();
											if (selO != 'Select '+$(this).data('title')){
												options['option'+$(this).data('option')] = $(this).find('a').first().data('variant');
											}
											else {
												options[$(this).data('option')] = selO;
											}
										});
									}
									

									$(this).find('a').each(function(index, el) {
										options[curO] = $(this).data('variant');
										var currentVariant = variantForOptions(options[1],options[2],options[3]);
										var extraText = ' - ' + BCCShopify.formatMoney(currentVariant.price, '${{amount}}');
										if (currentVariant == -1){
											extraText = ' - N/A';
										}
										else {
											if (!currentVariant.available) extraText = ' - Sold Out';
										}
										$(this).find('span').html(extraText)
									});
								});
								if (override){
									var title = $(currentGroup).data('title');
									var curO = $(currentGroup).data('option');
									$(optionSelectors).each(function(j, el) {
										if (this == currentGroup) return;
										var selO = $(this).find('button').html();
										if (selO != 'Select '+$(this).data('title')){
											options['option'+$(this).data('option')] = $(this).find('a').first().data('variant');
										}
										else {
											options[$(this).data('option')] = null;
										}
									});
									

									$(currentGroup).find('a').each(function(index, el) {
										options[curO] = $(this).data('variant');
										var currentVariant = variantForOptions(options[1],options[2],options[3]);
										var extraText = ' - ' + BCCShopify.formatMoney(currentVariant.price, '${{amount}}');
										if (currentVariant == -1){
											extraText = ' - N/A';
										}
										else {
											if (!currentVariant.available) extraText = ' - Sold Out';
										}
										$(this).find('span').html(extraText)
									});
								}
							}
							function updateQuantity(button){
								var dropdownToggle = $(button).parent().parent().parent().find('button');
								var newQuantity = $(button).text();
								var currentActiveButton = $(button).parent().parent().find('.active');

								$(currentActiveButton).removeClass('active');
								$(dropdownToggle).text(newQuantity);
								$(button).parent().addClass('active');
								$('#quantity').val(newQuantity);
							}
							function updateProduct(button){
								// Get Data about the clicked button
								var btnGroup = $(button).parents('.btn-group');
								var dropdownToggle = $(btnGroup).find('button');
								var optionIndex = $(btnGroup).data('option');
								var newVariantData = $(button).attr('data-variant');
								var currentActiveButton = $(btnGroup).find('.active');
								$(currentActiveButton).removeClass('active');
								$(dropdownToggle).text(newVariantData);
								$(button).parent().addClass('active');
								
								// Get options and update variant selector
								var option1 = $('.product-options-array li.active a[data-option=1]').attr('data-variant');
								var option2 = $('.product-options-array li.active a[data-option=2]').attr('data-variant');
								var option3 = $('.product-options-array li.active a[data-option=3]').attr('data-variant');
								var variant = variantForOptions(option1,option2,option3);

								var override = false;
								if (variant == -1){
									override = true;
									var nullGroups = [];
									if (optionIndex == 1 || optionIndex == 2){
										variant = variantForOptions(option1,option2,null);
										nullGroups.push($('#product-options-table .btn-group[data-option=3]'));
									}
									else if (optionIndex == 3){
										variant = variantForOptions(option1,null,option3);
										nullGroups.push($('#product-options-table .btn-group[data-option=2]'));
									}
									
									if (variant == -1){
										if (optionIndex == 1){
											variant = variantForOptions(option1,null,null);
											nullGroups.push($('#product-options-table .btn-group[data-option=2]'));
										}
										else if (optionIndex == 2){
											variant = variantForOptions(null,option2,null);
											nullGroups.push($('#product-options-table .btn-group[data-option=1]'));
										}
										else if (optionIndex == 3){
											variant = variantForOptions(null,null,option3);
											nullGroups.push($('#product-options-table .btn-group[data-option=1]'));
										}
									}
									$(nullGroups).each(function(index, el) {
										var button = $(this).find('button');
										$(button).html('Select '+$(this).data('title'));
										$(this).find('.active').removeClass('active');
									});
								}
								// Update Add to Cart button based on availability. 
								
								$('.product-price span').html(BCCShopify.formatMoney(variant.price, '${{amount}}'));
								$('#variant-ids').val(variant.id);
								if (variant.available){
									$('#add-to-cart').removeAttr('disabled');
									$('#add-to-cart').val('ADD TO CART');
									$('#add-to-cart').show();
									$('#variant-out-of-stock').hide();
								}
								else{
									$('#add-to-cart').attr('disabled', 'disabled');
									$('#add-to-cart').val('Out of Stock');
									$('#add-to-cart').hide();
									$('#variant-out-of-stock').show();
									$('#ISR_form_variant').val(variant.id);
								}
								$('#ISR_button').hide();
								$('#variant-out-of-stock #ISR_button').show();
								updateSelectors(variant, $(button).parents('.btn-group')[0], override);
							} // End of updateProduct()

							$('body').on('click','#product-options .btn-group a',function(event){
								event.preventDefault();
								if ($(this).hasClass('disabled')) return false;
								updateProduct(this);
							});
							$('body').on('click','#product-quantity .btn-group a',function(event){
								event.preventDefault();
								updateQuantity(this);
							});
							var addedAddons = [];
							$('body').on('change','input[name="addons"]',function(event){
								var value = $(this).val();
								if($(this).is(":checked")) {
									addedAddons.push(value);
								}
								else {
									var index = addedAddons.indexOf(value);
									if (index > -1) {
										addedAddons.splice(index, 1);
									}
								}
							});
							$('body').on('click','#add-to-cart',function(event){
								var addonCount = addedAddons.length;
								if (addonCount){
									var currentCount = 0;
									var addonsToAdd = [];
									$(addedAddons).each(function(index, el) {
										addonsToAdd.push({id:parseInt(this)});
									});
									BCCShopify.addItems(addonsToAdd,function(data){
										$('#add-item-form').submit();
									});
									event.preventDefault();
								}
							});
							$(function(){
								updateSelectors(product.variants[0]);
							});
						</script>
						<div id="product-add" class="row">
