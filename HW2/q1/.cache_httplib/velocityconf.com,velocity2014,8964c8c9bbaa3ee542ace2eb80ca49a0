  <script>
  
     yepnope('//cdn.oreillystatic.com/en/assets/1/event/113/velocity2014_btf_home_css.css');  
  
  yepnope({
    load: ['//cdn.oreillystatic.com/en/assets/1/eventprovider/1/superfish_v1.3.1_min.js',  '//cdn.oreillystatic.com/en/assets/1/eventprovider/1/accordion_min.js'],
    complete: function () {
        $(function(){
          //load large images and BTF images
           $('#headlining.row').css('background', '#00aba9 url(//cdn.oreillystatic.com/en/assets/1/event/113/velocity2014_bolt.gif) center top no-repeat');
           $('#stay_informed iframe').attr('src', 'http://cdn.oreillystatic.com/en/assets/1/eventprovider/1/email_signup_widget.htm?conference=velocity&locationSource=velocity14_ca_widget&colorLink=e0922f&colorText=eee'); 
           $('div.stay_connected a.chicklet').css({'background-color': 'transparent', 'background-image': 'url(//cdn.oreillystatic.com/conferences/velocity/assets2012/social-buttons/social.png)', 'background-repeat': 'no-repeat'});
           $('#sponsor_title').css('background', '#cacaca url(//cdn.oreillystatic.com/en/assets/1/eventprovider/1/sponsors_vertical.gif) 97% center no-repeat');   

            /* init nav */
           $("#top_level_nav #nav").superfish();
        
           /* accordian mainly used on speaker resources */
           $(".accordion .module h2").accordion({clickTitle: 'h2', slideTime: '30'});

           /* make links with class=external open up external */
           $("a.external").click(function (e) {
              e.preventDefault();
