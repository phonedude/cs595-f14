<script type="text/javascript">
	$(function() {
		var stat = $('.emis-stat'),
			bubs = $('.emis-bubmap'),
			about = $('#emis-about>p'),
			cel;
		stat.click(function() {
			cel = $(this);
			if(!cel.hasClass('selected')) {
				stat.removeClass('selected');
				cel.addClass('selected');
				bubs.removeClass('selected');
				$('#' + cel.attr('data-emismap')).addClass('selected');
			}
		});

		about.click(function(e) {
			about.addClass('active');
			e.preventDefault || e.preventDefault();
			return false;
		})
		$('#emis-top-layer').click(function() {
			about.removeClass('active');
		});

		//init
		$('.emis-stat:first-of-type').click();
	});
</script>
	    
	
	</div>
	



			
	             		
		
		
    

		
		
			
            
			
			
	 		






