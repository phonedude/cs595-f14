      <!-- Scripts -->
      <script type="text/javascript">
        $(document).ready(function(){
            //Script for quote rotation
            var quotes = $(".quote");
            var quoteIndex = -1;

            function showNextQuote() {
                ++quoteIndex;
                quotes.eq(quoteIndex % quotes.length)
                    .fadeIn(1000)
                    .delay(8000)
                    .fadeOut(1000, showNextQuote);
            }

            showNextQuote();

            //Script for navbar toggle collapse
            function close_toggle() {
               if ($(window).width() <= 768) {
                  $('.nav a').on('click', function(){
                      $(".navbar-toggle").click();
                  });
               }
               else {
                 $('.nav a').off('click');
               }
            }
            close_toggle();

            $(window).resize(close_toggle);

            });
      </script>
