    function trackShare(eVar25) {
        var beforeLinkTrackVars = s.linkTrackVars;
        var beforeLinkTrackEvents = s.linkTrackEvents;
        var beforeEvents = s.events;
        var beforeEVar25 = s.eVar25;

        s=s_gi(s_account);
        s.linkTrackVars='eVar25,events';
        s.linkTrackEvents='event19';
        s.events='event19';
        s.eVar25=eVar25;
        s.tl(true,'o','social share');

        // Reset the vars as the state will continue across subsequent requests
        s.linkTrackVars=beforeLinkTrackVars;
        s.linkTrackEvents=beforeLinkTrackEvents;
        s.events=beforeEvents;
        s.eVar25=beforeEVar25;


    }
