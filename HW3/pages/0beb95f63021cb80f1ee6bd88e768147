P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "%3Cdiv%3EThe%20White%20House%20Office%20of%20Information%20and%20Regulatory%20Affairs%20%28OIRA%29%20is%20the%20United%20States%26%238217%3Bs%20regulatory%20overseer.%20In%20%3CI%3EValuing%20Life%3C%2FI%3E%2C%20Cass%20R.%20Sunstein%20draws%20on%20his%20firsthand%20experience%20as%20the%20Administrator%20of%20OIRA%20from%202009%20to%202012%20to%20argue%20that%20we%20%3CI%3Ecan%3C%2FI%3E%20humanize%20regulation%26%23151%3Band%20save%20lives%20in%20the%20process.%3CBR%3E%20%20%3Cbr%3E%20%20%20As%20OIRA%20Administrator%2C%20Sunstein%20helped%20oversee%20regulation%20in%20a%20broad%20variety%20of%20areas%2C%20including%20highway%20safety%2C%20health%20care%2C%20homeland%20security%2C%20immigration%2C%20energy%2C%20environmental%20protection%2C%20and%20education.%20This%20background%20allows%20him%20to%20describe%20OIRA%20and%20how%20it%20works%26%23151%3Band%20how%20it%20can%20work%20better%26%23151%3Bfrom%20an%20on-the-ground%20perspective.%20Using%20real-world%20examples%2C%20many%20of%20them%20drawn%20from%20today%26%238217%3Bs%20headlines%2C%20Sunstein%20makes%20a%20compelling%20case%20for%20improving%20cost-benefit%20analysis%2C%20a%20longtime%20cornerstone%20of%20regulatory%20decision-making%2C%20and%20for%20taking%20account%20of%20variables%20that%20are%20hard%20to%20quantify%2C%20such%20as%20dignity%20and%20personal%20privacy.%20He%20also%20shows%20how%20regulatory%20decisions%20about%20health%2C%20safety%2C%20and%20life%20itself%20can%20benefit%20from%20taking%20into%20account%20behavioral%20and%20psychological%20research%2C%20including%20new%20findings%20about%20what%20scares%20us%2C%20and%20what%20does%20not.%20By%20better%20accounting%20for%20people%26%238217%3Bs%20fallibility%2C%20Sunstein%20argues%2C%20we%20can%20create%20regulation%20that%20is%20simultaneously%20more%20human%20and%20more%20likely%20to%20achieve%20its%20goals.%3CBR%3E%20%20%3Cbr%3E%20%20%20In%20this%20highly%20readable%20synthesis%20of%20insights%20from%20law%2C%20policy%2C%20economics%2C%20and%20psychology%2C%20Sunstein%20breaks%20down%20the%20intricacies%20of%20the%20regulatory%20system%20and%20offers%20a%20new%20way%20of%20thinking%20about%20regulation%20that%20incorporates%20human%20dignity%26%23150%3B%20and%20an%20insistent%20focus%20on%20the%20consequences%20of%20our%20choices.%3C%2Fdiv%3E",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    






