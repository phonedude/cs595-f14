<script src="http://cdn.gotraffic.net/assets/speed_read-e43fa71b529b389ac160907c4a918833.js"></script>
<script>
  Bcom.currentPage = new Bcom.Pages.Article();
  PageApi = Bcom.Tracker.Page;
  PageApi.setDescription('blp.news/davos//story');
  PageApi.setWebCategory('davos-news');
  PageApi.setBBAttributor('bn');
  PageApi.setWebCategoryUserAction('Curation');
  PageApi.setStandout("false");
  Bcom.global_var.STORY_GROUP_ID = "MGO45X07SXL001";
  Bcom.global_var.STORY_AUTHOR_NAME = "Catherine Hickley";
  Bcom.global_var.STORY_PUB_TIME = "2013-01-16 18:00:00 -0500";
  Bcom.global_var.IS_NATIVE = false;
  $(function () {
      function flash($el, flashCount) {
        for(var i = 0; i < flashCount; i++) {
          $el.animate({opacity: 0.25}).animate({opacity: 1});
        }
      }
  
      $(".article_body").sharable({
        charLength: 100,
        callback: function(settings){
          flash($(settings.twitterSelector), 4);
        }
      });
  
      function initFlyout(country)
       {
        var sponsorUrl = "http://ad-apac.doubleclick.net/clk;280918608;107718252;k";
        country = country.toLowerCase();
  
        $(".flyout").addClass(country)
                    .flyout({
                             // this is a callback that allows us to define how the data is sourced into the flyout
                             data : function flyoutData(settings)
                                     {
                                      function flyoutContent()
                                       {
                                        var $html,
                                            $recommended = $(settings.track),
                                            $article = $recommended.find("li:not(.instructions):eq(0)"),
                                            html = $article.html(),
                                            thumbnailID = $(html).attr("data-img") || "";
  
                                        $html = $(html);
  
                                        if (country !== "us" && thumbnailID !== "")
                                         $('<img src="http://www.bloomberg.com/image/' + thumbnailID + '.jpg" height="75" width="130">').prependTo($html);
  
                                        if ( !$('#recommended-reading').exists() ){
                                          $(settings.content).before("<span id='recommended-reading'>RECOMMENDED READING</span>");
                                        }
  
                                        $(settings.content).html($html);
  
                                        Bcom.Modules.Flyout.addRecommendedVideo(Bcom.global_var.STORY_GROUP_ID);
  
                                        return $(settings.content);
                                       }
                                      function transitionFlyout(action, data)
                                       {
                                        var $container = data.container,
                                            map = {
                                                   close : ['removeClass', -600],
                                                   open : ['addClass', 0]
                                                  },
                                            settings = data.settings;
  
                                        if (transitionSupport === true)
                                         $container[map[action][0]](settings.show)
                                        else
                                         $container.animate({
                                                             right: map[action][1]
                                                            },
                                                            250);
                                       }
  
                                      var $this = this,
                                          impression = false,
                                          transitionSupport = typeof document.documentElement.style.transition === "string";
  
                                      Bcom.Tracker
                                          .Events
                                          .bindTracking('x.flyout',
                                                        [
                                                         {
                                                          action: 'right-module_flyoutclose',
                                                          category: 'link',
                                                          label: window.location.href,
                                                          target: $(document),
                                                          trackInComscore: false
                                                         }
                                                        ]);
  
                                      Bcom.Tracker
                                          .Events
                                          .bindTracking('show.flyout',
                                                        [
                                                         {
                                                          action: 'article_flyout',
                                                          category: 'impression',
                                                          label: window.location.href,
                                                          target: this,
                                                          trackInComscore: false
                                                         }
                                                        ]);
  
                                      Bcom.Tracker
                                          .Events
                                          .bindTracking('mousedown',
                                                        [
                                                         {
                                                          action: 'right-module_flyout',
                                                          category: 'link',
                                                          label: window.location.href,
                                                          target: this.find(".content"),
                                                          comscoreTrackAtEndOfSession: true,
                                                          comscoreParams: {
                                                                           area: 'right-module',
                                                                           feature: 'flyout',
                                                                           ns_type: 'standard',
                                                                           position: 'bottom'
                                                                          }
                                                         }
                                                        ]);
  
                                      $(document).on("recommended",
                                                     flyoutContent)
                                                 .on("stories.article",
                                                     function articleMoreStoriesEvent(evt)
                                                      {
                                                       $this.trigger((($(".sidebar_on").length > 0)
                                                                      ? "close"
                                                                      : "open") + ".flyout");
                                                      })
                                                 $this.on("hide.flyout",
                                                     function flyoutHideEvent(evt, data)
                                                      {
                                                       transitionFlyout('close', data);
                                                      })
                                                 .on("show.flyout",
                                                     function flyoutShowEvent(evt, data)
                                                      {
                                                       transitionFlyout('open', data);
                                                       if (country === "us" && impression === false) {
                                                        (document.createElement('img')).src = " http://ad-apac.doubleclick.net/ad/N4478.285981.BLOOMBERGJP/B8061027;sz=1x1";
                                                        (document.createElement('img')).src = " http://ohlone.vizu.com/a.gif?vzcid=10413&vzadid=bloombergmodule&vzsid=bloomberg&ord=[RANDOM]";
                                                      }
  
                                                 impression = true;
                                                });
  
                                      return flyoutContent().html();
                                     },
                             // this is a callback that allows us to define whether or not the flyout should slide in
                             filter : function flyoutFilter(settings)
                                       {
                                        return ($(".sidebar_on").length > 0 || $(settings.content).html() === "")
                                                 ? true
                                                 : false;
                                       },
                             track : "#bcom_articlepage_right_rail_recommended"
                            });
       }
  
      var country,
          enableCountry = false,
          enableIE8 = false,
          geoIPinstance = BLOOMBERG.geoIPInstance;
  
      if (enableIE8 === true
          || BB.isIE8() === false)
       {
        country = (enableCountry && geoIPinstance.getCountryIfPresent() || "");
  
        if (enableCountry && !country)
         geoIPinstance.getCountry(initFlyout);
        else
         initFlyout(country);
       }
     });
</script>


