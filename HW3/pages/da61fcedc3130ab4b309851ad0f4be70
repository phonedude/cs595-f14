				<!-- Article Headline -->
				<h1>Survey: YouTube Stars More Popular Than Mainstream Celebs Among U.S. Teens</h1>

				<div class="indent">
					<!-- Share Tools -->
					<section class="module-share-tools">

	<div class="share-ui-container">

		<ul class="social-buttons social">
							<li class="share-email">
