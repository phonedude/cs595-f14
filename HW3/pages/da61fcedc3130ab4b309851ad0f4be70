	<article class="hero-image">
		

		<div class="wrap">

			<!-- Article Header -->
			<header>

				<div class="indent">
					<div class="tab">

						<!-- Breadcrumb -->
						<ul class="breadcrumb" itemscope itemtype="http://data-vocabulary.org/Breadcrumb">
							<li itemscope itemtype="http://data-vocabulary.org/Breadcrumb"><a href="/" class="category" itemprop="url" >Home</a></li><li><a href="http://variety.com/v/digital/" class="category  category-digital">Digital</a></li><li><a href="http://variety.com/c/news/">News</a></li>						</ul>

