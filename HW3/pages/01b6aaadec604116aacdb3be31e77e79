jQ(function() {
    jQ('#article-wrapper .factbox-container .factbox').each(function() {
        var node = jQ(this),
            className = node.attr('class');
        node.attr('data-component', 'comp: r2: ' + className).addClass('trackable-component');
    });
});
</script>

                                            				
			        

			            						
					
					
                                                                                                                                                                                                                            

