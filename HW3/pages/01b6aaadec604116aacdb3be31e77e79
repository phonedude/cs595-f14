                function showSponsoredFeaturex31(slotJson) {
                    if (typeof(slotJson) === 'object') {
                        var node = jQ('.json-features.x31');
                        convertJSONtoAd(
                                node.find('img:first-child'),
                                node.find('a.t6'),
                                node.find('p:last-child'),
                                slotJson,
                                node.find('.hd').children().eq(0),
                                'x31'
                        );
                    }
                }

