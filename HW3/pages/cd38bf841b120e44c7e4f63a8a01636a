<!-- MathJax Latex Plugin installed: Disabled as no shortcodes on this page --><script type='text/javascript' src='http://s.radar.oreilly.com/wp-includes/js/comment-reply.min.js?ver=4.0'></script>
<script type="text/javascript">
jQuery(document).ready(function(){
  jQuery('.fluid-width-video-wrapper iframe').each(function(){
    var youtube_url = jQuery(this).attr('src');
    if(youtube_url.substring(0,2) == "//"){
      youtube_url = youtube_url.substring(2);
    }
    var p = /^(?:https?:\/\/)?(?:www\.)?(?:youtu\.be\/|youtube\.com\/(?:embed\/|v\/|watch\?v=|watch\?.+&v=))((\w|-){11})(?:\S+)?$/;
    if(youtube_url.match(p)){
      var youtube = youtube_url.split('/');
      var youtube_last_index = youtube[4].split('?');
      var youtube_id = youtube_last_index[0];
      var new_div = document.createElement("div");
      jQuery(new_div).addClass('js-lazyYT');
      jQuery(new_div).attr('data-youtube-id',youtube_id);
      jQuery(new_div).attr('data-ratio','16:9');
      jQuery(this).after(new_div);
      jQuery(this).parent().css("padding","0");
      jQuery(this).remove();
      jQuery('.lazyYT-title').hide();
    }
  });
  jQuery('.js-lazyYT').lazyYT();
});

</script>
