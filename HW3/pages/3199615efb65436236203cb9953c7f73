          <script type="text/javascript">
            function newPortraitImage(image, width, height)
            {
                var pieces = image.split(/[\s\/]+/);
                pieces.pop(pieces.length - 1);
                pieces[0] = pieces[0] + '/';
                var newpopImg = width + 'x' + height + '.jpg';
                pieces.push(newpopImg)
                return pieces.join("/");
            }
            function galleriaNewIframe(iframeHTML)
            {
                var tempEl = document.createElement('div');
                tempEl.innerHTML = iframeHTML;
                var iframeEls = tempEl.getElementsByTagName('iframe');
                var newIframe = document.createElement('iframe');
                newIframe.setAttribute('src', iframeEls[0].getAttribute('src'));
                tempEl.remove();
                newIframe.setAttribute('scrolling', 'no');
                newIframe.setAttribute('frameborder', 0);
                newIframe.setAttribute('marginwidth', 0);
                newIframe.setAttribute('marginheight', 0);
                newIframe.setAttribute('width', 0);
                newIframe.setAttribute('height', 0);
                iframeHTML = newIframe.outerHTML;
                return iframeHTML;
            }
            function galleriaResizeIframe(iframeHTML, width, height)
            {
                iframeHTML = iframeHTML.replace(/(&|&amp;|\?)width=\d+/i,"$1" + 'width=' + width);
                iframeHTML = iframeHTML.replace(/(&|&amp;|\?)height=\d+/gi,"$1" + 'height=' + height);
                iframeHTML = iframeHTML.replace(/ width=(["']?)\d+(["']?)/i,' width=' + "$1" + width + "$2");
                iframeHTML = iframeHTML.replace(/ height=(["']?)\d+(["']?)/i,' height=' + "$1" + height + "$2");
                return iframeHTML;
            }
            function galleriaResizeObject(objectHTML, width, height)
            {
                var tempEl = document.createElement('div');
                tempEl.innerHTML = objectHTML;
                var objectEls = tempEl.getElementsByTagName('object');
                var paramEls = objectEls[0].getElementsByTagName('param');
                for (var i = 0; i < paramEls.length; i++) {
                    if (paramEls[i].getAttribute('name') == 'width') {
                        paramEls[i].setAttribute('value', width);
                    } else if (paramEls[i].getAttribute('name') == 'height') {
                        paramEls[i].setAttribute('value', height);
                    }
                }
                objectHTML = objectEls[0].outerHTML;
                tempEl.remove();
                return objectHTML;
            }

