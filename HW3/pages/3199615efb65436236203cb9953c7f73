            var GALLERIA_IMAGE_LOAD_COUNT = 0;

            var imageData = new Array();
            var winWidth = window.innerWidth;
            var winHeight = window.innerHeight;
            var popImgSize = '832'
            var defaultPortrait = 'http://ww4.dev.hdnux.com/photos/23/75/51/5233243/4/premium_gallery_portrait.png';
            var defaultLandscape = 'http://ww4.dev.hdnux.com/photos/23/75/51/5233243/4/premium_gallery_landscape.png';
            if (winHeight >= winWidth){
				popImgSize = winHeight;
			}else{
				popImgSize = winWidth;
			}
			if(popImgSize > 2000){
				popImgSize = 2000;
			}
            
            
            var numImages = articleGalleryData.length;
            for (var i = 0; i < numImages; i++) {
                var thumb =  articleGalleryData[i]['thumb'] || '';
                var description = articleGalleryData[i]['caption'] || '';
                var title = articleGalleryData[i]['displayTitle'] || '';
                var credit = articleGalleryData[i]['credit'] || '' ;
                var portrait_photo = '';
                var landscape_photo = '';
                var layerHtml = '';
                var isBCV = false;
                var isIframe = false;
                var vidLayer = '';
                var iframeSrc = '';
                portrait_photo = (articleGalleryData[i]['portrait'])
                    ? articleGalleryData[i]['portrait'] : defaultPortrait;
                var output = newPortraitImage(portrait_photo, popImgSize, popImgSize);
                landscape_photo = (articleGalleryData[i]['landscape'])
                    ? articleGalleryData[i]['landscape'] : (defaultLandscape + '#' + i);
                if (articleGalleryData[i]['mediaContent']) {
                    if (HDN.skipGalleryMediaObjects) {
                        continue;
                    }
                    if (articleGalleryData[i]['mediaContent'].match(/<iframe /i)) {
                        vidLayer = galleriaNewIframe(articleGalleryData[i]['mediaContent']);
                        isIframe = true;
                    } else {
                        vidLayer = articleGalleryData[i]['mediaContent'];
                        isBCV = true;
                    }
                } else if (articleGalleryData[i]['item']) {
                    layerHtml = articleGalleryData[i]['item'];
                } else if (articleGalleryData[i]['text']) {
                    if (HDN.skipGalleryFreeformObjects) {
                        continue;
                    }
                    if (articleGalleryData[i]['text'].match(/<iframe /i)) {
                        vidLayer = galleriaNewIframe(articleGalleryData[i]['text']);
                        isIframe = true;
                    } else {
                        LayerHtml = articleGalleryData[i]['text'];
                    }
                } else if (articleGalleryData[i]['Iframe']){
                    iframeSrc = articleGalleryData[i]['Iframe'];
                }
                var temp = {'thumb': thumb, 'image': output, 'big': output, 'description': description,
                    'title' : title, 'credit': credit, 'bizClass': articleGalleryData[i]['bizClass'],
                    'id': articleGalleryData[i]['id']};
                if (layerHtml) {
                    temp.layer = layer_html;
                }
                if (vidLayer) {
                    temp.vidLayer = vidLayer;
                    temp.isBCV = isBCV;
                    temp.isIframe = isIframe;
                }
                if (iframeSrc) {
                    temp.iframe = iframeSrc;
                }
                imageData.push(temp);
            }
            galleria_data = imageData;
          </script>
          
