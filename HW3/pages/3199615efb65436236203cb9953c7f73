  <script type="text/javascript" src="/js/hdn/premium/utils/galleria-1.3.3/galleria-1.3.3.js?r5.22.1.2"></script>
  <script type="text/javascript" src="/js/hdn/premium/utils/galleria-1.3.3/themes/hdn_responsive/galleria.hdn_responsive.min.js?r5.22.1.2"></script>
  <link id="galleria-theme" rel="stylesheet" href="/js/hdn/premium/utils/galleria-1.3.3/themes/hdn_responsive/galleria.hdn_responsive.css?r5.22.1.2">
  <link id="galleria-theme" rel="stylesheet" href="/js/hdn/premium/utils/galleria-1.3.3/themes/hdn_responsive/galleria.hdn_sfgate_responsive.css?r5.22.1.2">

  <script type="text/javascript">
    // Test for omniture functions  --- That are only defined for some premium sites 
    if (typeof window.premiumSlideshowImageEvent === 'undefined') {
        premiumSlideshowImageEvent = function(){};
    }
    if (typeof window.premiumSlideshowAction === 'undefined') {
        premiumSlideshowAction = function(){};
    }
    var GALLERIA_MODE_PLAYING = false;
    var GALLERIA_LIGHTBOX_OPEN_OMNI = false;

    if (window['galleria_data'] !== undefined) {
        Galleria.run('.media', {dataSource: galleria_data, responsive: true, height: 0.5625});
    }
    function galleriaFindChild() {
        if ($('.galleria-stage .galleria-images .galleria-image:nth-child(1)').css('z-index') == 1) {
            return 1;
        }
        return 2;
    }
    function galleriaDoVid(gData) {
        var imgChild = galleriaFindChild();
        var gI = '.galleria-stage .galleria-images .galleria-image:nth-child(' + imgChild + ')';
        var gIL = gI + ' .galleria-layer';
        $(gIL).show();
        $(gIL).width($(gI).width());
        $(gIL).height($('.galleria-container .galleria-stage').height()
            - $('.galleria-container .galleria-bar').height());
        if (gData.isIframe) {
            $(gIL).html(galleriaResizeIframe(gData.vidLayer, $(gIL).width(), $(gIL).height()));
        } else if (gData.isBCV) {
            $(gIL).html(galleriaResizeObject(gData.vidLayer, $(gIL).width(), $(gIL).height()));
            brightcove.createExperiences();
        }
    }
    Galleria.on('image', function(e) {
        premiumSlideshowImageEvent((this.getIndex() + 1),'inline', '24');
        if (GALLERIA_MODE_PLAYING === true){
            premiumSlideshowAction('29');
        }
        $('.galleria-stage .galleria-image .galleria-layer').hide();
        $('.galleria-stage .galleria-image .galleria-layer').empty();
        if (!GALLERIA_LIGHTBOX_OPEN_OMNI && e.galleriaData.vidLayer) {
            galleriaDoVid(e.galleriaData);
        }
    });
    Galleria.on('loadstart', function(e) {});
    function galleriaCreateLBL() {
        var gBCII = '.galleria-lightbox-box .galleria-lightbox-content .galleria-lightbox-image .galleria-image';
        if (!$(gBCII + ' .galleria-layer').length) {
            $(gBCII).prepend('<div class=\'galleria-layer\'></div>');
            $(gBCII + ' .galleria-layer').css({
                'z-index':'99999',
                'position':'relative',
                'top':$('.galleria-lightbox-box .galleria-lightbox-content .galleria-lightbox-image').css('top'),
                'height':$('.galleria-lightbox-box .galleria-lightbox-content .galleria-lightbox-image').height(),
                'width':$('.galleria-lightbox-box .galleria-lightbox-content .galleria-lightbox-image').width(),
            });
        }
        $(gBCII + ' .galleria-layer').hide();
    }
    Galleria.on('lightbox_image', function(e) {
        premiumSlideshowImageEvent((this.getIndex() + 1),'overlay', '23');
        var gData = e.scope._data[e.scope._active];
        var lData = e.scope._data[this._lightbox.active];
        $('.galleria-stage .galleria-image .galleria-layer').hide();
        $('.galleria-stage .galleria-image .galleria-layer').empty();
        if (lData.vidLayer) {
            galleriaCreateLBL();
            var gBCI = '.galleria-lightbox-box .galleria-lightbox-content .galleria-lightbox-image';
            var gBCIIL = gBCI + ' .galleria-image .galleria-layer';
            $(gBCIIL).show();
            if (lData.isIframe) {
                $(gBCIIL).html(galleriaResizeIframe(lData.vidLayer, $(gBCI).width(), $(gBCI).height()));
            } else if (lData.isBCV) {
                $(gBCIIL).html(galleriaResizeObject(lData.vidLayer, $(gBCI).width(), $(gBCI).height()));
                brightcove.createExperiences();
            }
        }
    });
    Galleria.on('lightbox_open', function(e) {
        premiumSlideshowAction('21');
        GALLERIA_LIGHTBOX_OPEN_OMNI = true;
        var gData = e.scope._data[e.scope._active];
        if (gData.vidLayer) {
            galleriaCreateLBL();
            $('.galleria-stage .galleria-image .galleria-layer').hide();
            $('.galleria-stage .galleria-image .galleria-layer').empty();
        }
    });
    Galleria.on('lightbox_close', function(e) {
        premiumSlideshowAction('22');
        GALLERIA_LIGHTBOX_OPEN_OMNI = false;
        var gData = e.scope._data[e.scope._active];
        $('.galleria-lightbox-box .galleria-lightbox-content .galleria-lightbox-image .galleria-image'
            + ' .galleria-layer').hide();
        if (gData.vidLayer) {
            galleriaDoVid(gData);
        }
    });
    Galleria.on('play', function(e) {
      premiumSlideshowAction('27');
      GALLERIA_MODE_PLAYING = true;    
    });
  </script>


