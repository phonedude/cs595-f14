        <a class="prev browse left"><span>LEFT</span></a>
      <ol class="">
      {{#items}}
        <li>
        <div class="news item  odd">
          <div class="thumbnail">
            <div class="thumbnail_container">
              <a href="{{url}}"><img alt="{{headline}}" height="80" src="{{thumbnail_url}}" width="140" />
