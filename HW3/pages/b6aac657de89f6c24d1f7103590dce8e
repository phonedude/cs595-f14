        //hides comment # if it is 0
        function load_post_count(){
            setTimeout(function(){
                var num_comments,
                    num_posts = $('#dsq-num-posts').text(),
                    total_posts = $('#dsq-total-posts').text();
                if(total_posts == ""){
                    num_comments = num_posts;
                }else{
                    num_comments = total_posts;
                }
                if (num_comments != "0" && num_comments != ""){
                    $('#video_info_container li:nth-child(2) h2').html("Comments ("+num_comments+")");
                }
            }, 3500)
        }

        //comment count is loaded with a window.onload function, want to make sure we add count after load
        var oldOnLoad = window.onload;
        if (typeof window.onload != 'function') {
          window.onload = load_post_count;
        } else {
          window.onload = function() {
            if (oldOnLoad) {oldOnLoad();}
            load_post_count();
          }
        }
      var bgVidId = '',
          video_to_play = null;

      if(window.location.hash==="#playlist" && BLOOMBERG.global_var.enable_playlist){
          $('#related_videos').remove();
          $('#most_popular').remove();
          $('#recommended').remove();
          $('#taboola-horizontalx4').remove();
          $('#taboola-grid-2x2-video').remove();
          $('body').addClass('long_form');

          //set timeout because at the point that this page renders, playlist_tout hasn't rendered yet
          setTimeout(function(){
                video_to_play = $('#playlist_tout li.current').next('li').find('.item_content a').attr('href');
          },3000);
      } else {
          window._taboola = window._taboola || [];
          _taboola.push({video:'zVsDKv6sTPu7YrKjycWLYg'});
          _taboola.push({mode:'grid-2x2-video',container:'taboola-grid-2x2-video'});

          setTimeout(function(){
              video_to_play = BLOOMBERG.BVP.nextVideoFromTaboola();
              if (BLOOMBERG.queue){
                  BLOOMBERG.queue.refresh_icon('#taboola-grid-2x2-video, #taboola-horizontalx4');
              }
          }, 7000);
      }

      function receiveOoyalaEvent(playerId, eventName, eventParams) {
          if ((eventName == "countdownComplete") && video_to_play != null){
              BLOOMBERG.tracker.EVENTTRACK.record('VideoAuto',eventName,bgVidId,0,true);
              window.location = video_to_play;
          }
      }

      BLOOMBERG.BVP.initRightRailVideoRecommender();

      //BVP analog of the above Ooyala callback
      $(document).ready(function() {
        try {
          _V_('bvp_container').ready(function(){
            this.on('ended', function() {
              BLOOMBERG.BVP.displayCountdownPanel();
            });
          });
        }
        catch (err){ /* video js throws an exception when it can't find a video player */ }
      });

