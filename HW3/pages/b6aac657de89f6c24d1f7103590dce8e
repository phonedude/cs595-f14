  </script>

<script type="text/javascript">
    $(function(){
        $("#toggle_playlist").click(function(){
            var down = $('#playlist_tout').css('height')=='25px';
            if(down){
                $("#playlist_tout").animate({height:'160px'}, 'slow');
            } else {
                $("#playlist_tout").animate({height:'25px'}, 'slow');
            }

        });
    });
</script>
