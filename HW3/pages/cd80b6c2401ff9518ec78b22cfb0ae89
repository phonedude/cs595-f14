			function get_contact_info(contact) {
				var contact_array = contact.split("|");
				var package = contact_array[0];
				var cipher = contact_array[1];
				var label = contact_array[2];		

				var link = "";
				var char = "";

				for (i=0; i < package.length; i++) {
					if (cipher.indexOf(package.charAt(i))==-1) {
						char = package.charAt(i);
						link += (char);
					} else {     
						char = (cipher.indexOf(package.charAt(i))-package.length+cipher.length) % cipher.length;
						link += (cipher.charAt(char));
					}
				}

				if (label == "") {label = link}

				document.write("<a href='mailto:"+link+"'>"+label+"</a>");
			}
		</script>



