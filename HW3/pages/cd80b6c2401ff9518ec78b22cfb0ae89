<script type="text/javascript">
// Custom javascripts
function post_past_conference() {
	var textobj = document.getElementById('past_conferences');
	var vconferenceid = textobj.value;

  if (vconferenceid == '') {
		document.conference.action = "conference.jsp?tabname=overview";  
  } else {
		document.conference.action = "conference.jsp?confid="+vconferenceid+"&tabname=overview";	
	}
	document.conference.submit();	
	return true;
}
function readmore(id) {
  $("#"+id+" .body").toggleClass('show');
  if  ($("#"+id+" .continue-link span").text() == 'Read More') {
  	$("#"+id+" .continue-link span").text('Hide');
  } else {
  	$("#"+id+" .continue-link span").text('Read More');
  }
}
function post_AttendeeSortOrder(sortcolumn) {	
	document.conference.action = "conference.jsp?confid=93&tabname=attendees&sortcolumn="+sortcolumn+"";
	document.conference.submit();
	return true;
}
function post_show_fullsite() {	
	document.conference.action = "conference.jsp?confid=93&tabname=agenda";
	document.conference.submit();
	return true;
}
function post_show_mobilesite() {	
	document.conference.action = "conference.jsp?confid=93&tabname=agenda&agent=iPhone";
	document.conference.submit();
	return true;
}
</script>

