    <script src="//cdn.taboola.com/libtrc/the-atlantic/tfa.js"></script>
    
    <script type="text/javascript" src="http://admin.brightcove.com/js/BrightcoveExperiences_all.js"></script>
    

    <div class="video-player" id="main-video-player">
        <object id="myExperience3099991193001" class="BrightcoveExperience">
            <param name="bgcolor" value="#FFFFFF" />
            <param name="width" value="640" />
            <param name="height" value="360" />
            <param name="playerID" value="2750365925001" />
            <param name="playerKey" value="AQ~~,AAAABvb_NGE~,DMkZt2E6wO1IXPkNi49fGrHA0kYD2ape" />
            <param name="isVid" value="true" />
            <param name="isUI" value="true" />
            <param name="dynamicStreaming" value="true" />
            <param name="wmode" value="transparent" />
            <param name="language" value="en" />
            
            <param name="linkBaseURL" value="http://www.theatlantic.com/video/index/358557/magical-new-zealand/" />
            
            <param name="@videoPlayer" value="3261128732001" />
            <param name="eventsMap" value="//cdn.theatlantic.com/static/front/flash/brightcove/events_map.xml?v=2" />

            <param name="includeAPI" value="true" />
            <param name="templateLoadHandler" value="onTemplateLoaded" />
            <param name="htmlFallback" value="true" />

            
                <param name="autoStart" value="true" />
            
        </object>

    </div>

    <script type="text/javascript">
        var build_brightcove_experiences = function(){
            // Brightcove should be ready, but if this is loaded
            // via ajax after a YouTube video, it's possible it won't
            // be, because jQuery's DOM insertion of <script> is asynchronous.
            // If this happens, try again in a moment until the script
            // is loaded.
            if (window.brightcove !== undefined) {
                brightcove.createExperiences();
            } else {
                window.setTimeout(build_brightcove_experiences, 250);
            }
        };
        build_brightcove_experiences();
    </script>

    <script type="text/javascript">
        var bcExp;
        var modVP;
        var modExp;
        var modCon;
        var modAd;

        /**
         * Ensure the views actually exist before firing mediaComplete.
         * This prevents players in iframes from throwing errors.
         */
        function handleMediaComplete(){
            if (window.AtlanticVideo && AtlanticVideo.VideoArticleView) {
                $(document).trigger("video:complete");
            };
        }

        /**
         * There are two different Brightcove API's.
         * One works for Flash, one doesn't.
         * We'll try the flash one first.
         * Our goal is to get valid ModVP (player)
         * and ModExp (experience) variables set so we
         * can add mediaComplete and resize handlers, respectively.
         */
        var onTemplateLoaded = function(experienceID) {

            player = brightcove.getExperience(experienceID);
            if (player){  // In HTML5, this will be undefined.
                modVP = player.getModule(APIModules.VIDEO_PLAYER);
                modExp = player.getModule(APIModules.EXPERIENCE);
            } else {
                player = brightcove.api.getExperience(experienceID);
                modExp = player.getModule(brightcove.api.modules.APIModules.EXPERIENCE);
                player = brightcove.api.getExperience(experienceID);
                modVP = player.getModule(brightcove.api.modules.APIModules.VIDEO_PLAYER);
                modExp = player.getModule(brightcove.api.modules.APIModules.EXPERIENCE);
                modCon = player.getModule(brightcove.api.modules.APIModules.CONTENT);
            }
            modVP.addEventListener("mediaComplete", handleMediaComplete);
            modVP.addEventListener(brightcove.api.events.MediaEvent.PLAY, function(){
                // Taboola video tracking
                window._tfa = window._tfa || [];
                window._tfa.push({notify:"action", name: 'video_view'});

            });

        };

        function resizeBrightcovePlayer(evt) {
            var $player = $('#main-video-player');
            if ($player && modExp) {
                var resizeWidth = $player.width();
                var resizeHeight = 9 / 16 * $player.width(); // We don't know the height until we reflow
                modExp.setSize(resizeWidth, resizeHeight);
            }
            return true;
        }

        window.onresize = resizeBrightcovePlayer;
    </script>


    <!-- End of Brightcove Player -->









        
