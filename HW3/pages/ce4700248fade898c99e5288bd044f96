<!-- Size Listeners -->
<script>
Atlantic.Ad.sizeListener(/^boxfeatures/, function($unit, size) {
    if (size !== false) {
        $unit.parent().show();
    }
});

Atlantic.Ad.sizeListener(/.*/, function($unit, size) {
    if (!$unit.attr("data-ad-starts-hidden") || size === false) {
        return;
    }

    $unit.parents('[data-enable-if-ad=' + $unit.attr('id') + ']').show();
});
</script>


