<script>
  //<![CDATA[
    !(function(){
      var formSubmitted = false;
      function findForm(){
        return document.getElementById('new_signature');
      }
    
      function findButton(){
        var button;
        var anchors = findForm().getElementsByTagName('a');
        for (var i = 0; i < anchors.length; i++) {
          if (/submit/.test(anchors[i].className)){
            button = anchors[i];
          }
        }
        return button;
      }
    
      function onSubmit(e){
        if (e.preventDefault){
          e.preventDefault();
        }
        formSubmitted = true;
    
        var b = findButton();
        b.className = b.className + " submitting";
        b.setAttribute("disabled", "disabled");
        return false;
      }
    
      function initialize(){
        var f = findForm();
        var b = findButton();
        if (f.addEventListener){
          f.addEventListener('submit', onSubmit, false);
          b.addEventListener('click', onSubmit, false);
        }
        else if (f.attachEvent){
          f.attachEvent('submit', onSubmit);
          b.attachEvent('click', onSubmit);
        }
      }
    
      // to be invoked by petition_sign_form.js when app code is loaded
      Change.checkAndClearSignatureSubmissionCapture = function(){
        var submitted = formSubmitted;
        formSubmitted = false;
        var f = findForm();
        var b = findButton();
        if (f.removeEventListener){
          f.removeEventListener("submit", onSubmit);
          b.removeEventListener("click", onSubmit);
        }
        else if (f.detachEvent){
          f.detachEvent("submit", onSubmit);
          b.detachEvent("click", onSubmit);
        }
        return submitted;
      }
    
      initialize();
    })();
  //]]>
</script>
</form>


</div>
