<script type="text/javascript">
	if (showWelcomeAd == true){
		function reloadScrollBars() {
			document.documentElement.style.overflow = 'auto';  // firefox, chrome
			document.body.scroll = "yes"; // ie only
			$('body').removeClass('hideScroll');
		}
		function unloadScrollBars() {
			document.documentElement.style.overflow = 'hidden';  // firefox, chrome
			document.body.scroll = "no"; // ie only
			$('body').addClass('hideScroll');
		}
		function goBack(){$('.welcome-overlay').hide();reloadScrollBars();}
		$('.welcome-overlay').show();
		unloadScrollBars();
		var timer = 18000;
		var count= timer/1000;
		var timeSpent=0;
		var counter;
		function countdown(){
			count=count-1;
			timeSpent=timeSpent+1;
			if (count <= 0) {
				clearInterval(counter);
				ga('send', 'event', 'Overlay Welcome Ad', 'Auto Redirect to Site');
				goBack();
				//counter ended, do something here
				return;
			}
			//document.getElementById("countdown").innerHTML=count;
		}
		counter=setInterval("countdown();",1000); //1000 will run it every 1 second
		$("#welcomeFrame").attr("src", "/ad_includes/welcome.html");
		$(window).ready(function() {
				ga('send', 'event', 'Overlay Welcome Ad', 'Impression');
			$('#wait').click(function() {
				clearInterval(counter);
				ga('send', 'event', 'Overlay Welcome Ad', 'Skip Ad', 'Time Spent on Ad',timeSpent);
			});
			//setTimeout(function(){document.getElementById("wait").style.display = "block";},3000);
		});
	}
</script>

