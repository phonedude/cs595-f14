<script type="text/javascript">

	jQuery(function($){
	  $(".tweet").tweet({
	    join_text: "auto",
	    username: "AccountingToday",
	    avatar_size: 48,
	    count: 3,
	    auto_join_text_default: "- ",
	    auto_join_text_ed: "- ",
	    auto_join_text_ing: "- ",
	    auto_join_text_reply: "- ",
	    auto_join_text_url: "- ",
	    loading_text: "loading tweets..."
	  });
	});

	$(document).ready(function() {

        $('#tab-twitter').click(function() {

			$('#tab-twitter').removeClass("socialtab-off");
			$('#tab-facebook').removeClass("socialtab-on");
			$('#tab-linkedin').removeClass("socialtab-on");
			
			
			$('#tab-twitter').addClass("socialtab-on");
			$('#tab-facebook').addClass("socialtab-off");
			$('#tab-linkedin').addClass("socialtab-off");
			
			$('#twitter-content').show();
			$('#facebook-content').hide();
			$('#linkedin-content').hide();
          
        });
		
        $('#tab-facebook').click(function() {
	
			$('#tab-twitter').removeClass("socialtab-on");
			$('#tab-facebook').removeClass("socialtab-off");
			$('#tab-linkedin').removeClass("socialtab-on");
			
			
			$('#tab-twitter').addClass("socialtab-off");
			$('#tab-facebook').addClass("socialtab-on");
			$('#tab-linkedin').addClass("socialtab-off");
			
			$('#twitter-content').hide();
			$('#facebook-content').show();
			$('#linkedin-content').hide();
		});
	
	$('#tab-linkedin').click(function() {

			$('#tab-twitter').removeClass("socialtab-on");
			$('#tab-facebook').removeClass("socialtab-on");
			$('#tab-linkedin').removeClass("socialtab-off");
			
			
			$('#tab-twitter').addClass("socialtab-off");
			$('#tab-facebook').addClass("socialtab-off");
			$('#tab-linkedin').addClass("socialtab-on");
			
			$('#twitter-content').hide();
			$('#facebook-content').hide();
			$('#linkedin-content').show();
          
        });
	});

    </script>
