      <h1 class="title light" itemprop="title headline">Live Chat!</h1>
      
      

      

      
      

      
        
        
          

            <div class="byline">

              By

              
                
                  

                  
                     <a href="http://www.fastcompany.com/user/fast-company-staff" itemprop="byline" class="navigate" data-resource-type="user" rel="author">Fast Company Staff</a>
                  
                

              
