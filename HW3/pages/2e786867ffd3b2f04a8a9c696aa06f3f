	$(window).load(function() {
		var author = $('.auth_header h2.auth_name').text();
		var authorFirst = author.charAt(0);
		var authorLast = author.substr(author.indexOf(" ") + 1);
		var mailto = 'mailto:'+authorFirst+authorLast+'@sfchronicle.com';
		$('.auth_sidebar .social-links ul li.email a').attr({'href': mailto, 'onclick':'#'});
	});
</script>


