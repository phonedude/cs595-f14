<script type="text/javascript" >
    jQuery(document).ready(function () {
        var current_href = $(location).attr('href');
        var current_title = $(document).attr('title');
        var relPattrn = /Releases/g;
        var yyyPattrn = /yyy/g;
        var xxxPattrn = /xxx/g;
        var zzzPattrn = /zzz/g;
        var tweet_href = "https://twitter.com/intent/tweet?original_referer=yyy&text=xxx&tw_p=tweetbutton&url=zzz&via=DARPA";
        current_title = (relPattrn.test(current_href)) ? current_title.substring(11) : current_title;

        tweet_href = tweet_href.replace("yyy", current_href);
        tweet_href = tweet_href.replace("xxx", current_title);
        tweet_href = tweet_href.replace("zzz", current_href);

        $("#ctl00_SocialBar_sbh_0").attr("href", tweet_href);

        ctl00_SocialBar_sbh_0

    });
