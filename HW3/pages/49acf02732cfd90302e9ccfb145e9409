     <script>$(function() {
             // syntax highlighting for code blocks
             $("pre > code").each(function(i, node) {
                 // specify the code language
                 // TODO: this won't be needed once using JATS 1.1
                 var $node = $(node);
                 var preformatType = $node.attr('data-jats-preformat-type');

                 if (preformatType && preformatType !== 'code') {
                     $node.addClass('language-' + preformatType);
                 }

                 hljs.highlightBlock(node);
             });

             // initialise the follow button
             PeerJ.Event.Follow.init(true);
     });</script>
