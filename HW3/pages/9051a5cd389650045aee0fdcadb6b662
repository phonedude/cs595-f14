		$( document ).ready(function() {

			// Add class for JS-specific styles/functionality
			$("html").removeClass("no-js").addClass("js");

			/* On document ready and on blur, check if Get Updates email input
			   has user data. Ignore spaces-only data using trim. If user has
			   entered data, hide overlaid label. If not, show label. */
			var getUpdatesInput = $("#content #secondary #email_updates input#email");
			var getUpdatesLabel = $("#content #secondary #email_updates label.email");
			function evalGetUpdates() {
				if(!$.trim(getUpdatesInput.val())) { // null after a trim
					getUpdatesLabel.removeClass("hidden");
				}
				else {
					getUpdatesLabel.addClass("hidden");				
				}
			}
			evalGetUpdates();
			getUpdatesInput.blur(function(){
				evalGetUpdates();
			});

		});
	</script>

