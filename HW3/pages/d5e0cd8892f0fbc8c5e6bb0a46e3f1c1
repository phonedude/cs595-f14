    <section class="the_brck">
      <!--start: gradient-->
      <div class="gradient" id="thebrck">
       <div class="center sml-center">
         <h2>THE BRCK</h2>
         <hr class="custom" />
                  <a href="http://www.brck.com/shop/" class="pill_button">BUY YOURS</a>
       </div>  
      </div>
      <!--start: render-->
      <div class="center"><img src="http://www.brck.com/wp-content/themes/brcknew/assets/img/content/render.jpg"></div>
      <!--start: features-->
      <ul class="features_nav">
        <li><a href="#mobility" class="mobility scroll"><img src="http://www.brck.com/wp-content/themes/brcknew/assets/img/content/icon_mobile.svg">MOBILITY</a></li>
        <li><a href="#versatile" class="versatile scroll"><img src="http://www.brck.com/wp-content/themes/brcknew/assets/img/content/icon_versatile.svg">VERSATILE</a></li>
