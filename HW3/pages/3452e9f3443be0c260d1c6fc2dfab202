<script type="text/javascript">
    // Tracking for every page for Downloads module
    $(document).ready(function () {
        // Downloads RR-article-header-section component
        $("div.article-header.section div.downloads a").each(function () {
            $(this).click(function () {
                VELIR.DMS.RaiseSecondarySimpleDownloadEvent($(this).get(0));
            });
        });

        // Downloads RR component
        $("div.secondary div.downloads a").each(function () {
            $(this).click(function () {
                VELIR.DMS.RaiseSecondarySimpleDownloadEvent($(this).get(0));
            });
        });

        // Downloads below primary content
        $("div.primary div.downloads a").each(function () {
            $(this).click(function () {
                VELIR.DMS.RaisePrimarySimpleDownloadEvent($(this).get(0));
            });
        });
    });
</script>
<!-- End of DMS Simple Download Analytics Code -->




