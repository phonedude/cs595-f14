<script type="text/javascript" language="javascript">
    
    function triggerLinkEvent(obj, linkTitle, eventName, eVarName, eVarValue, usePageNameForEvarValue, isCustomLink) {
        var s = s_gi(s_account);
        if (eventName != '') {
            s.linkTrackVars = 'events';
            s.linkTrackEvents = eventName;
            s.events = eventName;
        }
        if (eVarName != '') {
            if (s.linkTrackVars == 'None')
                s.linkTrackVars = eVarName;
            else
                s.linkTrackVars = eVarName + "," + s.linkTrackVars;
            var myEval;
            if (usePageNameForEvarValue)
                myEval = "s." + eVarName + " = '" + "" + "';";
            else
                myEval = "s." + eVarName + " = '" + eVarValue + "';";
            eval(myEval);
        }
        if (isCustomLink) {
            if (s.linkTrackVars == 'None')
                s.linkTrackVars = "prop27,prop28";
            else
                s.linkTrackVars = "prop27,prop28," + s.linkTrackVars;

            s.prop27 = linkTitle;
            s.prop28 = "";
        }
        s.tl(obj, 'o', linkTitle);
    }

</script>

