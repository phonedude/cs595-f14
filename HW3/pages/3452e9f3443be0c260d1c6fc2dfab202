<script type="text/javascript">
	$(document).ready(function() {
		VELIR.PRT.CurrentPage = '{4F5B7A33-A5BC-493A-9D4F-CBA92749ABC2}';
		var suppressModal = VELIR.PRT.HasNewsletterCookie() || VELIR.PRT.HasSeenModal() || false || $(window).width() < 700;
		VELIR.PRT.TriggerDmsEvent = true;
		VELIR.PRT.TriggerEmailModalEvent = true && !suppressModal;
		VELIR.PRT.EnableFullReadEvent = true;
		VELIR.PRT.ReadTime = 10000;
        
		if (VELIR.PRT.EnableFullReadEvent) {
			// Set ViewHeight and Threshold based on window and doc heights
			VELIR.PRT.ViewHeight = $(window).height();
			VELIR.PRT.ViewHeightThreshold = .7 * $(document).height();
			VELIR.PRT.MaxHeightViewed = VELIR.PRT.ViewHeight;

			$(window).scroll(VELIR.PRT.OnScroll);
			setTimeout(VELIR.PRT.OnReadTimeElapsed, VELIR.PRT.ReadTime);
		}
	});
</script>
<!-- End of PageReadTracker (PRT) Read Event Analytics Code -->


