    <script type="text/javascript">

        function launchVideo(uri, containerId) {
            var ndnv = document.createElement('script');
            ndnv.setAttribute('type','text/javascript');
            ndnv.src = uri;
            document.getElementById(containerId).appendChild(ndnv);
        }

         try
        {
            if (
                    typeof _meterProcessor != 'undefined'  // Content item is (potentially), metered...
                            &&
                            _meterProcessor.isBeyondLimit()  // ...item at or beyond, subscription 'wall'...
                            &&
                            !_meterProcessor.hasMessageExclusionMark()  // ...so, go ahead and show....
                            &&
                            _meterProcessor.hasMessageEvent()  // ...'wall' message, but no NDN video.
                    )
            {
                if (window.console && console.log)
                    console.log('ndn display omitted');
            }
            else
            {
                launchVideo(
                        'http://embed.newsinc.com/thumbnail/embed.js?wid=20999&parent=ndn_launcher_20999',
                        'ndn_launcher_20999');

                if (window.console && console.log)
                    console.log('ndn display called');
            }
        }
        catch (e)
        {
            if (window.console && console.log)
                console.log('Error checking properties for NDN exclusion: ' +
                        e.message);
        }

    </script>

