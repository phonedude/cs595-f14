  <div class="content">  <b>JUMP TO: </b><br />
  <div class="month_pkr_wrap">
    <input type="button" value="A Specific Month" id="month_pkr_button">
    <input type="text" id="month_pkr" class="mtz-monthpicker-widgetcontainer">
  </div>
  <form id="blog-changer" action="" method="post">
      <select id="blog-jump-nav" name="blog-nav">
          <option>Other White House Blogs</option>
          <option value="http://www.whitehouse.gov/blog">The White House Blog</option>
          <option value="http://www.whitehouse.gov/strongmiddleclass/blog">Middle Class Task Force</option>
          <option value="http://www.whitehouse.gov/administration/eop/cea/blog">Council of Economic Advisers</option>
          <option value="http://www.whitehouse.gov/administration/eop/ceq/blog">Council on Environmental Quality</option>
          <option value="http://www.whitehouse.gov/administration/eop/cwg/blog">Council on Women and Girls</option>
          <option value="http://whitehouse.gov/omb/blog">Office of Management and Budget</option>
          <option value="http://www.whitehouse.gov/administration/eop/ope/blog">Office of Public Engagement</option>
          <option value="http://www.whitehouse.gov/administration/eop/ostp/blog">Office of Science &amp; Tech Policy</option>
          <option value="http://www.whitehouse.gov/administration/eop/oua/blog">Office of Urban Affairs</option>
          <option value="http://whitehouse.gov/open/blog"> Open Government</option>
          <option value="http://www.whitehouse.gov/administration/eop/ofbnp/blog">Faith and Neighborhood Partnerships</option>
          <option value="http://www.ustr.gov/about-us/press-office/blog">US Trade Representative</option>
          <option value="http://ofsubstance.gov">Office National Drug Control Policy</option>
      </select>
      <input type="submit" value="Go" id="blog-submit" />
  </form>

  <script type="text/javascript">
    /**
     * Date Picker
     */

    $(function() {

      var currentDate = new Date();
      var currentYear = currentDate.getFullYear();
      var currentMonth = currentDate.getMonth() + 1;
      var futureMonths = new Array();

      for (i = currentMonth + 1; i <= 12; i++) {
        futureMonths.push(i);
      }

      // Define options and build the month picker.
      var options = {
        pattern: 'yyyy/mm',
        selectedYear: currentYear,
        startYear: 2009,
        finalYear: currentYear,
        openOnFocus: false // Let's use a button to show the widget.
      };

      $('#month_pkr').monthpicker(options);

      // Set up disabled months on the first load, and re-evaluate for any change in the year.
      $('#month_pkr').monthpicker('disableMonths', futureMonths);
      $('#month_pkr').monthpicker().bind('monthpicker-change-year', function (e, year) {
        $('#month_pkr').monthpicker('disableMonths', []); // (re)enables all
        if (year === String(currentYear)) {
          $('#month_pkr').monthpicker('disableMonths', futureMonths);
        }
      });

      // Show the widget on button click.
      $('#month_pkr_button').bind('click', function () {
        $('#month_pkr').monthpicker('show');
      });

      // When a month is chosen, browse to that page.
      $('#month_pkr').monthpicker().bind('monthpicker-click-month', function (e, month) {
        window.location = '/blog/' + $('input#month_pkr').val();
      })

      // Blog dropdown functionality.
      $("#blog-submit").hide();
      $("#blog-changer select").change(function() {
        window.location = $("#blog-changer select option:selected").val();
      })
   });
  </script></div>
</div>
