					<h1>Leila Madrone: Solar Energy Roboticist</h1>
					<h2>Otherlab’s Leila Madrone is trying to make solar power finally work</h2>
						
					<div class="metadata" role="contentinfo">
						<div class="metadata" role="contentinfo">
	<div class="byline">By David Schneider 
	    <div>Posted <label style='cursor:default' datetime="">28 Jan 2014 | 17:03 GMT</label></div>
	</div>

	    <div style="float: right;" class="addthis_toolbox addthis_default_style" addthis:title="Leila Madrone: Solar Energy Roboticist"> 	
	    <ul class="inline-list article-share-options" class="addthis_toolbox addthis_default_style">
		    <li><a href="#" class="addthis_button_compact"><span class="share icon" id="shareIconTop">&nbsp;</span><span>Share</span></a> </li>
			<li class="sep">|</li>
		    <li><a href="#" class="addthis_button_email"><span class="email icon">&nbsp;</span><span class="addthis_button_print">Email</span></a></li>
			<li class="sep">|</li>
		    <li><a href="#" class="addthis_button_print"><span class="print icon">&nbsp;</span><span>Print</span></a></li>
		</ul>
	</div>

</div>
		
						

<script type="text/javascript">
var addthis_config = {
	ui_cobrand: "IEEE Spectrum", 
	data_track_clickback:true, 
	ui_508_compliant: true, 
	data_ga_property: 'UA-747464-1'
}

var addthis_share = {
	    url_transforms: {
	        clean: true, 
	        shorten: {
				twitter: 'bitly'
		    }
	    },
	    shorteners: {
			bitly: {
				login: 'ieeespectrum',
				apiKey: 'R_b7de0f8b69a2fad0397ee4360bf00ea4'
			}
