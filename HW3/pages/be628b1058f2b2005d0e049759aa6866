  <script type="text/javascript">
  	/**
	 * Displays a modal to capture email addresses.
	 * @requires jQuery
	 * @requires Bootstrap.modal
	 */
	var emailCollector = (function($) {
	    window.google_conversion_id = 999433887;
	    window.google_conversion_language = "en";
	    window.google_conversion_format = "2";
	    window.google_conversion_color = "ffffff";
	    window.google_conversion_label = "Rst2CInhjwQQn83I3AM";
	    window.google_conversion_value = 0;

	    var _baseUrl = 'https://www.chairish.com/';

	    var signupDate = function() {
	        var d = new Date();
	        var mm = d.getMonth().toString();
	        if (mm.length == 1) mm = '0' + mm;
	        var dd = d.getDate().toString();
	        if (dd.length == 1) dd = '0' + dd;
	        return [d.getFullYear(), mm, dd].join('-');
	    };

	    var _trackSuccess = function(trackingUrl) {
	        trackingUrl = trackingUrl || '/email/create/success';
	        window._gaq && _gaq.push(['_setCustomVar', 1, 'Email Signup', signupDate(), 1]);
	        window._gaq && _gaq.push(['_trackPageview', trackingUrl]);
	    }

	    // Initialize the inline signup form with a hack:
	    // - Load the modal form from the chairisher with the time-based signature.
	    // - Inject the email address included in the inline form.
	    // - Submit the data from the modal form.
	    $('.inline-email-signup').on('submit', function(e) {
	        e.preventDefault();

	        var inlineForm = $(this);
	        var email = inlineForm.find('[name=email]').val();

	        if (!email) return;

	        inlineForm.find('.msg').text('Saving...');

	        $.ajax({
	            url: _baseUrl+'/email/create',
	            dataType: 'jsonp',
	            success: function(json) {
	                var modalContent = $('<div></div>').html(json.html);
	                var modalForm = modalContent.find('form');
	                modalForm.find('#id_email').val(email);

	                $.ajax({
	                    url: modalForm.attr('action'),
	                    dataType: 'jsonp',
	                    data: modalForm.serialize(),
	                    success: function(json, textStatus, jqXHR) {
	                        inlineForm.find('.msg').text('Thanks for signing up!');
	                        inlineForm[0].reset();
	                        if (!json.error) {
	                            posted = true;
	                            _setCookie(_tryForNTimes);
	                            _trackSuccess('/email/inline/success');
	                        }
	                    }
	                });
	            }
	        });
	    });
	})(jQuery);
  </script>

