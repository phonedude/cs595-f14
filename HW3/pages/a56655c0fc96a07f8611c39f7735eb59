	( function( $ ) {
		var dayName = [ 'Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday' ];
		var monthName = [ 'January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December' ];
	
		var activeTab = 'first';
	
		function listProgram( data ) {
			var c = $( "#program-grid .content" ),
				sc = $( "#program-speakers" ),
				all_speakers = new Array();
			for ( var session in data ) {
				var sess = $( "<div class=\"session\" />" );
				sess.append( "<div class=\"time\">" + data[session].humanTime + "</div>" );
				sess.append( "<div class=\"name\"><a href=\"http://www.strataconf.com/strata2014/public/schedule/detail/" + data[session].id + "\">" + data[session].name + "</a></div>" );
				
				var speakers = $( "<div class=\"speakers\" />" );
				
				
				for ( var i = 0; i < data[session].speakers.length; i++ ) {
					speakers.append(data[session].speakers[i].name );
                                        if (data[session].speakers[i].title.length ) { speakers.append( ", " + data[session].speakers[i].title ); }
                                        if (data[session].speakers[i].affiliation.length ) { speakers.append( ", " + data[session].speakers[i].affiliation ); }
					if ( i != data[session].speakers.length - 1 ) speakers.append( "; " );
					
					all_speakers.push(data[session].speakers[i]); //add speaker to data listing
				}
				
				

				sess.append( speakers );
				sess.append( "<div class=\"description\">" + data[session].description + "</div>" );
				
				c.append( sess );
			}
			//remove duplicate speakers
			Array.prototype.unique = function() {
				 var hash = {}, result = [];
                  
			    for (var i = 0; i < this.length; i++) {
			    	if (!(this[i].name in hash)) { 
			    		hash[this[i].name] = true;
			    		result.push(this[i]);
			    	}
			    }			    	
			    return result;
			}
			
			var speaker_set = all_speakers.unique();
			
			for ( var i = 0; i < speaker_set.length; i++ ) {
				
				// Add speaker images.
				if ( speaker_set[i].photo ) {
					var s = $( "<div class=\"speaker\"></div>" );
					s.append( "<div class=\"head\"><div class=\"frame\"><img src=\"" + speaker_set[i].photo + "\" alt=\"" + speaker_set[i].name + "\" style=\"border: 0 none;\"><a class=\"border\" href=\"" + speaker_set[i].url + "\"></a></div></div>" );
					s.append( "<div class=\"caption\"><span class=\"name\"><a href=\"" + speaker_set[i].url + "\">" + speaker_set[i].name + "</a></span> <span class=\"affiliation\"></span></div>" );
					sc.append( s );
				}
			}
		}
	
