			var i, c, l = x.length;
			for ( i = 0; i < l; i++ ) {
				c = x[i];
			
				c.date = c.date.replace( /0+([0-9]+)/ig, "$1" );
				c.time = c.time.replace( /^0+([0-9]+)/ig, "$1" );
			
				c.humanDate = c.date;
				c.humanTime = c.time;
