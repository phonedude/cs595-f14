<div id="yt-popup-overlay">
<div id="yt-popup" class="popup">
<div class="popup-header"><a class="close" href="#">close</a></div>
<div class="popup-window">&nbsp;</div>
</div>
</div>
<script type="text/javascript">
$(function() {
    var ytPopupOverlay = $('#yt-popup-overlay'),
        ytPopup = $('#yt-popup'),
        ytPopupClose = ytPopup.find('a.close'),
        ytPopupLoad = 0;

    ytPopupOverlay.prependTo('body');

    $('#yt-popup-frame').load(function() {
        if (ytPopupLoad < 2) {
            ytPopupLoad++;
        } else {
            $('#yt-popup-frame').css('opacity', 0);
            //$('#yt-popup-overlay').animate({ opacity: 0 }, 200);
            ytClosePopup();
        }
        //console.log(ytPopupLoad);
    });

    function startYTPopup(conf) {
        var ytPopup = $('#yt-popup');
        ytShowPopup(); 
    } //startYTPopup()

    function ytClosePopup() {
        ytPopupOverlay.animate({
            'opacity': 0
        }, 300, function() {
            ytPopupOverlay.css('display', 'none');
            $('#yt-popup-frame').remove();
        });
        ytPopupLoad = 1;
    } //ytClosePopup()
    ytPopupOverlay.click(function() {
        ytClosePopup();
    });

    ytPopupClose.click(function(e) {
        e.preventDefault();
        ytClosePopup();
    });

    $(document).keydown(function(e) {
        if (e.which == 27) { //escape key
            ytClosePopup();
        }
    });

    ytPopup.click(function(event) {
        event.stopPropagation();
    });

    function ytShowPopup() {
        ytPopup.css('margin-top', ($(window).scrollTop() + 140) + "px");
        ytPopupOverlay.css({
            'opacity': 0,
            'display': 'block',
            'height': jQuery('html').height()
        }).animate({
            'opacity': 1
        }, 200);
        $('#yt-popup-frame').css({"opacity": 1, "background": "white url('http://cdn.oreillystatic.com/en/assets/1/eventprovider/1/loading-hex999_120x40.gif') center center no-repeat"});
    } //ytShowPopup()
    $('a.yt_link').on('click', function(evt) {
        var conf_video = $(this).data('url'),
            selected_div = $('#popup_content');
        evt.preventDefault();
        startYTPopup();
        $('.popup-window').append('<iframe src="'+ conf_video +'" id="yt-popup-frame" frameborder="0" height="335" width="600"></iframe>') 
        
    });
});
</script>
 










 

