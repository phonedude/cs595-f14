                },
      tools : {
          config : function () {
              $('.apm_sharing_tab').hide();
              $('#apm_sharing_tab_controls li a.hasDrawer').click(function(e) {
                  if($(this).parent().is('.active')) {
                    $('#apm_sharing_tab_controls li').removeClass('active');
                    $('.apm_sharing_tab').hide();
                    e.preventDefault();
                  }
                  else {
                    $('#apm_sharing_tab_controls li').removeClass('active');
                    $(this).parent().addClass('active');
                    var currentTab = $(this).attr('href');
                    $('.apm_sharing_tab').hide();
                    $(currentTab).show();
                    e.preventDefault();
                  }
              });
              $("a.popup").click( function(e) {
                var strFeatures="width=650,height=200,scrollbars=no,menubar=no,height=200,width=650,resizable=no,toolbar=no,location=no,status=no";
                window.open($(this).attr('href'), "popupWindow", strFeatures);
                e.preventDefault();
              });
          }
      }
		});
    $("#apm_player_volume_status, #apm_player_volume_slider_wrapper").hover( function() {
        $("#apm_media_player").css("overflow", "visible");
      },
      function() {
        $("#apm_media_player").css("overflow", "hidden");
      }
    );
