    <script>
        
        function loginSuccess(){
            $('#login-popup').hide();
            $('#login-popup input[type=text], #login-popup input[type=password]').val('');
            //$('#user-menu').show();
            $('#login-popup, #user-menu, #forgot-password-popup').css('display', '');
            $('.user-login').removeClass('loggedout').addClass('loggedin');
            $('.touchfeed').removeClass('hidden');
            $('.flag').removeClass('hidden');
            $('.follow').removeClass('hidden');
            $('.logintext').text('My Account');
            $('.fb-btn-img').css('display', 'none');
        }        
	
      window.fbAsyncInit = function() {
        FB.init({
          appId      : '315314855224709',
          status     : true,
          xfbml      : true
        });	  

        if ($('.user-login').hasClass('loggedin')) {
                loginSuccess();
        }
        else{
                FB.getLoginStatus(function(response) {
                  if (response.status === 'connected' && !$("#flagLogout").length) {
                        // the user is logged in and has authenticated your
                        // app, and response.authResponse supplies
                        // the user's ID, a valid access token, a signed
                        // request, and the time the access token 
                        // and signed request each expire
                        $.ajax({
                                url: '/website/ajax/login.php',
                                data: {auth: JSON.stringify(response.authResponse)},
                                type: 'POST',
                                dataType: 'html'
                        }).done(function (msg) {
/* 					localStorage.setItem("loggedIn", "1");
                                localStorage.setItem("autoLogin", "1");
*/					if(msg == '1'){
                                        loginSuccess();
                                }else if(msg == '0'){
                                        $.ajax({
                                                url: '/website/ajax/signup.php',
                                                data: {auth: JSON.stringify(response.authResponse)},
                                                type: 'POST',
                                                dataType: 'html'
                                        }).done(function (msg) {
                                                loginSuccess();
                                        });					
                                } 
                        });						
                  } else if (response.status === 'not_authorized') {
                        // the user is logged in to Facebook, 
                        // but has not authenticated your app
                  } else {
                        // the user isn't logged in to Facebook.
                  }
                 });	
        }		
  
      };

      (function(d, s, id){
         var js, fjs = d.getElementsByTagName(s)[0];
         if (d.getElementById(id)) {return;}
         js = d.createElement(s); js.id = id;
         js.src = "//connect.facebook.net/en_US/all.js";
         fjs.parentNode.insertBefore(js, fjs);
       }(document, 'script', 'facebook-jssdk'));
    </script><!-- PC Software Download popup -->
	
