<script>
var getUrlVars = function(){
  var vars = { };
  var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
    vars[key] = value;   
  });
  return vars;   
};

var stype_filters = getUrlVars(); 

var filterSessions = function (session_filter, stype_name, stype_desc) {
	$('#filters > a').removeClass('filtering');
    session_filter.addClass('filtering');
	$('.stype_description').hide();
	$('#en_proceedings').before(stype_desc);
	stype_desc.show();

	$('.en_session').show();//show all by default
	if (stype_name != "All") {
		$('.en_session').each(function() {
		    var current_stype = $(this).data('sessionType');
		    if (current_stype != stype_name){
		    	$(this).hide();
		    } 
		});
	}
	
};

$(function() {

	$('#filters > a').each(function() {
		var stype_description_id = $(this).data('description');
		var stype_description = $('#' + stype_description_id);
		var stype_name = $(this).data('filter');

		if (stype_filters.filter == stype_description_id) {
			filterSessions($(this), stype_name, stype_description);
		}

		$(this).on('click', function(e) {
			e.preventDefault();
	        filterSessions($(this), stype_name, stype_description);
			
		});

	});
});
</script>


