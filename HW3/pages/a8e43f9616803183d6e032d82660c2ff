						function checkIfDefault(keyword) {
							var defaultSearchTerm = "Search";
							var defaultEmailTerm = "Enter Email";
								if((keyword == defaultSearchTerm) || (keyword == defaultEmailTerm)) {
									return '';
								}
								else {
									return keyword;
								}
							}
