			function popUp(URL,width,height) {
				day = new Date();
				id = day.getTime();
				eval("page" + id + " = window.open(URL, '" + id + "', 'toolbar=0,scrollbars=0,location=0,status=0,menubar=0,resizable=no,width="+width+",height="+height+",left = 640,top = 175');");
			}
			
			YAHOO.namespace("example.container");
	function getStatistics() {
		YAHOO.example.container.dialog1.show();
		var content = document.getElementById("content");        
    	//content.innerHTML = "";
    
	     var callback = {
	            success : function(o) {
	                content.innerHTML = o.responseText;
	            },
	            failure : function(o) {
	                content.innerHTML = o.responseText;
	               
	                content.innerHTML = "CONNECTION FAILED!";
	            }
	        }
	        // Connect to our data source and load the data
	        var conn = YAHOO.util.Connect.asyncRequest("GET", "statistics.html?command=MidStatistics&mid=3103", callback);
    }

	function init() {
	
	// Define various event handlers for Dialog

	// Instantiate the Dialog
	YAHOO.example.container.dialog1 = new YAHOO.widget.Dialog("statisticContent", 
							{ width : "650px",
							  zindex:4,
							  fixedcenter:true,  
							  close: true,
							  draggable: true,
							  draggable: false, 
							  modal: true,
							  visible : false
							  
							});

	YAHOO.example.container.dialog1.render();
	
	//YAHOO.util.Event.addListener("stati", "click", YAHOO.example.container.dialog1.show, YAHOO.example.container.dialog1, true);
	YAHOO.util.Event.addListener("stati", "click",getStatistics);
	}
	//YAHOO.util.Event.addListener("show", "click", getStatistics(););
YAHOO.util.Event.onDOMReady(init);
