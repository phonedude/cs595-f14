	<script type="text/javascript">
		Event.observe(window, 'load', function() {
	   if (!window.XMLHttpRequest) {
	      // IE6, older browsers       //new HoverBehavior('tr');
	  	btn=$$('span.tool3')[0];
	  		  	Event.observe(btn, 'mouseover', function(){
		  		  	e=$$('span.tip3')[0];
		  		  	e.setStyle({
					display: 'block',
					 zIndex: '100',
					 position: 'absolute',
					 width: '130px',
					 top: '1.2em',
					 left: '0',
					 lineHeight: '1.2em',
					 padding:'10px',
					 border: '1px solid #999999',
					 backgroundColor: '#f7f7ee',
					 fontFamily:' arial, helvetica, sans-serif',
					 fontSize: '10px',
					 fontWeight: 'normal',
					 color: '#333333',
					 textAlign: 'right'
					});
	  		  	});
		   		Event.observe(btn, 'mouseout', function(){
		   		e=$$('span.tip3')[0];
		   			e.setStyle({
					display: 'none'
					});
		   		});      	        	  
	    }
	   
	});


		(function($) {
		    $.fn.tipsy = function(opts) {

		        opts = $.extend({fade: false, gravity: 'n'}, opts || {});
		        var tip = null, cancelHide = false;

		        this.hover(function() {
		            
		            $.data(this, 'cancel.tipsy', true);

		            var tip = $.data(this, 'active.tipsy');
		            if (!tip) {
		                tip = $('<div class="tipsy"><div class="tipsy-inner">' + $(this).attr('title') + '</div></div>');
		                tip.css({position: 'absolute', zIndex: 100000});
		                $(this).attr('title', '');
		                $.data(this, 'active.tipsy', tip);
		            }
		            
		            var pos = $.extend({}, $(this).offset(), {width: this.offsetWidth, height: this.offsetHeight});
		            tip.remove().css({top: 0, left: 0, visibility: 'hidden', display: 'block'}).appendTo(document.body);
		            var actualWidth = tip[0].offsetWidth, actualHeight = tip[0].offsetHeight;
		            
		            switch (opts.gravity.charAt(0)) {
		                case 'n':
		                    tip.css({top: pos.top + pos.height, left: pos.left + pos.width / 2 - actualWidth / 2}).addClass('tipsy-north');
		                    break;
		                case 's':
		                    tip.css({top: pos.top - actualHeight, left: pos.left + pos.width / 2 - actualWidth / 2}).addClass('tipsy-south');
		                    break;
		                case 'e':
		                    tip.css({top: pos.top + pos.height / 2 - actualHeight / 2, left: pos.left - actualWidth}).addClass('tipsy-east');
		                    break;
		                case 'w':
		                    tip.css({top: pos.top + pos.height / 2 - actualHeight / 2, left: pos.left + pos.width}).addClass('tipsy-west');
		                    break;
		            }

		            if (opts.fade) {
		                tip.css({opacity: 0, display: 'block', visibility: 'visible'}).animate({opacity: 1});
		            } else {
		                tip.css({visibility: 'visible'});
		            }

		        }, function() {
		            $.data(this, 'cancel.tipsy', false);
		            var self = this;
		            setTimeout(function() {
		                if ($.data(this, 'cancel.tipsy')) return;
		                var tip = $.data(self, 'active.tipsy');
		                if (opts.fade) {
		                    tip.stop().fadeOut(function() { $(this).remove(); });
		                } else {
		                    tip.remove();
		                }
		            }, 100);
		            
		        });

		    };
		})(jQuery);
		jQuery(function() {
		    var button = jQuery('#loginButton');
		    var box = jQuery('#loginBox');
		    var form = jQuery('#loginForm');
		    button.removeAttr('href');
		    button.mouseup(function(login) {
		    	if (box.is(":visible")) {
		        	box.slideUp(500, function() {
		        		button.toggleClass('active');
		        	});
	        		jQuery('#loginOverlay').fadeOut();
		    	} else {
		    		jQuery('#loginOverlay').fadeIn();
			        button.toggleClass('active');
			        box.slideDown(500);			        	
			        
		    	}
		    });
		    form.mouseup(function() { 
		        return false;
		    });
		    jQuery(this).mouseup(function(login) {
		    	if (jQuery(login.target).hasClass("loginButtonImage")) {
		    		return;
		    	}		    	
		        if(!(jQuery(login.target).parent('#loginButton').length > 0)) {
		            button.removeClass('active');
		            box.slideUp();
	            	jQuery('#loginOverlay').fadeOut();
		        }
		        setTimeout(function() {
		    	jQuery("#loginBody").show();
		    	jQuery("#loginForm").show();
	    		jQuery("#forgot").hide();
		        },500);
		    });
		    jQuery("#forgotLink").click(function() {
		    	if (jQuery("#forgot").is(":visible")) {
		    		jQuery("#forgot").fadeOut(200, function() {
		    			jQuery("#loginBody").fadeIn(200);
		    		});
		    	} else {
		    		jQuery("#loginBody").fadeOut(200, function() {
		    		jQuery("#forgot").fadeIn(200);
		    		});
		    	}
		    });
		    
		    jQuery("#forgotButton").click(function() {
				var email = jQuery("#forgotEmail").val();
			 	jQuery.ajax({type:'GET', url: 'ajaxLoginRegister.html?command=forgotPassword&email='+email, success: function(response) {
					var responseObject = jQuery.parseJSON(response);
					if (responseObject.status=='success') {
						jQuery("#forgotsuccess").html(responseObject.message);
						jQuery("#forgoterror").html("");
					} else {
						jQuery("#forgotsuccess").html("");
						jQuery("#forgoterror").html(responseObject.message);
					}
			 	}});
				return false;
			});
			jQuery('#checkbox').tipsy({gravity: 'e'});   
			jQuery('#checkboxLabel').tipsy({gravity: 'e'});   
			jQuery('#ecornerLoginLinkImg').tipsy({gravity: 'e'});   
			jQuery('#googleLoginLinkImg').tipsy({gravity: 'e'});   
	    	jQuery("#loginForm").show();
    		jQuery("#forgot").hide();

		    
			
		    
			
		    jQuery("#googleLoginLink").click(function() {
		    	jQuery.ajax({type:'GET', url: 'openId.html', success: function(response) {
		    		jQuery("#openIdRedirectDiv").html(response);
		    		document.forms['openid-form-redirection'].submit();
		    	}});
		    	jQuery("#googleLoginLink").click(function() {});
		    	jQuery("#ecornerLoginLink").click(function() {});
		    	jQuery("#googleLoginLinkImg").attr("src","/gfx/login/preloader4.gif");
		    	jQuery("#googleLoginLinkImg").css("padding-top","8px");
		    });
			
		});		

		jQuery(function() {
		    var button = jQuery('#registerButton');
		    var box = jQuery('#registerBox');
		    var form = jQuery('#registerForm');
		    button.removeAttr('href');
		    
		    button.mouseup(function(register) {
		    	if (box.is(":visible")) {
		        	box.slideUp(500, function() {
		        		button.toggleClass('active');
		        	});
	        		jQuery('#registerOverlay').fadeOut();
		    	} else {
		    		jQuery('#registerOverlay').fadeIn();
			        button.toggleClass('active');
		        	box.slideDown(500);
		        	trackRegistration('OpenRegisterDialog', null, null);
		    	}
		    });
		    form.mouseup(function() { 
		        return false;
		    });

		    jQuery(this).mouseup(function(register) {
		    	if (jQuery(register.target).hasClass("registerButtonImage")) {
		    		return;
		    	}		    	
		        if(!(jQuery(register.target).parent('#registerButton').length > 0)) {
		            button.removeClass('active');
		            box.slideUp();
	            	jQuery('#registerOverlay').fadeOut();
		        }
		        setTimeout(function() {
			    	jQuery("#registerChoice").show();
			    	jQuery("#registerForm").hide();
					jQuery("#registererror").html("");
		        },500);
		        
		    });
		    
		    jQuery("#ecornerRegisterLink").click(function() {
	        	trackRegistration('OpenRegisterDialogECorner', null, null);
		    	jQuery("#registerChoice").fadeOut(200, function() {
		    		jQuery("#registerForm").fadeIn(200);
		    	});
		    });
		    jQuery("#googleRegisterLink").click(function() {
	        	trackRegistration('OpenRegisterDialogGoogle', null, null);
		    	jQuery.ajax({type:'GET', url: 'openId.html', success: function(response) {
		    		jQuery("#openIdRedirectDiv").html(response);
		    		document.forms['openid-form-redirection'].submit();
		    	}});
		    	jQuery("#googleRegisterLink").click(function() {});
		    	jQuery("#ecornerRegisterLink").click(function() {});
		    	jQuery("#googleRegisterLinkImg").attr("src","/gfx/login/preloader4.gif");
		    	jQuery("#googleRegisterLinkImg").css("padding-top","3px");
		    });
/* 		    jQuery("#otherRegisterBenefitsLink").click(function() {
		    	if (jQuery("#otherRegisterBenefits").is(":visible")) {
		    		jQuery("#otherRegisterBenefits").fadeOut(200);
		    	} else {
		    		jQuery("#otherRegisterBenefits").fadeIn(200, function() {
		    			jQuery("#otherRegisterBenefitsLink").fadeOut(200);
		    		});
		    	}
		    });
 */		    
		    jQuery("#register").click(function() {
				var params = jQuery("#registerForm").serialize();
			 	jQuery.ajax({type:'GET', url: 'ajaxLoginRegister.html?command=register&'+params, success: function(response) {
					var responseObject = jQuery.parseJSON(response);
					if (responseObject.status=='success') {
						
                		
						
							trackRegistration('Ecorner', 'Profile', jQuery("#registerProfile").val());
						
						
						
						
						
						 	window.location = "index.html?status=welcome";
						
						
					} else {
						jQuery("#registererror").html(responseObject.message);
					}
			 	}});
				return false;
			});
		    jQuery("#registerEmail").blur(function() {
		    	jQuery(this).mailcheck({
		    	    suggested: function(element, suggestion) {
		    	    	jQuery("#emailSuggestionLink").html(suggestion.full);
		    	    	jQuery("#emailSuggestionDiv").fadeIn(200);
		    	    },
		    	    empty: function(element) {
		    	    	jQuery("#emailSuggestionDiv").fadeOut(200);
		    	    }
		    	});
		    });
		    jQuery("#emailSuggestionLink").click(function() {
    	    	jQuery("#emailSuggestionDiv").fadeOut(200, function() {
    	    		jQuery("#registerEmail").val(jQuery("#emailSuggestionLink").html());
    	    	});
		    });
		    
			jQuery('#ecornerRegisterLinkImg').tipsy({gravity: 'e'});   
			jQuery('#googleRegisterLinkImg').tipsy({gravity: 'e'});   
		    
		});		
	    
		function trackRegistration(category, label, value) {
		    
		    
			
			_gaq.push(['_setAccount', 'UA-101591-4']);
			
			    

			_gaq.push(['_trackEvent', 'Register', category, label, value]);
			return true;
		}

</script>

