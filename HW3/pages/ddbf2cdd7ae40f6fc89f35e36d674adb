									    } else {  //Others
									        script.onload = function(){
									            callback();
									        };
									    }
									    script.src = url;
									    document.getElementsByTagName("head")[0].appendChild(script);
									}					      	
								    jQuery(document).ready(
								    	function() {
													jwplayer('player').setup({
												        flashplayer: '/swf/mediaplayer-5.10/player.swf',
