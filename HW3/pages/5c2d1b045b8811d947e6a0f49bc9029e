<script language="javascript" type="text/javascript">
//<![CDATA[
var UA = navigator.userAgent.toLowerCase();
var isIE = (UA.indexOf('msie') >= 0) ? true : false;
if(isIE)
    {
    var ofs=document.documentElement.offsetHeight-document.body.offsetHeight;
    if(document.documentElement.offsetHeight>document.body.offsetHeight)
        {
        document.getElementById("divContent").style.height=document.getElementById("divContent").offsetHeight+ofs+"px";
        }
    }
else
    {
    var ofs=document.documentElement.clientHeight-document.documentElement.offsetHeight;
    if(document.documentElement.clientHeight>document.documentElement.offsetHeight)
        {
        document.getElementById("divContent").style.height=document.getElementById("divContent").offsetHeight+ofs+"px";
        }
    }
//]]>
</script>
